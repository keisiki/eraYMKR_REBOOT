;--------------------------------------------------------
;오나금주취득 함수 @SELFPLAY_7
;인수 ARG에 캐릭터 번호, 인수 ARG:1에 플레이 내용을 넣어 TRYCALL. 내용에 응해 파라미터나 주를 취득한다.
;인수 ARG:1이  1이외라면 C로, 1이라면 대상을 좋아하는 성감대에서 자위를 실시한다.
;--------------------------------------------------------
@SELFPLAY_7, ARG, ARG:1
LOCAL:4 = TARGET
TARGET = ASSI

;조수중 자위
TARGET = ASSI
TCVAR:10 += 1
TCVAR:2 += 1
;성감대 판정
LOCAL = FAV_FEEL()
;C의 경우
IF LOCAL == 1 || ARG:1 != 1
    PRINTFORML %CALLNAME:TARGET%의 쾌C?  ??＋{ABL:자위중독*300}
    JUEL:0 += ABL:자위중독*300
;B의 경우
ELSEIF LOCAL == 4
    PRINTFORML %CALLNAME:TARGET%의 쾌B?  ??＋{ABL:자위중독*300}
    JUEL:3 += ABL:자위중독*300
;V의 경우
ELSEIF LOCAL == 2
    PRINTFORML %CALLNAME:TARGET%의 쾌V?  ??＋{ABL:자위중독*300}
    JUEL:1 += ABL:자위중독*300
;A의 경우
ELSEIF LOCAL == 3
    PRINTFORML %CALLNAME:TARGET%의 쾌A?  ??＋{ABL:자위중독*300}
    JUEL:2 += ABL:자위중독*300
ENDIF
PRINTFORML %CALLNAME:TARGET%의 욕정?  ??＋{ABL:자위중독*200}
JUEL:13 += ABL:자위중독*200
PRINTFORML %CALLNAME:TARGET%의 굴복?  ??＋{ABL:자위중독*500}
JUEL:14 += ABL:자위중독*500
PRINTFORMW %CALLNAME:TARGET%의 치정?  ??＋{ABL:자위중독*300}
JUEL:15 += ABL:자위중독*300
PRINTL 　
;페니스가 있는 경우
IF TALENT:121 || TALENT:120
    ;-------------------------------------------------
    ;사정 게이지 체크
    ;-------------------------------------------------
    LOCAL:1 = 0
    ;ABL:기교를 본다
    IF ABL:기교 == 0
        LOCAL:1 = 500
    ELSEIF ABL:기교 == 1
        LOCAL:1 = 600
    ELSEIF ABL:기교 == 2
        LOCAL:1 = 700
    ELSEIF ABL:기교 == 3
        LOCAL:1 = 800
    ELSEIF ABL:기교 == 4
        LOCAL:1 = 1000
    ELSE
        LOCAL:1 = 1100
    ENDIF
    ;ABL:자위중독을 본다
    IF ABL:자위중독 == 0
        TIMES LOCAL:1, 0.80
    ELSEIF ABL:자위중독 == 1
        TIMES LOCAL:1, 0.90
    ELSEIF ABL:자위중독 == 2
        TIMES LOCAL:1, 1.00
    ELSEIF ABL:자위중독 == 3
        TIMES LOCAL:1, 1.10
    ELSEIF ABL:자위중독 == 4
        TIMES LOCAL:1, 1.20
    ELSE
        TIMES LOCAL:1, 1.30
    ENDIF
    ;ABL:욕망을 본다
    IF ABL:욕망 == 0
        TIMES LOCAL:1, 1.00
    ELSEIF ABL:욕망 == 1
        TIMES LOCAL:1, 1.10
    ELSEIF ABL:욕망 == 2
        TIMES LOCAL:1, 1.20
    ELSEIF ABL:욕망 == 3
        TIMES LOCAL:1, 1.30
    ELSEIF ABL:욕망 == 4
        TIMES LOCAL:1, 1.40
    ELSE
        TIMES LOCAL:1, 1.50
    ENDIF
        
    ;C자위의 경우 3배로 한다
    SIF LOCAL == 1 || ARG:2 != 1
        LOCAL:1 *= 3
    
    BASE:TARGET:사정 += LOCAL:1
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(TARGET)

;대량 사정
IF TFLAG:91 == 2
    TCVAR:3 += 2
    TCVAR:4 += 1
    PRINTFORML %CALLNAME:TARGET% 대량 사정
    ;P에 정액 더러움
    STAIN:TARGET:자지 |= 1p2
    ;조수가 사정 플래그(이것을 넣지 않는 이유는 조교중의 주인이 의도해 사정시킨 것이 아니다=말에 의한 폭발을 막는 의미가 있다)
    ;TFLAG:36 = TFLAG:91
PRINTL 　
;통상의 사정
ELSEIF TFLAG:91 == 1
    TCVAR:3 += 1
    PRINTFORML %CALLNAME:TARGET% 사정
    ;P에 정액 더러움
    STAIN:TARGET:자지 |= 1p2
    ;조수가 사정 플래그(이것을 넣지 않는 이유는 조교중의 주인이 의도해 사정시킨 것이 아니다=말에 의한 폭발을 막는 의미가 있다)
    ;TFLAG:36 = TFLAG:91
ENDIF
BASE:사정 = LIMIT(BASE:사정 - EJAC*(TFLAG:91), 0, EJAC-1)
;LUNATIC??(은)는 사정할 때마다 체력이 감소
CALL EXHAUST_EJAC, ASSI, TFLAG:91
;후타나리이든지 첫사정의 처리
SIF EXP:사정경험 == 0 && TALENT:120 == 0 && TFLAG:91 > 0
    TCVAR:50 += 1

SIF TFLAG:91 > 0
    WAIT
TARGET = LOCAL:4

RETURN 0