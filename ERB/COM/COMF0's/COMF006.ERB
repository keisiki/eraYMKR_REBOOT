;실행할 수 있는가 판정
;-------------------------------------------------
IF TFLAG:699 == 0
    CALL COM_ORDER
    ;실행할 수 없다
    SIF RESULT == 0
        RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = %LOCALS%
CALL TRAIN_MESSAGE_COM006

;-------------------------------------------------
;사정 게이지 체크(후타나리 · 남자인 경우)
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:기교 == 0
    LOCAL = 50
ELSEIF ABL:기교 == 1
    LOCAL = 200
ELSEIF ABL:기교 == 2
    LOCAL = 300
ELSEIF ABL:기교 == 3
    LOCAL = 400
ELSEIF ABL:기교 == 4
    LOCAL = 500
ELSE
    LOCAL = 600
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;혀놀림을 본다
SIF TALENT:52
    TIMES LOCAL , 2.00
;고양이혀를 본다
SIF TALENT:54
    TIMES LOCAL , 0.70
    
;조교자의 Ｃ감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF EXIST_PENIS(PLAYER)
    BASE:PLAYER:사정 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:기력 += 20

SOURCE:22 = 100
SOURCE:31 = 10

;위쪽에서 계산한 더러움 데이터
SOURCE:30 = TFLAG:242*20 + 10

;ABL:봉사정신을 본다
IF ABL:봉사정신 == 0
    SOURCE:15 = 50
    SOURCE:16 = 0
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:봉사정신 == 1
    SOURCE:15 = 150
    SOURCE:16 = 50
    TIMES SOURCE:30 , 2.50
ELSEIF ABL:봉사정신 == 2
    SOURCE:15 = 200
    SOURCE:16 = 100
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:봉사정신 == 3
    SOURCE:15 = 250
    SOURCE:16 = 180
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:봉사정신 == 4
    SOURCE:15 = 300
    SOURCE:16 = 300
    TIMES SOURCE:30 , 0.50
ELSE
    SOURCE:15 = 350
    SOURCE:16 = 500
    TIMES SOURCE:30 , 0.10
ENDIF

;ABL:기교를 본다
IF ABL:기교 == 0
    TIMES SOURCE:15 , 0.50
    TIMES SOURCE:16 , 0.50
ELSEIF ABL:기교 == 1
    TIMES SOURCE:15 , 0.80
    TIMES SOURCE:16 , 0.80
ELSEIF ABL:기교 == 2
    TIMES SOURCE:15 , 1.00
    TIMES SOURCE:16 , 1.00
ELSEIF ABL:기교 == 3
    TIMES SOURCE:15 , 1.50
    TIMES SOURCE:16 , 1.50
ELSEIF ABL:기교 == 4
    TIMES SOURCE:15 , 2.50
    TIMES SOURCE:16 , 2.50
ELSE
    TIMES SOURCE:15 , 4.00
    TIMES SOURCE:16 , 4.00
ENDIF

;키스마
IF TALENT:176
    TIMES SOURCE:15 , 2.50
    TIMES SOURCE:16 , 2.50
ENDIF
;조교자가 키스마
IF TALENT:PLAYER:176
    TIMES SOURCE:15 , 2.50
    TIMES SOURCE:16 , 2.50
    ;조교자가 助手で키스마だと技巧に応じて욕망を習得の珠が僅かに入る
    IF ASSIPLAY
        JUEL:PLAYER:11 += (ABL:PLAYER:기교 + 1) * 50
        JUEL:PLAYER:13 += (ABL:PLAYER:기교 + 1) * 50
    ENDIF
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91
    TIMES SOURCE:16 , 3.00

    ;ABL:정액중독을 본다
    IF ABL:정액중독 == 0
        TIMES SOURCE:16 , 2.00
        TIMES SOURCE:22 , 2.00
        SOURCE:24 = 0
    ELSEIF ABL:정액중독 == 1
        TIMES SOURCE:16 , 2.50
        TIMES SOURCE:22 , 1.60
        SOURCE:24 = 100
    ELSEIF ABL:정액중독 == 2
        TIMES SOURCE:16 , 3.00
        TIMES SOURCE:22 , 1.00
        SOURCE:24 = 300
    ELSEIF ABL:정액중독 == 3
        TIMES SOURCE:16 , 4.00
        TIMES SOURCE:22 , 0.70
        SOURCE:24 = 700
    ELSEIF ABL:정액중독 == 4
        TIMES SOURCE:16 , 5.00
        TIMES SOURCE:22 , 0.40
        SOURCE:24 = 1500
    ELSE
        TIMES SOURCE:16 , 6.00
        TIMES SOURCE:22 , 0.10
        SOURCE:24 = 6000
    ENDIF

    ;射精先タイプの設定
    TFLAG:703 = 22
    ;射精先選択の可・不可
    TFLAG:704 = 0
    ;汎用的な処理はこの関数で行う
    ;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
    CALL SAMEN_SHOOT
ENDIF

;노예의 입⇔조교자의 입의 불결이 이동
STAIN:입 |= STAIN:PLAYER:입
STAIN:PLAYER:입 |= STAIN:입

;[傀儡]と[精神崩壊]と[壊造人格]を持っておらず[친애]か[연모][망신]持ちだと愛情経験が加算
SIF ((TALENT:150 && TALENT:151) || TALENT:152) && TALENT:167 == 0 && TALENT:168 == 0 && TALENT:169 == 0 && ASSIPLAY == 0
    TCVAR:21 += 1

TCVAR:23 += 1
TCVAR:PLAYER:23 += 1
CFLAG:172 += (ASSIPLAY == 0) ? 1 # 0

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 3
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 3
ENDIF

;奉仕快楽経験入手判定
TFLAG:100 |= 1
;주인 경험 플래그
SIF ASSIPLAY == 0
    TFLAG:50 += 1

RETURN 1

