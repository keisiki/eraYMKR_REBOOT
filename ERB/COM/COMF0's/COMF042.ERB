;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 10
DOWNBASE:기력 += 150

SOURCE:14 = 800
SOURCE:22 = 1800
SOURCE:31 = 900

;ABL:봉사 정신을 본다
IF ABL:봉사정신 == 0
    SOURCE:15 = 420
    SOURCE:16 = 150
    SOURCE:30 = 400
ELSEIF ABL:봉사정신 == 1
    SOURCE:15 = 500
    SOURCE:16 = 300
    SOURCE:30 = 300
ELSEIF ABL:봉사정신 == 2
    SOURCE:15 = 580
    SOURCE:16 = 600
    SOURCE:30 = 150
ELSEIF ABL:봉사정신 == 3
    SOURCE:15 = 660
    SOURCE:16 = 900
    SOURCE:30 = 50
ELSEIF ABL:봉사정신 == 4
    SOURCE:15 = 740
    SOURCE:16 = 1500
    SOURCE:30 = 20
ELSE
    SOURCE:15 = 820
    SOURCE:16 = 2200
    SOURCE:30 = 0
ENDIF

;ABL:B감각을 본다
IF ABL:B감각 == 0
