ELSEIF ABL:기교 == 1
    LOCAL = 2100
ELSEIF ABL:기교 == 2
    LOCAL = 2900
ELSEIF ABL:기교 == 3
    LOCAL = 4000
ELSEIF ABL:기교 == 4
    LOCAL = 5000
ELSE
    LOCAL = 6000
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사 정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)
;정액 중독을 본다
LOCAL = EJACULATIONCHECK_ABL_31_2(TARGET, LOCAL)

;구가 있을 때
IF TEQUIP:42
