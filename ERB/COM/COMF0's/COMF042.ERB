ELSEIF ABL:B감각 == 1
    LOCAL:1 = 200
ELSEIF ABL:B감각 == 2
    LOCAL:1 = 400
ELSEIF ABL:B감각 == 3
    LOCAL:1 = 800
ELSEIF ABL:B감각 == 4
    LOCAL:1 = 1200
ELSE
    LOCAL:1 = 1500
ENDIF

;B민감, B둔감
IF TALENT:106
    TIMES LOCAL:1 , 1.20
ELSEIF TALENT:107
    TIMES LOCAL:1 , 0.70
ENDIF
;큰 가슴, 폭유
IF TALENT:112 || TALENT:113
