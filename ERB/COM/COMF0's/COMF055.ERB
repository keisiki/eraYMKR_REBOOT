    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 보물 다루듯이 조심스럽게 %CALLNAME:PLAYER%의 애널을 자극하기 시작했다….
;헌신적인 경우
ELSEIF TALENT:헌신적
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %조사선택(CALLNAME:TARGET, "가")% 애널을 만질 수 있게 하자,
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 열심히 자극해 %조사선택(CALLNAME:PLAYER, "를")% 즐겁게 했다….
;도착적인 경우
ELSEIF TALENT:도착적
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 애널을 만지도록 했다.
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 더러운 곳을 만지는 자신에게 도취되었다.
;반발각인持ちで연모がない場合
ELSEIF MARK:반발각인 && TALENT:연모 == 0
    PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% 싫어서 버둥대는 %CALLNAME:TARGET%에게 애널을 억지로 만지게 했다….
;그 외의 경우
ELSE
    PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 애널을 만지게 했다….
ENDIF

;-------------------------------------------------
;고유 실행 판정
;-------------------------------------------------
@COM_ORDER_55, ARG
LOCAL:99 = ARG
;ABL:욕망
IF ABL:욕망
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = ABL:욕망
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %ABLNAME:11%LV{LOCAL:99}({LOCAL:99})
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = ABL:봉사정신
    TFLAG:240 += LOCAL:99 * 3
    RESULTS = %RESULTS% %ABLNAME:13%LV{LOCAL:99}({LOCAL:99 * 3})
ENDIF
;ABL:ＢＬ끼＆TALENT:남자
IF TALENT:PLAYER:남자 && TALENT:남자
    SIF LOCAL:99
        RESULTS = %RESULTS% - 
    ;↓のLOCAL:99の目安(수음5, フェラ/A愛撫強制10)
    LOCAL:99 = 8
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:120%끼리({LOCAL:99})
    IF ABL:BL끼
        LOCAL:99 = ABL:BL끼
        TFLAG:240 += LOCAL:99 * 4
        RESULTS = %RESULTS% + %ABLNAME:23%LV{LOCAL:99}({LOCAL:99 * 4})
    ENDIF
ENDIF
;쾌락각인
IF MARK:쾌락각인
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = MARK:쾌락각인
    TFLAG:240 += LOCAL:99 * 2
    RESULTS = %RESULTS% %MARKNAME:1%LV{LOCAL:99}({LOCAL:99 * 2})
ENDIF

;부끄럼쟁이
IF TALENT:부끄럼쟁이
    RESULTS = %RESULTS% - 
    LOCAL:99 = 1
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:35%({LOCAL:99})
ENDIF
;냄새둔감
IF TALENT:냄새둔감
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 2
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:61%({LOCAL:99})
;냄새민감
ELSEIF TALENT:냄새민감
    RESULTS = %RESULTS% - 
    LOCAL:99 = 5
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:62%({LOCAL:99})
ENDIF
;헌신적
IF TALENT:헌신적
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 4
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:63%({LOCAL:99})
ENDIF
;불결무시
IF TALENT:불결무시
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 3
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:64%({LOCAL:99})
ENDIF
;쾌감의 부정
IF TALENT:쾌감을부정
    RESULTS = %RESULTS% - 
    LOCAL:99 = 1
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:71%({LOCAL:99})
ENDIF
;도착적
IF TALENT:도착적
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 3
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:80%({LOCAL:99})
ENDIF
;サド
IF TALENT:새드
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 3
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:81%({LOCAL:99})
ENDIF
;연모
IF TALENT:연모 && ASSIPLAY == 0
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 5
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:150%({LOCAL:99})
ENDIF

;조교자의 Ａ의 더러움
TFLAG:242 = COMORDER_STAIN_CALC(4, 0)

;불결 있음
IF TFLAG:242
    RESULTS = %RESULTS% - 
    TFLAG:240 -= TFLAG:242
    RESULTS = %RESULTS% 불결 있음
    ;냄새둔감
    IF TALENT:냄새둔감
        RESULTS = %RESULTS%%TALENTNAME:61%
    ELSEIF TALENT:냄새민감
        RESULTS = %RESULTS%%TALENTNAME:62%
    ENDIF
    RESULTS = %RESULTS%({TFLAG:242})
    LOCAL:99 = 1
ENDIF

