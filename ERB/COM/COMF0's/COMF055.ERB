@COM_ABLE55
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:55 > 0
    RETURN 0
;A조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:VA이상조교필터 & 2
    RETURN 0
;조교자와 조교 대상의 쌍방의 A감각이 2이상 없으면 안 됨
SIF ABL:PLAYER:A감각 < 2 || ABL:A감각 < 2
    RETURN 0
;순종 4 미만·레즈끼(BL끼) 4 미만의 ASSI의 경우, 조교 대상이 동성이라면 실행할 수 없다(망신이나 복종이라면 OK)
IF ASSIPLAY
    SIF (ABL:ASSI:순종 < 4 || ((TALENT:남자 == 0 && TALENT:ASSI:남자 == 0 && ABL:ASSI:레즈끼 < 4) || (TALENT:남자 && TALENT:ASSI:남자 && ABL:ASSI:BL끼 < 4))) && TALENT:ASSI:망신 == 0 && TALENT:ASSI:복종 == 0
    RETURN 0
ENDIF
;수면중은 불가
SIF EQUIP:14
    RETURN 0
;실신중은 불가
SIF TFLAG:899 > 0
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
    RETURN 0
;조수가 훈도시 착용중의 경우 기교 4이상 필수
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17 && ABL:기교 < 4
    RETURN 0
;나체정식 중에는 안 됨
SIF TEQUIP:60
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM55
PRINTL 강제 애널 애무

;-------------------------------------------------
;실행할 수 있는가 판정
;-------------------------------------------------
IF TFLAG:699 == 0
    CALL COM_ORDER
    ;실행할 수 없다
    SIF RESULT == 0
        RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = 강제 애널 애무
CALL TRAIN_MESSAGE_COM055

;-------------------------------------------------
;사정 게이지 체크(후타나리의 경우)
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:기교 == 0
    LOCAL = 100
ELSEIF ABL:기교 == 1
    LOCAL = 300
ELSEIF ABL:기교 == 2
    LOCAL = 500
ELSEIF ABL:기교 == 3
    LOCAL = 800
ELSEIF ABL:기교 == 4
    LOCAL = 1200
ELSE
    LOCAL = 1600
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;조교자의 Ｃ감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF EXIST_PENIS(PLAYER)
    BASE:PLAYER:사정 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 10
DOWNBASE:기력 += 100

SOURCE:12 = 500
SOURCE:22 = 1200
SOURCE:31 = 1200

;위쪽에서 계산한 더러움 데이터
SOURCE:30 = TFLAG:242*70 + 40

;ABL:봉사정신을 본다
IF ABL:봉사정신 == 0
    SOURCE:15 = 200
    SOURCE:16 = 50
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:봉사정신 == 1
    SOURCE:15 = 250
    SOURCE:16 = 100
    TIMES SOURCE:30 , 2.50
ELSEIF ABL:봉사정신 == 2
    SOURCE:15 = 300
    SOURCE:16 = 150
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:봉사정신 == 3
    SOURCE:15 = 400
    SOURCE:16 = 300
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:봉사정신 == 4
    SOURCE:15 = 550
    SOURCE:16 = 600
    TIMES SOURCE:30 , 0.60
ELSE
    SOURCE:15 = 750
    SOURCE:16 = 1000
    TIMES SOURCE:30 , 0.30
ENDIF

;ABL:새드끼를 본다
IF ABL:새드끼 == 0
    SOURCE:18 = 420
    TIMES SOURCE:30 , 1.20
ELSEIF ABL:새드끼 == 1
    SOURCE:18 = 500
    TIMES SOURCE:30 , 1.10
ELSEIF ABL:새드끼 == 2
    SOURCE:18 = 580
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:새드끼 == 3
    SOURCE:18 = 660
    TIMES SOURCE:30 , 0.90
ELSEIF ABL:새드끼 == 4
    SOURCE:18 = 740
    TIMES SOURCE:30 , 0.80
ELSE
    SOURCE:18 = 820
    TIMES SOURCE:30 , 0.70
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    SOURCE:11 = 20
    TIMES SOURCE:18 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
    SOURCE:11 = 50
    TIMES SOURCE:18 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
    SOURCE:11 = 100
    TIMES SOURCE:18 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    SOURCE:11 = 200
    TIMES SOURCE:18 , 1.30
ELSE
    SOURCE:11 = 400
    TIMES SOURCE:18 , 1.60
ENDIF

;TALENT:헌신적을 본다
SIF TALENT:헌신적
    TIMES SOURCE:11 , 2.00

;TALENT:도착적을 본다
IF TALENT:도착적
    TIMES SOURCE:11 , 1.50
    TIMES SOURCE:12 , 1.50
    TIMES SOURCE:22 , 2.00
    TIMES SOURCE:30 , 0.70
    TIMES SOURCE:31 , 0.20
ENDIF

;TALENT:연모/망신/친애를 본다
IF (TALENT:연모 || TALENT:망신 || TALENT:친애) && ASSIPLAY == 0
    TIMES SOURCE:11 , 2.50
    TIMES SOURCE:22 , 1.20
ENDIF

;ABL:기교를 본다
IF ABL:기교 == 0
    TIMES SOURCE:15 , 0.50
    TIMES SOURCE:16 , 0.40
ELSEIF ABL:기교 == 1
    TIMES SOURCE:15 , 0.80
    TIMES SOURCE:16 , 0.60
ELSEIF ABL:기교 == 2
    TIMES SOURCE:15 , 1.00
    TIMES SOURCE:16 , 0.90
ELSEIF ABL:기교 == 3
    TIMES SOURCE:15 , 1.50
    TIMES SOURCE:16 , 1.20
ELSEIF ABL:기교 == 4
    TIMES SOURCE:15 , 2.50
    TIMES SOURCE:16 , 1.50
ELSE
    TIMES SOURCE:15 , 4.00
    TIMES SOURCE:16 , 2.00
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
    ;コマンドごとに固有の処理はこの下に書く
    TIMES SOURCE:15 , 3.00

    ;ABL:정액중독을 본다
    IF ABL:정액중독 == 0
        SOURCE:24 = 0
        TIMES SOURCE:16 , 2.00
        TIMES SOURCE:22, 2.00
    ELSEIF ABL:정액중독 == 1
        SOURCE:24 = 300
        TIMES SOURCE:16 , 2.50
        TIMES SOURCE:22, 1.60
    ELSEIF ABL:정액중독 == 2
        SOURCE:24 = 600
        TIMES SOURCE:16 , 3.00
        TIMES SOURCE:22, 1.00
    ELSEIF ABL:정액중독 == 3
        SOURCE:24 = 1500
        TIMES SOURCE:16 , 4.50
        TIMES SOURCE:22, 0.70
    ELSEIF ABL:정액중독 == 4
        SOURCE:24 = 3000
        TIMES SOURCE:16 , 6.00
        TIMES SOURCE:22, 0.40
    ELSE
        SOURCE:24 = 6000
        TIMES SOURCE:16 , 8.00
        TIMES SOURCE:22, 0.10
    ENDIF

    ;射精先タイプの設定
    TFLAG:703 = 3
    ;射精先選択の可・不可
    TFLAG:704 = 1
    ;汎用的な処理はこの関数で行う
    ;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
    CALL SAMEN_SHOOT
ENDIF

TCVAR:PLAYER:1 += 1

;노예의 손가락⇔조교자의 Ａ의 불결이 이동
STAIN:손 |= STAIN:PLAYER:애널
STAIN:PLAYER:애널 |= STAIN:손

;同性ならレズ（ＢＬ）経験が入る
IF TALENT:남자 == 0 && TALENT:PLAYER:남자 == 0
    TCVAR:12 += 4
ELSEIF TALENT:남자 && TALENT:PLAYER:남자
    TCVAR:13 += 6
ENDIF
;주인 경험 플래그
SIF ASSIPLAY == 0 && EXP:봉사쾌락경험 >= EXPLV:3
    TFLAG:50 += 1
;奉仕快楽経験入手判定
TFLAG:100 |= 1

RETURN 1

