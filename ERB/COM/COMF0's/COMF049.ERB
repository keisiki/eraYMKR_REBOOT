;겨드랑이잡
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE49
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:49 > 0
    RETURN 0
;조교자에게 페니스가 없으면 안 됨
SIF EXIST_BAR(PLAYER) == 0
    RETURN 0
;레이무나 사나에는 기교 1, 그 외는 기교 2가 필요
IF WAKI_TECH_BONUS(TARGET)
    SIF ABL:기교 < 1
        RETURN 0
ELSE
    SIF ABL:기교 < 2
        RETURN 0
ENDIF
;수면중은 불가
SIF EQUIP:14
    RETURN 0
;패닉중은 불가
SIF TFLAG:108 > 0
    RETURN 0
;실신중은 불가
SIF TFLAG:899 > 0
    RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:40
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;나체정식 중에는 안 됨
SIF TEQUIP:60
    RETURN 0
;조수가 훈도시 착용중의 경우는 안 됨
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17
    RETURN 0
;웨딩 드레스·고스로리 착용중의 경우 기교 4이상 필수
SIF (TEQUIP:59 == 19 || TEQUIP:59 == 20) && ABL:PLAYER:기교 < 4
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;수음を改造して制作
@COM49
PRINTL 겨드랑이잡

;-------------------------------------------------
;실행할 수 있는가 판정
;-------------------------------------------------
IF TFLAG:699 == 0
    CALL COM_ORDER
    ;실행할 수 없다
    SIF RESULT == 0
        RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = 겨드랑이잡
CALL TRAIN_MESSAGE_COM049

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:기교 == 0
    LOCAL = 50
ELSEIF ABL:기교 == 1
    LOCAL = 100
ELSEIF ABL:기교 == 2
    LOCAL = 450
ELSEIF ABL:기교 == 3
    LOCAL = 1000
ELSEIF ABL:기교 == 4
    LOCAL = 1600
ELSE
    LOCAL = 2200
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_4(TARGET, LOCAL)
;봉사정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_3(TARGET, LOCAL)

;조교자의 Ｃ감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

;腋巫女補正
SIF NO:TARGET == 1 || NO:TARGET == 42
    TIMES LOCAL , 1.50

SIF EXIST_PENIS(PLAYER)
    BASE:PLAYER:사정 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 10
DOWNBASE:기력 += 150

SOURCE:22 = 600
SOURCE:28 = 2000
SOURCE:31 = 200

;위쪽에서 계산한 더러움 데이터
SOURCE:30 = TFLAG:242*10 + 100

;ABL:봉사정신을 본다
IF ABL:봉사정신 == 0
    SOURCE:15 = 50
    SOURCE:16 = 20
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:봉사정신 == 1
    SOURCE:15 = 100
    SOURCE:16 = 50
    TIMES SOURCE:30 , 3.00
ELSEIF ABL:봉사정신 == 2
    SOURCE:15 = 200
    SOURCE:16 = 100
    TIMES SOURCE:30 , 2.00
ELSEIF ABL:봉사정신 == 3
    SOURCE:15 = 300
    SOURCE:16 = 200
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:봉사정신 == 4
    SOURCE:15 = 400
    SOURCE:16 = 300
    TIMES SOURCE:30 , 0.50
ELSE
    SOURCE:15 = 500
    SOURCE:16 = 500
    TIMES SOURCE:30 , 0.10
ENDIF

;ABL:기교를 본다
IF ABL:기교 <= 1
    TIMES SOURCE:15 , 0.30
    TIMES SOURCE:16 , 0.30
ELSEIF ABL:기교 == 2
    TIMES SOURCE:15 , 0.50
    TIMES SOURCE:16 , 0.50
ELSEIF ABL:기교 == 3
    TIMES SOURCE:15 , 0.80
    TIMES SOURCE:16 , 0.80
ELSEIF ABL:기교 == 4
    TIMES SOURCE:15 , 1.00
    TIMES SOURCE:16 , 1.00
ELSE
    TIMES SOURCE:15 , 1.50
    TIMES SOURCE:16 , 1.50
ENDIF

;ABL:노출벽을 본다
IF ABL:노출증 == 0
    TIMES SOURCE:28 , 0.80
ELSEIF ABL:노출증 == 1
    TIMES SOURCE:28 , 1.00
ELSEIF ABL:노출증 == 2
    TIMES SOURCE:28 , 1.10
ELSEIF ABL:노출증 == 3
    TIMES SOURCE:28 , 1.20
ELSEIF ABL:노출증 == 4
    TIMES SOURCE:28 , 1.30
ELSE
    TIMES SOURCE:28 , 1.50
ENDIF

;腋巫女補正
IF NO:TARGET == 1 || NO:TARGET == 42
    TIMES SOURCE:28 , 1.50
    TIMES SOURCE:30 , 0.80
    TIMES SOURCE:31 , 0.80
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
    ;コマンドごとに固有の処理はこの下に書く
    TIMES SOURCE:15 , 3.00

    ;ABL:정액중독을 본다
    IF ABL:정액중독 == 0
        SOURCE:24 = 0
        TIMES SOURCE:16 , 2.00
        TIMES SOURCE:22, 2.00
    ELSEIF ABL:정액중독 == 1
        SOURCE:24 = 200
        TIMES SOURCE:16 , 2.50
        TIMES SOURCE:22, 1.60
    ELSEIF ABL:정액중독 == 2
        SOURCE:24 = 600
        TIMES SOURCE:16 , 3.00
        TIMES SOURCE:22, 1.00
    ELSEIF ABL:정액중독 == 3
        SOURCE:24 = 1500
        TIMES SOURCE:16 , 4.00
        TIMES SOURCE:22, 0.70
    ELSEIF ABL:정액중독 == 4
        SOURCE:24 = 3000
        TIMES SOURCE:16 , 5.00
        TIMES SOURCE:22, 0.40
    ELSE
        SOURCE:24 = 6000
        TIMES SOURCE:16 , 6.00
        TIMES SOURCE:22, 0.10
    ENDIF

    ;射精先タイプの設定
    TFLAG:703 = 11
    ;射精先選択の可・不可
    TFLAG:704 = 1
    ;汎用的な処理はこの関数で行う
    ;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
    CALL SAMEN_SHOOT
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 4
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 4
ENDIF
;奉仕快楽経験入手判定
TFLAG:100 |= 1

;조교자가 후타나리
SIF TALENT:PLAYER:121
    SOURCE:22 /= 2

RETURN 1

;--------------------------------------------------
;명령 거부 메시지
;--------------------------------------------------
@DENIAL_MESSAGE_COM_49
CALL KOJO_DENIAL_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF ASSIPLAY
    PRINTFORML %CALLNAME:TARGET%에게 %CALLNAME:PLAYER%의 페니스를 겨드랑이로 끼워서 봉사하도록 명령해 보았지만 거절당했다.
ELSE
    PRINTFORML %CALLNAME:TARGET%에게 자신의 페니스를 겨드랑이로 끼워서 봉사하도록 명령해 보았지만 거절당했다.
ENDIF
PRINTW 좀 더 조교가 필요한 것 같다.

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM049
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 겨드랑이를 조금 벌린 후, 그 사이에 자지를 끼우고 비비기 시작했다.
IF TALENT:150 == 0 && TALENT:160 == 0 && TALENT:170 == 0 && MARK:반발각인 > 1
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 자신의 겨드랑이를 왕복하는 자지를 더럽다는 눈초리로 보고 있다.
ELSEIF ABL:봉사정신 < 3 && ABL:노출증 < 3
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 완전 질렸다는 듯한 표정으로 겨드랑이로 계속 마찰하고 있다….
ELSE
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 간지럽다는 듯이 약간 몸을 꼬면서, 
    PRINTW 자신의 겨드랑이에 앞뒤로 왕복하는 그것을 홍조 띤 시선으로 바라보고 있다….
ENDIF

;-------------------------------------------------
;고유 실행 판정
;-------------------------------------------------
@COM_ORDER_49, ARG
LOCAL:99 = ARG
;ABL:욕망
IF ABL:욕망
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = ABL:욕망
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %ABLNAME:11%LV{LOCAL:99}({LOCAL:99})
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = ABL:봉사정신
    TFLAG:240 += LOCAL:99 * 4
    RESULTS = %RESULTS% %ABLNAME:13%LV{LOCAL:99}({LOCAL:99 * 4})
ENDIF

;ABL:노출벽
IF ABL:노출증 >= 2
    SIF LOCAL:99
        RESULTS = %RESULTS% +
    LOCAL:99 = ABL:노출증
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %ABLNAME:14%LV{LOCAL:99}({LOCAL:99})
ENDIF

;ABL:정액중독
IF ABL:정액중독
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = ABL:정액중독
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %ABLNAME:31%LV{LOCAL:99}({LOCAL:99})
ENDIF

;쾌락각인
IF MARK:쾌락각인
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = MARK:쾌락각인
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %MARKNAME:1%LV{LOCAL:99}({LOCAL:99})
ENDIF

;PALAM:욕정
GETPALAMLV PALAM:13, 5
LOCAL:2 = RESULT
IF LOCAL:2
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = LOCAL:2
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %PALAMNAME:13%LV{LOCAL:2}({LOCAL:99})
ENDIF

;부끄럼쟁이
IF TALENT:35
    RESULTS = %RESULTS% - 
    LOCAL:99 = 3
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:35%({LOCAL:99})
;恥薄い
ELSEIF TALENT:36
    SIF LOCAL:99
        RESULTS = %RESULTS% +
    LOCAL:99 = 1
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:36%({LOCAL:99})
ENDIF
;男嫌い
IF TALENT:남성혐오 && TALENT:PLAYER:120
    RESULTS = %RESULTS% - 
    LOCAL:99 = 5
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:37%({LOCAL:99})
;女嫌い
ELSEIF TALENT:여성혐오 && TALENT:PLAYER:120 == 0
    RESULTS = %RESULTS% - 
    LOCAL:99 = 5
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:38%({LOCAL:99})
ENDIF
;냄새둔감
IF TALENT:61
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 1
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:61%({LOCAL:99})
;냄새민감
ELSEIF TALENT:62
    RESULTS = %RESULTS% - 
    LOCAL:99 = 3
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:62%({LOCAL:99})
ENDIF
;쾌감의 부정
IF TALENT:71
    RESULTS = %RESULTS% - 
    LOCAL:99 = 1
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:71%({LOCAL:99})
ENDIF
;도착적
IF TALENT:80
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 2
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:80%({LOCAL:99})
ENDIF
;연모
IF TALENT:150 && ASSIPLAY == 0
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 5
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:150%({LOCAL:99})
ENDIF

;조교자가 후타나리
IF TALENT:PLAYER:121
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 8
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %조사처리(CALLNAME:PLAYER,"가")% %TALENTNAME:121%({LOCAL:99})
ENDIF

;実行者が霊夢か早苗（腋巫女補正という事で……）
IF NO:TARGET == 1 || NO:TARGET == 42
    SIF LOCAL:99
        RESULTS = %RESULTS% +
    LOCAL:99 = 3
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% 무녀 보정({LOCAL:99})
ENDIF

;조교자의 Ｐ의 더러움
TFLAG:242 = COMORDER_STAIN_CALC(2, 0)
;腋扱きでは汚れがそれほど影響しない
TFLAG:242 /= 3

;불결 있음
IF TFLAG:242
    RESULTS = %RESULTS% - 
    TFLAG:240 -= TFLAG:242
    RESULTS = %RESULTS% 불결 있음
    ;냄새둔감
    IF TALENT:61
        RESULTS = %RESULTS%%TALENTNAME:61%
    ELSEIF TALENT:62
        RESULTS = %RESULTS%%TALENTNAME:62%
    ENDIF
    RESULTS = %RESULTS%({TFLAG:242})
    LOCAL:99 = 1
ENDIF

TFLAG:241 = 19
