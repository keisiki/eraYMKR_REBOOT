ELSE
    STAIN:애널 |= STAIN:PLAYER:보지
    STAIN:PLAYER:보지 |= STAIN:애널
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 4
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 4
ENDIF
