    SOURCE:22 = 1800
ELSEIF ABL:A감각 == 1
    SOURCE:2 = 37
    SOURCE:22 = 1850
ELSEIF ABL:A감각 == 2
    SOURCE:2 = 150
    SOURCE:22 = 1900
ELSEIF ABL:A감각 == 3
    SOURCE:2 = 350
    SOURCE:22 = 2150
ELSEIF ABL:A감각 == 4
    SOURCE:2 = 550
    SOURCE:22 = 2500
