@COM_ABLE91
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:91 > 0
    RETURN 0
;수치 플레이계 확장 커맨드를 유효하게 하지 않으면 안 됨
SIF (FLAG:커맨드확장 & 512) == 0
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
;해제는 나체정식안 이외 배워 개에서도 가능
SIF TEQUIP:60 && TEQUIP:59 == 2
    RETURN 1
;아이템「스쿨 수영복」을 가지고 있는지, 아이템 없음 설정이 아니면 안 됨
SIF ITEM:45 == 0 && NOITEM == 0
    RETURN 0
;[남자]의 경우, 함락 하고 있지 않으면 안 됨
SIF TALENT:120 && TALENT:150 == 0 && TALENT:160 == 0 && TALENT:170 == 0
    RETURN 0
;순종이 3이상 없으면 안 됨
SIF ABL:순종 < 3
    RETURN 0
;수면중은 불가
SIF EQUIP:14
    RETURN 0
;B계 장착도구 사용중은 불가
SIF TEQUIP:35 || TEQUIP:36
    RETURN 0
;긴박 중에는 불가
SIF TEQUIP:40
    RETURN 0
;아이마스크 장착 중에는 불가
SIF TEQUIP:41
    RETURN 0
;목욕탕 플레이중은 플레이 매트가 없으면 안 됨
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
    RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
    RETURN 0
;00목욕탕 입욕 중에는 불가
SIF TEQUIP:55
    RETURN 0
;의사 플레이 중에는 불가
SIF TEQUIP:57
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;알몸 앞치마 중에는 불가
SIF TEQUIP:59 == 1
    RETURN 0
;코스프레 중에는 불가
SIF TEQUIP:59 > 2
    RETURN 0
;나체정식 중에는 안 됨
SIF TEQUIP:60
    RETURN 0
RETURN 1

