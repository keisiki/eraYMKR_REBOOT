@COM91
PRINTL 스쿠미즈 플레이

;-------------------------------------------------
;실행할 수 있는가 판정
;-------------------------------------------------
IF TFLAG:699 == 0
    CALL COM_ORDER
    ;실행할 수 없다
    SIF RESULT == 0
        RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = 스쿠미즈 플레이
CALL TRAIN_MESSAGE_COM091

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 5
DOWNBASE:기력 += 40

LOCAL:1 = 500
LOCAL:2 = 500

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES LOCAL:1 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES LOCAL:1 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES LOCAL:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES LOCAL:1 , 1.10
ELSE
    TIMES LOCAL:1 , 1.20
ENDIF
;ABL:노출벽을 본다
IF ABL:노출증 == 0
    SOURCE:24 += 0
    SOURCE:19 += 0
    TIMES LOCAL:1, 1.00
ELSEIF ABL:노출증 == 1
    SOURCE:24 += 50
    SOURCE:19 += 50
    TIMES LOCAL:1, 1.20
ELSEIF ABL:노출증 == 2
    SOURCE:24 += 100
    SOURCE:19 += 100
    TIMES LOCAL:1, 1.40
ELSEIF ABL:노출증 == 3
    SOURCE:24 += 200
    SOURCE:19 += 200
    TIMES LOCAL:1, 1.60
ELSEIF ABL:노출증 == 4
    SOURCE:24 += 400
    SOURCE:19 += 400
    TIMES LOCAL:1, 2.00
ELSE
    SOURCE:24 += 800
    SOURCE:19 += 800
    TIMES LOCAL:1, 3.00
ENDIF
;ABL:마조끼를 본다
IF ABL:마조끼 == 0
    TIMES LOCAL:1 , 0.80
ELSEIF ABL:마조끼 == 1
    TIMES LOCAL:1 , 1.00
ELSEIF ABL:마조끼 == 2
    TIMES LOCAL:1 , 1.30
ELSEIF ABL:마조끼 == 3
    TIMES LOCAL:1 , 1.60
ELSEIF ABL:마조끼 == 4
    TIMES LOCAL:1 , 2.00
ELSE
    TIMES LOCAL:1 , 3.00
ENDIF

;臆病
SIF TALENT:10
    TIMES LOCAL:2 , 2.00
;目立ちたがり
SIF TALENT:28
    TIMES LOCAL:1 , 1.50
;解放
SIF TALENT:33
    TIMES LOCAL:1 , 1.50
;부끄럼쟁이
SIF TALENT:35
    TIMES LOCAL:2 , 2.00
;巨乳
IF TALENT:112
    SOURCE:20 = 500
;爆乳
ELSEIF TALENT:113
    SOURCE:20 = 800
ENDIF
;[작은체형]
IF TALENT:128
    TIMES LOCAL:1 , 1.50
    TIMES LOCAL:2 , 1.50
ENDIF
;유아유치유아퇴행
SIF TALENT:130 || TALENT:131 || TALENT:159
    TIMES LOCAL:1 , 2.00

SOURCE:12 += LOCAL:1
SOURCE:22 += LOCAL:1 /2
SOURCE:31 += LOCAL:2

;남자
IF TALENT:120
    SOURCE:20 += 1600
    SOURCE:28 += LOCAL:1 /2
    TIMES SOURCE:31 , 2.50
;絶壁
ELSEIF TALENT:110
    SOURCE:28 += LOCAL:1 /4
;貧乳
ELSEIF TALENT:111
    SOURCE:28 += LOCAL:1 /6
;普通以上のおっぱいの女
ELSE
    SOURCE:28 += LOCAL:1 /8
ENDIF

SOURCE:25 += LOCAL:2 /5
;유아유치유아퇴행
SIF TALENT:130 || TALENT:131 || TALENT:159
    SOURCE:25 += SOURCE:31/2

;スク水の着脱
IF TEQUIP:59 == 2
    TEQUIP:59 = 0
ELSE
    TEQUIP:59 = 2
    ;調教対象のコスプレ経験
    CFLAG:50 += 1
ENDIF
RETURN 1

