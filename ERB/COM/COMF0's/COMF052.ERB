@COM_ABLE52
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:52 > 0
    RETURN 0
;조교자와 조교 대상의 쌍방으로 페니스가 없으면 안 됨
SIF (EXIST_BAR(PLAYER) == 0) || (EXIST_BAR(TARGET) == 0)
    RETURN 0
;주인이 [금단의 지식] 소유가 아닐 때, 체격차이가 2 단계 이내나 조교자가 [금단의 지식] 소유가 아니면 안 됨
SIF TALENT:MASTER:56 == 0 && COMMON_BODYSIZE_CHECK(PLAYER, TARGET) == 0
    RETURN 0
;순종 4 미만·레즈끼(BL끼) 4 미만의 ASSI의 경우, 조교 대상이 동성이라면 실행할 수 없다(소악마인가 망신이나 복종이라면 OK)
IF ASSIPLAY
    SIF (ABL:ASSI:순종 < 4 || ((TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:레즈끼 < 4) || (TALENT:120 && TALENT:ASSI:120 && ABL:ASSI:BL끼 < 4))) && TALENT:ASSI:93 == 0 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
    RETURN 0
ENDIF
;수면중은 불가
SIF EQUIP:14
    RETURN 0
;패닉중은 불가
SIF TFLAG:108 > 0
    RETURN 0
;실신중은 불가
SIF TFLAG:899 > 0
    RETURN 0
;C계 장착도구 사용중은 불가
SIF TEQUIP:30
    RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:40
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;00목욕탕 입욕 중에는 불가
SIF TEQUIP:55
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;조수가 훈도시 착용중의 경우는 안 됨
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17
    RETURN 0
;훈도시 착용중의 경우는 안 됨
SIF TEQUIP:59 == 17
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교중일 시 주인의 촉수사역Lv2 이하는 안 됨
SIF TEQUIP:90 && ABL:MASTER:촉수사역 <= 2
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;수음을 개조해서 제작
@COM52
PRINTL 육봉마찰

;-------------------------------------------------
;실행할 수 있는가 판정
;-------------------------------------------------
IF TFLAG:699 == 0
    CALL COM_ORDER
    ;실행할 수 없다
    SIF RESULT == 0
        RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = 육봉마찰
CALL TRAIN_MESSAGE_COM052

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:조교자의 기교를 본다
IF ABL:PLAYER:기교 == 0
    LOCAL = 120
ELSEIF ABL:PLAYER:기교 == 1
    LOCAL = 350
ELSEIF ABL:PLAYER:기교 == 2
    LOCAL = 1000
ELSEIF ABL:PLAYER:기교 == 3
    LOCAL = 2400
ELSEIF ABL:PLAYER:기교 == 4
    LOCAL = 3500
ELSE
    LOCAL = 4300
ENDIF

;기교를 본다
LOCAL = EJACULATIONCHECK_ABL_12_1(TARGET, LOCAL)
;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_4(TARGET, LOCAL)
;봉사정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_3(TARGET, LOCAL)

;조교자의 Ｃ감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF EXIST_PENIS(PLAYER)
    BASE:PLAYER:사정 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 20
DOWNBASE:기력 += 150

SOURCE:31 = 750

;위쪽에서 계산한 더러움 데이터
SOURCE:30 = TFLAG:242*20 + 80

;ABL:봉사정신을 본다
IF ABL:봉사정신 == 0
    SOURCE:15 = 50
    SOURCE:16 = 20
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:봉사정신 == 1
    SOURCE:15 = 100
    SOURCE:16 = 50
    TIMES SOURCE:30 , 3.00
ELSEIF ABL:봉사정신 == 2
    SOURCE:15 = 200
    SOURCE:16 = 100
    TIMES SOURCE:30 , 2.00
ELSEIF ABL:봉사정신 == 3
    SOURCE:15 = 300
    SOURCE:16 = 200
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:봉사정신 == 4
    SOURCE:15 = 400
    SOURCE:16 = 300
    TIMES SOURCE:30 , 0.50
ELSE
    SOURCE:15 = 500
    SOURCE:16 = 500
    TIMES SOURCE:30 , 0.10
ENDIF

;ABL:기교를 본다
IF ABL:기교 == 0
    TIMES SOURCE:15 , 0.50
    TIMES SOURCE:16 , 0.50
ELSEIF ABL:기교 == 1
    TIMES SOURCE:15 , 0.60
    TIMES SOURCE:16 , 0.60
ELSEIF ABL:기교 == 2
    TIMES SOURCE:15 , 0.70
    TIMES SOURCE:16 , 0.70
ELSEIF ABL:기교 == 3
    TIMES SOURCE:15 , 1.00
    TIMES SOURCE:16 , 1.00
ELSEIF ABL:기교 == 4
    TIMES SOURCE:15 , 1.20
    TIMES SOURCE:16 , 1.20
ELSE
    TIMES SOURCE:15 , 1.50
    TIMES SOURCE:16 , 1.50
ENDIF

;ABL:Ｃ감각을 본다
IF ABL:C감각 == 0
    SOURCE:0 = 10
ELSEIF ABL:C감각 == 1
    SOURCE:0 = 37
ELSEIF ABL:C감각 == 2
    SOURCE:0 = 150
ELSEIF ABL:C감각 == 3
    SOURCE:0 = 350
ELSEIF ABL:C감각 == 4
    SOURCE:0 = 550
ELSE
    SOURCE:0 = 750
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:0 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:0 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:0 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:0 , 1.30
ELSE
    TIMES SOURCE:0 , 1.60
ENDIF

;淫茎
IF TALENT:171
    SOURCE:11 = 500
    TIMES SOURCE:30 , 0.80
    TIMES SOURCE:31 , 0.80
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
    ;コマンドごとに固有の処理はこの下に書く
    TIMES SOURCE:15 , 3.00

    ;ABL:정액중독을 본다
    IF ABL:정액중독 == 0
        SOURCE:24 = 0
        TIMES SOURCE:16 , 2.00
    ELSEIF ABL:정액중독 == 1
        SOURCE:24 = 200
        TIMES SOURCE:16 , 2.50
    ELSEIF ABL:정액중독 == 2
        SOURCE:24 = 600
        TIMES SOURCE:16 , 3.00
    ELSEIF ABL:정액중독 == 3
        SOURCE:24 = 1500
        TIMES SOURCE:16 , 4.00
    ELSEIF ABL:정액중독 == 4
        SOURCE:24 = 3000
        TIMES SOURCE:16 , 5.00
    ELSE
        SOURCE:24 = 6000
        TIMES SOURCE:16 , 6.00
    ENDIF

    ;射精先タイプの設定
    TFLAG:703 = 14
    ;射精先選択の可・不可
    TFLAG:704 = 0
    ;汎用的な処理はこの関数で行う
    ;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
    CALL SAMEN_SHOOT
ENDIF

;射精に関係なく行われる処理はここから(汚れ部位追加の可能性も考慮して元となったらしき部分の処理だけは残すことに)
;노예의 P⇔조교자의 Ｐ의 불결이 이동
STAIN:자지 |= STAIN:PLAYER:자지
STAIN:PLAYER:자지 |= STAIN:자지

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 4
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 4
ENDIF
;奉仕快楽経験入手判定
TFLAG:100 |= 1

;조교자가 후타나리
SIF TALENT:PLAYER:121
    SOURCE:22 /= 2

RETURN 1

