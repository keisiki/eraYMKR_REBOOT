;=============================================================================
;손가락 삽입
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE4
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:4 > 0
    RETURN 0
;조수 조교 혹은 순애 모드가 무효의 경우는, V조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:2 & 1 && ((FLAG:12 & 32) == 0 || ASSIPLAY)
    RETURN 0
;이상건을 체크해, 그것이 안되면 V조교 필터가 오프로 되어 있지 않으면 안 됨
CALL EXCOM_ABLE605
SIF RESULT == 0 && FLAG:2 & 1
    RETURN 0
;남자라면 안 됨
SIF TALENT:120
    RETURN 0
;윤활 부족해, 순종 4 미만·레즈끼 4 미만의 ASSI의 경우, 조교 대상이 동성이라면 실행할 수 없다(새드나 새드끼 Lv3 이상, 또는 망신이나 복종이 있다면 OK)
IF PALAM:10 < PALAMLV:2 && ASSIPLAY
    SIF (ABL:ASSI:순종 < 4 || (TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:레즈끼 < 4)) && TALENT:ASSI:81 == 0 && ABL:ASSI:새드끼 < 3 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
        RETURN 0
ENDIF
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;훈도시 착용중의 경우 조교자의 기교 4이상 필수
SIF TEQUIP:59 == 17 && ABL:PLAYER:기교 < 4
    RETURN 0
;촉수 조교중일 시 주인의 촉수사역Lv2이하는 안 됨
SIF TEQUIP:90 && ABL:MASTER:촉수사역 <= 2
    RETURN 0
;촉수 삽입중은 안됨
SIF TEQUIP:90 && (TEQUIP:20 || TEQUIP:21)
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM4

;주인조교에 순애모드가 유효에 대상이 처녀 또는 V경험이 없을 경우엔 손가락으로 V를 어루만진다.로 
IF ASSIPLAY == 0 && FLAG:12 & 32 && (TALENT:처녀 || EXP:V경험 <= 0)
    CALL EXCOM_ABLE605
    SIF RESULT == 1
        JUMP COM605
ENDIF

;前回の調教が指挿入れかつプレイヤの技巧３以上または前回の調教Ｇスポット刺激またはクスコ挿入中ならＧスポット刺激に
IF (PREVCOM == 4 && ABL:PLAYER:기교 >= 3) || PREVCOM == 600 || TEQUIP:21
    CALL EXCOM_ABLE600
    SIF RESULT == 1
        JUMP COM600
ENDIF

PRINTL 손가락 삽입
COM_NAME = 손가락 삽입
CALL TRAIN_MESSAGE_COM004

;Ｖ経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:1 = 1

DOWNBASE:0 += 30
DOWNBASE:기력 += 80

SOURCE:12 = 300
SOURCE:15 = 200

;ABL:Ｖ감각을 본다
IF ABL:V감각 == 0
    SOURCE:1 = 10
    SOURCE:22 = 150
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 50
    SOURCE:22 = 250
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 250
    SOURCE:22 = 400
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 600
    SOURCE:22 = 700
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 1200
    SOURCE:22 = 1300
ELSE
    SOURCE:1 = 1800
    SOURCE:22 = 2000
ENDIF

;EXP:V경험을 본다
IF EXP:V경험 < EXPLV:1
    TIMES SOURCE:1 , 0.20
    TIMES SOURCE:22 , 0.20
    SOURCE:20 = 300
ELSEIF EXP:V경험 < EXPLV:2
    TIMES SOURCE:1 , 0.50
    TIMES SOURCE:22 , 0.50
    SOURCE:20 = 180
ELSEIF EXP:V경험 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:22 , 0.80
    SOURCE:20 = 80
ELSEIF EXP:V경험 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:22 , 1.00
    SOURCE:20 = 30
ELSEIF EXP:V경험 < EXPLV:5
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:22 , 1.20
    SOURCE:20 = 0
ELSE
    TIMES SOURCE:1 , 1.80
    TIMES SOURCE:22 , 1.50
    SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.10
    SOURCE:20 += 700
    TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.20
    SOURCE:20 += 200
    TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:20 , 0.80
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:20 , 0.50
ELSE
    TIMES SOURCE:1 , 2.00
    TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.50
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.20
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.50
ELSE
    TIMES SOURCE:1 , 1.80
ENDIF

;V민감둔감을 본다
;快Ｖ自体のチェックは後でまとめてやる
IF TALENT:103
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:22 , 1.50
ELSEIF TALENT:102
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:22 , 0.60
ENDIF

;탈자궁이 있고 자궁탈 상태일 시 자궁경험을 본다.
IF TALENT:탈자궁 && TEQUIP:20 == 8
    TCVAR:47 += 2
    IF EXP:자궁경험 < EXPLV:1
        TIMES SOURCE:1 , 0.50
        TIMES SOURCE:20 , 2.00
        SOURCE:25 = 1000
        TIMES DOWNBASE:0 , 5.00
        TIMES DOWNBASE:기력 , 5.00
    ELSEIF EXP:자궁경험 < EXPLV:2
        TIMES SOURCE:1 , 0.60
        TIMES SOURCE:20 , 1.50
        SOURCE:25 = 750
        TIMES DOWNBASE:0 , 4.00
        TIMES DOWNBASE:기력 , 4.00
    ELSEIF EXP:자궁경험 < EXPLV:3
        TIMES SOURCE:1 , 0.80
        TIMES SOURCE:20 , 1.20
        SOURCE:25 = 500
        TIMES DOWNBASE:0 , 3.00
        TIMES DOWNBASE:기력 , 3.00
    ELSEIF EXP:자궁경험 < EXPLV:4
        TIMES SOURCE:1 , 1.00
        TIMES SOURCE:20 , 1.00
        SOURCE:25 = 300
        TIMES DOWNBASE:0 , 2.00
        TIMES DOWNBASE:기력 , 2.00
    ELSEIF EXP:자궁경험 < EXPLV:5
        TIMES SOURCE:1 , 1.20
        TIMES SOURCE:20 , 0.80
        SOURCE:25 = 200
        TIMES DOWNBASE:0 , 2.00
        TIMES DOWNBASE:기력 , 2.00
    ELSE
        TIMES SOURCE:1 , 1.50
        TIMES SOURCE:20 , 0.60
        SOURCE:25 = 100
        TIMES DOWNBASE:0 , 1.50
        TIMES DOWNBASE:기력 , 1.50
    ENDIF
ENDIF

;처녀에다 정조관념
SIF TALENT:처녀 == 1 && TALENT:30
    TIMES SOURCE:22 , 2.00

TCVAR:0 += 1

;노예의 Ｖ⇔조교자의 손가락의 불결이 이동
    STAIN:보지 |= STAIN:PLAYER:손
    STAIN:PLAYER:손 |= STAIN:보지

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 1
ENDIF
RETURN 1

