;=============================================================================
;애널 바이브
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE13
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:13 > 0
    RETURN 0
;A조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:VA이상조교필터 & 2
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;훈도시 착용중의 경우 조교자의 기교 4이상 필수
SIF TEQUIP:59 == 17 && ABL:PLAYER:기교 < 4
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
;해제는 언제라도 가능
SIF TEQUIP:25 == 1
    RETURN 1
;아이템을 가지고 있는지, 아이템 없음 설정이 아니면 안 됨
SIF ITEM:9 == 0 && NOITEM == 0
    RETURN 0
;그 조교중에 관장 플레이＋배설이 있었을 경우, 조교자가 더러운 무시 혹은 악취 둔감을 가지지 않으면 실행할 수 없다
SIF TFLAG:140 & 256 && TALENT:PLAYER:61 == 0 && TALENT:PLAYER:64 == 0
    RETURN 0
;조교 대상이 [소인]의 경우는 주인이 [금단의 지식] 소유가 아니면 안 됨
SIF TALENT:127 && TALENT:MASTER:56 == 0
    RETURN 0
;윤활 부족해, 순종 4 미만·레즈끼(BL끼) 4 미만의 ASSI의 경우, 조교 대상이 동성이라면 실행할 수 없다(새드나 새드끼 Lv3 이상, 또는 망신이나 복종이 있다면 OK)
IF PALAM:10 < PALAMLV:3 && ASSIPLAY
    SIF (ABL:ASSI:순종 < 4 || ((TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:레즈끼 < 4) || (TALENT:120 && TALENT:ASSI:120 && ABL:ASSI:BL끼 < 4))) && TALENT:ASSI:81 == 0 && ABL:ASSI:새드끼 < 3 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
        RETURN 0
;윤활이 너무 낮으면(자) 사용할 수 없다(조교자가 새디즘 또는 드 S 또는 새드끼 Lv3 이상이라면 OK)
ELSEIF PALAM:10 < PALAMLV:2 && TALENT:PLAYER:81 == 0 && TALENT:PLAYER:177 == 0 && ABL:PLAYER:새드끼 < 3
    RETURN 0
ENDIF
;다른 A계 장착도구 사용중은 불가
SIF (TEQUIP:25 && TEQUIP:25 != 1) || TEQUIP:26
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;----------------------------------------------------------
;애널 바이브(SELECTCOM=13), 촉수 애널 삽입(SELECTCOM=502)
;----------------------------------------------------------
;촉수 애널 삽입은 COMF500.ERB로부터 JUMP 해 온다.
@COM13
IF TEQUIP:90
    PRINTL 애널 촉수
    COM_NAME = 애널 촉수
ELSE
    COM_TOOL_NAME = 애널 바이브
    IF FLAG:1100 & 1
        ;技師パッチ有効なら特殊な名前チェック
        FLAG:1110 = 9
        CALL NAMECHECK_CUSTOMTOOL
    ELSE

        COM_TOOL_CALLNAME = %COM_TOOL_NAME%
    ENDIF
    PRINTFORML %COM_TOOL_NAME%
    COM_NAME = 애널 바이브
ENDIF

DOWNBASE:0 += 60
DOWNBASE:기력 += 150

;ABL:A감각을 본다
IF ABL:A감각 == 0
    SOURCE:2 = 80
    SOURCE:22 = 300
ELSEIF ABL:A감각 == 1
    SOURCE:2 = 250
    SOURCE:22 = 800
ELSEIF ABL:A감각 == 2
    SOURCE:2 = 600
    SOURCE:22 = 1400
ELSEIF ABL:A감각 == 3
    SOURCE:2 = 1000
    SOURCE:22 = 1800
ELSEIF ABL:A감각 == 4
    SOURCE:2 = 1300
    SOURCE:22 = 2100
ELSE
    SOURCE:2 = 1700
    SOURCE:22 = 2400
ENDIF

;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
    TIMES SOURCE:2 , 0.50
    SOURCE:20 = 2000
ELSEIF EXP:A경험 < EXPLV:2
    TIMES SOURCE:2 , 1.00
    SOURCE:20 = 300
ELSEIF EXP:A경험 < EXPLV:3
    TIMES SOURCE:2 , 1.10
    SOURCE:20 = 50
ELSEIF EXP:A경험 < EXPLV:4
    TIMES SOURCE:2 , 1.20
    SOURCE:20 = 10
ELSEIF EXP:A경험 < EXPLV:5
    TIMES SOURCE:2 , 1.40
    SOURCE:20 = 0
ELSE
    TIMES SOURCE:2 , 1.60
    SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:2 , 0.40
    SOURCE:20 += 800
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:2 , 0.80
    SOURCE:20 += 500
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 300
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:2 , 1.40
    SOURCE:20 += 120
ELSE
    TIMES SOURCE:2 , 1.80
    SOURCE:20 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:2 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:2 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:2 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:2 , 1.10
ELSE
    TIMES SOURCE:2 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:2 , 0.80
ELSEIF ABL:순종 == 1
    TIMES SOURCE:2 , 0.90
ELSEIF ABL:순종 == 2
    TIMES SOURCE:2 , 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:2 , 1.10
ELSEIF ABL:순종 == 4
    TIMES SOURCE:2 , 1.20
ELSE
    TIMES SOURCE:2 , 1.30
ENDIF

;[작은체형]
SIF TALENT:128
    TIMES SOURCE:20 , 2.00

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:22 , 0.60
    TIMES SOURCE:31 , 0.60
ENDIF

;처녀로 정조관념
IF TALENT:처녀 == 1 && TALENT:30
    SOURCE:22 /= 3
ELSEIF TALENT:처녀 && TALENT:30
    SOURCE:22 /= 2
ENDIF

IF (FLAG:1100 & 1) && TEQUIP:90 == 0
    ;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
    ;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
    FLAG:1110 = 9
    FLAG:1111 = 0
    FLAG:1112 = SOURCE:2
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:2 = FLAG:1120
ENDIF

CALL TRAIN_MESSAGE_COM13

;애널 바이브의 착탈
IF TEQUIP:25
    IF TEQUIP:90
        SIF TEQUIP:94
            CALL KANTYO_EGGPLAY_OUT
    ENDIF
    TEQUIP:25 = 0
ELSE
    IF TEQUIP:90
        STAIN:애널 |= 2
        STAIN:애널 |= 4
        STAIN:애널 |= 64
        ;애널의 직후라면 효과 증강
        IF PREVCOM == 45
            TIMES SOURCE:2 , 1.50
            TIMES SOURCE:22 , 2.50
            SOURCE:24 += 4000
            SIF TALENT:173
                SOURCE:24 += 4000
        ENDIF
    ENDIF
    TEQUIP:25 = 1
ENDIF

TCVAR:1 += 2

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 1
ENDIF

;커맨드 속성：촉수
IF TEQUIP:90
    TFLAG:98 = 8
;커맨드 속성：도구(조교자)
ELSE
    TFLAG:98 = 1
ENDIF
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;-------------------------------------------------
;애널 바이브 삽입중
;-------------------------------------------------
@EQUIP_COM13
IF TEQUIP:90
    PRINTL <애널 촉수 삽입 중>
ELSE
    COM_TOOL_NAME = 애널 바이브
    IF FLAG:1100 & 1
        ;技師パッチ有効なら特殊な名前チェック
        FLAG:1110 = 9
        CALL NAMECHECK_CUSTOMTOOL
    ENDIF
    PRINTFORML <%COM_TOOL_NAME% 삽입 중>
ENDIF

DOWNBASE:0 += 30
DOWNBASE:기력 += 80

;ABL:A감각을 본다
IF ABL:A감각 == 0
    LOCAL = 40
    LOCAL:1 = 300
ELSEIF ABL:A감각 == 1
    LOCAL = 120
    LOCAL:1 = 800
ELSEIF ABL:A감각 == 2
    LOCAL = 300
    LOCAL:1 = 1400
ELSEIF ABL:A감각 == 3
    LOCAL = 500
    LOCAL:1 = 1800
ELSEIF ABL:A감각 == 4
    LOCAL = 650
    LOCAL:1 = 2100
ELSE
    LOCAL = 850
    LOCAL:1 = 2400
ENDIF

;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
    TIMES LOCAL , 0.50
    LOCAL:2 = 2000
ELSEIF EXP:A경험 < EXPLV:2
    TIMES LOCAL , 1.00
    LOCAL:2 = 300
ELSEIF EXP:A경험 < EXPLV:3
    TIMES LOCAL , 1.10
    LOCAL:2 = 50
ELSEIF EXP:A경험 < EXPLV:4
    TIMES LOCAL , 1.20
    LOCAL:2 = 10
ELSEIF EXP:A경험 < EXPLV:5
    TIMES LOCAL , 1.40
    LOCAL:2 = 0
ELSE
    TIMES LOCAL , 1.60
    LOCAL:2 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES LOCAL , 0.40
    LOCAL:2 += 800
ELSEIF PALAM:10 < PALAMLV:2
    TIMES LOCAL , 0.80
    LOCAL:2 += 500
ELSEIF PALAM:10 < PALAMLV:3
    TIMES LOCAL , 1.00
    LOCAL:2 += 300
ELSEIF PALAM:10 < PALAMLV:4
    TIMES LOCAL , 1.40
    LOCAL:2 += 120
ELSE
    TIMES LOCAL , 1.80
    LOCAL:2 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES LOCAL , 1.10
ELSE
    TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES LOCAL , 0.80
ELSEIF ABL:순종 == 1
    TIMES LOCAL , 0.90
ELSEIF ABL:순종 == 2
    TIMES LOCAL , 1.00
ELSEIF ABL:순종 == 3
    TIMES LOCAL , 1.10
ELSEIF ABL:순종 == 4
    TIMES LOCAL , 1.20
ELSE
    TIMES LOCAL , 1.30
ENDIF

;[작은체형]
SIF TALENT:128
    TIMES LOCAL:2 , 2.00

;처녀로 정조관념
IF TALENT:처녀 == 1 && TALENT:30
    LOCAL:1 /= 3
ELSEIF TALENT:처녀 && TALENT:30
    LOCAL:1 /= 2
ENDIF

IF (FLAG:1100 & 1) && TEQUIP:90 == 0
    ;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
    ;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
    FLAG:1110 = 9
    FLAG:1111 = 0
    FLAG:1112 = LOCAL
    CALL SOURCECHECK_CUSTOMTOOL
    LOCAL = FLAG:1120
ENDIF

SOURCE:2 += LOCAL
SOURCE:20 += LOCAL:2
SOURCE:22 += LOCAL:1

;자위 실행시는 경험＋1
IF SELECTCOM == 7 || SELECTCOM == 124 || SELECTCOM == 125 || SELECTCOM == 127 || SELECTCOM == 182 || SELECTCOM == 205 || SELECTCOM == 628
    TCVAR:1 += 2
ELSE
    TCVAR:1 += 1
ENDIF

;촉수 경험 플래그
SIF TEQUIP:90
    TFLAG:90 += 1

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM13
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF TEQUIP:90
    IF TEQUIP:25
        PRINTFORMW 촉수를 %CALLNAME:TARGET%의 항문에서 뽑아냈다….
    ELSE
        PRINTFORM 거대한 촉수가 %CALLNAME:TARGET%의 애널에 들어가며 비벼대고 있다
        ;애널의 직후에 음고소유 대상의 경우
        IF PREVCOM == 45 && TALENT:173
            PRINTL .
            PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 소원 대로 엉덩이를 만져 준 것에 만족한 듯한 \@(TEQUIP:42) ? 표정을 보이고# 교성을 지르고 \@ 있다….
        ELSE
            PRINTW ….
        ENDIF
    ENDIF
ELSE
    IF TEQUIP:25
        PRINTFORML %CALLNAME:TARGET%의 항문에서 %조사선택(COM_TOOL_NAME, "를")% 뽑았다.
        IF EQUIP:14 == 0 && TFLAG:899 == 0
            PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 
            IF EXP:A경험 > 180
                PRINT 질구와 입에서 군침을 흘리면서, 엉덩이를 계속 벌름거리고 
            ELSEIF EXP:A경험 > 140
                PRINT 젖은 눈동자로 간절히 원하듯이 엉덩이를 흔들고 
            ELSEIF EXP:A경험 > 100
                PRINT 모자란다는 듯이 엉덩이를 쳐들고 
            ELSEIF EXP:A경험 > 75
                PRINT 조금 아깝다는 듯이 엉덩이를 흔들고 
            ELSEIF EXP:A경험 > 40
                PRINT 엉덩이를 머뭇거리며 살짝 안심한 표정을 짓고 
            ELSEIF EXP:A경험 > 20
                PRINT 강제적인 배설감에 볼을 붉히며 수줍어하고 
            ELSE
                PRINT 자극을 괴로운 표정으로 참고 
            ENDIF
            PRINTW 있다….
        ENDIF
    ELSE
        PRINTFORML %CALLNAME:TARGET%의 항문에 %조사선택(COM_TOOL_CALLNAME, "를")% 꾹 대었다…. 
        IF EQUIP:14 == 0 && TFLAG:899 == 0
            PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 
            IF EXP:A경험 > 180
                PRINT 녹아버릴 듯한 표정으로, 자신의 엉덩이를 벌리고 
            ELSEIF EXP:A경험 > 140
                PRINT 삽입을 기다리며 애가 탄다는 듯이 엉덩이를 흔들고 
            ELSEIF EXP:A경험 > 100
                PRINT 뺨을 붉히며 기대감에 엉덩이를 흔들고 
            ELSEIF EXP:A경험 > 75
                PRINT 오므라든 항문을 열어대는 바이브의 자극에 몸을 떨고 
            ELSEIF EXP:A경험 > 40
                PRINT 기대와 부정이 섞인듯한 복잡한 표정을 짓고 
            ELSEIF EXP:A경험 > 20
                PRINT 부끄럼쟁이에 붉게 물든 얼굴을 숨기고 
            ELSE
                PRINT 부정한 구멍을 범해지는 치욕감과 굴욕감이 뒤섞인 표정이 되어 
            ENDIF
            PRINTL 있다.
            PRINT 그런 
        ELSE
            PRINT 그리고 아직 닫혀있는 
        ENDIF
        PRINTFORM %CALLNAME:TARGET%의 
        SIF PALAM:10 >= PALAMLV:4
            PRINT 점액 투성이가 되어있는 
        PRINTFORMW 항문에 %조사선택(COM_TOOL_CALLNAME, "를")% 꽂았다….
    ENDIF
ENDIF
