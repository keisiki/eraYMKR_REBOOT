;실행 판정
;………………………………………………
@COM_ABLE46
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:46 > 0
    RETURN 0
;노예가 고양이혀라면 고양이 페라 패치 유효하지 않으면 안 됨
SIF TALENT:고양이혀 && (FLAG:조교모드 & 16) == 0
    RETURN 0
;노예가 고양이혀라면 기교 2 필요
SIF TALENT:고양이혀 && ABL:기교 < 2
    RETURN 0
;수면중은 불가
SIF EQUIP:14
    RETURN 0
;실신중은 불가
SIF TFLAG:899 > 0
    RETURN 0
;볼 개그 사용중은 불가
SIF TEQUIP:42
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43 && TALENT:목분리 == 0
    RETURN 0
;나체정식 중에는 안 됨
SIF TEQUIP:60
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교중일 시 주인의 촉수사역Lv2 이하는 안 됨
SIF TEQUIP:90 && ABL:MASTER:촉수사역 <= 2
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM46
PRINTL 강제발핥기

;-------------------------------------------------
;실행할 수 있을까의 판정
;-------------------------------------------------
IF TFLAG:699 == 0
    CALL COM_ORDER
    ;실행할 수 없다
    SIF RESULT == 0
        RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = 강제발핥기
CALL TRAIN_MESSAGE_COM046

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 10
DOWNBASE:기력 += 200

SOURCE:22 = 1400
SOURCE:31 = 1000

;위 쪽으로 계산한 더러운 데이터
SOURCE:30 = TFLAG:242*20 + 300

;ABL:봉사 정신을 본다
IF ABL:봉사정신 == 0
    SOURCE:15 = 400
    SOURCE:16 = 50
    TIMES SOURCE:22 , 0.80
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:봉사정신 == 1
    SOURCE:15 = 480
    SOURCE:16 = 100
    TIMES SOURCE:22 , 0.90
    TIMES SOURCE:30 , 2.50
ELSEIF ABL:봉사정신 == 2
    SOURCE:14 = 400
    SOURCE:15 = 550
    SOURCE:16 = 200
    TIMES SOURCE:22 , 1.00
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:봉사정신 == 3
    SOURCE:14 = 600
    SOURCE:15 = 620
    SOURCE:16 = 300
    TIMES SOURCE:22 , 1.10
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:봉사정신 == 4
    SOURCE:14 = 800
    SOURCE:15 = 700
    SOURCE:16 = 500
    TIMES SOURCE:22 , 1.20
    TIMES SOURCE:30 , 0.50
ELSE
    SOURCE:14 = 1000
    SOURCE:15 = 800
    SOURCE:16 = 750
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:30 , 0.10
ENDIF

;ABL:마조끼를 본다
IF ABL:마조끼 == 0
    TIMES SOURCE:22 , 0.80
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:마조끼 == 1
    TIMES SOURCE:22 , 0.90
    TIMES SOURCE:30 , 1.20
ELSEIF ABL:마조끼 == 2
    TIMES SOURCE:22 , 1.00
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:마조끼 == 3
    TIMES SOURCE:22 , 1.20
    TIMES SOURCE:30 , 0.80
ELSEIF ABL:마조끼 == 4
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:30 , 0.60
ELSE
    TIMES SOURCE:22 , 2.00
    TIMES SOURCE:30 , 0.30
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    SOURCE:11 = 20
ELSEIF PALAM:13 < PALAMLV:2
    SOURCE:11 = 50
ELSEIF PALAM:13 < PALAMLV:3
    SOURCE:11 = 100
ELSEIF PALAM:13 < PALAMLV:4
    SOURCE:11 = 200
ELSE
    SOURCE:11 = 400
ENDIF

;TALENT:헌신적을 본다
SIF TALENT:63
    TIMES SOURCE:11 , 2.00

;TALENT:도착적을 본다
IF TALENT:80
    TIMES SOURCE:11 , 1.50
    TIMES SOURCE:22 , 2.00
    TIMES SOURCE:30 , 0.70
    TIMES SOURCE:31 , 0.20
ENDIF

;TALENT:연모/망신/친애를 본다
IF (TALENT:연모 || TALENT:망신 || TALENT:친애) && ASSIPLAY == 0
    TIMES SOURCE:11 , 2.50
    TIMES SOURCE:22 , 1.20
ENDIF

;ABL:기교를 본다
IF ABL:기교 == 0
    TIMES SOURCE:15 , 0.50
    TIMES SOURCE:16 , 0.50
ELSEIF ABL:기교 == 1
    TIMES SOURCE:15 , 0.80
    TIMES SOURCE:16 , 0.80
ELSEIF ABL:기교 == 2
    TIMES SOURCE:15 , 1.00
    TIMES SOURCE:16 , 1.00
ELSEIF ABL:기교 == 3
    TIMES SOURCE:15 , 1.20
    TIMES SOURCE:16 , 1.20
ELSEIF ABL:기교 == 4
    TIMES SOURCE:15 , 1.50
    TIMES SOURCE:16 , 1.50
ELSE
    TIMES SOURCE:15 , 2.00
    TIMES SOURCE:16 , 2.00
ENDIF

;대상의 입의 더러운⇔조교자의 다리의 더러움이 서로 이동
STAIN:입 |= STAIN:PLAYER:다리
STAIN:PLAYER:다리 |= STAIN:입

IF TALENT:120 == 0 && TALENT:PLAYER:남자 == 0
    TCVAR:12 += 7
ELSEIF TALENT:120 && TALENT:PLAYER:남자
    TCVAR:13 += 7
ENDIF
;주인 경험 플래그
SIF ASSIPLAY == 0 && EXP:펠라경험 >= EXPLV:3
    TFLAG:50 += 1
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

RETURN 1

;--------------------------------------------------
;명령 거부의 메시지
;--------------------------------------------------
@DENIAL_MESSAGE_COM_46
CALL KOJO_DENIAL_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF ASSIPLAY
    PRINTFORML %CALLNAME:TARGET%에게 %CALLNAME:PLAYER%의 발을 핥도록 명령해보았지만 거부했다.
ELSE
    PRINTFORML %CALLNAME:TARGET%에게 자신의 발을 핥도록 명령해보았지만 거부했다.
ENDIF
PRINTW 좀 더 조교가 필요한 것 같다.

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM046
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF TEQUIP:55 == 1
    PRINTL 전신에 대량의 음충이 달라붙어 다니는 상태로
ELSEIF TEQUIP:55 == 2
    SIF TFLAG:135 == 1
        PRINT 칠성 
    SIF TFLAG:135 == 2
        PRINT 전기 
    PRINTL 뱀장어가 들끓는 목욕탕에 어깨까지 잠긴 채
ELSEIF TEQUIP:55 == 3
    PRINTL 전신에 로션을 뒤집어쓴 상태로
ELSEIF TEQUIP:55 == 4
    PRINTL 전신에 슬라임들이 들러붙은 채로
ELSEIF TEQUIP:55 == 5
    PRINTL 전신이 미약에 잠겨 있는 상태로
ELSEIF TEQUIP:52
    IF TIME == 0
        PRINT 태양빛이 눈부신 푸른 하늘 아래서 
    ELSE
        PRINT 달빛이 부드러운 심야의 환상향 안에서 
    ENDIF
    IF TALENT:겁쟁이 || TALENT:프라이드가높음 || TALENT:35
        PRINTL 이런 부끄럽기 짝이 없는 모습을 누군가에게 보이는 게 아닌가 주저주저 하면서
    ELSE
        PRINTL 바람에 맨 살이 쓰다듬어지는 감각에 미약하게 떨며
    ENDIF
ELSEIF TEQUIP:11
    PRINTL 약 때문인지 어딘가 멍해 보이는 듯한 시선을 허공에 보내면서
ENDIF
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 앞으로 내민 %CALLNAME:PLAYER%의 발을 핥고 있다….
;-------------------------------------------------
;고유의 실행 판정
;-------------------------------------------------
@COM_ORDER_46, ARG
LOCAL:99 = ARG
;ABL:욕망
IF ABL:욕망
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = ABL:욕망
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %ABLNAME:11%LV{LOCAL:99}({LOCAL:99})
ENDIF
;ABL:봉사 정신
IF ABL:봉사정신
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = ABL:봉사정신
    TFLAG:240 += LOCAL:99 * 4
    RESULTS = %RESULTS% %ABLNAME:13%LV{LOCAL:99}({LOCAL:99 * 4})
ENDIF

;쾌락 각인
IF MARK:쾌락각인
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = MARK:쾌락각인
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %MARKNAME:1%LV{LOCAL:99}({LOCAL:99})
ENDIF

;PALAM:욕정
GETPALAMLV PALAM:13, 5
LOCAL:2 = RESULT
IF LOCAL:2
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = LOCAL:2
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %PALAMNAME:13%LV{LOCAL:2}({LOCAL:99})
ENDIF

;부끄럼쟁이
IF TALENT:35
    RESULTS = %RESULTS% - 
    LOCAL:99 = 1
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:35%({LOCAL:99})
ENDIF
;남자 싫다
IF TALENT:남성혐오 && TALENT:PLAYER:남자
    RESULTS = %RESULTS% - 
    LOCAL:99 = 12
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:37%({LOCAL:99})
;여자 증오
ELSEIF TALENT:여성혐오 && TALENT:PLAYER:남자 == 0
    RESULTS = %RESULTS% - 
    LOCAL:99 = 12
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:38%({LOCAL:99})
ENDIF
;악취 둔감
IF TALENT:61
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 1
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:61%({LOCAL:99})
;악취 민감
ELSEIF TALENT:62
    RESULTS = %RESULTS% - 
    LOCAL:99 = 3
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:62%({LOCAL:99})
ENDIF
;헌신적
IF TALENT:63
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 6
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:63%({LOCAL:99})
ENDIF
;쾌감의 부정
IF TALENT:71
    RESULTS = %RESULTS% - 
    LOCAL:99 = 1
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:71%({LOCAL:99})
ENDIF
;연모
IF TALENT:연모 && ASSIPLAY == 0
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 5
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% %TALENTNAME:150%({LOCAL:99})
ENDIF

;조교자의 다리의 더러움
TFLAG:242 = COMORDER_STAIN_CALC(6, 0)

;서로불결
IF TFLAG:242
    RESULTS = %RESULTS% - 
    TFLAG:240 -= TFLAG:242
    RESULTS = %RESULTS% 서로불결
    ;악취 둔감
    IF TALENT:61
        RESULTS = %RESULTS%, %TALENTNAME:61%
    ELSEIF TALENT:62
        RESULTS = %RESULTS%, %TALENTNAME:62%
    ENDIF
    RESULTS = %RESULTS%({TFLAG:242})
    LOCAL:99 = 1
ENDIF

