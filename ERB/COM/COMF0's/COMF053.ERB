@COM_ABLE53
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:53 > 0
    RETURN 0
IF FLAG:조교모드 & 32
    ;접어지고 커맨드를 유효하게 하고 있는 경우
    ;조교자가 남자인가, 후타나리 한편 구슬 있어가 아니면 안 됨
    SIF TALENT:PLAYER:120 == 0 && TALENT:PLAYER:121 != 2
        RETURN 0
ELSE
    ;조교자가 남자가 아니면 안 됨
    SIF TALENT:PLAYER:120 == 0
        RETURN 0
ENDIF
;수면중은 불가
SIF EQUIP:14
    RETURN 0
;실신중은 불가
SIF TFLAG:899 > 0
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;나체정식 중에는 안 됨
SIF TEQUIP:60
    RETURN 0
;조수가 훈도시 착용중의 경우는 안 됨
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교중일 시 주인의 촉수사역Lv2 이하는 안 됨
SIF TEQUIP:90 && ABL:MASTER:촉수사역 <= 2
    RETURN 0
RETURN 1

