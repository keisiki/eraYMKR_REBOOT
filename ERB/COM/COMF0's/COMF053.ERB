;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = 불알 애무
CALL TRAIN_MESSAGE_COM053

;-------------------------------------------------
;사정 게이지 체크（후타나리の場合）
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:기교 == 0
    LOCAL = 100
ELSEIF ABL:기교 == 1
    LOCAL = 300
ELSEIF ABL:기교 == 2
    LOCAL = 500
ELSEIF ABL:기교 == 3
    LOCAL = 800
ELSEIF ABL:기교 == 4
    LOCAL = 1200
ELSE
    LOCAL = 1600
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;조교자의 Ｃ감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF EXIST_PENIS(PLAYER)
    BASE:PLAYER:사정 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 10
DOWNBASE:기력 += 80

SOURCE:22 = 600
SOURCE:31 = 200

;위쪽에서 계산한 더러움 데이터
SOURCE:30 = TFLAG:242*10 + 50

;ABL:봉사정신을 본다
IF ABL:봉사정신 == 0
    SOURCE:15 = 250
    SOURCE:16 = 50
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:봉사정신 == 1
    SOURCE:15 = 300
    SOURCE:16 = 100
    TIMES SOURCE:30 , 2.50
ELSEIF ABL:봉사정신 == 2
    SOURCE:15 = 350
    SOURCE:16 = 170
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:봉사정신 == 3
    SOURCE:15 = 400
    SOURCE:16 = 250
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:봉사정신 == 4
    SOURCE:15 = 450
    SOURCE:16 = 350
    TIMES SOURCE:30 , 0.50
ELSE
    SOURCE:15 = 500
    SOURCE:16 = 500
    TIMES SOURCE:30 , 0.10
ENDIF


;ABL:기교를 본다
IF ABL:기교 == 0
    TIMES SOURCE:15 , 0.50
    TIMES SOURCE:16 , 0.50
ELSEIF ABL:기교 == 1
    TIMES SOURCE:15 , 0.80
    TIMES SOURCE:16 , 0.80
ELSEIF ABL:기교 == 2
    TIMES SOURCE:15 , 1.00
    TIMES SOURCE:16 , 1.00
ELSEIF ABL:기교 == 3
    TIMES SOURCE:15 , 1.20
    TIMES SOURCE:16 , 1.20
ELSEIF ABL:기교 == 4
    TIMES SOURCE:15 , 1.50
    TIMES SOURCE:16 , 1.50
ELSE
    TIMES SOURCE:15 , 2.00
    TIMES SOURCE:16 , 2.00
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
    ;コマンドごとに固有の処理はこの下に書く
    TIMES SOURCE:15 , 3.00

    ;ABL:정액중독을 본다
    IF ABL:정액중독 == 0
        SOURCE:24 = 0
        TIMES SOURCE:16 , 2.00
        TIMES SOURCE:22, 2.00
    ELSEIF ABL:정액중독 == 1
        SOURCE:24 = 150
        TIMES SOURCE:16 , 2.50
        TIMES SOURCE:22, 1.60
    ELSEIF ABL:정액중독 == 2
        SOURCE:24 = 400
        TIMES SOURCE:16 , 3.00
        TIMES SOURCE:22, 1.00
    ELSEIF ABL:정액중독 == 3
        SOURCE:24 = 800
        TIMES SOURCE:16 , 4.50
        TIMES SOURCE:22, 0.70
    ELSEIF ABL:정액중독 == 4
        SOURCE:24 = 2000
        TIMES SOURCE:16 , 6.00
        TIMES SOURCE:22, 0.40
    ELSE
        SOURCE:24 = 4000
        TIMES SOURCE:16 , 8.00
        TIMES SOURCE:22, 0.10
    ENDIF

    ;射精先タイプの設定
    TFLAG:703 = 3
    ;射精先選択の可・不可
    TFLAG:704 = 1
    ;汎用的な処理はこの関数で行う
    ;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
    CALL SAMEN_SHOOT
ENDIF

;射精に関係なく行われる処理はここから
;노예의 手⇔조교자의 Ｐ의 불결이 이동
STAIN:손 |= STAIN:PLAYER:자지
STAIN:PLAYER:자지 |= STAIN:손

;同性ならレズ（ＢＬ）経験が入る
IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 3
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 3
ENDIF

RETURN 1

