SIF TALENT:MASTER:91 == 0 && TALENT:MASTER:92 == 0
    RETURN 0
;조수가 奉仕精神3이상, 기교4이상이 아니면 불가
SIF ABL:ASSI:기교 < 4 || ABL:ASSI:봉사정신 < 3
    RETURN 0
;조교 대상 · 助手のいずれかがオトコだとダメ
SIF TALENT:120 || TALENT:ASSI:120
    RETURN 0
