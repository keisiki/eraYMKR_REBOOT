    RETURN 0
;조교 대상이 [貧乳]だと기교4이상 필요
ELSEIF TALENT:111 && ABL:기교 < 4
    RETURN 0
;조교 대상이 [巨乳]なら기교2이상 필요
ELSEIF TALENT:112 && ABL:기교 < 2
    RETURN 0
;조교 대상이 [爆乳]なら기교LV1이상 필요
ELSEIF TALENT:113 && ABL:기교 < 1
    RETURN 0
;조교 대상이 그 외의 경우에는 기교3이상 필요
ELSEIF TALENT:111 == 0 && TALENT:112 == 0 && TALENT:113 == 0 && ABL:기교 < 3
    RETURN 0
ENDIF
;조수가 [絶壁]だと不可
IF TALENT:ASSI:110
    RETURN 0
;조수가 [貧乳]だと기교4이상 필요
ELSEIF TALENT:ASSI:111 && ABL:ASSI:기교 < 4
    RETURN 0
;조수가 [巨乳]なら기교2이상 필요
ELSEIF TALENT:ASSI:112 && ABL:ASSI:기교 < 2
    RETURN 0
;조수가 [爆乳]なら기교LV1이상 필요
ELSEIF TALENT:ASSI:113 && ABL:ASSI:기교 < 1
    RETURN 0
;조수가 그 외의 경우에는 기교3이상 필요
ELSEIF TALENT:ASSI:111 == 0 && TALENT:ASSI:112 == 0 && TALENT:ASSI:113 == 0 && ABL:ASSI:기교 < 3
    RETURN 0
ENDIF
;수면 중에는 불가
SIF EQUIP:14
    RETURN 0
;패닉 중에는 불가
SIF TFLAG:108 > 0
    RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
    RETURN 0
;사라시 장착 중에는 불가
SIF TEQUIP:36
    RETURN 0
;조수가 褌と裸Ｙシャツ以外のコスプレ中はだめ
SIF ASSI >= 0 && TEQUIP:ASSI:59 > 1 && TEQUIP:ASSI:59 != 11 && TEQUIP:ASSI:59 != 17
    RETURN 0
;훈도시와 알몸 와이셔츠 외의 코스프레 중에는 안 됨
SIF TEQUIP:59 > 2 && TEQUIP:59 != 11 && TEQUIP:59 != 17
    RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:40
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;한증탕과 암반욕 외의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

