@COM_ABLE335
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:335 > 0
    RETURN 0
;조교자奉仕 · ウフフ系커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 2048) == 0
    RETURN 0
;Ｂ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 8) == 0
    RETURN 0
;조교 대상에게 페니스가 없으면 불가
SIF EXIST_BAR(TARGET) == 0
    RETURN 0
;조교자가 남자면 안 됨
SIF TALENT:PLAYER:120
    RETURN 0
;조교자가 고양이혀일 경우 고양이 펠라 패치 유효가 아니면 불가
SIF TALENT:PLAYER:54 && (FLAG:14 & 16) == 0
    RETURN 0
;조교자가 고양이혀일 경우 조교자에게 기교4 필요
SIF TALENT:PLAYER:54 && ABL:PLAYER:기교 < 4
    RETURN 0
;순종4미만 · 레즈끼4미만의 ASSI일 경우, 조교 대상이 동성이라면 실행할 수 없다(망신か복종ならＯＫ)
IF ASSIPLAY
    SIF (ABL:ASSI:순종 < 4 || (TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:레즈끼 < 4)) && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
    RETURN 0
ENDIF
;주인이 [금단의 지식]持ちじゃないとき, 体格差が2段階以内か조교자가 [금단의 지식]持ちじゃないとダメ
SIF TALENT:MASTER:56 == 0 && COMMON_BODYSIZE_CHECK(PLAYER, TARGET) == 0
    RETURN 0
;性器が汚れているとMASTERの場合はできない(조교자가 汚れ無視か愛液, Ｐ, 母乳のよごれならＯＫ)
SIF STAIN:2 >= 4 && (STAIN:2 != 18 && STAIN:2 != 19) && ASSIPLAY == 0 && TALENT:MASTER:64 == 0
    RETURN 0
;性器が汚れており, 汚臭敏感で순종4미만의 ASSI일 경우はできない(조교자가 汚れ無視か愛液, Ｐ, 母乳のよごれならＯＫ)
IF STAIN:2 >= 4 && (STAIN:2 != 18 && STAIN:2 != 19) && ASSIPLAY
    SIF ABL:ASSI:순종 < 4 && TALENT:ASSI:62 && TALENT:ASSI:64 == 0
        RETURN 0
ENDIF
;[절벽]이면 불가
IF TALENT:PLAYER:110
    RETURN 0
;[貧乳]だと기교5이상 필요
ELSEIF TALENT:PLAYER:111 && ABL:PLAYER:기교 < 5
    RETURN 0
;[巨乳]なら기교3이상 필요
ELSEIF TALENT:PLAYER:112 && ABL:PLAYER:기교 < 3
    RETURN 0
;[爆乳]なら기교LV2이상 필요
ELSEIF TALENT:PLAYER:113 && ABL:PLAYER:기교 < 2
    RETURN 0
;그 외의 경우에는 기교4이상 필요
ELSEIF TALENT:PLAYER:111 == 0 && TALENT:PLAYER:112 == 0 && TALENT:PLAYER:113 == 0 && ABL:PLAYER:기교 < 4
    RETURN 0
ENDIF
;오나홀 · 전극 오나홀사용 중에는 불가
SIF TEQUIP:30 == 2 || TEQUIP:30 == 6
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;조수가 褌と裸Ｙシャツ以外のコスプレ中はだめ
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 > 2 && TEQUIP:ASSI:59 != 11 && TEQUIP:ASSI:59 != 17
    RETURN 0
;훈도시 착용 중일 경우는 안 됨
SIF TEQUIP:59 == 17
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

