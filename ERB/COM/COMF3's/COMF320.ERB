    SOURCE:11 = 10
ELSEIF ABL:봉사정신 == 1
    SOURCE:11 = 30
ELSEIF ABL:봉사정신 == 2
    SOURCE:11 = 80
ELSEIF ABL:봉사정신 == 3
    SOURCE:11 = 150
ELSEIF ABL:봉사정신 == 4
    SOURCE:11 = 230
ELSE
    SOURCE:11 = 200 + (ABL:봉사정신 * 20)
ENDIF

;ABL:마조끼를 본다
IF ABL:마조끼 == 0
    TIMES SOURCE:10 , 0.80
    TIMES SOURCE:11 , 0.80
    SOURCE:18 = 10
    SOURCE:19 = 10
ELSEIF ABL:마조끼 == 1
    TIMES SOURCE:10 , 1.00
    TIMES SOURCE:11 , 1.00
    SOURCE:18 = 60
    SOURCE:19 = 40
ELSEIF ABL:마조끼 == 2
    TIMES SOURCE:10 , 1.20
    TIMES SOURCE:11 , 1.30
    SOURCE:18 = 120
    SOURCE:19 = 80
ELSEIF ABL:마조끼 == 3
    TIMES SOURCE:10 , 1.40
    TIMES SOURCE:11 , 1.40
    SOURCE:18 = 240
    SOURCE:19 = 140
ELSEIF ABL:마조끼 == 4
    TIMES SOURCE:10 , 1.50
    TIMES SOURCE:11 , 1.70
    SOURCE:18 = 360
    SOURCE:19 = 220
ELSE
    SOURCE:10 = SOURCE:10 * (ABL:마조끼 * 10 + 120) / 100
    SOURCE:11 = SOURCE:11 * (ABL:마조끼 * 15 + 125) / 100
    SOURCE:18 = 150 + (ABL:마조끼 * 35)
    SOURCE:19 = 100 + (ABL:마조끼 * 20)
ENDIF

