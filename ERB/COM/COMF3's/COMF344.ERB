;=============================================================================
;서로 젖 주무르기
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE344
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:344 > 0
    RETURN 0
;조교자奉仕 · ウフフ系커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 2048) == 0
    RETURN 0
;조교자か조교 대상이 オトコだとダメ
SIF TALENT:PLAYER:남자 || TALENT:120
    RETURN 0
;순종4미만 · 레즈끼3미만의 ASSI일 경우, 조교 대상이 동성이라면 실행할 수 없다(망신か복종ならＯＫ)
IF ASSIPLAY
    SIF (ABL:ASSI:순종 < 4 || (TALENT:120 == 0 && TALENT:ASSI:남자 == 0 && ABL:ASSI:레즈끼 < 3)) && TALENT:ASSI:망신 == 0 && TALENT:ASSI:복종 == 0
    RETURN 0
ENDIF
;레즈끼3이상限定
SIF ABL:레즈끼 < 3
    RETURN 0
;수면 중에는 불가
SIF EQUIP:14
    RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
    RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:40
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;조수가 ウェディングドレス · ゴスロリ着用中の場合助手に기교4이상 필수
SIF ASSI >= 0 && ASSIPLAY && (TEQUIP:ASSI:59 == 19 || TEQUIP:ASSI:59 == 20) && ABL:ASSI:기교 < 4
    RETURN 0
;ウェディングドレス · ゴスロリ着用中の場合기교4이상 필수
SIF (TEQUIP:59 == 19 || TEQUIP:59 == 20) && ABL:PLAYER:기교 < 4
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM344
PRINTL 서로 젖 주무르기
COM_NAME = 서로 젖 주무르기 
CALL TRAIN_MESSAGE_COM344

DOWNBASE:0 += 7
DOWNBASE:기력 += 70

SOURCE:12 = 90
SOURCE:15 = 80
SOURCE:30 = 30

;ABL:B감각을 본다
IF ABL:B감각 == 0
    SOURCE:3 = 20
    SOURCE:11 = 50
ELSEIF ABL:B감각 == 1
    SOURCE:3 = 100
    SOURCE:11 = 100
ELSEIF ABL:B감각 == 2
    SOURCE:3 = 500
    SOURCE:11 = 160
ELSEIF ABL:B감각 == 3
    SOURCE:3 = 1200
    SOURCE:11 = 200
ELSEIF ABL:B감각 == 4
    SOURCE:3 = 2000
    SOURCE:11 = 230
ELSE
    SOURCE:3 = 2800
    SOURCE:11 = 250
ENDIF

;조교자의 B감각을 본다
SOURCE:14 = 50 * (1 + ABL:PLAYER:B감각)

;음유
IF TALENT:음유
    TIMES SOURCE:3 , 1.40
    TIMES SOURCE:11 , 1.40
ENDIF

;조교자가［유아］,［유치］
IF TALENT:PLAYER:유아 || TALENT:PLAYER:유치
    TIMES SOURCE:3 , 1.20
    TIMES SOURCE:11 , 1.20
ENDIF
;조교자가［유아 퇴행］
SIF TALENT:PLAYER:유아퇴행
    TIMES SOURCE:11 , 1.20

;노예의 B⇔조교자의 손가락의 불결이 이동
STAIN:가슴 |= STAIN:PLAYER:손
STAIN:PLAYER:손 |= STAIN:가슴
;노예의 손가락⇔조교자의 B의 불결이 이동
STAIN:손 |= STAIN:PLAYER:가슴
STAIN:PLAYER:가슴 |= STAIN:손

IF TALENT:120 == 0 && TALENT:PLAYER:남자 == 0
    TCVAR:12 += 7
ELSEIF TALENT:120 && TALENT:PLAYER:남자
    TCVAR:13 += 7
ENDIF

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM344
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF (TALENT:PLAYER:유아 || TALENT:PLAYER:유치 || TALENT:PLAYER:유아퇴행) && (TALENT:130 || TALENT:131 || TALENT:유아퇴행)
    PRINTFORMW %조사선택(CALLNAME:PLAYER, "와")% %조사선택(CALLNAME:TARGET, "는")% 마치 아기가 어머니에게 젖을 요구하듯이 서로의 가슴을 비벼댔다.
ELSEIF TALENT:PLAYER:음유 && TALENT:음유
    PRINTFORMW %조사선택(CALLNAME:PLAYER, "와")% %조사선택(CALLNAME:TARGET, "는")% 서로의 가슴을 불쾌한 손놀림으로 비볐다….
ELSE
    PRINTFORMW %조사선택(CALLNAME:PLAYER, "와")% %조사선택(CALLNAME:TARGET, "는")% 서로의 가슴을 비볐다….
ENDIF
