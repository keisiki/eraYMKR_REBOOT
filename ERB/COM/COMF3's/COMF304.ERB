@COM_ABLE304
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:304 > 0
    RETURN 0
;조수가 없으면 불가
SIF ASSI < 0
    RETURN 0
;助手じゃないとダメ(フェラチオから来た場合は無視)
SIF SELECTCOM != 41 && PLAYER != ASSI
    RETURN 0
;主人にペニスがないとダメ
SIF EXIST_BAR(MASTER) == 0
    RETURN 0
;助手にペニスがないとダメ
SIF EXIST_BAR(ASSI) == 0
    RETURN 0
;주인이 【魅惑】か【謎の魅力】じゃないとダメ
SIF TALENT:MASTER:매혹 == 0 && TALENT:MASTER:수수께끼의매력 == 0
    RETURN 0
;対象が猫舌だと猫フェラパッチ有効でないとダメ
SIF TALENT:고양이혀 && (FLAG:조교모드 & 16) == 0
    RETURN 0
;猫舌だと기교5 · 奉仕精神5 · 순종5 · フェラ경험100が필요
SIF TALENT:고양이혀 && (ABL:기교 < 5 || ABL:봉사정신 < 5 || ABL:순종 < 5 || EXP:펠라경험 < 100)
    RETURN 0
;수면 중에는 불가
SIF EQUIP:14
    RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
    RETURN 0
;패닉 중에는 불가
SIF TFLAG:108 > 0
    RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:40
    RETURN 0
;볼 개그 사용 중에는 불가
SIF TEQUIP:42
    RETURN 0
;조수가 훈도시 착용 중일 경우에는 불가
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

