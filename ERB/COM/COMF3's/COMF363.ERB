@COM_ABLE363
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:363 > 0
    RETURN 0
;特殊風呂系커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 1024) == 0
    RETURN 0
;風呂を改装していないとダメ
SIF (FLAG:욕탕개조 & 8) == 0
    RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 1
    RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 2
    RETURN 0
;一度もスライム風呂に入ったことが無い場合, 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 4 && (CFLAG:23 & 4) == 0
    RETURN 0
;해제는 언제든 가능
SIF TEQUIP:55 == 4
    RETURN 1
;기계면 불가
SIF TALENT:기계
    RETURN 0
;수면 중에는 불가
SIF EQUIP:14
    RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
    RETURN 0
;전극사용 중에는 불가
SIF TEQUIP:20 == 4 || TEQUIP:25 == 4 || TEQUIP:30 == 4 || TEQUIP:30 == 6 || TEQUIP:32 == 4 || TEQUIP:35 == 4
    RETURN 0
;거미집에 걸려 있을 때는 불가
SIF TEQUIP:40 == 7
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;야외면 안 됨
SIF TEQUIP:52
    RETURN 0
;욕실 플레이 중에는 불가
SIF TEQUIP:53
    RETURN 0
;그밖의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 4
    RETURN 0
;의사 선생님 플레이 중에는 불가
SIF TEQUIP:57
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;코스프레 중에는 불가
SIF TEQUIP:59 > 2
    RETURN 0
;알몸 앞치마 중에는 불가
SIF TEQUIP:59 == 1
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;윤활과 슬라임의 점액에 의해 아픔은 거의 없는＋V경험을 주는 일 없이, 처녀막도 손상시키는 일 없이 쾌V의 주가 들어간다
;반면, 일탈과 불결의 상승수가 높은＋이상 경험이 적으면 반발 필수의 디메리트도 존재
;또, 조교 개시부터 V경험을 1개에서도 넣고 있을까 V감각이 1이상의 경우는 V절정과 쾌V의 값이 일정치를 넘으면(자) 처녀막을 찢어지게 된다
@COM363
PRINTL 슬라임 목욕탕
COM_NAME = 슬라임 목욕탕

LOCAL:2 = 1
LOCAL:3 = 1
;[남자]
SIF TALENT:120
    LOCAL:2 = 0
;바이브래이터등의 기구를 삽입하고 있다
SIF TEQUIP:20 || TEQUIP:21
    LOCAL:2 = 0
;애널 바이브래이터등의 기구를 삽입하고 있다
SIF TEQUIP:25 || TEQUIP:26
    LOCAL:3 = 0

CALL TRAIN_MESSAGE_COM363

IF TEQUIP:55 == 4
    TEQUIP:55 = 0
    TFLAG:155 = 0
ELSE
    TEQUIP:55 = 4
    SOURCE:30 = 15000
    SOURCE:19 = 15000
    SOURCE:31 = 15000
    IF LOCAL:2
        ;ABL:V감각을 본다
        IF ABL:V감각 == 0
            SOURCE:1 = 20
        ELSEIF ABL:V감각 == 1
            SOURCE:1 = 60
        ELSEIF ABL:V감각 == 2
            SOURCE:1 = 180
        ELSEIF ABL:V감각 == 3
            SOURCE:1 = 360
        ELSEIF ABL:V감각 == 4
            SOURCE:1 = 600
        ELSE
            SOURCE:1 = 900
        ENDIF
        SOURCE:20 += 300
        SOURCE:30 += 3000
        SOURCE:31 += 5000
    ENDIF
    IF LOCAL:3
        ;ABL:A감각을 본다
        IF ABL:A감각 == 0
            SOURCE:2 = 40
            SOURCE:22 = 500
        ELSEIF ABL:A감각 == 1
            SOURCE:2 = 100
            SOURCE:22 = 600
        ELSEIF ABL:A감각 == 2
            SOURCE:2 = 200
            SOURCE:22 = 800
        ELSEIF ABL:A감각 == 3
            SOURCE:2 = 500
            SOURCE:22 = 1100
        ELSEIF ABL:A감각 == 4
            SOURCE:2 = 800
            SOURCE:22 = 1500
        ELSE
            SOURCE:2 = 1200
            SOURCE:22 = 2000
        ENDIF
        SOURCE:20 += 500
        SOURCE:30 += 4000
        SOURCE:31 += 5000
    ENDIF
    IF LOCAL:2 || LOCAL:3
        ;PALAM:윤활을 본다
        IF PALAM:10 < PALAMLV:3
            TIMES SOURCE:1 , 1.00
            TIMES SOURCE:2 , 1.00
            SOURCE:30 += 1200
        ELSEIF PALAM:10 < PALAMLV:4
            TIMES SOURCE:1 , 1.20
            TIMES SOURCE:2 , 1.20
            SOURCE:30 += 1700
        ELSE
            TIMES SOURCE:1 , 1.40
            TIMES SOURCE:2 , 1.40
            SOURCE:30 += 2200
        ENDIF
    ENDIF
    LOCAL:1 = 100
    ;EXP:이상 경험을 본다
    IF EXP:이상경험 < EXPLV:1
        TIMES LOCAL:1 , 3.00
    ELSEIF EXP:이상경험 < EXPLV:2
        TIMES LOCAL:1 , 2.50
    ELSEIF EXP:이상경험 < EXPLV:3
        TIMES LOCAL:1 , 2.00
    ELSEIF EXP:이상경험 < EXPLV:4
        TIMES LOCAL:1 , 1.00
    ELSEIF EXP:이상경험 < EXPLV:5
        TIMES LOCAL:1 , 0.80
    ELSE
        TIMES LOCAL:1 , 0.60
    ENDIF
    ;ABL:C감각을 본다
    IF ABL:C감각 == 0
        SOURCE:0 = 50
    ELSEIF ABL:C감각 == 1
        SOURCE:0 = 150
    ELSEIF ABL:C감각 == 2
        SOURCE:0 = 800
    ELSEIF ABL:C감각 == 3
        SOURCE:0 = 1400
    ELSEIF ABL:C감각 == 4
        SOURCE:0 = 2000
    ELSE
        SOURCE:0 = 2700
    ENDIF
    ;ABL:B감각을 본다
    IF ABL:B감각 == 0
        SOURCE:3 = 40
    ELSEIF ABL:B감각 == 1
        SOURCE:3 = 100
    ELSEIF ABL:B감각 == 2
        SOURCE:3 = 250
    ELSEIF ABL:B감각 == 3
        SOURCE:3 = 800
    ELSEIF ABL:B감각 == 4
        SOURCE:3 = 1500
    ELSE
        SOURCE:3 = 2300
    ENDIF
    ;감정결여
    SIF TALENT:22
        TIMES LOCAL:1 , 0.60
    ;[작은체형]
    SIF TALENT:128
        TIMES SOURCE:20 , 1.20

    SOURCE:25 += LOCAL:1 * 8

    ;겁쟁이
    SIF TALENT:10
        TIMES LOCAL:1 , 2.00

    DOWNBASE:0 += LOCAL:1 / 4
    DOWNBASE:기력 += LOCAL:1 / 3
    SOURCE:31 += LOCAL:1 * 3

    IF LOCAL:3
        TFLAG:155 |= 2
        STAIN:애널 = 8
    ENDIF
    IF LOCAL:2
        TFLAG:155 |= 1
        STAIN:보지 = 1
    ENDIF
    STAIN:손 = 0
    STAIN:자지 = 2
    IF EXP:이상경험 < 3
        ;이상 경험이 적으면 반발 각인이 붙지만 그 처리는 TRACHECK.ERB에 맡긴다. 여기에서는 플래그만 세워 둔다.
        IF MARK:반발각인 <= 0 && MARK:쾌락각인 <= 1
            TFLAG:84 = 1
        ELSEIF MARK:반발각인 <= 1 && TALENT:연모 == 0 && TALENT:복종 == 0 && TALENT:사역마 == 0 && TALENT:음란 == 0 && (MARK:쾌락각인 <= 2 || EXP:이상경험 < 5)
            TFLAG:84 = 2
        ELSEIF MARK:반발각인 <= 2 && TALENT:처녀 == 1 && TALENT:처녀 && TALENT:연모 == 0 && TALENT:복종 == 0 && TALENT:사역마 == 0 && TALENT:음란 == 0 && EXP:이상경험 < 7
            TFLAG:84 = 3
        ENDIF
    ENDIF
    IF (CFLAG:23 & 4) == 0
        TCVAR:50 += 1
        CFLAG:23 |= 4
    ENDIF
ENDIF
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;--------------------------------------------------
;슬라임 목욕탕 입욕중
;--------------------------------------------------
@EQUIP_COM363
PRINTL <슬라임 목욕탕 입욕 중>

LOCAL:3 = 1
LOCAL:4 = 1
;[남자]
SIF TALENT:120
    LOCAL:3 = 0
;바이브래이터등의 기구를 삽입하고 있다
SIF TEQUIP:20 || TEQUIP:21
    LOCAL:3 = 0
;애널 바이브래이터등의 기구를 삽입하고 있다
SIF TEQUIP:25 || TEQUIP:26
    LOCAL:4 = 0

IF LOCAL:3 || TFLAG:155 & 1
    IF TALENT:처녀 && (ABL:V감각 || CFLAG:11 & 1) && PALAM:1 >= 6000 && EX:1 > 9
        SIF TALENT:처녀 == 1
            CFLAG:500 = 1010
        TALENT:처녀 = 0
        SOURCE:20 += 1000
        PRINTFORMW 자궁에 들어오지 못했던 슬라임들은, 기세가 지나쳐 %CALLNAME:TARGET%의 처녀막을 찢어 버렸다….
        CALL SETCOLOR_TORIKOMODE, "RED", 3
        PRINTW 【처녀 상실】
        RESETCOLOR
    ENDIF
    ;ABL:V감각을 본다
    IF ABL:V감각 == 0
        LOCAL:1 = 10
    ELSEIF ABL:V감각 == 1
        LOCAL:1 = 30
    ELSEIF ABL:V감각 == 2
        LOCAL:1 = 60
    ELSEIF ABL:V감각 == 3
        LOCAL:1 = 120
    ELSEIF ABL:V감각 == 4
        LOCAL:1 = 240
    ELSE
        LOCAL:1 = 480
    ENDIF
    
    SIF TFLAG:169 == 1 && LOCAL:3 == 0
        TIMES LOCAL:1 , 0.80
    
    SOURCE:1 += LOCAL:1
ENDIF
IF LOCAL:4 || TFLAG:155 & 2
    ;ABL:A감각을 본다
    IF ABL:A감각 == 0
        LOCAL:2 = 15
    ELSEIF ABL:A감각 == 1
        LOCAL:2 = 45
    ELSEIF ABL:A감각 == 2
        LOCAL:2 = 90
    ELSEIF ABL:A감각 == 3
        LOCAL:2 = 180
    ELSEIF ABL:A감각 == 4
        LOCAL:2 = 260
    ELSE
        LOCAL:2 = 520
    ENDIF
    
    SIF TFLAG:170 == 1 && LOCAL:4 == 0
        TIMES LOCAL:2 , 0.80
    
    SOURCE:2 += LOCAL:2
    SOURCE:22 += LOCAL:2
ENDIF

;ABL:C감각을 본다
IF ABL:C감각 == 0
    SOURCE:0 += 25
ELSEIF ABL:C감각 == 1
    SOURCE:0 += 50
ELSEIF ABL:C감각 == 2
    SOURCE:0 += 150
ELSEIF ABL:C감각 == 3
    SOURCE:0 += 300
ELSEIF ABL:C감각 == 4
    SOURCE:0 += 600
ELSE
    SOURCE:0 += 900
ENDIF
;ABL:B감각을 본다
IF ABL:B감각 == 0
    SOURCE:3 += 20
ELSEIF ABL:B감각 == 1
    SOURCE:3 += 40
ELSEIF ABL:B감각 == 2
    SOURCE:3 += 80
ELSEIF ABL:B감각 == 3
    SOURCE:3 += 160
ELSEIF ABL:B감각 == 4
    SOURCE:3 += 320
ELSE
    SOURCE:3 += 640
ENDIF

LOCAL = 100
;EXP:이상 경험을 본다
IF EXP:이상경험 < EXPLV:1
    TIMES LOCAL , 3.00
ELSEIF EXP:이상경험 < EXPLV:2
    TIMES LOCAL , 2.50
ELSEIF EXP:이상경험 < EXPLV:3
    TIMES LOCAL , 2.00
ELSEIF EXP:이상경험 < EXPLV:4
    TIMES LOCAL , 1.00
ELSEIF EXP:이상경험 < EXPLV:5
    TIMES LOCAL , 0.80
ELSE
    TIMES LOCAL , 0.60
ENDIF
;감정결여
SIF TALENT:22
    TIMES LOCAL , 0.60
;[소인]
SIF TALENT:127
    TIMES LOCAL , 1.80
;[작은체형]
SIF TALENT:128
    TIMES LOCAL , 1.20

SOURCE:25 += LOCAL * 5

;겁쟁이
SIF TALENT:10
    TIMES LOCAL , 2.00

DOWNBASE:0 += LOCAL / 2
DOWNBASE:기력 += LOCAL / 3

SOURCE:30 += LOCAL * 5
SOURCE:19 += 1000
SOURCE:31 += LOCAL * 2

STAIN:손 = 0
STAIN:자지 = 2

TIMES SOURCE:20 , 1.20

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM363
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF TEQUIP:55 == 4
    PRINTFORMW %조사선택(CALLNAME:TARGET, "를")% 슬라임 속에서 꺼내자, 약간 안도한 것 같은 얼굴을 하고 있었다.
ELSE
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %조사선택(CALLNAME:TARGET, "를")% 슬라임으로 채워진 목욕탕통안에 빠뜨렸다.
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 슬라임이 분비하는 체액의 감촉에 몸부림치고 있다….
    IF TEQUIP:25 == 0 && TEQUIP:26 == 0
        PRINTL
        PRINTFORML 슬라임들은 %CALLNAME:TARGET%의 무방비인 애널에 침입을 시도했다.
        PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 슬라임을 막으려 했지만, 미끌미끌한 슬라임의 체액 때문에 
        PRINTW 장내를 유린당해버렸다….
    ENDIF
    IF TALENT:120 == 0 && TEQUIP:20 == 0 && TEQUIP:21 == 0
        PRINTL
        SIF TEQUIP:25 == 0 && TEQUIP:26 == 0
            PRINT 또 다른 
        PRINTFORML 슬라임들은 무늬 비어있는 %CALLNAME:TARGET%의 질구로 쇄도했다.
        IF TALENT:처녀
            PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 처녀막을 찢지 않고 자궁 내에 침입한 슬라임들이
            PRINTL 여기 저기 자궁벽을 기어가는 무서운 감촉에 아무 말도 하지 못하고 있다.
            PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 눈을 크게 뜨고 자신의 비소를 바라보고 있다….
        ELSE
            PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 일반적으로는 경험할 수 없는 감촉에
            PRINTW 눈을 크게 뜬 채로 굳어버렸다….
        ENDIF
    ENDIF
ENDIF
