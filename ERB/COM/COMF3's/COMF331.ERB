ELSEIF ABL:C감각 == 1
    SOURCE:0 = 1700
ELSEIF ABL:C감각 == 2
    SOURCE:0 = 2400
ELSEIF ABL:C감각 == 3
    SOURCE:0 = 3000
ELSEIF ABL:C감각 == 4
    SOURCE:0 = 3500
ELSE
    SOURCE:0 = 4000
