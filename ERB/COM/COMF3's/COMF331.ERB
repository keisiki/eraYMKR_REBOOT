@COM_ABLE331
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:331 > 0
    RETURN 0
;조교자奉仕 · ウフフ系커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 2048) == 0
    RETURN 0
;조수가 없으면 불가
SIF ASSI < 0
    RETURN 0
;性器が汚れている場合フェラするはできない(조교자가 汚れ無視か愛液, Ｐ, 母乳のよごれならＯＫ)
SIF STAIN:자지 >= 4 && (STAIN:자지 != 18 && STAIN:자지 != 19) && TALENT:MASTER:불결무시 == 0
    RETURN 0
;性器が汚れており, 汚臭敏感で순종4미만의 ASSI일 경우フェラはできない(조교자가 汚れ無視か愛液, Ｐ, 母乳のよごれならＯＫ)
IF STAIN:자지 >= 4 && (STAIN:자지 != 18 && STAIN:자지 != 19) 
    SIF ABL:ASSI:순종 < 4 && TALENT:ASSI:냄새민감 && TALENT:ASSI:불결무시 == 0
        RETURN 0
ENDIF
;순종4미만 · 레즈끼(BL끼) 4 미만의 ASSI일 경우, 조교 대상이 동성이라면 실행할 수 없다(망신か복종があるならＯＫ)
SIF (ABL:ASSI:순종 < 4 || ((TALENT:남자 == 0 && TALENT:ASSI:남자 == 0 && ABL:ASSI:레즈끼 < 4) || (TALENT:남자 && TALENT:ASSI:남자 && ABL:ASSI:BL끼 < 4))) && TALENT:ASSI:망신 == 0 && TALENT:ASSI:복종 == 0
    RETURN 0
;主人と助手のいずれかもしくは쌍방이 猫舌だと猫フェラパッチ有効でないとダメ
SIF (TALENT:MASTER:고양이혀 || TALENT:ASSI:고양이혀) && (FLAG:조교모드 & 16) == 0
    RETURN 0
;主人と助手のいずれかもしくは쌍방이 猫舌だと該当者の기교4필요
SIF (TALENT:MASTER:고양이혀 && ABL:MASTER:기교 < 4) || (TALENT:ASSI:고양이혀 && ABL:ASSI:기교 < 4)
    RETURN 0
;조교 대상에게 페니스가 없으면 불가
SIF EXIST_BAR(TARGET) == 0
    RETURN 0
;오나홀 · 전극 오나홀사용 중에는 불가
SIF TEQUIP:30 == 2 || TEQUIP:30 == 6
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;한증탕과 암반욕 외의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;훈도시 착용 중일 경우는 안 됨
SIF TEQUIP:59 == 17
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

