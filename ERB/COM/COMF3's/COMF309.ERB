        RESULTS = %RESULTS% + 
    LOCAL:99 = ABL:ASSI:봉사정신
    TFLAG:240 += LOCAL:99 * 4
    RESULTS = %RESULTS% 조수의 %ABLNAME:13%LV{LOCAL:99}({LOCAL:99 * 4})
ENDIF

;조수의 쾌락 각인
IF MARK:ASSI:쾌락각인
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = MARK:ASSI:쾌락각인
    TFLAG:240 += LOCAL:99 * 2
    RESULTS = %RESULTS% 조수의 %MARKNAME:1%LV{LOCAL:99}({LOCAL:99 * 2})
ENDIF

