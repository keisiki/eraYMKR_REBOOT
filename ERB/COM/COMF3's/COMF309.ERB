;    RESULTS = %RESULTS% 조수의 %PALAMNAME:13%LV{LOCAL:2}({LOCAL:99})
;ENDIF

;조수가 부끄러워해
IF TALENT:ASSI:35
    RESULTS = %RESULTS% - 
    LOCAL:99 = 1
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% 조수가 %TALENTNAME:35%({LOCAL:99})
ENDIF
;조수가 악취 둔감
IF TALENT:ASSI:61
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 1
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% 조수가 %TALENTNAME:61%({LOCAL:99})
;조수가 악취 민감
ELSEIF TALENT:ASSI:62
    RESULTS = %RESULTS% - 
    LOCAL:99 = 1
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% 조수가 %TALENTNAME:62%({LOCAL:99})
ENDIF
;조수가 헌신적
IF TALENT:ASSI:63
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 6
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% 조수가 %TALENTNAME:63%({LOCAL:99})
ENDIF
;조수가 쾌감의 부정
IF TALENT:ASSI:71
    RESULTS = %RESULTS% - 
    LOCAL:99 = 1
    TFLAG:240 -= LOCAL:99
    RESULTS = %RESULTS% 조수가 %TALENTNAME:71%({LOCAL:99})
ENDIF
;조수가 연모
IF TALENT:ASSI:150
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 5
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% 조수가 %TALENTNAME:150%({LOCAL:99})
ENDIF
;조수가 키스마
IF TALENT:ASSI:176
    SIF LOCAL:99
        RESULTS = %RESULTS% + 
    LOCAL:99 = 10
    TFLAG:240 += LOCAL:99
    RESULTS = %RESULTS% 조수가 %TALENTNAME:176%({LOCAL:99})
ENDIF

;조교자의 입의 더러움(조수의 수정이므로, 대상을 퇴피해 계산한 뒤에 되돌린다)
LOCAL:98 = TARGET
TARGET = ASSI
TFLAG:242 = COMORDER_STAIN_CALC(0, 0)
TARGET = LOCAL:98
;키스에서는 더러움이 그만큼 영향을 주지 않는다
TFLAG:242 /= 2

;서로 불결
IF TFLAG:242
    RESULTS = %RESULTS% - 
    TFLAG:240 -= TFLAG:242
    RESULTS = %RESULTS% 서로 불결
    ;악취 둔감
    IF TALENT:ASSI:61
        RESULTS = %RESULTS%,%조사처리(CALLNAME:ASSI,"가")% %TALENTNAME:61%
    ELSEIF TALENT:ASSI:62
        RESULTS = %RESULTS%,%조사처리(CALLNAME:ASSI,"가")% %TALENTNAME:62%
    ENDIF
    RESULTS = %RESULTS%({TFLAG:242})
    LOCAL:99 = 1
ENDIF

