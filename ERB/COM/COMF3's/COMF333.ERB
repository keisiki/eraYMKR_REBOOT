    SOURCE:15 = 1500
ELSEIF ABL:C감각 == 2
    SOURCE:0 = 1500
    SOURCE:15 = 1800
ELSEIF ABL:C감각 == 3
    SOURCE:0 = 2000
    SOURCE:15 = 2300
ELSEIF ABL:C감각 == 4
    SOURCE:0 = 2500
    SOURCE:15 = 2700
ELSE
