;-------------------------------------------------
DOWNBASE:0 += 60
DOWNBASE:기력 += 260

SOURCE:10 = 700
SOURCE:11 = 800
SOURCE:14 = 1200
SOURCE:18 = 550
SOURCE:24 = 600
SOURCE:31 = 500

