TCVAR:PLAYER:1 += 1
SIF ASSIPLAY == 0 && EXIST_BAR(TARGET)
    CFLAG:주인의A범한회수 += 1

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
;노예의 사정은 TRAIN_CHECK.ERB에서 관리
    ;사정처 타입의 설정
    TFLAG:703 = 16
    ;사정처 선택의 가능·불가
    TFLAG:704 = 0
    ;노예 사정처 선정이 끝난 플래그
    TFLAG:705 = 1

IF EXIST_BAR(TARGET)
    ;노예의 P⇔조교자의 A의 불결이 이동
    STAIN:자지 |= STAIN:PLAYER:애널
    STAIN:PLAYER:애널 |= STAIN:자지
ENDIF

IF TALENT:남자 == 0 && TALENT:PLAYER:남자 == 0
    TCVAR:12 += 10
ELSEIF TALENT:남자 && TALENT:PLAYER:남자
    TCVAR:13 += 10
ENDIF

RETURN 1

