        TIMES SOURCE:16 , 5.00
        TIMES SOURCE:22, 0.40
    ELSE
        SOURCE:24 = 6000
        TIMES SOURCE:16 , 6.00
        TIMES SOURCE:22, 0.10
    ENDIF

    ;射精先タイプの設定
    TFLAG:703 = 4
    ;射精先選択の可・不可
    TFLAG:704 = 1
    ;汎用的な処理はこの関数で行う
    ;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
    CALL SAMEN_SHOOT
