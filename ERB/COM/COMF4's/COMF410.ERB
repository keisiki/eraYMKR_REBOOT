@COM_ABLE410
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:410 > 0
    RETURN 0
;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 16384) == 0
    RETURN 0
;ヤマメのスカートの中は蜘蛛の脚という設定でないとダメ
SIF (FLAG:커맨드확장 & 65536) == 0
    RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 2
    RETURN 0
;조교자가 ヤマメでないとダメ
SIF NO:PLAYER != 58
    RETURN 0
;조교 대상이 [소인]일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:127 && TALENT:MASTER:56 == 0
    RETURN 0
;グルグル巻きにしないとダメ
SIF TEQUIP:40 != 6 && TEQUIP:40 != 7
    RETURN 0
;ボールギャグ装着中はダメ
SIF TEQUIP:42
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;욕실 플레이 중에는 불가
SIF TEQUIP:53
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;조수가 코스프레 중에는 불가
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 > 1
    RETURN 0
;알몸 앞치마, 학교수영복 플레이, 코스프레 중에는 불가
SIF TEQUIP:59
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

