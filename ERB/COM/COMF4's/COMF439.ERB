;=============================================================================
;양 구멍 요석
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE439
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:439 > 0
    RETURN 0
;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 16384) == 0
    RETURN 0
;고문계, 정신조작계커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 16) == 0
    RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
    RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
    RETURN 0
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4
    RETURN 0
;조교자もしくは조교 대상이 天子でないとダメ
SIF NO:TARGET != 52 && NO:PLAYER != 52
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
;해제는 언제든 가능
SIF TEQUIP:11 == 2 && TEQUIP:25 == 2
    RETURN 1
;남자라면 안 됨
SIF TALENT:120
    RETURN 0
;일정의 Ｖ · A경험이 필요
IF EXP:0 < EXPLV:4 || EXP:1 < EXPLV:4
    RETURN 0
ENDIF
;그 조교 중에 관장 플레이＋배설이 있었을 경우, 조교자가 汚れ無視もしくは汚臭鈍感を持たないなら実行できない
SIF TFLAG:140 & 256 && TALENT:PLAYER:61 == 0 && TALENT:PLAYER:64 == 0
    RETURN 0
;ASSI일 경우, 순종3이상이거나 새드, または망신か복종じゃないとできない。
IF ASSIPLAY
    SIF ABL:ASSI:순종 < 4 && TALENT:ASSI:81 == 0 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
        RETURN 0
ENDIF
;[작은체형]なら순종이 4이상かつ마조끼が4이상じゃないと実行できない
IF TALENT:128
    SIF ABL:순종 < 4 || ABL:마조끼 < 4
        RETURN 0
ENDIF
;조교 대상이 [소인]일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:127 && TALENT:MASTER:56 == 0
    RETURN 0
;그밖의 Ｖ계 장착구 사용 중에는 불가
SIF (TEQUIP:20 && TEQUIP:20 != 2) || TEQUIP:21
    RETURN 0
;그밖의 Ａ계 장착구 사용 중에는 불가
SIF (TEQUIP:25 && TEQUIP:25 != 2) || TEQUIP:26
    RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
    RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
    RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;알몸 앞치마, 학교수영복 플레이, 코스프레 중에는 불가
SIF TEQUIP:59
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM439
PRINTL 양 구멍 요석
COM_NAME = 양구멍 요석
CALL TRAIN_MESSAGE_COM439

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
SOURCE:20 = 1800
SOURCE:19 = 2800
SOURCE:18 = 1500
SOURCE:12 = 2500

SOURCE:31 = 2500
SOURCE:17 = 2500


IF TEQUIP:20 == 2 && TEQUIP:25 == 2
    DOWNBASE:0 += 600
    DOWNBASE:1 += 500

    ;ABL:V감각을 본다
    IF ABL:V감각 == 0
        SOURCE:1 = 2000
    ELSEIF ABL:V감각 == 1
        SOURCE:1 = 2500
    ELSEIF ABL:V감각 == 2
        SOURCE:1 = 3000
    ELSEIF ABL:V감각 == 3
        SOURCE:1 = 4000
    ELSEIF ABL:V감각 == 4
        SOURCE:1 = 5000
    ELSE
        SOURCE:1 = 6000
    ENDIF
    
    ;ABL:A감각을 본다
    IF ABL:A감각 == 0
        SOURCE:2 = 1000
        SOURCE:11 = 10
        SOURCE:22 = 100
    ELSEIF ABL:A감각 == 1
        SOURCE:2 = 2000
        SOURCE:11 = 30
        SOURCE:22 = 700
    ELSEIF ABL:A감각 == 2
        SOURCE:2 = 3000
        SOURCE:11 = 100
        SOURCE:22 = 1500
    ELSEIF ABL:A감각 == 3
        SOURCE:2 = 4500
        SOURCE:11 = 200
        SOURCE:22 = 3000
    ELSEIF ABL:A감각 == 4
        SOURCE:2 = 6000
        SOURCE:11 = 450
        SOURCE:22 = 5000
    ELSE
        SOURCE:2 = 8000
        SOURCE:11 = 750
        SOURCE:22 = 8000
    ENDIF

    ;요석을 끼고 있던 턴보다 쾌V와 체력 소모 수정
    IF TFLAG:169
        SOURCE:1 = 250 * TFLAG:169
        DOWNBASE:0 += 50 * TFLAG:169
    ENDIF
    ;어널 요석을 끼고 있던 턴보다 쾌A와 체력 소모 수정
    IF TFLAG:170
        SOURCE:2 = 200 * TFLAG:170
        DOWNBASE:0 += 75 * TFLAG:170
    ENDIF
ELSE
    DOWNBASE:0 += 100
    DOWNBASE:1 += 300
    
    ;ABL:V감각을 본다
    IF ABL:V감각 == 0
        SOURCE:1 = 10
    ELSEIF ABL:V감각 == 1
        SOURCE:1 = 50
    ELSEIF ABL:V감각 == 2
        SOURCE:1 = 100
    ELSEIF ABL:V감각 == 3
        SOURCE:1 = 200
    ELSEIF ABL:V감각 == 4
        SOURCE:1 = 600
    ELSE
        SOURCE:1 = 1000
    ENDIF

    ;ABL:A감각을 본다
    IF ABL:A감각 == 0
        SOURCE:2 = 80
        SOURCE:22 = 300
    ELSEIF ABL:A감각 == 1
        SOURCE:2 = 250
        SOURCE:22 = 800
    ELSEIF ABL:A감각 == 2
        SOURCE:2 = 600
        SOURCE:22 = 1400
    ELSEIF ABL:A감각 == 3
        SOURCE:2 = 1000
        SOURCE:22 = 1800
    ELSEIF ABL:A감각 == 4
        SOURCE:2 = 1300
        SOURCE:22 = 2100
    ELSE
        SOURCE:2 = 1700
        SOURCE:22 = 2400
    ENDIF
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 += 50
ELSEIF EXP:0 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    SOURCE:20 += 10
ELSEIF EXP:0 < EXPLV:5
    TIMES SOURCE:1 , 1.40
    SOURCE:20 += 0
ELSE
    TIMES SOURCE:1 , 1.60
    SOURCE:20 = 0
ENDIF

;EXP:A경험을 본다
IF EXP:1 < EXPLV:3
    TIMES SOURCE:2 , 0.50
    SOURCE:20 += 5000
    SOURCE:18 += 1000
ELSEIF EXP:1 < EXPLV:4
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 2000
    SOURCE:18 += 1000
ELSEIF EXP:1 < EXPLV:5
    TIMES SOURCE:2 , 1.20
    SOURCE:20 += 1000
    SOURCE:18 += 200
ELSE
    TIMES SOURCE:1 , 1.40
    SOURCE:20 += 600
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.20
    TIMES SOURCE:2 , 0.20
    SOURCE:18 += 1000
    SOURCE:20 += 1900
    TIMES SOURCE:20 , 9.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:2 , 0.40
    SOURCE:18 += 800
    SOURCE:20 += 1250
    TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 0.60
    SOURCE:18 += 600
    SOURCE:20 += 1000
    TIMES SOURCE:20 , 1.50
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.30
    TIMES SOURCE:2 , 1.00
    SOURCE:18 += 200
    SOURCE:20 += 200
    TIMES SOURCE:20 , 0.30
ELSE
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:2 , 1.30
    TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:2 , 0.60
    TIMES SOURCE:22 , 0.60
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:2 , 0.80
    TIMES SOURCE:22 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:22 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:22 , 1.20
ELSE
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:2 , 1.40
    TIMES SOURCE:22 , 1.40
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:1 , 0.50
    TIMES SOURCE:2 , 0.70
    TIMES SOURCE:11 , 0.60
    TIMES SOURCE:18, 2.00
ELSEIF ABL:순종 == 1
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:2 , 0.90
    TIMES SOURCE:11 , 0.80
    TIMES SOURCE:18, 1.20
ELSEIF ABL:순종 == 2
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:11 , 1.00
    TIMES SOURCE:18, 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.10
    TIMES SOURCE:11 , 1.20
    TIMES SOURCE:18, 0.60
ELSEIF ABL:순종 == 4
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:11 , 1.40
    TIMES SOURCE:18, 0.30
ELSE
    TIMES SOURCE:1 , 1.70
    TIMES SOURCE:2 , 1.30
    TIMES SOURCE:11 , 1.60
    TIMES SOURCE:18, 0.10
ENDIF

;[작은체형]
SIF TALENT:128
    TIMES SOURCE:20 , 3.00

;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:103
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:102
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:31 , 0.60
ENDIF

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:18 , 1.50
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:18 , 0.60
    TIMES SOURCE:22 , 0.60
    TIMES SOURCE:31 , 0.60
ENDIF

;정조관념
IF TALENT:30
    TIMES SOURCE:11 , 0.60
    TIMES SOURCE:18, 1.80
;정조무구애
ELSEIF TALENT:31
    TIMES SOURCE:18, 0.30
ENDIF

;요석의 착탈
IF TEQUIP:20 == 2 && TEQUIP:25 == 2
    SOURCE:31 *= 2
    TFLAG:169 = 0
    TFLAG:170 = 0
    TCVAR:0 += 25
    TCVAR:1 += 25
    TCVAR:32 += 3
    TCVAR:33 += 3
    TEQUIP:20 = 0
    TEQUIP:25 = 0
ELSE
    LOCAL:1 = 0
    TCVAR:0 += 5
    TCVAR:1 += 2
    SIF EXP:32 == 0
        LOCAL:1 += 1
    SIF EXP:33 == 0
        LOCAL:1 += 1
    SIF LOCAL:1
        TCVAR:50 += LOCAL:1
    TCVAR:32 += 1
    TCVAR:33 += 1
    TEQUIP:20 = 2
    TEQUIP:25 = 2
ENDIF

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM439
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF TEQUIP:20 == 2 && TEQUIP:25 == 2
    PRINTFORML %CALLNAME:TARGET%의 질구와 항문에 끼워져 있던 요석을 뽑아 냈다.
    PRINTFORML 질 내의 압박감에서 해방된 %조사선택(CALLNAME:TARGET, "가")% 잠깐 숨을 돌리는 순간,
    PRINTW 말하기 어려운 쾌감이 엄습해왔다.
    PRINTL
    PRINTFORML 하반신에게 전해지는 강렬한 자극을 참을 수 없게 되어,
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 짐승과 같은 큰 소리로 외치며
    PRINTL 항문과 보지로부터 대량의 액체를 흩뿌렸다.
    PRINTFORML 이윽고 힘이 다했는지 %조사선택(CALLNAME:TARGET, "는")%
    PRINTW 스스로의 흘려 보낸 액체 위에 쓰러져 사지를 경련시켰다….
ELSE
    PRINTFORM %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 
    SIF PALAM:10 >= PALAMLV:5
        PRINT 충분히 
    SIF PALAM:10 >= PALAMLV:3
        PRINT 점액으로 미끌거리는 
    IF TALENT:128
        PRINT 어린 균열과 
    ELSE
        PRINT 보지와 
    ENDIF
    SIF PALAM:10 >= PALAMLV:4
        PRINT 점액 투성이가 된 
    PRINTL 보지와 항문에 요석을 찔러 넣었다….
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 양 구멍을 한계까지 넓혀서
    PRINTW 교반되어지는 자극에 견디지 못하고 소리 없이 절규하고 있다….
ENDIF
