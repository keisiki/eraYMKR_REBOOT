STAIN:PLAYER:자지 |= STAIN:보지

SIF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 7

;주인 경험 플래그
SIF ASSIPLAY == 0
    TFLAG:50 += (ABL:V감각 >= 3) ?  3 # 2

;조교자가 후타나리
SIF TALENT:PLAYER:121
    SOURCE:22 /= 2
