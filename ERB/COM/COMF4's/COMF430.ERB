    SOURCE:12 = 12000
    SOURCE:31 = 140000
ELSEIF EXP:V경험 < EXPLV:2
    TIMES SOURCE:1 , 0.60
    SOURCE:20 = 640
ELSEIF EXP:V경험 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 = 80
ELSEIF EXP:V경험 < EXPLV:4
