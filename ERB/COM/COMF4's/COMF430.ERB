    ;실행할 수 없다
    SIF RESULT == 0
        RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = 수레바퀴
CALL TRAIN_MESSAGE_COM430

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1
SIF TALENT:처녀 && TALENT:150 && ASSIPLAY == 0
    TFLAG:2 = TALENT:처녀

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:기교 == 0
    LOCAL = 1000
ELSEIF ABL:기교 == 1
    LOCAL = 1300
ELSEIF ABL:기교 == 2
    LOCAL = 1700
ELSEIF ABL:기교 == 3
    LOCAL = 2200
ELSEIF ABL:기교 == 4
    LOCAL = 3000
ELSE
    LOCAL = 4500
ENDIF

;윤활을 본다
LOCAL = EJACULATIONCHECK_PALAM_10_3(TARGET, LOCAL)
;욕정을 본다
