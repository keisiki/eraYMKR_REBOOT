        TIMES SOURCE:11 , 0.60
        TIMES SOURCE:31, 15.00
        SOURCE:32 = 17000
    ELSE
        TIMES SOURCE:11 , 0.60
        TIMES SOURCE:31, 15.00
        SOURCE:32 = 1500
    ENDIF
;정조무구애
ELSEIF TALENT:31
    IF TALENT:처녀 == 1
        TIMES SOURCE:31, 0.50
        SOURCE:32 = 450
    ELSE
        TIMES SOURCE:31, 0.50
    ENDIF
ELSE
    SIF TALENT:처녀 == 1
        SOURCE:32 = 4500
ENDIF

;PALAM:욕정
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:11 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:11 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:11 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:11 , 1.50
ELSEIF PALAM:13 < PALAMLV:5
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:11 , 1.80
ELSE
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:11 , 2.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
