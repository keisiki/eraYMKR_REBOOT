    SOURCE:11 = 20
    TIMES SOURCE:15 , 0.50
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 90
    SOURCE:11 = 70
    TIMES SOURCE:15 , 0.80
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 300
    SOURCE:11 = 120
    TIMES SOURCE:15 , 1.00
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 600
    SOURCE:11 = 190
    TIMES SOURCE:15 , 1.20
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 900
    SOURCE:11 = 330
    TIMES SOURCE:15 , 1.50
