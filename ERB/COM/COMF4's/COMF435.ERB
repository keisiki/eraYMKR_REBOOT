@COM435
;조교 대상이 긴박 하고 있지 않는 반발 각인 3의 하크타크였던 경우, 역습 될 가능성대
IF NO:TARGET == 26 && TFLAG:199 && MARK:9 >= 3 && TEQUIP:40 == 0 && FLAG:15 & 32768
    IF RAND:4 > 0 && (TFLAG:140 & 32) == 0
        TFLAG:140 |= 32
        JUMP CAVED
    ENDIF
ENDIF

PRINTL 뿔 삽입 
COM_NAME = 뿔 삽입 
CALL TRAIN_MESSAGE_COM435

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

DOWNBASE:0 += 700
DOWNBASE:1 += 350

SOURCE:11 = 200
SOURCE:20 = 2500
SOURCE:12 = 1200
SOURCE:22 = 1800
SOURCE:31 = 2500

;ABL:V감각을 본다
IF ABL:1 == 0
    SOURCE:1 = 1700
ELSEIF ABL:1 == 1
    SOURCE:1 = 2000
ELSEIF ABL:1 == 2
    SOURCE:1 = 2500
ELSEIF ABL:1 == 3
    SOURCE:1 = 3000
ELSEIF ABL:1 == 4
    SOURCE:1 = 3500
ELSE
    SOURCE:1 = 4000
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
    TIMES SOURCE:1 , 0.20
    SOURCE:20 += 5000
    SIF ASSIPLAY && TALENT:PLAYER:120 == 0
        TCVAR:50 += 2
ELSEIF EXP:0 < EXPLV:2
    TIMES SOURCE:1 , 0.60
    SOURCE:20 += 600
ELSEIF EXP:0 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 += 300
ELSEIF EXP:0 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    SOURCE:20 += 30
ELSEIF EXP:0 < EXPLV:5
    TIMES SOURCE:1 , 1.30
    SOURCE:20 += 3
ELSE
    TIMES SOURCE:1 , 1.80
    SOURCE:20 += 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.10
    SOURCE:20 += 900
    TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.40
    SOURCE:20 += 250
    TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:20 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:20 , 0.20
ELSE
    TIMES SOURCE:1 , 1.80
    TIMES SOURCE:20 , 0.10
ENDIF

;정조관념
IF TALENT:30
    IF TALENT:처녀 == 1
        TIMES SOURCE:11 , 0.60
        SOURCE:32 = 10000
    ELSE
        TIMES SOURCE:11 , 0.60
        SOURCE:32 = 1000
    ENDIF
;정조무구애
ELSEIF TALENT:31
    IF TALENT:처녀 == 1
        TIMES SOURCE:11 , 0.60
        SOURCE:32 = 300
    ENDIF
ELSE
    SIF TALENT:처녀 == 1
        SOURCE:32 = 3000
ENDIF

;[작은체형]
SIF TALENT:128
    TIMES SOURCE:20 , 2.00

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.60
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.20
ELSE
    TIMES SOURCE:1 , 1.50
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
    TIMES SOURCE:1 , 0.50
    TIMES SOURCE:32 , 2.00
ELSEIF ABL:10 == 1
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:32 , 1.50
ELSEIF ABL:10 == 2
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:32 , 1.00
ELSEIF ABL:10 == 3
    TIMES SOURCE:1 , 1.30
    TIMES SOURCE:32 , 0.80
ELSEIF ABL:10 == 4
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:32 , 0.60
ELSE
    TIMES SOURCE:1 , 2.00
    TIMES SOURCE:32 , 0.30
ENDIF

TCVAR:0 += 20
TCVAR:32 += 1

SIF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 4
;주인 경험 플래그
SIF ASSIPLAY == 0
    TFLAG:50 += (ABL:1 >= 3) ?  2 # 1

RETURN 1

