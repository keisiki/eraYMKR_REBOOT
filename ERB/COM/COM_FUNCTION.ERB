;=============================================================================
;조교 중의 조합 판정
;=============================================================================
;-------------------------------------------------
;조교 중의 페어 판정
;-------------------------------------------------
;奴隷と助手、主人と助手等特定のペアを判定。呼び出し元からARG及びARG:1に代入される
;素股→サンドウィッチへの派生判定や、Ｗ系コマンドにおいて特定ペアのときの表示変更に使用
@JUDGE_TRAIN_PAIR(ARG, ARG:1)
TFLAG:175 = 0
;スカーレット姉妹
IF (NO:ARG == 10 && NO:(ARG:1) == 11) || (NO:ARG == 11 && NO:(ARG:1) == 10)
    TFLAG:175 = 1
;霊夢と早苗
ELSEIF (NO:ARG == 1 && NO:(ARG:1) == 42) || (NO:ARG == 42 && NO:(ARG:1) == 1)
    TFLAG:175 = 2
;秋姉妹
ELSEIF (NO:ARG == 37 && NO:(ARG:1) == 38) || (NO:ARG == 38 && NO:(ARG:1) == 37)
    TFLAG:175 = 3
;綿月姉妹
ELSEIF (NO:ARG == 53 && NO:(ARG:1) == 54) || (NO:ARG == 54 && NO:(ARG:1) == 53)
    TFLAG:175 = 4
;蓮子とメリー
ELSEIF (NO:ARG == 49 && NO:(ARG:1) == 50) || (NO:ARG == 50 && NO:(ARG:1) == 49)
    TFLAG:175 = 5
;リリー達
ELSEIF (NO:ARG == 15 && NO:(ARG:1) == 56) || (NO:ARG == 56 && NO:(ARG:1) == 15)
    TFLAG:175 = 6
;古明地姉妹
ELSEIF (NO:ARG == 61 && NO:(ARG:1) == 64) || (NO:ARG == 64 && NO:(ARG:1) == 61)
    TFLAG:175 = 7
;幻月と夢月姉妹
ELSEIF (NO:ARG == 112 && NO:(ARG:1) == 113) || (NO:ARG == 113 && NO:(ARG:1) == 112)
    TFLAG:175 = 8
;ゆかりんとゆゆ様
ELSEIF (NO:ARG == 20 && NO:(ARG:1) == 22) || (NO:ARG == 22 && NO:(ARG:1) == 20)
    TFLAG:175 = 9
;ルナサとメルラン
ELSEIF (NO:ARG == 18 && NO:(ARG:1) == 17) || (NO:ARG == 17 && NO:(ARG:1) == 18)
    TFLAG:175 = 10
;リリカとルナサ
ELSEIF (NO:ARG == 16 && NO:(ARG:1) == 18) || (NO:ARG == 18 && NO:(ARG:1) == 16)
    TFLAG:175 = 11
;リリカとメルラン
ELSEIF (NO:ARG == 16 && NO:(ARG:1) == 17) || (NO:ARG == 17 && NO:(ARG:1) == 16)
    TFLAG:175 = 12
ENDIF

;=============================================================================
;특정 경향 · 계열의 조교 커맨드를 판정하는 식중함수
;=============================================================================
;以下において、ARG…判断すべきコマンドのTrain.csvでの番号（たいていはSELECTCOMかPREVCOMが入ってくる？）
;-------------------------------------------------
;自慰系コマンドかどうか？
;-------------------------------------------------
@CHECK_COM_MASTURBATION(ARG)
#FUNCTION
RETURNF GROUPMATCH(ARG, 7, 124, 125, 127, 182, 205, 613, 628) ? 1 # 0

;자주 쓰일 것 같은 코드를 상위에 배치 
;사용예시) CHECK_COM_INSERT_VAGINA(PREVCOM) & (1p0 | 1p1 | 1p2 | 1p3 | 1p4)
;사용예시) CHECK_COM_INSERT_VAGINA(PREVCOM) & (1p5 - 1)
;1p5 -1 = 1p0 | 1p1 | 1p2 | 1p3 | 1p4
@CHECK_COM_INSERT_VAGINA(ARG)
#FUNCTION
SELECTCASE ARG
    ;대면 삽입 명령 (대면가슴애무)
    CASE 20, 22, 27, 617
        RETURNF 1p0
    ;배면 삽입 명령 (미다레보탄, 후배위가슴애무, 후배위수음)
    CASE 21, 23, 28, 612, 616, 630
        RETURNF 1p1
