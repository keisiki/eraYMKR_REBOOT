;=============================================================================
;촉수 액독뿜기
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE585
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:585 > 0
    RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 4096) == 0
    RETURN 0
;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 16384) == 0
    RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;해제는 언제든 가능
SIF TEQUIP:95 & 2
    RETURN 1
;조교자는 촉수사역LV3이상 필요
SIF ABL:PLAYER:촉수사역 < 3
    RETURN 0
;촉수가 [HINA촉수]を得ていないとダメ
SIF (FLAG:촉수특수능력 & 32) == 0
    RETURN 0
;雛を촉수生贄に捧げていない(=雛が何らかの形で存在している)とダメ
SIF (FLAG:촉수특수산제물 & 32) == 0
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;촉수생지를 통해서 촉수와 융합한 병아리를 소환,
;꼬드김정적인 시선을 향하면서 전신의 더러움을 빨고 취한다……라고 하는 초샤워로 할 수 있는플레이
@COM585
PRINTL 촉수 액독뿜기 
COM_NAME = 촉수 액땜
CALL TRAIN_MESSAGE_COM585

DOWNBASE:0 += 5
DOWNBASE:기력 += 80

;더러움의 계산
LOCAL:23 = 0
LOCAL:21 = 0
LOCAL:22 = 0
REPEAT 8
    ;다리의 더러움은 고려하지 않는다
    SIF COUNT == 6
        CONTINUE
    ;변수 초기화
    LOCAL:COUNT = 0
    ;애액의 더러움을 계산(보지는 제외하다)
    SIF STAIN:COUNT & 1 && COUNT != 3
        LOCAL:COUNT += 1
    ;페니스의 더러움을 계산(페니스는 제외하다)
    SIF STAIN:COUNT & 2 && COUNT != 2
        LOCAL:COUNT += 1
    ;정액의 더러움을 계산
    IF STAIN:COUNT & 4
        LOCAL:COUNT += 1
        LOCAL:23 += 1
    ENDIF
    ;애널의 더러움을 계산(애널은 제외하다)
    SIF STAIN:COUNT & 8 && COUNT != 4
        LOCAL:COUNT += 1
    ;모유의 더러움을 계산
    SIF STAIN:COUNT & 16
        LOCAL:COUNT += 1
    ;파과의 피의 더러움을 계산
    SIF STAIN:COUNT & 32
        LOCAL:COUNT += 1
    ;점액의 더러움을 계산
    SIF STAIN:COUNT & 64
        LOCAL:COUNT += 1
REND

;촉수 관통, 재갈 없음이라면 입의 더러움을 리셋
IF TEQUIP:25 != 3 && TEQUIP:42 == 0
    LOCAL:21 += LOCAL:0 * 50
    STAIN:입 = 0
ENDIF

;손의 더러움을 리셋
LOCAL:21 += LOCAL:1 * 25
STAIN:손 = 0

;[남자],[후타나리]를 가지고 있지 않으면 무시
IF TALENT:남자 == 0 && TALENT:후타나리 == 0
    LOCAL:2 = 0
    STAIN:자지 = 0
;오나호르, 콘돔([남자]의 경우는＋카테터) 없음이라면 페니스의 더러움을 리셋
ELSEIF TEQUIP:30 == 0 && TEQUIP:31 == 0 && (TALENT:남자 == 0 || (TEQUIP:32 == 0 && TALENT:남자))
    SOURCE:0 += LOCAL:0 * 40 * (1 + ABL:C감각)
    LOCAL:21 += LOCAL:0 * 75 * (1 + ABL:C감각)
    STAIN:자지 = 2
ENDIF

;[남자]라면 무시
IF TALENT:남자
    LOCAL:3 = 0
    STAIN:보지 = 0
;촉수 삽입계, 쿠스코 없음이라면 보지의 더러움을 리셋
ELSEIF TEQUIP:20 == 0 && TEQUIP:21 == 0
    SOURCE:1 += LOCAL:3 * 50 * (1 + ABL:V감각)
    LOCAL:21 += LOCAL:3 * 100 * (1 + ABL:V감각)
    STAIN:보지 = 1
ENDIF

;촉수 애널 삽입계 없음이라면 애널의 더러움을 리셋
IF TEQUIP:20 == 0
    SOURCE:2 += LOCAL:4 * 60 * (1 + ABL:A감각)
    LOCAL:21 += LOCAL:4 * 120 * (1 + ABL:A감각)
    STAIN:애널 = 8
ENDIF

;[남자]라면 무시
IF TALENT:남자
    LOCAL:5 = 0
    STAIN:가슴 = 0
;촉수 유두 고문계 없음이라면 가슴의 더러움을 리셋
ELSEIF TEQUIP:35 == 0
    SOURCE:3 += LOCAL:5 * 30 * (1 + ABL:B감각)
    LOCAL:21 += LOCAL:5 * 50 * (1 + ABL:B감각)
    STAIN:가슴 = 0
ENDIF

;머리카락의 더러움을 리셋
STAIN:발 = 0

;레즈비언 중독
SIF ABL:레즈중독
    TIMES LOCAL:21 , 1.20
;여자 증오
SIF TALENT:부끄럼없음
    TIMES LOCAL:21 , 0.50

;ABL:정액 중독을 본다
IF ABL:정액중독 == 0
    LOCAL:23 = 0
ELSEIF ABL:정액중독 == 1
    LOCAL:23 *= 50
ELSEIF ABL:정액중독 == 2
    LOCAL:23 *= 100
ELSEIF ABL:정액중독 == 3
    LOCAL:23 *= 200
ELSEIF ABL:정액중독 == 4
    LOCAL:23 *= 400
ELSE
    LOCAL:23 *= 800
ENDIF

;병아리와의 궁합 있고 없음으로 보정이 상하
LOCAL:20 = 39
IF RELATION:(LOCAL:20) > 0 && RELATION:(LOCAL:20) != 100
    LOCAL:21 *= RELATION:(LOCAL:20)
    LOCAL:21 /= 100
    LOCAL:22 *= RELATION:(LOCAL:20)
    LOCAL:22 /= 100
    LOCAL:23 *= RELATION:(LOCAL:20)
    LOCAL:23 /= 100
ENDIF

SOURCE:19 += LOCAL:22
SOURCE:24 += (LOCAL:21 + LOCAL:22)/2
SOURCE:30 += LOCAL:23
SOURCE:31 += LOCAL:21
REPEAT 8
    SIF COUNT == 6
        CONTINUE
    SOURCE:31 += LOCAL:COUNT * 150
REND

IF TEQUIP:95 & 2
    TEQUIP:95 -= 2
ELSE
    TEQUIP:95 |= 2
ENDIF

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

