;=============================================================================
;촉수 애액 채집
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE535
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:535 > 0
    RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
    RETURN 0
;Ｖ系조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
    RETURN 0
;대상의 V확장경험이 0일 때 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && EXP:V확장경험 == 0
    RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;조교자는 촉수사역LV2이상 필요
SIF ABL:PLAYER:촉수사역 < 2
    RETURN 0
;남자라면 안 됨
SIF TALENT:120
    RETURN 0
;처녀면 불가
SIF TALENT:처녀 == 1
    RETURN 0
;V조율경험이 10미만이면 불가
SIF EXP:V조율경험 < 10
    RETURN 0
;幽霊膣鏡삽입中は不可
SIF TEQUIP:21
    RETURN 0
;그밖의 Ｖ계 촉수責め中は不可
SIF TEQUIP:20 && TEQUIP:20 != 7
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM535
JUMP COM153
RETURN 1
