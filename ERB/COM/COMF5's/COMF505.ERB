;=============================================================================
;촉수 페니스 고문
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE505
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:505 > 0
    RETURN 0
;조교 대상에게 페니스가 없으면 불가
SIF EXIST_BAR(TARGET) == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;그밖의 Ｃ계 촉수 고문 중에는 불가
SIF TEQUIP:30 && TEQUIP:30 != 2
    RETURN 0
;조교 대상이 オトコのとき촉수尿道責め中やコンドーム装着中はダメ
SIF TALENT:남자 && (TEQUIP:31 || TEQUIP:32)
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM505
JUMP COM16
RETURN 1
