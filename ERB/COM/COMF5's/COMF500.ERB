    TEQUIP:91 = 0
    ;촉수 최면
    TEQUIP:92 = 0
    ;촉수질내 산란
    TEQUIP:93 = 0
    ;촉수장내 산란
    TEQUIP:94 = 0
    ;촉수 치키치키
    TEQUIP:96 = 0

    ;촉수 독심 고문
    TEQUIP:74 = 0
    ;
    TEQUIP:77 = 0
    ;캐릭터 촉수 범용
    TEQUIP:95 = 0

    ;촉수 사정 게이지 리셋
    TFLAG:997 = 0
    TFLAG:998 = 0
    RETURN 1
ENDIF
TEQUIP:90 = 1

LOCAL:1 = 100
;EXP:촉수 경험을 본다
IF EXP:촉수경험 < EXPLV:1
    TIMES LOCAL:1 , 2.50
ELSEIF EXP:촉수경험 < EXPLV:2
    TIMES LOCAL:1 , 2.00
ELSEIF EXP:촉수경험 < EXPLV:3
    TIMES LOCAL:1 , 1.50
ELSEIF EXP:촉수경험 < EXPLV:4
    TIMES LOCAL:1 , 1.00
ELSEIF EXP:촉수경험 < EXPLV:5
    TIMES LOCAL:1 , 0.80
ELSE
    TIMES LOCAL:1 , 0.60
ENDIF

;ABL:촉수 중독을 본다
IF ABL:촉수중독 == 0
    TIMES LOCAL:1 , 1.00
ELSEIF ABL:촉수중독 == 1
    TIMES LOCAL:1 , 0.90
ELSEIF ABL:촉수중독 == 2
    TIMES LOCAL:1 , 0.80
ELSEIF ABL:촉수중독 == 3
    TIMES LOCAL:1 , 0.70
ELSEIF ABL:촉수중독 == 4
    TIMES LOCAL:1 , 0.60
ELSE
    TIMES LOCAL:1 , 0.50
ENDIF
;감정결여
SIF TALENT:22
    TIMES LOCAL:1 , 0.60
;기생
SIF TALENT:146
    TIMES LOCAL:1 , 0.70
;회란
SIF TALENT:145
    TIMES LOCAL:1 , 0.80
;촉수 이식
SIF CFLAG:41
    TIMES LOCAL:1 , 0.75

LOCAL:2 = LOCAL:1
TIMES LOCAL:2 , 0.75

DOWNBASE:0 += LOCAL:2
DOWNBASE:기력 += LOCAL:2 * 2
SOURCE:25 += LOCAL:1 * 10

;겁쟁이
SIF TALENT:10
    TIMES LOCAL:1 , 2.00

SOURCE:31 += LOCAL:1 * 5

;커맨드 속성：촉수
TFLAG:98 = 8
RETURN 1

