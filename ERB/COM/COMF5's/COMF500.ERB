    IF TEQUIP:93 && TEQUIP:94
        CALL DOUBLE_EGGPLAY_OUT
    ELSEIF TEQUIP:93
        CALL EGGPLAY_OUT
    ELSEIF TEQUIP:94
        CALL KANTYO_EGGPLAY_OUT
    ENDIF

    ;촉수 삽입(바이브래이터외)
    TEQUIP:20 = 0
    TEQUIP:21 = 0
    ;어널 촉수(어널 바이브래이터외)
    TEQUIP:25 = 0
    ;촉수 구리 꾸짖고(클리캡외)
    TEQUIP:30 = 0

    ;촉수 카테터(카테터외)
    TEQUIP:32 = 0
    ;촉수 유두 꾸짖고(유두캡외)
    TEQUIP:35 = 0
    ;촉수 긴박(줄)
    TEQUIP:40 = 0
    ;촉수구욕(재갈)
    SIF TEQUIP:42 == 2 || TEQUIP:42 == 4
        TEQUIP:42 = 0
    ;촉수비혈삽입(코훅)
    SIF TEQUIP:44 == 2
        TEQUIP:44 = 0
    ;촉수이혈삽입(이전)
