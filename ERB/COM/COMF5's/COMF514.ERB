;=============================================================================
;촉수 스마타
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE514
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:514 > 0
    RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 4096) == 0
    RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;조교 대상にペニスがついているとダメ
SIF EXIST_BAR(TARGET)
    RETURN 0
;윤활이 높지 않으면 불가
SIF PALAM:10 < 2000
    RETURN 0
;Ｖ계 촉수責め中は不可
SIF TEQUIP:20 || TEQUIP:21
    RETURN 0
;촉수尿道삽입中はダメ
SIF TEQUIP:32
    RETURN 0
;촉수緊縛中は不可
SIF TEQUIP:40
    RETURN 0
;인어는 안됨
SIF TALENT:인어 == 1
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM514
PRINTL 촉수 스마타
COM_NAME = 촉수 스마타
CALL TRAIN_MESSAGE_COM514

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 70
DOWNBASE:기력 += 150

SOURCE:30 = 1500
SOURCE:22 = 1200
SOURCE:31 = 400

;ABL:봉사 정신을 본다
IF ABL:봉사정신 == 0
    SOURCE:15 = 50
    SOURCE:16 = 60
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:봉사정신 == 1
    SOURCE:15 = 75
    SOURCE:16 = 80
    TIMES SOURCE:30 , 2.50
ELSEIF ABL:봉사정신 == 2
    SOURCE:15 = 100
    SOURCE:16 = 120
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:봉사정신 == 3
    SOURCE:15 = 150
    SOURCE:16 = 300
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:봉사정신 == 4
    SOURCE:15 = 200
    SOURCE:16 = 400
    TIMES SOURCE:30 , 0.50
ELSE
    SOURCE:15 = 300
    SOURCE:16 = 600
    TIMES SOURCE:30 , 0.10
ENDIF

;ABL:C감각을 본다
IF ABL:C감각 == 0
    LOCAL:1 = 0
ELSEIF ABL:C감각 == 1
    LOCAL:1 = 10
ELSEIF ABL:C감각 == 2
    LOCAL:1 = 50
ELSEIF ABL:C감각 == 3
    LOCAL:1 = 100
ELSEIF ABL:C감각 == 4
    LOCAL:1 = 250
ELSE
    LOCAL:1 = 500
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES LOCAL:1 , 0.30
    TIMES SOURCE:15 , 0.60
ELSEIF PALAM:10 < PALAMLV:2
    TIMES LOCAL:1 , 0.60
    TIMES SOURCE:15 , 0.80
ELSEIF PALAM:10 < PALAMLV:3
    TIMES LOCAL:1 , 1.00
    TIMES SOURCE:15 , 1.00
ELSEIF PALAM:10 < PALAMLV:4
    TIMES LOCAL:1 , 1.50
    TIMES SOURCE:15 , 1.20
ELSEIF PALAM:10 < PALAMLV:5
    TIMES LOCAL:1 , 2.00
    TIMES SOURCE:15 , 1.40
ELSE
    TIMES LOCAL:1 , 2.50
    TIMES SOURCE:15 , 1.60
ENDIF

;ABL:촉수 중독을 본다
IF ABL:촉수중독 == 0
    TIMES LOCAL:1 , 1.00
    TIMES SOURCE:15 , 0.60
    TIMES SOURCE:16 , 0.60
    TIMES SOURCE:30 , 2.00
ELSEIF ABL:촉수중독 == 1
    TIMES LOCAL:1 , 1.20
    TIMES SOURCE:15 , 0.80
    TIMES SOURCE:16 , 0.80
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:촉수중독 == 2
    TIMES LOCAL:1 , 1.50
    TIMES SOURCE:15 , 1.00
    TIMES SOURCE:16 , 1.00
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:촉수중독 == 3
    TIMES LOCAL:1 , 1.80
    TIMES SOURCE:15 , 1.30
    TIMES SOURCE:16 , 1.30
    TIMES SOURCE:30 , 0.50
ELSEIF ABL:촉수중독 == 4
    TIMES LOCAL:1 , 2.00
    TIMES SOURCE:15 , 1.80
    TIMES SOURCE:16 , 1.80
    TIMES SOURCE:30 , 0.10
ELSE
    TIMES LOCAL:1 , 2.40
    TIMES SOURCE:15 , 2.20
    TIMES SOURCE:16 , 2.20
    TIMES SOURCE:30 , 0.00
ENDIF
SOURCE:0 += LOCAL:1

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 1
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM514
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
PRINTFORML 좌우에서 가까워져 온 촉수는 %CALLNAME:TARGET%의 다리를 모아
PRINTFORM 가랑이에 끼워지는 것 같은 느낌으로 해, 다른 촉수가 %CALLNAME:TARGET%의 
IF TALENT:128
    PRINT 어린 균열을 
ELSE
    PRINT 음순을 
ENDIF
PRINTW 비비기 시작했다….
