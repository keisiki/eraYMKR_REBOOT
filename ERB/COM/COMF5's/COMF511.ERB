;=============================================================================
;촉수 간질이기
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE511
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:511 > 0
    RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 4096) == 0
    RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;촉수가 [磯巾着型촉수]を得ていないとダメ
SIF (FLAG:75 & 256) == 0
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;앗 나무응의 전용기술을 사용해 돌려 본다
@COM511
PRINTL 촉수 간질이기
COM_NAME = 촉수 간질이기
;말용으로 TFLAG:201을 사용. (0=배꼽 간질여, 1=페니스/클리토리스 간질여, 2=버스트 간질여, 3=애널 간질임)
TFLAG:201 = 0

IF FLAG:조교모드 & 8
    PRINTL 촉수로 어디를 간질일까?
    PRINTL [0] - 배꼽
    IF TALENT:120 || TALENT:121
        PRINTL [1] - 페니스
    ELSE
        PRINTL [1] - 클리토리스
    ENDIF
    PRINTL [2] - 바스트
    PRINTL [3] - 애널
    $INPUT_LOOP
    INPUT
    IF RESULT == 0
        LOCAL:2 = 0
    ELSEIF RESULT == 1
        LOCAL:2 = 1
    ELSEIF RESULT == 2
        LOCAL:2 = 3
    ELSEIF RESULT == 3
        LOCAL:2 = 5
    ELSE
        GOTO INPUT_LOOP
    ENDIF
ELSE
    LOCAL:2 = RAND:7
ENDIF
;말용
IF LOCAL:2 == 0
    TFLAG:201 = 0
ELSEIF LOCAL:2 == 1 || LOCAL:2 == 2
    TFLAG:201 = 1
ELSEIF LOCAL:2 == 3 || LOCAL:2 == 4
    TFLAG:201 = 2
ELSE
    TFLAG:201 = 3
ENDIF

CALL TRAIN_MESSAGE_COM511

DOWNBASE:0 += 40
DOWNBASE:기력 += 70

SOURCE:15 = 120
SOURCE:30 = 2000
SOURCE:12 = 200
SOURCE:22 = 200
SOURCE:31 = 2000

IF LOCAL:2 == 0
    ;배꼽 고문
    LOCAL:1 = 500
    ;MARK:반발 각인을 본다
    IF MARK:반발각인 < 2
        TIMES LOCAL:1, 1.25
    ELSEIF MARK:반발각인 == 2
        TIMES LOCAL:1, 1.00
    ELSE
        TIMES LOCAL:1, 0.75
    ENDIF

    ;MARK:굴복 각인을 본다
    IF MARK:굴복각인 > 2
        TIMES LOCAL:1, 1.25
    ELSEIF MARK:굴복각인 == 2
        TIMES LOCAL:1, 1.00
    ELSEIF MARK:굴복각인 == 1
        TIMES LOCAL:1, 0.75
    ELSE
        TIMES LOCAL:1, 0.50
    ENDIF

    ;MARK:쾌락 각인을 본다
    IF MARK:쾌락각인 > 2
        TIMES LOCAL:1, 1.50
    ELSEIF MARK:쾌락각인 == 2
        TIMES LOCAL:1, 1.25
    ELSEIF MARK:쾌락각인 == 1
        TIMES LOCAL:1, 1.00
    ELSE
        TIMES LOCAL:1, 0.75
    ENDIF

    ;MARK:고통 각인을 본다
    IF MARK:고통각인 > 2
        TIMES LOCAL:1, 1.30
    ELSEIF MARK:고통각인 == 2
        TIMES LOCAL:1, 1.00
    ELSEIF MARK:고통각인 == 1
        TIMES LOCAL:1, 0.70
    ELSE
        TIMES LOCAL:1, 0.30
    ENDIF

    ;ABL:마조끼를 본다
    IF ABL:마조끼 == 0
        TIMES LOCAL:1, 0.50
        SOURCE:24 = 40
    ELSEIF ABL:마조끼 == 1
        TIMES LOCAL:1, 0.75
        SOURCE:24 = 80
    ELSEIF ABL:마조끼 == 2
        TIMES LOCAL:1, 1.00
        SOURCE:24 = 160
    ELSEIF ABL:마조끼 == 3
        TIMES LOCAL:1, 1.20
        SOURCE:24 = 320
    ELSEIF ABL:마조끼 == 4
        TIMES LOCAL:1, 1.50
        SOURCE:24 = 650
    ELSE
        TIMES LOCAL:1, 2.00
        SOURCE:24 = 1500
    ENDIF

    ;프라이드 높다
    IF TALENT:16
        TIMES LOCAL:1, 0.75
    ;프라이드 낮다
    ELSEIF TALENT:17
        TIMES LOCAL:1, 1.50
    ENDIF
    ;자제심
    SIF TALENT:20
        TIMES LOCAL:1, 0.50
    ;저항
    SIF TALENT:34
        TIMES LOCAL:1, 0.75
    ;약점
    SIF TALENT:39
        TIMES LOCAL:1, 2.00
    ;도착적
    SIF TALENT:80
        TIMES LOCAL:1, 2.00
    ;중독 하기 쉽다
    SIF TALENT:84
        TIMES LOCAL:1, 1.50
    SOURCE:23 += LOCAL:1

    IF LOCAL:1 > 0
        FOR COUNT, 20, 23
            IF PALAM:COUNT
                PRINTFORML %CALLNAME:TARGET%의 %조사처리(PALAMNAME:COUNT, "가")% 하락했다.
                PALAM:COUNT = MAX(PALAM:COUNT - LOCAL:1, 0)
            ENDIF
        NEXT
    ENDIF
ELSEIF LOCAL:2 == 1 || LOCAL:2 == 2
    ;클리 고문
    ;ABL:C감각을 본다
    IF ABL:C감각 == 0
        SOURCE:0 = 50
        SOURCE:11 = 50
    ELSEIF ABL:C감각 == 1
        SOURCE:0 = 100
        SOURCE:11 = 100
    ELSEIF ABL:C감각 == 2
        SOURCE:0 = 500
        SOURCE:11 = 150
    ELSEIF ABL:C감각 == 3
        SOURCE:0 = 1500
        SOURCE:11 = 250
    ELSEIF ABL:C감각 == 4
        SOURCE:0 = 2500
        SOURCE:11 = 400
    ELSE
        SOURCE:0 = 3000
        SOURCE:11 = 600
    ENDIF

    ;C민감
    IF TALENT:100
        TIMES SOURCE:0, 1.50
    ;C둔감
    ELSEIF TALENT:101
        TIMES SOURCE:0, 0.75
    ENDIF
    ;음핵/음경
    SIF TALENT:171
        TIMES SOURCE:0, 2.00

    LOCAL:1 = SOURCE:11
    SOURCE:30 += LOCAL:1 / 2
    SOURCE:31 += LOCAL:1
    LOCAL:1 = SOURCE:0
    SOURCE:23 += LOCAL:1 / 2

ELSEIF LOCAL:2 == 3 || LOCAL:2 == 4
    ;버스트 고문
    ;ABL:B감각을 본다
    IF ABL:B감각 == 0
        SOURCE:11 = 50
        SOURCE:3 = 50
    ELSEIF ABL:B감각 == 1
        SOURCE:11 = 100
        SOURCE:3 = 100
    ELSEIF ABL:B감각 == 2
        SOURCE:11 = 150
        SOURCE:3 = 200
    ELSEIF ABL:B감각 == 3
        SOURCE:11 = 250
        SOURCE:3 = 600
    ELSEIF ABL:B감각 == 4
        SOURCE:11 = 400
        SOURCE:3 = 1500
    ELSE
        SOURCE:11 = 600
        SOURCE:3 = 2100
    ENDIF

    ;B민감
    IF TALENT:106
        TIMES SOURCE:3, 1.50
    ;B둔감
    ELSEIF TALENT:107
        TIMES SOURCE:3, 0.75
    ENDIF
    ;절벽
    IF TALENT:110
        TIMES SOURCE:3, 2.00
    ;빈유
    ELSEIF TALENT:111
        TIMES SOURCE:3, 2.00
    ;큰 가슴
    ELSEIF TALENT:112
        TIMES SOURCE:3, 0.75
    ;폭유
    ELSEIF TALENT:113
        TIMES SOURCE:3, 0.75
    ENDIF
    ;음유
    SIF TALENT:174
        TIMES SOURCE:3, 2.00

    LOCAL:1 = SOURCE:11
    SOURCE:30 += LOCAL:1 / 2
    SOURCE:31 += LOCAL:1
    LOCAL:1 = SOURCE:3
    SOURCE:23 += LOCAL:1 / 2
    STAIN:가슴 |= 64
ELSE
    ;애널 고문
    ;ABL:A감각을 본다
    IF ABL:A감각 == 0
        SOURCE:2 = 20
        LOCAL:1 = 150
    ELSEIF ABL:A감각 == 1
        SOURCE:2 = 80
        LOCAL:1 = 250
    ELSEIF ABL:A감각 == 2
        SOURCE:2 = 240
        LOCAL:1 = 400
    ELSEIF ABL:A감각 == 3
        SOURCE:2 = 500
        LOCAL:1 = 650
    ELSEIF ABL:A감각 == 4
        SOURCE:2 = 1400
        LOCAL:1 = 1000
    ELSE
        SOURCE:2 = 2100
        LOCAL:1 = 1500
    ENDIF

    ;A민감
    IF TALENT:104
        TIMES SOURCE:2, 1.50
    ;A둔감
    ELSEIF TALENT:105
        TIMES SOURCE:2, 0.75
    ENDIF
    ;음고
    SIF TALENT:173
        TIMES SOURCE:2, 2.00

    SOURCE:30 += LOCAL:1 /2
    SOURCE:22 += LOCAL:1
    SOURCE:31 += LOCAL:1
    SOURCE:23 += LOCAL:1 /2
    STAIN:애널 |= 64

    TCVAR:1 += 1
ENDIF

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 1
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM511
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF TFLAG:201 == 0
    PRINT 끄트머리가 무수히 나눠진 촉수가 
    SIF TEQUIP:40
        PRINT 사지를 결박 되어 움직일 수 없는 
    PRINTFORML %CALLNAME:TARGET%에게 가까워지자,
    PRINTFORMW 마치 배꼽의 때를 남김없이 없애버릴 것 처럼 %CALLNAME:TARGET%의 배꼽을 괴롭혀댔다….
ELSEIF TFLAG:201 == 1
    PRINT 끄트머리가 솔과 같이 무수히 나눠진 촉수가 
    SIF TEQUIP:40
        PRINT 사지를 결박 되어 움직일 수 없는 
    PRINTFORML %CALLNAME:TARGET%에게 가까워지자,
    PRINTFORM %CALLNAME:TARGET%의 
    SIF PALAM:13 >= PALAMLV:3 || TALENT:1
        PRINT 비대해진 
    IF TALENT:120 || TALENT:121
        PRINTW 음경을 더듬어, 그 앞에 열린 작은 구멍에 자신을 집어넣으려 퍼덕댔다….
    ELSE
        PRINTW 음핵을 더듬어, 그 뒤의 틈새에 끄트머리를 감고 퍼덕댔다….
    ENDIF
ELSEIF TFLAG:201 == 2
    PRINT 끄트머리가 무수히 나눠진 촉수가 
    SIF TEQUIP:40
        PRINT 사지를 결박 되어 움직일 수 없는 
    PRINTFORML %CALLNAME:TARGET%에게 가까워지자,
    PRINTFORMW 유륜을 더듬거나 끄트머리를 %CALLNAME:TARGET%의 유선에 비벼대는 것 같은 동작을 시작했다….
ELSE
    PRINT 끄트머리가 무수히 나눠진 촉수가 
    SIF TEQUIP:44
        PRINT 사지를 결박 되어 움직일 수 없는 
    PRINTFORML %CALLNAME:TARGET%에게 가까워지자,
    PRINTFORMW 초조한 듯이 %CALLNAME:TARGET%의 항문을 더듬기 시작했다….
ENDIF
