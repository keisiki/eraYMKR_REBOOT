;=============================================================================
;촉수 방전 페니스 고문
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE515
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:515 > 0
    RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 4096) == 0
    RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
    RETURN 0
;조교 대상에게 페니스가 없으면 불가
SIF EXIST_BAR(TARGET) == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;그밖의 Ｃ계 촉수 고문 중에는 불가
SIF TEQUIP:30 && TEQUIP:30 != 6
    RETURN 0
;조교 대상이 オトコのとき촉수尿道責め中やコンドーム装着中はダメ
SIF TALENT:남자 && (TEQUIP:31 || TEQUIP:32)
    RETURN 0
;촉수가 [放電촉수]を得ていないとダメ
SIF (FLAG:촉수능력 & 4) == 0
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM515
JUMP COM17
RETURN 1
