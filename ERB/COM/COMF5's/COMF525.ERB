;=============================================================================
;촉수 착유
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE525
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:525 > 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 4
    RETURN 0
;남자라면 안 됨
SIF TALENT:120
    RETURN 0
;조교자는 촉수사역LV2이상 필요
SIF ABL:PLAYER:촉수사역 < 2
    RETURN 0
;그밖의 Ｂ계 촉수責め中は不可
SIF TEQUIP:35 && TEQUIP:35 != 2
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM525
JUMP COM191
RETURN 1

@SYOKUSYU_MILK
;［B감도］3이상,［모유 체질］없음 또한[작은체형]과［빈유］와［절벽］없음 또한［남자］없음
IF ABL:B감각 >= 3 && TALENT:134 == 0 && (TALENT:128 == 0 || (TALENT:110 == 0 && TALENT:111 == 0)) && TALENT:120 == 0
    PRINTFORMW 촉수에게 젖을 빨리는 동안에 %조사선택(CALLNAME:TARGET, "는")% 모유가 나오게 되었다. 
    TALENT:134 = 1
    CALL CALC_GAUGE3_SLAVE, TARGET, 1
ENDIF
