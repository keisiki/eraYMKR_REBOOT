;=============================================================================
;촉수 소장 고문
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE523
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:523 > 0
    RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
    RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
    RETURN 0
;조교자는 촉수사역LV3이상 필요
SIF ABL:PLAYER:촉수사역 < 3
    RETURN 0
;촉수가 [遠隔精密制御]を得ていないとダメ
SIF (FLAG:75 & 512) == 0
    RETURN 0
;기계면 불가
SIF TALENT:206
    RETURN 0
;그밖의 Ａ계 촉수責め中は不可
SIF TEQUIP:25 && TEQUIP:25 != 2
    RETURN 0
;卵が邪魔で小腸まで到達できない
SIF TEQUIP:94
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM523
PRINTL 촉수 소장 고문
COM_NAME = 촉수 소장 고문
CALL TRAIN_MESSAGE_COM523

DOWNBASE:0 += 220
DOWNBASE:1 += 220

LOCAL:2 = 1000

;ABL:촉수 중독을 본다
IF ABL:촉수중독 == 0
    LOCAL:5 += 0
    TIMES LOCAL:2 , 1.00
ELSEIF ABL:촉수중독 == 1
    LOCAL:5 += 50
    TIMES LOCAL:2 , 1.20
ELSEIF ABL:촉수중독 == 2
    LOCAL:5 += 120
    TIMES LOCAL:2 , 1.40
ELSEIF ABL:촉수중독 == 3
    LOCAL:5 += 350
    TIMES LOCAL:2 , 1.60
ELSEIF ABL:촉수중독 == 4
    LOCAL:5 += 900
    TIMES LOCAL:2 , 1.80
ELSE
    LOCAL:5 += 2000
    TIMES LOCAL:2 , 2.00
ENDIF

;ABL:A감각을 본다
IF ABL:A감각 == 0
    LOCAL:1 = 80
    LOCAL:4 = 300
ELSEIF ABL:A감각 == 1
    LOCAL:1 = 250
    LOCAL:4 = 800
ELSEIF ABL:A감각 == 2
    LOCAL:1 = 600
    LOCAL:4 = 1400
ELSEIF ABL:A감각 == 3
    LOCAL:1 = 1000
    LOCAL:4 = 1800
ELSEIF ABL:A감각 == 4
    LOCAL:1 = 1600
    LOCAL:4 = 2100
ELSE
    LOCAL:1 = 2700
    LOCAL:4 = 2400
ENDIF

;ABL:마조끼를 본다
IF ABL:마조끼 == 0
    LOCAL:6 = 1500
ELSEIF ABL:마조끼 == 1
    LOCAL:6 = 800
ELSEIF ABL:마조끼 == 2
    LOCAL:6 = 200
ELSEIF ABL:마조끼 == 3
    LOCAL:6 = 0
ELSEIF ABL:마조끼 == 4
    LOCAL:6 = 0
ELSE
    LOCAL:6 = 0
ENDIF

LOCAL:3 = 1800

;EXP:촉수 경험을 본다
IF EXP:촉수경험 < EXPLV:1
    TIMES LOCAL:3 , 3.00
ELSEIF EXP:촉수경험 < EXPLV:2
    TIMES LOCAL:3 , 2.50
ELSEIF EXP:촉수경험 < EXPLV:3
    TIMES LOCAL:3 , 2.00
ELSEIF EXP:촉수경험 < EXPLV:4
    TIMES LOCAL:3 , 1.50
ELSEIF EXP:촉수경험 < EXPLV:5
    TIMES LOCAL:3 , 1.00
ELSE
    TIMES LOCAL:3 , 0.80
ENDIF

;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
    TIMES LOCAL:1 , 0.50
    LOCAL:3 += 2000
ELSEIF EXP:A경험 < EXPLV:2
    TIMES LOCAL:1 , 1.00
    LOCAL:3 += 300
ELSEIF EXP:A경험 < EXPLV:3
    TIMES LOCAL:1 , 1.10
    LOCAL:3 += 50
ELSEIF EXP:A경험 < EXPLV:4
    TIMES LOCAL:1 , 1.20
    LOCAL:3 += 10
ELSEIF EXP:A경험 < EXPLV:5
    TIMES LOCAL:1 , 1.40
    LOCAL:3 += 0
ELSE
    TIMES LOCAL:1 , 1.60
    LOCAL:3 += 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES LOCAL:1 , 0.40
    LOCAL:3 += 1200
ELSEIF PALAM:10 < PALAMLV:2
    TIMES LOCAL:1 , 0.80
    LOCAL:3 += 700
ELSEIF PALAM:10 < PALAMLV:3
    TIMES LOCAL:1 , 1.00
    LOCAL:3 += 400
ELSEIF PALAM:10 < PALAMLV:4
    TIMES LOCAL:1 , 1.40
    LOCAL:3 += 150
ELSE
    TIMES LOCAL:1 , 1.80
    LOCAL:3 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES LOCAL:1 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES LOCAL:1 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES LOCAL:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES LOCAL:1 , 1.10
ELSE
    TIMES LOCAL:1 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES LOCAL:1 , 0.80
ELSEIF ABL:순종 == 1
    TIMES LOCAL:1 , 0.90
ELSEIF ABL:순종 == 2
    TIMES LOCAL:1 , 1.00
ELSEIF ABL:순종 == 3
    TIMES LOCAL:1 , 1.10
ELSEIF ABL:순종 == 4
    TIMES LOCAL:1 , 1.20
ELSE
    TIMES LOCAL:1 , 1.30
ENDIF

;도착적
SIF TALENT:80
    TIMES LOCAL:6 , 0.80

;[작은체형]
IF TALENT:128
    TIMES LOCAL:3 , 2.00
    SIF TALENT:130 || TALENT:131
        TIMES LOCAL:1 , 2.00
ENDIF

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
    TIMES LOCAL:3 , 1.50
    TIMES LOCAL:4 , 1.50
    TIMES LOCAL:6 , 1.50
ELSEIF TALENT:104
    TIMES LOCAL:3 , 0.60
    TIMES LOCAL:4 , 0.60
    TIMES LOCAL:6 , 0.60
ENDIF

SOURCE:25 += LOCAL:3
;처녀로 정조관념
SIF TALENT:처녀 == 1 && TALENT:30
    LOCAL:4 /= 3

SOURCE:2 += LOCAL:1
SOURCE:23 += LOCAL:2
SOURCE:20 += LOCAL:3
SOURCE:22 += LOCAL:4
SOURCE:24 += LOCAL:5
SOURCE:31 += LOCAL:6

TCVAR:1 += 4
;촉수 경험 플래그
TFLAG:90 = 0

;커맨드 속성：촉수
TFLAG:98 = 8

;촉수의 착탈
IF TEQUIP:25 == 2
    TEQUIP:25 = 0
ELSE
    TEQUIP:25 = 2
ENDIF
RETURN 1

;-------------------------------------------------
;촉수 소장 능숙중
;-------------------------------------------------
@EQUIP_COM523
PRINTL <촉수 소장 고문 중>

DOWNBASE:0 += 25
DOWNBASE:1 += 95

LOCAL:1 = 100

;ABL:촉수 중독을 본다
IF ABL:촉수중독 == 0
    LOCAL:4 += 0
    TIMES LOCAL:1 , 1.00
ELSEIF ABL:촉수중독 == 1
    LOCAL:4 += 50
    TIMES LOCAL:1 , 1.20
ELSEIF ABL:촉수중독 == 2
    LOCAL:4 += 120
    TIMES LOCAL:1 , 1.40
ELSEIF ABL:촉수중독 == 3
    LOCAL:4 += 350
    TIMES LOCAL:1 , 1.60
ELSEIF ABL:촉수중독 == 4
    LOCAL:4 += 900
    TIMES LOCAL:1 , 1.80
ELSE
    LOCAL:4 += 2000
    TIMES LOCAL:1 , 2.00
ENDIF

;ABL:A감각을 본다
IF ABL:A감각 == 0
    LOCAL = 40
    LOCAL:3 = 150
ELSEIF ABL:A감각 == 1
    LOCAL = 150
    LOCAL:3 = 250
ELSEIF ABL:A감각 == 2
    LOCAL = 400
    LOCAL:3 = 500
ELSEIF ABL:A감각 == 3
    LOCAL = 700
    LOCAL:3 = 800
ELSEIF ABL:A감각 == 4
    LOCAL = 1300
    LOCAL:3 = 1200
ELSE
    LOCAL = 1800
    LOCAL:3 = 1700
ENDIF

;ABL:마조끼를 본다
IF ABL:마조끼 == 0
    LOCAL:5 = 1500
ELSEIF ABL:마조끼 == 1
    LOCAL:5 = 800
ELSEIF ABL:마조끼 == 2
    LOCAL:5 = 200
ELSEIF ABL:마조끼 == 3
    LOCAL:5 = 0
ELSEIF ABL:마조끼 == 4
    LOCAL:5 = 0
ELSE
    LOCAL:5 = 0
ENDIF

LOCAL:2 = 380

;EXP:촉수 경험을 본다
IF EXP:촉수경험 < EXPLV:1
    TIMES LOCAL:2 , 3.00
ELSEIF EXP:촉수경험 < EXPLV:2
    TIMES LOCAL:2 , 2.50
ELSEIF EXP:촉수경험 < EXPLV:3
    TIMES LOCAL:2 , 2.00
ELSEIF EXP:촉수경험 < EXPLV:4
    TIMES LOCAL:2 , 1.50
ELSEIF EXP:촉수경험 < EXPLV:5
    TIMES LOCAL:2 , 1.00
ELSE
    TIMES LOCAL:2 , 0.80
ENDIF

;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:2
    TIMES LOCAL , 1.00
    LOCAL:2 += 300
ELSEIF EXP:A경험 < EXPLV:3
    TIMES LOCAL , 1.10
    LOCAL:2 += 50
ELSEIF EXP:A경험 < EXPLV:4
    TIMES LOCAL , 1.20
    LOCAL:2 += 10
ELSEIF EXP:A경험 < EXPLV:5
    TIMES LOCAL , 1.40
    LOCAL:2 += 0
ELSE
    TIMES LOCAL , 1.60
    LOCAL:2 += 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES LOCAL , 0.40
    LOCAL:2 += 1000
ELSEIF PALAM:10 < PALAMLV:2
    TIMES LOCAL , 0.80
    LOCAL:2 += 600
ELSEIF PALAM:10 < PALAMLV:3
    TIMES LOCAL , 1.00
    LOCAL:2 += 300
ELSEIF PALAM:10 < PALAMLV:4
    TIMES LOCAL , 1.40
    LOCAL:2 += 100
ELSE
    TIMES LOCAL , 1.80
    LOCAL:2 += 50
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES LOCAL , 1.10
ELSE
    TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES LOCAL , 0.80
ELSEIF ABL:순종 == 1
    TIMES LOCAL , 0.90
ELSEIF ABL:순종 == 2
    TIMES LOCAL , 1.00
ELSEIF ABL:순종 == 3
    TIMES LOCAL , 1.10
ELSEIF ABL:순종 == 4
    TIMES LOCAL , 1.20
ELSE
    TIMES LOCAL , 1.30
ENDIF

;프라이드 높다
IF TALENT:16
    TIMES LOCAL:3 , 1.20
    TIMES LOCAL:5 , 1.20
;프라이드 낮다
ELSEIF TALENT:17
    TIMES LOCAL:3 , 0.80
    TIMES LOCAL:5 , 0.80
ENDIF
;무관심
IF TALENT:21
    TIMES LOCAL:3 , 0.80
    TIMES LOCAL:5 , 0.80
ENDIF
;정조관념
SIF TALENT:30
    TIMES LOCAL:5 , 2.00
;도착적
SIF TALENT:80
    TIMES LOCAL:5 , 0.80

;[작은체형]
IF TALENT:128
    TIMES LOCAL:2 , 2.00
    SIF TALENT:130 || TALENT:131
        TIMES LOCAL , 2.00
ENDIF

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
    TIMES LOCAL:2 , 1.50
    TIMES LOCAL:3 , 1.50
    TIMES LOCAL:5 , 1.50
ELSEIF TALENT:104
    TIMES LOCAL:2 , 0.60
    TIMES LOCAL:3 , 0.60
    TIMES LOCAL:5 , 0.60
ENDIF

SOURCE:25 += LOCAL:2
;처녀로 정조관념
SIF TALENT:처녀 == 1 && TALENT:30
    LOCAL:3 /= 3

SOURCE:2 += LOCAL
SOURCE:23 += LOCAL:1
SOURCE:20 += LOCAL:2
SOURCE:22 += LOCAL:3
SOURCE:24 += LOCAL:4
SOURCE:31 += LOCAL:5

TCVAR:1 += 2
;촉수 경험 플래그
TFLAG:90 += 1
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM523
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF TEQUIP:25 == 2
    PRINTFORML %CALLNAME:TARGET%의 소장 안에서 날뛰던 촉수는,
    PRINTFORML %조사선택(CALLNAME:PLAYER, "가")% 명하자 천천히 소장에서 멀어져
    PRINTW 대장을 통과해 이윽고 애널로 빠져 나왔다….
ELSE
    PRINTFORML 가늘고 긴 촉수가 %CALLNAME:TARGET%의 애널에 기어들어 비벼댔다.
    PRINTL 촉수는 천천히 대장을 거슬러 올라,
    PRINTW 소장 속에서 요염하게 꿈틀댔다….
ENDIF
