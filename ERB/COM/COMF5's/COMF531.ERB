;=============================================================================
;촉수 애널 확장
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE531
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:531 > 0
    RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
    RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
    RETURN 0
;조교자는 촉수사역LV2이상 필요
SIF ABL:PLAYER:촉수사역 < 2
    RETURN 0
;촉수가 [ゴーヤ型촉수]を得ていないとダメ
SIF (FLAG:75 & 128) == 0
    RETURN 0
;ASSI일 경우, 순종3이상이거나 새드じゃないとできない。
IF ASSIPLAY
    SIF ABL:ASSI:순종 < 4 && TALENT:ASSI:81 == 0
        RETURN 0
ENDIF
;[작은체형]なら순종이 4이상かつ마조끼が4이상じゃないと実行できない
IF TALENT:128
    SIF ABL:순종 < 4 || ABL:마조끼 < 4
        RETURN 0
ENDIF
;일정의 A경험이 필요
SIF EXP:A경험 < 150
    RETURN 0
;Ａ계 촉수責め中は不可
SIF TEQUIP:25
    RETURN 0
;卵が邪魔で확장できない
SIF TEQUIP:94
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM531
PRINTL 촉수 애널 확장
COM_NAME = 촉수 애널 확장
CALL TRAIN_MESSAGE_COM531

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 240
DOWNBASE:기력 += 130

SOURCE:20 = 1200
SOURCE:12 = 1200
SOURCE:22 = 1200
SOURCE:31 = 2200
SOURCE:23 = 1100

;ABL:A감각을 본다
IF ABL:A감각 == 0
    SOURCE:2 = 10
    SOURCE:11 = 10
    SOURCE:22 += 100
ELSEIF ABL:A감각 == 1
    SOURCE:2 = 30
    SOURCE:11 = 30
    SOURCE:22 += 700
ELSEIF ABL:A감각 == 2
    SOURCE:2 = 500
    SOURCE:11 = 100
    SOURCE:22 += 1500
ELSEIF ABL:A감각 == 3
    SOURCE:2 = 1000
    SOURCE:11 = 200
    SOURCE:22 += 3000
ELSEIF ABL:A감각 == 4
    SOURCE:2 = 1700
    SOURCE:11 = 450
    SOURCE:22 += 5000
ELSE
    SOURCE:2 = 2200
    SOURCE:11 = 750
    SOURCE:22 += 8000
ENDIF

;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
    TIMES SOURCE:2 , 0.10
    SOURCE:20 += 20000
ELSEIF EXP:A경험 < EXPLV:2
    TIMES SOURCE:2 , 0.30
    SOURCE:20 += 12000
ELSEIF EXP:A경험 < EXPLV:3
    TIMES SOURCE:2 , 0.50
    SOURCE:20 += 5000
ELSEIF EXP:A경험 < EXPLV:4
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 1800
ELSEIF EXP:A경험 < EXPLV:5
    TIMES SOURCE:2 , 1.40
    SOURCE:20 += 1000
ELSE
    TIMES SOURCE:2 , 1.60
    SOURCE:20 += 600
ENDIF

;EXP:촉수 경험을 본다
IF EXP:촉수경험 < EXPLV:1
    TIMES DOWNBASE:0 , 3.00
    TIMES DOWNBASE:기력 , 3.00
    SOURCE:20 += 1000
ELSEIF EXP:촉수경험 < EXPLV:2
    TIMES DOWNBASE:0 , 2.50
    TIMES DOWNBASE:기력 , 2.50
    SOURCE:20 += 800
ELSEIF EXP:촉수경험 < EXPLV:3
    TIMES DOWNBASE:0 , 2.00
    TIMES DOWNBASE:기력 , 2.00
    SOURCE:20 += 500
ELSEIF EXP:촉수경험 < EXPLV:4
    TIMES DOWNBASE:0 , 1.00
    TIMES DOWNBASE:기력 , 1.00
    SOURCE:20 += 100
ELSEIF EXP:촉수경험 < EXPLV:5
    TIMES DOWNBASE:0 , 0.80
    TIMES DOWNBASE:기력 , 0.80
    SOURCE:20 += 50
ELSE
    TIMES DOWNBASE:0 , 0.60
    TIMES DOWNBASE:기력 , 0.60
ENDIF

;ABL:촉수 중독을 본다
IF ABL:촉수중독 == 0
    SOURCE:24 = 1500
    TIMES SOURCE:23 , 0.50
ELSEIF ABL:촉수중독 == 1
    TIMES DOWNBASE:0 , 0.90
    TIMES DOWNBASE:기력 , 0.90
    SOURCE:24 = 1700
    TIMES SOURCE:23 , 0.80
ELSEIF ABL:촉수중독 == 2
    TIMES DOWNBASE:0 , 0.80
    TIMES DOWNBASE:기력 , 0.80
    SOURCE:24 = 2000
ELSEIF ABL:촉수중독 == 3
    TIMES DOWNBASE:0 , 0.70
    TIMES DOWNBASE:기력 , 0.70
    SOURCE:24 = 2500
    TIMES SOURCE:23 , 1.20
ELSEIF ABL:촉수중독 == 4
    TIMES DOWNBASE:0 , 0.60
    TIMES DOWNBASE:기력 , 0.60
    SOURCE:24 = 3300
    TIMES SOURCE:23 , 1.50
ELSE
    TIMES DOWNBASE:0 , 0.50
    TIMES DOWNBASE:기력 , 0.50
    SOURCE:24 = 4200
    TIMES SOURCE:23 , 1.80
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:2 , 0.40
    SOURCE:20 += 10000
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:2 , 0.80
    SOURCE:20 += 3600
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 1200
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:2 , 1.40
    SOURCE:20 += 200
ELSE
    TIMES SOURCE:2 , 1.80
    SOURCE:20 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:2 , 0.60
    TIMES SOURCE:22 , 0.60
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:2 , 0.80
    TIMES SOURCE:22 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:22 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:22 , 1.20
ELSE
    TIMES SOURCE:2 , 1.40
    TIMES SOURCE:22 , 1.40
ENDIF

;[작은체형]
SIF TALENT:128
    TIMES SOURCE:20 , 2.00

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:22 , 0.60
    TIMES SOURCE:31 , 0.60
ENDIF

;처녀로 정조관념
SIF TALENT:처녀 == 1 && TALENT:30
    SOURCE:22 /= 3

;더러운 처리
STAIN:애널 |= 2
STAIN:애널 |= 64

SIF EXP:A확장경험 == 0
    TCVAR:50 += 1
TCVAR:1 += 25
TCVAR:33 += 1

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 1
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM531
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
PRINTFORML %CALLNAME:TARGET%의 항문은 고야열매같이 엄청나게 굵은 촉수로 휘저어졌다.
PRINTL
IF ABL:A감각 <= 1
    PRINTFORMW 촉수가 뽑아 내진 후도 %CALLNAME:TARGET%의 항문은 경련하고 있는 것 같이 벌름거린다.
ELSEIF ABL:A감각 == 2
    PRINTFORMW 촉수가 뽑아진 후도 %CALLNAME:TARGET%의 항문은 뻐끔 입을 열고 있다.
ELSEIF ABL:A감각 == 3
    PRINTFORMW 촉수가 뽑아진 후도 %CALLNAME:TARGET%의 항문은 벌려진 채로 좀처럼 돌아오지 않는다.
ELSEIF ABL:A감각 == 4
    PRINTFORML 촉수가 뽑아진 후의 %CALLNAME:TARGET%의 항문에서는 살점이 밀려나와,
    PRINTW 장안이 훤히 들여다보이고 있다.
ELSE
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 크게 벌려진 자신의 항문에 관심을 가진 채로, 황홀한 표정을 짓고 있다.
ENDIF
