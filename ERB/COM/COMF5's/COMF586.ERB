    LOCAL:3 = 0
ELSE
    TIMES LOCAL:1 , 1.60
    LOCAL:3 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES LOCAL:1 , 0.40
    LOCAL:3 += 1200
ELSEIF PALAM:10 < PALAMLV:2
    TIMES LOCAL:1 , 0.80
    LOCAL:3 += 700
ELSEIF PALAM:10 < PALAMLV:3
    TIMES LOCAL:1 , 1.00
    LOCAL:3 += 400
ELSEIF PALAM:10 < PALAMLV:4
    TIMES LOCAL:1 , 1.40
    LOCAL:3 += 150
ELSE
    TIMES LOCAL:1 , 1.80
    LOCAL:3 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES LOCAL:1 , 0.80
    TIMES LOCAL:5 , 1.20
ELSEIF PALAM:13 < PALAMLV:2
    TIMES LOCAL:1 , 0.90
    TIMES LOCAL:5 , 1.10
ELSEIF PALAM:13 < PALAMLV:3
    TIMES LOCAL:1 , 1.00
    TIMES LOCAL:5 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES LOCAL:1 , 1.10
    TIMES LOCAL:5 , 0.90
ELSE
    TIMES LOCAL:1 , 1.20
    TIMES LOCAL:5 , 0.80
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES LOCAL:1 , 0.80
    TIMES LOCAL:5 , 1.30
ELSEIF ABL:순종 == 1
    TIMES LOCAL:1 , 0.90
    TIMES LOCAL:5 , 1.20
ELSEIF ABL:순종 == 2
    TIMES LOCAL:1 , 1.00
    TIMES LOCAL:5 , 1.10
ELSEIF ABL:순종 == 3
    TIMES LOCAL:1 , 1.10
    TIMES LOCAL:5 , 1.00
ELSEIF ABL:순종 == 4
    TIMES LOCAL:1 , 1.20
    TIMES LOCAL:5 , 0.90
ELSE
    TIMES LOCAL:1 , 1.30
    TIMES LOCAL:5 , 0.80
ENDIF

;도착적
IF TALENT:80
    TIMES LOCAL:2 , 1.50
    TIMES LOCAL:6 , 0.80
ENDIF
;[작은체형]
IF TALENT:128
    TIMES LOCAL:3 , 2.00
    SIF TALENT:130 || TALENT:131
        TIMES LOCAL:1 , 2.00
ENDIF

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
    TIMES LOCAL:3 , 1.50
    TIMES LOCAL:4 , 1.50
    TIMES LOCAL:6 , 1.50
ELSEIF TALENT:104
    TIMES LOCAL:3 , 0.60
    TIMES LOCAL:4 , 0.60
    TIMES LOCAL:6 , 0.60
ENDIF
;처녀로 정조관념
SIF TALENT:처녀 == 1 && TALENT:30
    LOCAL:4 /= 3

;문장과의 궁합 있고 없음으로 보정이 상하
LOCAL:7 = 32
IF RELATION:(LOCAL:7) > 0 && RELATION:(LOCAL:7) != 100
    LOCAL:1 *= RELATION:(LOCAL:7)
    LOCAL:1 /= 100
    LOCAL:2 *= RELATION:(LOCAL:7)
    LOCAL:2 /= 100
    LOCAL:3 *= RELATION:(LOCAL:7)
    LOCAL:3 /= 100
    LOCAL:4 *= RELATION:(LOCAL:7)
    LOCAL:4 /= 100
    ;트라우마는 강제적으로 증가
    IF RELATION:(LOCAL:7) < 100
        LOCAL:5 *= (200-RELATION:(LOCAL:7))
    ELSE
        LOCAL:5 *= RELATION:(LOCAL:7)
    ENDIF
    LOCAL:5 /= 100
    LOCAL:6 *= RELATION:(LOCAL:7)
    LOCAL:6 /= 100
ENDIF

SOURCE:2 += LOCAL:1
SOURCE:12 += LOCAL:2
SOURCE:20 += LOCAL:3
SOURCE:22 += LOCAL:4
SOURCE:25 += LOCAL:5
SOURCE:31 += LOCAL:6

TCVAR:1 += 5

;노출 쾌락 경험 입수 판정
TFLAG:100 |= 2

;카메라 촉수의 착탈
IF TEQUIP:25 == 4
    TEQUIP:25 = 0
ELSE
    TEQUIP:25 = 4
ENDIF

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

