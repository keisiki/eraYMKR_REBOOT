@COM_ABLE551
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:551 > 0
    RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 4096) == 0
    RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;촉수가 [鞭촉수]を得ていないとダメ
SIF (FLAG:75 & 1) == 0
    RETURN 0
RETURN 1

