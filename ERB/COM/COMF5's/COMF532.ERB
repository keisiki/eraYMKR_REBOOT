;=============================================================================
;촉수 양구멍 확장
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE532
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:532 > 0
    RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 4096) == 0
    RETURN 0
;V경험レベルが0かつ조교자가 オトコではない場合, 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 4 && EXP:V경험 < EXPLV:1 && TALENT:PLAYER:남자 == 0
    RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
    RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
    RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 1
    RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 2
    RETURN 0
;조교자는 촉수사역LV3이상 필요
SIF ABL:PLAYER:촉수사역 < 3
    RETURN 0
;촉수가 [ゴーヤ型촉수]を得ていないとダメ
SIF (FLAG:촉수능력 & 128) == 0
    RETURN 0
;남자라면 안 됨
SIF TALENT:남자
    RETURN 0
;ASSI일 경우, 순종4이상이거나 새드じゃないとできない。
IF ASSIPLAY
    SIF ABL:ASSI:순종 < 4 && TALENT:ASSI:새드 == 0
        RETURN 0
ENDIF
;[작은체형]なら순종이 4이상かつ마조끼が4이상じゃないと実行できない
IF TALENT:128
    SIF ABL:순종 < 4 || ABL:마조끼 < 4
        RETURN 0
ENDIF
;일정의 Ｖ · A경험이 필요
SIF EXP:V경험 < 300 || EXP:A경험 < 300
    RETURN 0
;Ｖ · Ａ계 촉수責め中は不可
SIF TEQUIP:20 || TEQUIP:21 || TEQUIP:25
    RETURN 0
;卵が邪魔で膣内洗浄できない
SIF TEQUIP:93
    RETURN 0
;卵が邪魔で浣腸できない
SIF TEQUIP:94
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;들릴까? 이쪽은 신세대 촉수.
;그다지 시간이 없기 때문에 잘 들어 줘.
;놈을 이인가 다투기 위해서(때문에)는 너의 도움이 필요하다.
;우리는 다른 구멍으로부터 동시에 삽입해,
;2개소의 성감대에서 서로 1회씩 절정 시키지 않으면 안 된다.
;물론, 어느 쪽인지 다른 한쪽만으로는 안된다. 2명 모두 성공하지 않는 이상 우리에게 미래는 없다.
;……선택의 여지는 없을 것 같다.
;좋아, 가겠어!
@COM532
PRINTL 촉수 양 구멍 확장
COM_NAME = 촉수 양구멍 확장
CALL TRAIN_MESSAGE_COM532

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 320
DOWNBASE:기력 += 200

SOURCE:20 = 2200
SOURCE:19 = 2800
SOURCE:18 = 500
SOURCE:12 = 2500
SOURCE:22 = 2700
SOURCE:31 = 7500
SOURCE:23 = 2300

;ABL:V감각을 본다
IF ABL:V감각 == 0
    SOURCE:1 = 40
    SOURCE:11 = 50
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 150
    SOURCE:11 = 150
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 400
    SOURCE:11 = 250
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 1000
    SOURCE:11 = 350
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 1700
    SOURCE:11 = 600
ELSE
    SOURCE:1 = 2200
    SOURCE:11 = 850
ENDIF

;ABL:A감각을 본다
IF ABL:A감각 == 0
    SOURCE:2 = 10
    SOURCE:11 += 10
    SOURCE:22 += 100
ELSEIF ABL:A감각 == 1
    SOURCE:2 = 30
    SOURCE:11 += 30
    SOURCE:22 += 700
ELSEIF ABL:A감각 == 2
    SOURCE:2 = 500
    SOURCE:11 += 100
    SOURCE:22 += 1500
ELSEIF ABL:A감각 == 3
    SOURCE:2 = 1000
    SOURCE:11 += 200
    SOURCE:22 += 3000
ELSEIF ABL:A감각 == 4
    SOURCE:2 = 1700
    SOURCE:11 += 450
    SOURCE:22 += 5000
ELSE
    SOURCE:2 = 2200
    SOURCE:11 += 750
    SOURCE:22 += 8000
ENDIF

;EXP:V경험을 본다
IF EXP:V경험 < EXPLV:1
    TIMES SOURCE:1 , 0.20
    SOURCE:20 += 20000
    SOURCE:18 += 2000
    SIF TALENT:PLAYER:남자 == 0
        TCVAR:50 += 1
ELSEIF EXP:V경험 < EXPLV:2
    TIMES SOURCE:1 , 0.60
    SOURCE:20 += 300
ELSEIF EXP:V경험 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 += 50
ELSEIF EXP:V경험 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    SOURCE:20 += 10
ELSEIF EXP:V경험 < EXPLV:5
    TIMES SOURCE:1 , 1.40
    SOURCE:20 += 0
ELSE
    TIMES SOURCE:1 , 1.60
    SOURCE:20 += 0
ENDIF

;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
    TIMES SOURCE:2 , 0.10
    SOURCE:20 += 5000
    SOURCE:18 += 1000
ELSEIF EXP:A경험 < EXPLV:2
    TIMES SOURCE:2 , 0.30
    SOURCE:20 += 2000
    SOURCE:18 += 1000
ELSEIF EXP:A경험 < EXPLV:3
    TIMES SOURCE:2 , 0.50
    SOURCE:20 += 2000
    SOURCE:18 += 1000
ELSEIF EXP:A경험 < EXPLV:4
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 2000
    SOURCE:18 += 1000
ELSEIF EXP:A경험 < EXPLV:5
    TIMES SOURCE:2 , 1.40
    SOURCE:20 += 1000
    SOURCE:18 += 200
ELSE
    TIMES SOURCE:1 , 1.60
    SOURCE:20 += 600
ENDIF

;EXP:촉수 경험을 본다
IF EXP:촉수경험 < EXPLV:1
    TIMES DOWNBASE:0 , 3.00
    TIMES DOWNBASE:기력 , 3.00
    SOURCE:20 += 1000
ELSEIF EXP:촉수경험 < EXPLV:2
    TIMES DOWNBASE:0 , 2.50
    TIMES DOWNBASE:기력 , 2.50
    SOURCE:20 += 800
ELSEIF EXP:촉수경험 < EXPLV:3
    TIMES DOWNBASE:0 , 2.00
    TIMES DOWNBASE:기력 , 2.00
    SOURCE:20 += 500
ELSEIF EXP:촉수경험 < EXPLV:4
    TIMES DOWNBASE:0 , 1.00
    TIMES DOWNBASE:기력 , 1.00
    SOURCE:20 += 100
ELSEIF EXP:촉수경험 < EXPLV:5
    TIMES DOWNBASE:0 , 0.80
    TIMES DOWNBASE:기력 , 0.80
    SOURCE:20 += 50
ELSE
    TIMES DOWNBASE:0 , 0.60
    TIMES DOWNBASE:기력 , 0.60
ENDIF

;ABL:촉수 중독을 본다
IF ABL:촉수중독 == 0
    SOURCE:24 = 1500
    TIMES SOURCE:23 , 0.50
ELSEIF ABL:촉수중독 == 1
    TIMES DOWNBASE:0 , 0.90
    TIMES DOWNBASE:기력 , 0.90
    SOURCE:24 = 1700
    TIMES SOURCE:23 , 0.80
ELSEIF ABL:촉수중독 == 2
    TIMES DOWNBASE:0 , 0.80
    TIMES DOWNBASE:기력 , 0.80
    SOURCE:24 = 2000
ELSEIF ABL:촉수중독 == 3
    TIMES DOWNBASE:0 , 0.70
    TIMES DOWNBASE:기력 , 0.70
    SOURCE:24 = 2500
    TIMES SOURCE:23 , 1.20
ELSEIF ABL:촉수중독 == 4
    TIMES DOWNBASE:0 , 0.60
    TIMES DOWNBASE:기력 , 0.60
    SOURCE:24 = 3300
    TIMES SOURCE:23 , 1.50
ELSE
    TIMES DOWNBASE:0 , 0.50
    TIMES DOWNBASE:기력 , 0.50
    SOURCE:24 = 4200
    TIMES SOURCE:23 , 1.80
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.20
    TIMES SOURCE:2 , 0.20
    SOURCE:18 += 1000
    SOURCE:20 += 1900
    TIMES SOURCE:20 , 9.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:2 , 0.40
    SOURCE:18 += 800
    SOURCE:20 += 1250
    TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 0.60
    SOURCE:18 += 600
    SOURCE:20 += 1000
    TIMES SOURCE:20 , 1.50
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.30
    TIMES SOURCE:2 , 1.00
    SOURCE:18 += 200
    SOURCE:20 += 200
    TIMES SOURCE:20 , 0.30
ELSE
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:2 , 1.30
    TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:2 , 0.60
    TIMES SOURCE:22 , 0.60
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:2 , 0.80
    TIMES SOURCE:22 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:22 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:22 , 1.20
ELSE
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:2 , 1.40
    TIMES SOURCE:22 , 1.40
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:1 , 0.50
    TIMES SOURCE:2 , 0.70
    TIMES SOURCE:11 , 0.60
    TIMES SOURCE:18, 2.00
ELSEIF ABL:순종 == 1
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:2 , 0.90
    TIMES SOURCE:11 , 0.80
    TIMES SOURCE:18, 1.20
ELSEIF ABL:순종 == 2
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:11 , 1.00
    TIMES SOURCE:18, 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.10
    TIMES SOURCE:11 , 1.20
    TIMES SOURCE:18, 0.60
ELSEIF ABL:순종 == 4
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:11 , 1.40
    TIMES SOURCE:18, 0.30
ELSE
    TIMES SOURCE:1 , 1.70
    TIMES SOURCE:2 , 1.30
    TIMES SOURCE:11 , 1.60
    TIMES SOURCE:18, 0.10
ENDIF

;[작은체형]
SIF TALENT:128
    TIMES SOURCE:20 , 3.00

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:18 , 1.50
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:18 , 0.60
    TIMES SOURCE:22 , 0.60
    TIMES SOURCE:31 , 0.60
ENDIF

;정조관념
IF TALENT:정조관념
    IF TALENT:처녀 == 1
        TIMES SOURCE:11 , 0.60
        TIMES SOURCE:18, 5.00
    ELSE
        TIMES SOURCE:11 , 0.60
        TIMES SOURCE:18, 1.80
    ENDIF
;정조무구애
ELSEIF TALENT:정조관둔감
    IF TALENT:처녀 == 1
        TIMES SOURCE:18, 0.50
    ELSE
        TIMES SOURCE:18, 0.30
    ENDIF
ENDIF

;더러운 처리
STAIN:보지 |= 2
STAIN:보지 |= 64
STAIN:애널 |= 2
STAIN:애널 |= 64

IF EXP:V확장경험 == 0 && EXP:A확장경험 == 0
    TCVAR:50 += 2
ELSEIF EXP:V확장경험 == 0 || EXP:A확장경험 == 0
    TCVAR:50 += 1
ENDIF
TCVAR:0 += 25
TCVAR:1 += 25
TCVAR:32 += 2
TCVAR:33 += 2

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 2
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM532
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
PRINTFORML %CALLNAME:TARGET%의 질과 항문은 고야열매 같이 엄청나게 굵은 촉수로 휘저어졌다.
PRINTL
IF ABL:V감각 >= 5 && ABL:A감각 >= 5
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 양 구멍이 확장된 여운을 맛보고 있다.
    PRINTFORML %CALLNAME:TARGET%의 질구와 항문은 완전히 펼쳐져 내용을 노출하고 있고,
    PRINTW 촉수가 빼내진 후에도 원래대로 돌아갈 기색을 보여주지 않고 있다….
ELSEIF ABL:V감각 > 4 || ABL:A감각 > 4
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 양 다리를 벌리고 녹초가 되어 있다.
    PRINTW 촉수에 의해 벌려진 질구와 항문에서는 점액이 흘러나오고 있다.
ENDIF
