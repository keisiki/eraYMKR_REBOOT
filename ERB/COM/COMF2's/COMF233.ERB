@COM_ABLE233
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:233 > 0
    RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
    RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
    RETURN 0
;조교 대상のV확장경험もしくはA확장경험이 ない場合には, 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && (EXP:V확장경험 <= 1 || EXP:A확장경험 <= 1)
    RETURN 0
;V경험LVが1미만で, 조교자가 オトコではない場合
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && EXP:V경험 < EXPLV:1 && TALENT:PLAYER:120 == 0
    RETURN 0
;남자라면 안 됨
SIF TALENT:120
    RETURN 0
;[작은체형]なら순종이 4이상かつ마조끼が4이상かつ확장경험10이상じゃないと実行できない
IF TALENT:128
    SIF ABL:순종 < 4 || ABL:마조끼 < 4 || EXP:V확장경험 < 10 || EXP:A확장경험 < 10
        RETURN 0
ENDIF
;조교 대상이 [소인]일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:127 && TALENT:MASTER:56 == 0
    RETURN 0
;ASSI일 경우, 순종4이상이거나 새드じゃないとできない。
IF ASSIPLAY
    SIF ABL:ASSI:순종 < 4 && TALENT:ASSI:81 == 0
        RETURN 0
ENDIF
;일정의 Ｖ · A경험이 필요([巨躯]の場合は軽減)
LOCAL = (TALENT:129) ? 150 # 300
SIF EXP:V경험 < LOCAL || EXP:A경험 < LOCAL
    RETURN 0
;그 조교 중에 관장 플레이＋배설이 있었을 경우, 조교자가 汚れ無視もしくは汚臭鈍感を持たないなら実行できない
SIF TFLAG:140 & 256 && TALENT:PLAYER:61 == 0 && TALENT:PLAYER:64 == 0
    RETURN 0
;Ｖ계 장착구 사용 중에는 불가
SIF TEQUIP:20 || TEQUIP:21
    RETURN 0
;Ａ계 장착구 사용 중에는 불가
SIF TEQUIP:25 || TEQUIP:26
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
    RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
    RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;알몸 앞치마, 학교수영복 플레이, 코스프레 중에는 불가
SIF TEQUIP:59
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM233
PRINTL 양구멍 피스트
COM_NAME = 양구멍 피스트
CALL TRAIN_MESSAGE_COM233

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 800
DOWNBASE:기력 += 500

SOURCE:20 = 1800
SOURCE:19 = 2800
SOURCE:18 = 1500
SOURCE:12 = 2500
SOURCE:22 = 2500
SOURCE:31 = 2500
SOURCE:17 = 2500

;ABL:V감각을 본다
IF ABL:V감각 == 0
    SOURCE:1 = 40
    SOURCE:11 = 50
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 150
    SOURCE:11 = 150
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 400
    SOURCE:11 = 250
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 1000
    SOURCE:11 = 350
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 1700
    SOURCE:11 = 600
ELSE
    SOURCE:1 = 2200
    SOURCE:11 = 850
ENDIF

;ABL:A감각을 본다
IF ABL:A감각 == 0
    SOURCE:2 = 10
    SOURCE:11 += 10
    SOURCE:22 += 100
ELSEIF ABL:A감각 == 1
    SOURCE:2 = 30
    SOURCE:11 += 30
    SOURCE:22 += 700
ELSEIF ABL:A감각 == 2
    SOURCE:2 = 500
    SOURCE:11 += 100
    SOURCE:22 += 1500
ELSEIF ABL:A감각 == 3
    SOURCE:2 = 1000
    SOURCE:11 += 200
    SOURCE:22 += 3000
ELSEIF ABL:A감각 == 4
    SOURCE:2 = 1700
    SOURCE:11 += 450
    SOURCE:22 += 5000
ELSE
    SOURCE:2 = 2200
    SOURCE:11 += 750
    SOURCE:22 += 8000
ENDIF

;EXP:V경험을 본다
IF EXP:V경험 < EXPLV:1
    TIMES SOURCE:1 , 0.20
    SOURCE:20 += 20000
    SOURCE:18 += 2000
    SIF TALENT:PLAYER:120 == 0
        TCVAR:50 += 1
ELSEIF EXP:V경험 < EXPLV:2
    TIMES SOURCE:1 , 0.60
    SOURCE:20 += 300
ELSEIF EXP:V경험 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 += 50
ELSEIF EXP:V경험 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    SOURCE:20 += 10
ELSEIF EXP:V경험 < EXPLV:5
    TIMES SOURCE:1 , 1.40
    SOURCE:20 += 0
ELSE
    TIMES SOURCE:1 , 1.60
    SOURCE:20 += 0
ENDIF

;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
    TIMES SOURCE:2 , 0.10
    SOURCE:20 += 5000
    SOURCE:18 += 1000
ELSEIF EXP:A경험 < EXPLV:2
    TIMES SOURCE:2 , 0.30
    SOURCE:20 += 2000
    SOURCE:18 += 1000
ELSEIF EXP:A경험 < EXPLV:3
    TIMES SOURCE:2 , 0.50
    SOURCE:20 += 2000
    SOURCE:18 += 1000
ELSEIF EXP:A경험 < EXPLV:4
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 2000
    SOURCE:18 += 1000
ELSEIF EXP:A경험 < EXPLV:5
    TIMES SOURCE:2 , 1.40
    SOURCE:20 += 1000
    SOURCE:18 += 200
ELSE
    TIMES SOURCE:1 , 1.60
    SOURCE:20 += 600
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.20
    TIMES SOURCE:2 , 0.20
    SOURCE:18 += 1000
    SOURCE:20 += 1900
    TIMES SOURCE:20 , 9.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:2 , 0.40
    SOURCE:18 += 800
    SOURCE:20 += 1250
    TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 0.60
    SOURCE:18 += 600
    SOURCE:20 += 1000
    TIMES SOURCE:20 , 1.50
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.30
    TIMES SOURCE:2 , 1.00
    SOURCE:18 += 200
    SOURCE:20 += 200
    TIMES SOURCE:20 , 0.30
ELSE
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:2 , 1.30
    TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:2 , 0.60
    TIMES SOURCE:22 , 0.60
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:2 , 0.80
    TIMES SOURCE:22 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:22 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:22 , 1.20
ELSE
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:2 , 1.40
    TIMES SOURCE:22 , 1.40
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:1 , 0.50
    TIMES SOURCE:2 , 0.70
    TIMES SOURCE:11 , 0.60
    TIMES SOURCE:18, 2.00
ELSEIF ABL:순종 == 1
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:2 , 0.90
    TIMES SOURCE:11 , 0.80
    TIMES SOURCE:18, 1.20
ELSEIF ABL:순종 == 2
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:11 , 1.00
    TIMES SOURCE:18, 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.10
    TIMES SOURCE:11 , 1.20
    TIMES SOURCE:18, 0.60
ELSEIF ABL:순종 == 4
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:11 , 1.40
    TIMES SOURCE:18, 0.30
ELSE
    TIMES SOURCE:1 , 1.70
    TIMES SOURCE:2 , 1.30
    TIMES SOURCE:11 , 1.60
    TIMES SOURCE:18, 0.10
ENDIF

;[작은체형]
SIF TALENT:128
    TIMES SOURCE:20 , 3.00

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:18 , 1.50
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:18 , 0.60
    TIMES SOURCE:22 , 0.60
    TIMES SOURCE:31 , 0.60
ENDIF

;정조관념
IF TALENT:30
    IF TALENT:처녀 == 1
        TIMES SOURCE:11 , 0.60
        TIMES SOURCE:18, 5.00
    ELSE
        TIMES SOURCE:11 , 0.60
        TIMES SOURCE:18, 1.80
    ENDIF
;정조무구애
ELSEIF TALENT:31
    IF TALENT:처녀 == 1
        TIMES SOURCE:18, 0.50
    ELSE
        TIMES SOURCE:18, 0.30
    ENDIF
ENDIF

IF EXP:V확장경험 == 0 && EXP:A확장경험 == 0
    TCVAR:50 += 2
ELSEIF EXP:V확장경험 == 0 || EXP:A확장경험 == 0
    TCVAR:50 += 1
ENDIF
TCVAR:0 += 25
TCVAR:1 += 25
TCVAR:32 += 2
TCVAR:33 += 2

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM233
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
PRINTFORML %CALLNAME:TARGET%의 질과 항문은 %CALLNAME:PLAYER%의 주먹으로 휘저어졌다.
PRINTL
;LOCAL   = 대상의 V감각과 A감각의 비싼 (분)편
;LOCAL:1 = 대상의 V감각과 A감각의 합계 Lv
LOCAL = MAX(ABL:V감각 , ABL:A감각)
LOCAL:1 = ABL:V감각 + ABL:A감각
IF LOCAL >= 5 && LOCAL:1 >= 10
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 2개의 주먹으로 몸 속을 유린당해, 격렬하게 느끼고 있다….
ELSEIF LOCAL > 4
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 격렬하게 숨을 몰아 쉬고 있지만, 쾌감도 확실히 느끼고 있는 것 같다….
ELSEIF LOCAL > 2
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 양 다리를 벌리고 녹초가 되어있다.
ELSE
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 괴로운 듯한 표정을 하고 있다.
ENDIF
IF LOCAL >= 5 && LOCAL:1 >= 10
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 완전히 벌려져 안을 훤히 보이고 있는, 질구와 항문의 주변을 양 손가락으로 어루만졌다….
ELSE
    SELECTCASE ABL:V감각
        CASE 0, 1
            PRINTFORML %CALLNAME:TARGET%의 질은 경련하고 있는 것 같이 벌름거린다.
        CASE 2
            PRINTFORML %CALLNAME:TARGET%의 질은 뻐끔 입을 열고 있다.
        CASE 3
            PRINTFORML %CALLNAME:TARGET%의 질은 벌려진 채로 좀처럼 돌아오지 않는다.
        CASE 4
            PRINTFORML %CALLNAME:TARGET%의 보지로부터는 질 벽의 살이 밀려나와,
            PRINTL 뻥 열린 질구의 안으로 자궁구가 훤히 보이고 있다.
        CASEELSE
            PRINTFORML %CALLNAME:TARGET%의 질은 요염하게 열려 마치 %조사선택(CALLNAME:PLAYER, "를")% 빨아들일 것 같은 모양을 하고 있다….
    ENDSELECT
    PRINT 한편, 
    SELECTCASE ABL:A감각
        CASE 0, 1
            PRINTFORMW 항문\@(ABL:V감각 > 1) ? 은 # 도\@ 경련하고 있는것 같이 벌름거린다….
        CASE 2
            PRINTFORMW 항문\@(ABL:V감각 != 2) ? 은 # 도\@ 뻐끔 입을 열고 있다….
        CASE 3
            PRINTFORMW 항문\@(ABL:V감각 != 3) ? 은 # 도\@ 벌려진 채로 좀처럼 돌아오지 않는다….
        CASE 4
            PRINTFORML 항문에서\@(ABL:V감각 != 4) ? 은 # 도\@ 살들이 밀려나와,
            PRINTW 장안이 훤히 들여다보이게 되어 있다….
        CASEELSE
            PRINTFORML 항문\@(ABL:V감각 < 5) ? 은 # 도\@ 다 열려져 있어,
            PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 손가락으로 거기를 어루만지고 돌리며 쾌감의 여운에 잠겨 있다….
    ENDSELECT
ENDIF
