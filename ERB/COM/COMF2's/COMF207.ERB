    ELSEIF EXP:35 < 10
        TIMES LOCAL:2, 4.00
    ELSEIF EXP:35 < 50
        TIMES LOCAL:2, 2.00
    ELSEIF EXP:35 < 100
        TIMES LOCAL:2, 1.50
    ELSEIF EXP:35 < 150
        TIMES LOCAL:2, 1.10
    ENDIF
