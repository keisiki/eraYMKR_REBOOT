@COM_ABLE628
RETURN 0

@EXCOM_ABLE628
;수치 플레이계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 512) == 0
    RETURN 0
;순종4미만 · 레즈끼(BL끼) 4 미만의 ASSI일 경우, 조교 대상이 동성이라면 실행할 수 없다(소악마か망신か복종ならＯＫ)
IF ASSIPLAY
    SIF (ABL:ASSI:순종 < 4 || ((TALENT:남자 == 0 && TALENT:ASSI:남자 == 0 && ABL:ASSI:레즈끼 < 4) || (TALENT:남자 && TALENT:ASSI:남자 && ABL:ASSI:BL끼 < 4))) && TALENT:ASSI:소악마 == 0 && TALENT:ASSI:망신 == 0 && TALENT:ASSI:복종 == 0
        RETURN 0
ENDIF
;野外もしくは公開自慰を行ったことがない場合の野外およびビデオ撮影中は, 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 4 && (CFLAG:첫특수커맨드 & 1) == 0 && (TEQUIP:50 || TEQUIP:52)
    RETURN 0
SIF ASSIPLAY && FLAG:VA이상조교필터 & 4 && (CFLAG:ASSI:첫특수커맨드 & 1) == 0 && (TEQUIP:50 || TEQUIP:52)
    RETURN 0
;수면 중에는 불가
SIF EQUIP:14
    RETURN 0
;바이브, 쿠스코, 애널바이브, 요석以外のＶ계 장착구, Ａ계 장착구 사용 중에는 불가
SIF TEQUIP:20 > 2 || TEQUIP:25 > 2 || TEQUIP:26
    RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:40
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
    RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
    RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
    RETURN 0
;조수가 훈도시 착용 중일 경우 , 助手の기교4이상 필수
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17 && ABL:ASSI:기교 < 4
    RETURN 0
;훈도시 착용 중일 경우 기교4이상 필수
SIF TEQUIP:59 == 17 && ABL:기교 < 4
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;バイブ、アナルバイブ、ビデオ撮影の時に内容変化
;ビデオ撮影中なら公開オナニー
;シャワー使用中ならシャワーオナニー
;読心中なら読心オナニー
;バイブ、アナルバイブ、或いは両方装備でバイブオナニー
@COM628
LOCALS = 
SIF TEQUIP:50
    LOCALS = %LOCALS%공개
SIF TEQUIP:52
    LOCALS = %LOCALS%야외
SIF TEQUIP:74
    LOCALS = %LOCALS%독심
SIF TEQUIP:54
    LOCALS = %LOCALS%샤워
IF TEQUIP:20 == 2 && TEQUIP:25 == 2
    LOCALS = %LOCALS%두 구멍 요석
ELSEIF TEQUIP:20 == 1 && TEQUIP:25 == 1
    LOCALS = %LOCALS%두 구멍 바이브
ELSEIF TEQUIP:20 == 1
    LOCALS = %LOCALS%바이브
ELSEIF TEQUIP:20 == 2
    LOCALS = %LOCALS%요석
ELSEIF TEQUIP:25 == 1
    LOCALS = %LOCALS%애널 바이브
ELSEIF TEQUIP:25 == 2
    LOCALS = %LOCALS%요석 애널
ENDIF
IF TEQUIP:20 || TEQUIP:25 || TEQUIP:50 || TEQUIP:54 || TEQUIP:74
    LOCALS = %LOCALS%상호 딸딸이
ELSE
    LOCALS = %LOCALS%상호자위
ENDIF
PRINTSL LOCALS

;-------------------------------------------------
;실행할 수 있는가 판정
;-------------------------------------------------
IF TFLAG:699 == 0
    CALL COM_ORDER,628
    ;실행할 수 없다
    SIF RESULT == 0
        RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = %LOCALS%
SELECTCOM = 628
CALL TRAIN_MESSAGE_COM628

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;조교자의 기교를 본다
IF ABL:PLAYER:기교 == 0
    LOCAL = 800
ELSEIF ABL:PLAYER:기교 == 1
    LOCAL = 1600
ELSEIF ABL:PLAYER:기교 == 2
    LOCAL = 2400
ELSEIF ABL:PLAYER:기교 == 3
    LOCAL = 3200
ELSEIF ABL:PLAYER:기교 == 4
    LOCAL = 4000
ELSE
    LOCAL = 4800
ENDIF

;조교자의 Ｃ감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_2(PLAYER, LOCAL)

;Ｃ感覚の上げられない主人かつ남자の場合2倍(ということにしておく&それでも10000には届かないんだぜウフフ)
SIF ASSIPLAY == 0 && TALENT:PLAYER:남자
    TIMES LOCAL , 2.00

SIF EXIST_PENIS(PLAYER)
    BASE:PLAYER:사정 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 10
DOWNBASE:기력 += 60

SOURCE:30 = 100
SOURCE:31 = 600
LOCAL:1 = 0
LOCAL:2 = 0
LOCAL:3 = 0
LOCAL:4 = 0

IF TEQUIP:50 || TEQUIP:74
    SOURCE:18 = 100
    SOURCE:19 = 50
ENDIF

;ABL:Ｃ감각을 본다
IF ABL:C감각 == 0
    SOURCE:0 = 15
    SOURCE:12 = 2000
    SOURCE:22 = 500
ELSEIF ABL:C감각 == 1
    SOURCE:0 = 50
    SOURCE:12 = 2300
    SOURCE:22 = 800
ELSEIF ABL:C감각 == 2
    SOURCE:0 = 300
    SOURCE:12 = 2600
    SOURCE:22 = 1200
ELSEIF ABL:C감각 == 3
    SOURCE:0 = 700
    SOURCE:12 = 2900
    SOURCE:22 = 1900
ELSEIF ABL:C감각 == 4
    SOURCE:0 = 1100
    SOURCE:12 = 3200
    SOURCE:22 = 2500
ELSE
    SOURCE:0 = 1600
    SOURCE:12 = 3500
    SOURCE:22 = 3000
ENDIF

;ABL:Ｂ감각을 본다
IF ABL:B감각 == 0
    SOURCE:3 = 15
ELSEIF ABL:B감각 == 1
    SOURCE:3 = 50
ELSEIF ABL:B감각 == 2
    SOURCE:3 = 300
ELSEIF ABL:B감각 == 3
    SOURCE:3 = 700
ELSEIF ABL:B감각 == 4
    SOURCE:3 = 1100
ELSE
    SOURCE:3 = 1600
ENDIF

;バイブ挿入中を確認
IF TEQUIP:20
    ;ABL:Ｖ감각을 본다
    IF ABL:V감각 == 0
        LOCAL:1 += 40
        LOCAL:4 += 150
    ELSEIF ABL:V감각 == 1
        LOCAL:1 += 120
        LOCAL:4 += 400
    ELSEIF ABL:V감각 == 2
        LOCAL:1 += 300
        LOCAL:4 += 700
    ELSEIF ABL:V감각 == 3
        LOCAL:1 += 500
        LOCAL:4 += 900
    ELSEIF ABL:V감각 == 4
        LOCAL:1 += 650
        LOCAL:4 += 1000
    ELSE
        LOCAL:1 += 850
        LOCAL:4 += 1200
    ENDIF

    ;EXP:V경험을 본다
    ;처녀는 있을 수 없다
    IF EXP:V경험 < EXPLV:2
        TIMES LOCAL:1 , 0.60
        LOCAL:3 += 150
    ELSEIF EXP:V경험 < EXPLV:3
        TIMES LOCAL:1 , 1.00
        LOCAL:3 += 20
    ELSEIF EXP:V경험 < EXPLV:4
        TIMES LOCAL:1 , 1.20
        LOCAL:3 += 0
    ELSEIF EXP:V경험 < EXPLV:5
        TIMES LOCAL:1 , 1.40
        LOCAL:3 += 0
    ELSE
        TIMES LOCAL:1 , 1.60
        LOCAL:3 += 0
    ENDIF
    ;V민감、둔감을 본다
    IF TALENT:V둔감
        TIMES LOCAL:3 , 1.50
        TIMES LOCAL:4 , 1.50
    ELSEIF TALENT:V민감
        TIMES LOCAL:3 , 0.60
        TIMES LOCAL:4 , 0.60
    ENDIF
    ;한 번 단독으로 계산
    SOURCE:22 += LOCAL:4
ENDIF

;アナルバイブ挿入中を確認
IF TEQUIP:25
    DOWNBASE:0 += 30
    DOWNBASE:기력 += 80

    ;ABL:Ａ감각을 본다
    IF ABL:A감각 == 0
        LOCAL:2 += 40
        LOCAL:4 += 150
    ELSEIF ABL:A감각 == 1
        LOCAL:2 += 120
        LOCAL:4 += 400
    ELSEIF ABL:A감각 == 2
        LOCAL:2 += 300
        LOCAL:4 += 700
    ELSEIF ABL:A감각 == 3
        LOCAL:2 += 500
        LOCAL:4 += 900
    ELSEIF ABL:A감각 == 4
        LOCAL:2 += 650
        LOCAL:4 += 1000
    ELSE
        LOCAL:2 += 850
        LOCAL:4 += 1200
    ENDIF

    ;EXP:A경험을 본다
    IF EXP:A경험 < EXPLV:1
        TIMES LOCAL:2 , 0.50
        LOCAL:3 += 1000
    ELSEIF EXP:A경험 < EXPLV:2
        TIMES LOCAL:2 , 1.00
        LOCAL:3 += 150
    ELSEIF EXP:A경험 < EXPLV:3
        TIMES LOCAL:2 , 1.10
        LOCAL:3 += 20
    ELSEIF EXP:A경험 < EXPLV:4
        TIMES LOCAL:2 , 1.20
        LOCAL:3 += 0
    ELSEIF EXP:A경험 < EXPLV:5
        TIMES LOCAL:2 , 1.40
        LOCAL:3 += 0
    ELSE
        TIMES LOCAL:2 , 1.60
        LOCAL:3 += 0
    ENDIF

    ;Ａ민감、둔감을 본다
    IF TALENT:A둔감
        TIMES LOCAL:3 , 1.50
        TIMES LOCAL:4 , 1.50
    ELSEIF TALENT:A민감
        TIMES LOCAL:3 , 0.60
        TIMES LOCAL:4 , 0.60
    ENDIF
    ;한 번 단독으로 계산
    SOURCE:22 += LOCAL:4
ENDIF

;シャワー使用中を確認
IF TEQUIP:54
    ;ABL:Ｃ감각을 본다
    IF ABL:C감각 == 0
        SOURCE:0 += 150
        SOURCE:12 += 1000
        SOURCE:22 = 50
    ELSEIF ABL:C감각 == 1
        SOURCE:0 += 400
        SOURCE:12 += 1300
        SOURCE:22 = 80
    ELSEIF ABL:C감각 == 2
        SOURCE:0 += 800
        SOURCE:12 += 1600
        SOURCE:22 = 120
    ELSEIF ABL:C감각 == 3
        SOURCE:0 += 1200
        SOURCE:12 += 1900
        SOURCE:22 = 190
    ELSEIF ABL:C감각 == 4
        SOURCE:0 += 1500
        SOURCE:12 += 2200
        SOURCE:22 = 250
    ELSE
        SOURCE:0 += 1800
        SOURCE:12 += 2500
        SOURCE:22 = 300
    ENDIF

    ;ABL:Ｖ감각을 본다
    IF ABL:V감각 == 0
        LOCAL:1 = 0
        LOCAL:4 = 0
    ELSEIF ABL:V감각 == 1
        LOCAL:1 = 100
        LOCAL:4 = 300
    ELSEIF ABL:V감각 == 2
        LOCAL:1 = 200
        LOCAL:4 = 400
    ELSEIF ABL:V감각 == 3
        LOCAL:1 = 300
        LOCAL:4 = 500
    ELSEIF ABL:V감각 == 4
        LOCAL:1 = 400
        LOCAL:4 = 600
    ELSE
        LOCAL:1 = 500
        LOCAL:4 = 700
    ENDIF

    ;ABL:Ａ감각을 본다
    IF ABL:A감각 == 0
        LOCAL:2 = 40
        LOCAL:4 += 150
    ELSEIF ABL:A감각 == 1
        LOCAL:2 = 120
        LOCAL:4 += 400
    ELSEIF ABL:A감각 == 2
        LOCAL:2 = 300
        LOCAL:4 += 700
    ELSEIF ABL:A감각 == 3
        LOCAL:2 = 500
        LOCAL:4 += 900
    ELSEIF ABL:A감각 == 4
        LOCAL:2 = 650
        LOCAL:4 += 1000
    ELSE
        LOCAL:2 = 850
        LOCAL:4 += 1200
    ENDIF

    ;V민감、둔감을 본다
    IF TALENT:V둔감
        TIMES LOCAL:3 , 1.50
        TIMES LOCAL:4 , 1.50
    ELSEIF TALENT:V민감
        TIMES LOCAL:3 , 0.60
        TIMES LOCAL:4 , 0.60
    ENDIF
    ;Ａ민감、둔감을 본다
    IF TALENT:A둔감
        TIMES LOCAL:3 , 1.50
        TIMES LOCAL:4 , 1.50
    ELSEIF TALENT:A민감
        TIMES LOCAL:3 , 0.60
        TIMES LOCAL:4 , 0.60
    ENDIF
    ;한 번 단독으로 계산
    SOURCE:22 += LOCAL:4
ENDIF

;バイブ、アナルバイブを計算
IF TEQUIP:20 || TEQUIP:25
    ;ＶかＡが上昇するとき、上昇に従ってSOURCE:0、SOURCE:3を減らす
    LOCAL:5 = ABL:V감각 + ABL:A감각
    IF LOCAL:5 <= 1
        TIMES SOURCE:0 , 1.00
        TIMES SOURCE:3 , 1.00
    ELSEIF LOCAL:5 <= 3
        TIMES SOURCE:0 , 0.90
        TIMES SOURCE:3 , 0.90
    ELSEIF LOCAL:5 <= 5
        TIMES SOURCE:0 , 0.80
        TIMES SOURCE:3 , 0.80
    ELSEIF LOCAL:5 <= 7
        TIMES SOURCE:0 , 0.70
        TIMES SOURCE:3 , 0.70
    ELSEIF LOCAL:5 <= 9
        TIMES SOURCE:0 , 0.60
        TIMES SOURCE:3 , 0.60
    ELSE
        TIMES SOURCE:0 , 0.50
        TIMES SOURCE:3 , 0.50
    ENDIF

    ;PALAM:윤활을 본다
    IF PALAM:10 < PALAMLV:1
        TIMES LOCAL:1 , 0.40
        TIMES LOCAL:2 , 0.40
        LOCAL:3 += 800
    ELSEIF PALAM:10 < PALAMLV:2
        TIMES LOCAL:1 , 0.80
        TIMES LOCAL:2 , 0.80
        LOCAL:3 += 500
    ELSEIF PALAM:10 < PALAMLV:3
        TIMES LOCAL:1 , 1.00
        TIMES LOCAL:2 , 1.00
        LOCAL:3 += 300
    ELSEIF PALAM:10 < PALAMLV:4
        TIMES LOCAL:1 , 1.40
        TIMES LOCAL:2 , 1.40
        LOCAL:3 += 120
    ELSE
        TIMES LOCAL:1 , 1.80
        TIMES LOCAL:2 , 1.80
        LOCAL:3 += 100
    ENDIF

    ;PALAM:욕정을 본다
    IF PALAM:13 < PALAMLV:1
        TIMES LOCAL:1 , 0.80
        TIMES LOCAL:2 , 0.80
    ELSEIF PALAM:13 < PALAMLV:2
        TIMES LOCAL:1 , 0.90
        TIMES LOCAL:2 , 0.90
    ELSEIF PALAM:13 < PALAMLV:3
        TIMES LOCAL:1 , 1.00
        TIMES LOCAL:2 , 1.00
    ELSEIF PALAM:13 < PALAMLV:4
        TIMES LOCAL:1 , 1.10
        TIMES LOCAL:2 , 1.10
    ELSE
        TIMES LOCAL:1 , 1.20
        TIMES LOCAL:2 , 1.20
    ENDIF

    ;ABL:순종을 본다
    IF ABL:순종 == 0
        TIMES LOCAL:1 , 0.80
        TIMES LOCAL:2 , 0.80
    ELSEIF ABL:순종 == 1
        TIMES LOCAL:1 , 0.90
        TIMES LOCAL:2 , 0.90
    ELSEIF ABL:순종 == 2
        TIMES LOCAL:1 , 1.00
        TIMES LOCAL:2 , 1.00
    ELSEIF ABL:순종 == 3
        TIMES LOCAL:1 , 1.10
        TIMES LOCAL:2 , 1.10
    ELSEIF ABL:순종 == 4
        TIMES LOCAL:1 , 1.20
        TIMES LOCAL:2 , 1.20
    ELSE
        TIMES LOCAL:1 , 1.30
        TIMES LOCAL:2 , 1.30
    ENDIF

    ;정조관념
    SIF TALENT:정조관념
        TIMES LOCAL:3 , 3.00
    ;小柄体形
    SIF TALENT:작은체형
        TIMES LOCAL:3 , 2.00
ENDIF

;샤워기를 계산
IF TEQUIP:54
    ;PALAM:윤활을 본다
    IF PALAM:10 < PALAMLV:1
        TIMES LOCAL:1 , 0.40
        TIMES LOCAL:2 , 0.40
        LOCAL:3 += 800
    ELSEIF PALAM:10 < PALAMLV:2
        TIMES LOCAL:1 , 0.80
        TIMES LOCAL:2 , 0.80
        LOCAL:3 += 500
    ELSEIF PALAM:10 < PALAMLV:3
        TIMES LOCAL:1 , 1.00
        TIMES LOCAL:2 , 1.00
        LOCAL:3 += 300
    ELSEIF PALAM:10 < PALAMLV:4
        TIMES LOCAL:1 , 1.40
        TIMES LOCAL:2 , 1.40
        LOCAL:3 += 120
    ELSE
        TIMES LOCAL:1 , 1.80
        TIMES LOCAL:2 , 1.80
        LOCAL:3 += 100
    ENDIF

    ;PALAM:욕정을 본다
    IF PALAM:13 < PALAMLV:1
        TIMES LOCAL:1 , 0.80
        TIMES LOCAL:2 , 0.80
    ELSEIF PALAM:13 < PALAMLV:2
        TIMES LOCAL:1 , 0.90
        TIMES LOCAL:2 , 0.90
    ELSEIF PALAM:13 < PALAMLV:3
        TIMES LOCAL:1 , 1.00
        TIMES LOCAL:2 , 1.00
    ELSEIF PALAM:13 < PALAMLV:4
        TIMES LOCAL:1 , 1.10
        TIMES LOCAL:2 , 1.10
    ELSE
        TIMES LOCAL:1 , 1.20
        TIMES LOCAL:2 , 1.20
    ENDIF

    ;ABL:순종을 본다
    IF ABL:순종 == 0
        TIMES LOCAL:1 , 0.80
        TIMES LOCAL:2 , 0.80
    ELSEIF ABL:순종 == 1
        TIMES LOCAL:1 , 0.90
        TIMES LOCAL:2 , 0.90
    ELSEIF ABL:순종 == 2
        TIMES LOCAL:1 , 1.00
        TIMES LOCAL:2 , 1.00
    ELSEIF ABL:순종 == 3
        TIMES LOCAL:1 , 1.10
        TIMES LOCAL:2 , 1.10
    ELSEIF ABL:순종 == 4
        TIMES LOCAL:1 , 1.20
        TIMES LOCAL:2 , 1.20
    ELSE
        TIMES LOCAL:1 , 1.30
        TIMES LOCAL:2 , 1.30
    ENDIF
ENDIF

SOURCE:1 = LOCAL:1
SOURCE:2 = LOCAL:2
SOURCE:20 = LOCAL:3

;ABL:기교를 본다
IF ABL:기교 == 0
    SOURCE:15 = 100
    TIMES SOURCE:0 , 0.30
    TIMES SOURCE:1 , 0.30
    TIMES SOURCE:2 , 0.30
    TIMES SOURCE:3 , 0.30
ELSEIF ABL:기교 == 1
    SOURCE:15 = 160
    TIMES SOURCE:0 , 0.70
    TIMES SOURCE:1 , 0.70
    TIMES SOURCE:2 , 0.70
    TIMES SOURCE:3 , 0.70
ELSEIF ABL:기교 == 2
    SOURCE:15 = 220
    TIMES SOURCE:0 , 1.00
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:3 , 1.00
ELSEIF ABL:기교 == 3
    SOURCE:15 = 280
    TIMES SOURCE:0 , 1.20
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:3 , 1.20
ELSEIF ABL:기교 == 4
    SOURCE:15 = 340
    TIMES SOURCE:0 , 1.40
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:2 , 1.40
    TIMES SOURCE:3 , 1.40
ELSE
    SOURCE:15 = 400
    TIMES SOURCE:0 , 1.60
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:2 , 1.60
    TIMES SOURCE:3 , 1.60
ENDIF

;ABL:자위중독을 본다
IF ABL:자위중독 == 0
    SOURCE:24 = 0
    TIMES SOURCE:0 , 1.00
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:3 , 1.00
ELSEIF ABL:자위중독 == 1
    SOURCE:24 = 100
    TIMES SOURCE:0 , 1.10
    TIMES SOURCE:1 , 1.10
    TIMES SOURCE:2 , 1.10
    TIMES SOURCE:3 , 1.10
ELSEIF ABL:자위중독 == 2
    SOURCE:24 = 300
    TIMES SOURCE:0 , 1.20
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:3 , 1.20
ELSEIF ABL:자위중독 == 3
    SOURCE:24 = 800
    TIMES SOURCE:0 , 1.30
    TIMES SOURCE:1 , 1.30
    TIMES SOURCE:2 , 1.30
    TIMES SOURCE:3 , 1.30
ELSEIF ABL:자위중독 == 4
    SOURCE:24 = 1500
    TIMES SOURCE:0 , 1.50
    TIMES SOURCE:1 , 1.50
    TIMES SOURCE:2 , 1.50
    TIMES SOURCE:3 , 1.50
