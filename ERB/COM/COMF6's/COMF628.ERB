    SOURCE:24 = 2500
    TIMES SOURCE:0 , 1.70
    TIMES SOURCE:1 , 1.70
    TIMES SOURCE:2 , 1.70
    TIMES SOURCE:3 , 1.70
ENDIF

;公開のときは、ABL:노출벽을 본다
IF TEQUIP:50
    IF ABL:노출증 == 0
        SOURCE:24 += 0
        TIMES SOURCE:0 , 1.00
        TIMES SOURCE:1 , 1.00
        TIMES SOURCE:2 , 1.00
        TIMES SOURCE:3 , 1.00
        TIMES SOURCE:12 , 1.00
    ELSEIF ABL:노출증 == 1
        SOURCE:24 += 100
        TIMES SOURCE:0 , 1.10
        TIMES SOURCE:1 , 1.10
        TIMES SOURCE:2 , 1.10
        TIMES SOURCE:3 , 1.10
        TIMES SOURCE:12 , 1.20
    ELSEIF ABL:노출증 == 2
        SOURCE:24 += 300
        TIMES SOURCE:0 , 1.20
