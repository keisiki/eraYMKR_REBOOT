    SOURCE:12 = 2000
    SOURCE:22 = 600
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 25
    SOURCE:12 = 2300
    SOURCE:22 = 1000
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 150
    SOURCE:12 = 2600
    SOURCE:22 = 1500
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 350
    SOURCE:12 = 2900
    SOURCE:22 = 2000
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 550
    SOURCE:12 = 3200
    SOURCE:22 = 2800
ELSE
    SOURCE:1 = 500
    SOURCE:12 = 3500
    SOURCE:22 = 3300
ENDIF

;EXP:V경험을 본다
IF EXP:V경험 < EXPLV:1
    TIMES SOURCE:1 , 0.50
    LOCAL:3 += 500
ELSEIF EXP:V경험 < EXPLV:2
    TIMES SOURCE:1, 1.00
    LOCAL:3 += 75
ELSEIF EXP:V경험 < EXPLV:3
    TIMES SOURCE:1 , 1.10
    LOCAL:3 += 10
ELSEIF EXP:V경험 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    LOCAL:3 += 0
ELSEIF EXP:V경험 < EXPLV:5
    TIMES SOURCE:1 , 1.40
    LOCAL:3 += 0
ELSE
    TIMES SOURCE:1 , 1.60
    LOCAL:3 += 0
ENDIF

;애널 바이브래이터 삽입중을 확인·계산
IF TEQUIP:25
    DOWNBASE:0 += 30
    DOWNBASE:기력 += 80

    ;ABL:A감각을 본다
    IF ABL:A감각 == 0
        LOCAL:2 += 40
        LOCAL:1 += 150
    ELSEIF ABL:A감각 == 1
        LOCAL:2 += 120
        LOCAL:1 += 400
    ELSEIF ABL:A감각 == 2
        LOCAL:2 += 300
        LOCAL:1 += 700
    ELSEIF ABL:A감각 == 3
        LOCAL:2 += 500
        LOCAL:1 += 900
    ELSEIF ABL:A감각 == 4
        LOCAL:2 += 650
        LOCAL:1 += 1000
    ELSE
        LOCAL:2 += 850
        LOCAL:1 += 1200
    ENDIF

    ;EXP:A경험을 본다
    IF EXP:A경험 < EXPLV:1
        TIMES LOCAL:2 , 0.50
        LOCAL:3 += 500
    ELSEIF EXP:A경험 < EXPLV:2
        TIMES LOCAL:2 , 1.00
        LOCAL:3 += 75
    ELSEIF EXP:A경험 < EXPLV:3
        TIMES LOCAL:2 , 1.10
        LOCAL:3 += 10
    ELSEIF EXP:A경험 < EXPLV:4
        TIMES LOCAL:2 , 1.20
        LOCAL:3 += 0
    ELSEIF EXP:A경험 < EXPLV:5
        TIMES LOCAL:2 , 1.40
        LOCAL:3 += 0
    ELSE
        TIMES LOCAL:2 , 1.60
        LOCAL:3 += 0
    ENDIF
    ;PALAM:윤활을 본다
    IF PALAM:10 < PALAMLV:1
        TIMES LOCAL:2 , 0.40
        TIMES SOURCE:1 , 0.40
        LOCAL:3 += 800
    ELSEIF PALAM:10 < PALAMLV:2
        TIMES LOCAL:2 , 0.80
        TIMES SOURCE:1 , 0.80
        LOCAL:3 += 500
    ELSEIF PALAM:10 < PALAMLV:3
        TIMES LOCAL:2 , 1.00
        TIMES SOURCE:1 , 1.00
        LOCAL:3 += 300
    ELSEIF PALAM:10 < PALAMLV:4
        TIMES LOCAL:2 , 1.40
        TIMES SOURCE:1 , 1.40
        LOCAL:3 += 120
    ELSE
        TIMES LOCAL:2 , 1.80
        TIMES SOURCE:1 , 1.80
        LOCAL:3 += 100
    ENDIF

    ;PALAM:욕정을 본다
    IF PALAM:13 < PALAMLV:1
        TIMES LOCAL:2 , 0.80
        TIMES SOURCE:1 , 0.80
    ELSEIF PALAM:13 < PALAMLV:2
        TIMES LOCAL:2 , 0.90
        TIMES SOURCE:1 , 0.90
    ELSEIF PALAM:13 < PALAMLV:3
        TIMES LOCAL:2 , 1.00
        TIMES SOURCE:1 , 1.00
    ELSEIF PALAM:13 < PALAMLV:4
        TIMES LOCAL:2 , 1.10
        TIMES SOURCE:1 , 1.10
    ELSE
        TIMES LOCAL:2 , 1.20
        TIMES SOURCE:1 , 1.20
    ENDIF

    ;ABL:순종을 본다
    IF ABL:순종 == 0
        TIMES LOCAL:1 , 0.80
        TIMES LOCAL:2 , 0.80
        TIMES SOURCE:1 , 0.80
    ELSEIF ABL:순종 == 1
        TIMES LOCAL:1 , 0.90
        TIMES LOCAL:2 , 0.90
        TIMES SOURCE:1 , 0.90
    ELSEIF ABL:순종 == 2
        TIMES LOCAL:1 , 1.00
        TIMES LOCAL:2 , 1.00
        TIMES SOURCE:1 , 1.00
    ELSEIF ABL:순종 == 3
        TIMES LOCAL:1 , 1.10
        TIMES LOCAL:2 , 1.10
        TIMES SOURCE:1 , 1.10
    ELSEIF ABL:순종 == 4
        TIMES LOCAL:1 , 1.20
        TIMES LOCAL:2 , 1.20
        TIMES SOURCE:1 , 1.20
    ELSE
        TIMES LOCAL:1 , 1.30
        TIMES LOCAL:2 , 1.30
        TIMES SOURCE:1 , 1.30
    ENDIF

    ;정조관념
    SIF TALENT:정조관념
        TIMES LOCAL:3, 3.00
    ;[작은체형]
    SIF TALENT:작은체형
        TIMES LOCAL:3 , 2.00
ENDIF

;A민감, 둔감을 본다
IF TALENT:A둔감
    TIMES LOCAL:3 , 1.50
    TIMES LOCAL:1 , 1.50
ELSEIF TALENT:A민감
    TIMES LOCAL:3 , 0.60
    TIMES LOCAL:1 , 0.60
ENDIF

;V민감, 둔감을 본다
IF TALENT:V둔감
    TIMES LOCAL:3 , 1.50
    TIMES LOCAL:1 , 1.50
ELSEIF TALENT:V민감
    TIMES LOCAL:3 , 0.60
    TIMES LOCAL:1 , 0.60
ENDIF

;한 번 단독으로 계산
SOURCE:22 += LOCAL:1

SOURCE:2 += LOCAL:2
SOURCE:20 += LOCAL:3

;ABL:기교를 본다
IF ABL:기교 == 0
    SOURCE:15 = 100
    TIMES SOURCE:2 , 0.30
ELSEIF ABL:기교 == 1
    SOURCE:15 = 160
    TIMES SOURCE:2 , 0.70
ELSEIF ABL:기교 == 2
    SOURCE:15 = 220
    TIMES SOURCE:2 , 1.00
ELSEIF ABL:기교 == 3
    SOURCE:15 = 280
    TIMES SOURCE:2 , 1.20
ELSEIF ABL:기교 == 4
    SOURCE:15 = 340
    TIMES SOURCE:2 , 1.40
ELSE
    SOURCE:15 = 400
    TIMES SOURCE:2 , 1.60
ENDIF

;ABL:자위 중독을 본다
IF ABL:자위중독 == 0
    SOURCE:24 = 0
    TIMES SOURCE:2 , 1.00
ELSEIF ABL:자위중독 == 1
    SOURCE:24 = 150
    TIMES SOURCE:2 , 1.10
ELSEIF ABL:자위중독 == 2
    SOURCE:24 = 330
    TIMES SOURCE:2 , 1.20
ELSEIF ABL:자위중독 == 3
    SOURCE:24 = 850
    TIMES SOURCE:2 , 1.30
ELSEIF ABL:자위중독 == 4
    SOURCE:24 = 1600
    TIMES SOURCE:2 , 1.50
ELSE
    SOURCE:24 = 2700
    TIMES SOURCE:2 , 1.70
ENDIF

;공개 때는, ABL:노출벽을 본다
IF TEQUIP:50
    IF ABL:노출증 == 0
        SOURCE:24 += 0
        TIMES SOURCE:1 , 1.00
        TIMES SOURCE:12 , 1.00
    ELSEIF ABL:노출증 == 1
        SOURCE:24 += 100
        TIMES SOURCE:1 , 1.10
        TIMES SOURCE:12 , 1.20
    ELSEIF ABL:노출증 == 2
        SOURCE:24 += 300
        TIMES SOURCE:1 , 1.20
        TIMES SOURCE:12 , 1.40
    ELSEIF ABL:노출증 == 3
        SOURCE:24 += 800
        TIMES SOURCE:1 , 1.30
        TIMES SOURCE:12 , 1.60
    ELSEIF ABL:노출증 == 4
        SOURCE:24 += 1500
        TIMES SOURCE:1 , 1.50
        TIMES SOURCE:12 , 2.00
    ELSE
        SOURCE:24 += 2500
        TIMES SOURCE:1 , 1.70
        TIMES SOURCE:12 , 3.00
    ENDIF
ENDIF

;비디오 촬영시는 경험에 플러스
IF TEQUIP:50
    TCVAR:1 += 1
    TCVAR:10 += 2
    TCVAR:11 += 2
ELSE
    TCVAR:1 += 1
    TCVAR:10 += 1
    TCVAR:11 += 1
ENDIF
IF TEQUIP:50 && (CFLAG:첫특수커맨드 & 1) == 0
    TCVAR:50 += 1
    CFLAG:첫특수커맨드 |= 1
ENDIF


;촬영시의 조교 내용 체크
SIF TEQUIP:25
    TFLAG:122 = 992

IF TALENT:남자 == 0 && TALENT:PLAYER:남자 == 0
    TCVAR:12 += 3
ELSEIF TALENT:남자 && TALENT:PLAYER:남자
    TCVAR:13 += 3
ENDIF

;노출 쾌락 경험 입수 판정
TFLAG:100 |= 2

RETURN 1

