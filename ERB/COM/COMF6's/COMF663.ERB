CALL TRAIN_MESSAGE_COM663

DOWNBASE:0 += 50
DOWNBASE:기력 += 150

SOURCE:31 = 400
LOCAL:1 = 0
LOCAL:2 = 0
LOCAL:3 = 0

IF TEQUIP:50
