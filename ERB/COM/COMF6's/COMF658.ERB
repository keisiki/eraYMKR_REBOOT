;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 16384) == 0
    RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 1
    RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 2
    RETURN 0
;그 조교 중에 관장 플레이＋배설이 있었을 경우, 조교자가 汚れ無視もしくは汚臭鈍感を持たないなら実行できない
SIF TFLAG:140 & 256 && TALENT:PLAYER:냄새둔감 == 0 && TALENT:PLAYER:불결무시 == 0
    RETURN 0
;[처녀]のときは, 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 4 && TALENT:처녀
    RETURN 0
;조교자가 アリスでないとダメ
SIF NO:PLAYER != 14
    RETURN 0
;조교자의 기교가 4無いとできない
SIF ABL:PLAYER:기교 < 4
    RETURN 0
;대상에게 페니스가 있다면 불가
SIF EXIST_BAR(TARGET)
    RETURN 0
;人形愛撫残り使用可能回数オーバー
SIF TFLAG:141 > 3
    RETURN 0
;바이브･애널바이브を両方持っているか, 아이템なし設定でないとダメ
SIF (ITEM:3 == 0 || ITEM:9 == 0) && NOITEM == 0
    RETURN 0
;縄使用中でないとダメ
SIF TEQUIP:40 == 0
    RETURN 0
;荊 · 거미줄で緊縛中のときはダメ
SIF TEQUIP:40 >= 5 && TEQUIP:40 <= 7
    RETURN 0
;アイマスク装着中でないとダメ
SIF TEQUIP:41 == 0
    RETURN 0
;ボールギャグ装着中でないとダメ
SIF TEQUIP:42 == 0
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
    RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
    RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;훈도시 착용 중일 경우는 안 됨
SIF TEQUIP:59 == 17
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;별명 「고속요등」. 줄＋아이마스크＋볼 개그라고 하는 삼중 긴박 상태로
;인형들이 바이브래이터와 애널 바이브래이터를 동시에 돌진해 피스톤 운동을 펼쳐 가는 커맨드
;……(와)과 마음대로 정의
@COM658
IF FLAG:1100 & 1
    ;技師パッチ有効なら、バイブとアナルバイブの選択
    COM_TOOL_NAME = 바이브
    FLAG:1110 = 3
    CALL NAMECHECK_CUSTOMTOOL
    FLAG:1110 = 9
    LOCALS:0 = %COM_TOOL_NAME%
    LOCALS:1 = %COM_TOOL_CALLNAME%
    COM_TOOL_NAME = 애널바이브
    CALL NAMECHECK_CUSTOMTOOL
    IF COM_TOOL_NAME == "애널바이브" && LOCALS:0 == "바이브"
        LOCALS:2 = 바이브
    ELSE
        LOCALS:2 = 조교기구
    ENDIF
    COM_TOOL_NAME = %LOCALS:1%
    COM_TOOL_NAME = %STR:4%
    COM_TOOL_CALLNAME = 그것들
    TSTR:10 = %LOCALS:2%
ELSE
    COM_TOOL_NAME = 바이브
    COM_TOOL_NAME = 애널바이브
    COM_TOOL_CALLNAME = 바이브
    TSTR:10 = 바이브
ENDIF
PRINTL 구속∇
COM_NAME = 구속∇
CALL TRAIN_MESSAGE_COM658
SELECTCOM = 658

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

DOWNBASE:0 += 90
DOWNBASE:기력 += 180

;ABL:V감각을 본다
IF ABL:V감각 == 0
    SOURCE:1 = 40
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 120
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 300
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 500
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 800
ELSE
    SOURCE:1 = 1500
ENDIF

;ABL:A감각을 본다
IF ABL:A감각 == 0
    SOURCE:2 = 40
    SOURCE:22 = 200
ELSEIF ABL:A감각 == 1
    SOURCE:2 = 120
    SOURCE:22 = 600
ELSEIF ABL:A감각 == 2
    SOURCE:2 = 300
    SOURCE:22 = 800
ELSEIF ABL:A감각 == 3
    SOURCE:2 = 500
    SOURCE:22 = 1200
ELSEIF ABL:A감각 == 4
    SOURCE:2 = 800
    SOURCE:22 = 1400
ELSE
    SOURCE:2 = 1400
    SOURCE:22 = 1800
ENDIF

;EXP:V경험을 본다
IF EXP:V경험 < EXPLV:1
    TIMES SOURCE:1 , 0.20
    SOURCE:20 = 5500
ELSEIF EXP:V경험 < EXPLV:2
    TIMES SOURCE:1 , 0.60
    SOURCE:20 = 300
ELSEIF EXP:V경험 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 = 50
ELSEIF EXP:V경험 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    SOURCE:20 = 10
ELSEIF EXP:V경험 < EXPLV:5
    TIMES SOURCE:1 , 1.40
    SOURCE:20 = 0
ELSE
    TIMES SOURCE:1 , 1.60
    SOURCE:20 = 0
ENDIF

;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
    TIMES SOURCE:2 , 0.50
    SOURCE:20 += 2000
ELSEIF EXP:A경험 < EXPLV:2
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 300
ELSEIF EXP:A경험 < EXPLV:3
    TIMES SOURCE:2 , 1.10
    SOURCE:20 += 50
ELSEIF EXP:A경험 < EXPLV:4
    TIMES SOURCE:2 , 1.20
    SOURCE:20 += 10
ELSEIF EXP:A경험 < EXPLV:5
    TIMES SOURCE:2 , 1.40
    SOURCE:20 += 0
ELSE
    TIMES SOURCE:2 , 1.60
    SOURCE:20 += 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.10
    TIMES SOURCE:2 , 0.40
    SOURCE:20 += 1800
    TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.40
    TIMES SOURCE:2 , 0.80
    SOURCE:20 += 900
    TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 300
    TIMES SOURCE:20 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:2 , 1.40
    SOURCE:20 += 120
    TIMES SOURCE:20 , 0.20
ELSE
    TIMES SOURCE:1 , 1.80
    TIMES SOURCE:2 , 1.80
    SOURCE:20 += 100
    TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:2 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.90
    TIMES SOURCE:2 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.10
    TIMES SOURCE:2 , 1.10
ELSE
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:2 , 0.80
ELSEIF ABL:순종 == 1
    TIMES SOURCE:1 , 0.90
    TIMES SOURCE:2 , 0.90
ELSEIF ABL:순종 == 2
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:1 , 1.10
    TIMES SOURCE:2 , 1.10
ELSEIF ABL:순종 == 4
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.20
ELSE
    TIMES SOURCE:1 , 1.30
    TIMES SOURCE:2 , 1.30
ENDIF

;[작은체형]
IF TALENT:128
    TIMES SOURCE:20 , 2.00
;[소인]
ELSEIF TALENT:127
    TIMES SOURCE:20 , 3.50
ENDIF
;새드이나 왕S
IF TALENT:PLAYER:새드 || TALENT:PLAYER:초S
    TIMES SOURCE:1 , 2.50
    TIMES SOURCE:2 , 2.50
    TIMES SOURCE:20 , 1.50
ENDIF

IF FLAG:1100 & 1
    ;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
    ;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
    ;バイブの分
    FLAG:1110 = 3
    FLAG:1111 = 0
    FLAG:1112 = SOURCE:1
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:1 = FLAG:1120
    ;アナルバイブの分
    FLAG:1110 = 9
    FLAG:1111 = 0
    FLAG:1112 = SOURCE:2
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:2 = FLAG:1120
ENDIF

TFLAG:141 += 2
TEQUIP:20 = 1
TEQUIP:25 = 1
TCVAR:0 += 2
TCVAR:1 += 2
IF TALENT:처녀 && (CFLAG:21 & 256) == 0
    IF ASSIPLAY && TALENT:PLAYER:남자 == 0
        LOCAL = 2
    ELSE
        LOCAL = 1
    ENDIF
    TCVAR:50 += LOCAL
    CFLAG:21 |= 256
ENDIF
RETURN 1

