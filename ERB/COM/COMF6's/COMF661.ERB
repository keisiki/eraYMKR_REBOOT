;=============================================================================
;꼬리 삽입
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE661
;개별 설정 커멘드 필터
SIF DITEMTYPE:999:661 > 0
    RETURN 0
;동물귀 이외 불가
SIF TALENT:동물귀 == 0
    RETURN 0
;V계 확장 커멘드를 유효하게 하지 않으면 안 됨
SIF (FLAG:커맨드확장 & 2) == 0
    RETURN 0
;V조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:VA이상조교필터 & 1
    RETURN 0
;대상의 V확장 경험이 0 때 이상계 조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:VA이상조교필터 & 4 && EXP:V확장경험 == 0
    RETURN 0
;조수 조교 혹은 순애 모드가 무효의 경우는, V조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:VA이상조교필터 & 1 && ((FLAG:SHOP이벤트확장 & 32) == 0 || ASSIPLAY)
    RETURN 0
;남자라면 안 됨
SIF TALENT:남자
    RETURN 0
;윤활 부족해, 순종 4 미만·레즈비언기분 4 미만의 ASSI의 경우, 조교 대상이 동성이라면 실행할 수 없다(새디즘이나 새디즘기분 Lv3 이상, 또는 망신이나 복종이 있다면 OK)
IF PALAM:10 < PALAMLV:2 && ASSIPLAY
    SIF (ABL:ASSI:순종 < 4 || (TALENT:남자 == 0 && TALENT:ASSI:남자 == 0 && ABL:ASSI:레즈끼 < 4)) && TALENT:ASSI:새드 == 0 && ABL:ASSI:새드끼 < 3 && TALENT:ASSI:망신 == 0 && TALENT:ASSI:복종 == 0
        RETURN 0
ENDIF
;승마중은 안 됨
SIF TEQUIP:43
    RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕중은 안 됨
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
    RETURN 0
;옴트프레이중은 안 됨
SIF TEQUIP:58
    RETURN 0
;곤착용중의 경우 조교자의 기교 4이상 필수
SIF TEQUIP:59 == 17 && ABL:PLAYER:기교 < 4
    RETURN 0
;촉수 조교중은 안 됨
SIF TEQUIP:90
    RETURN 0
;V계 장착도구 사용중은 불가
SIF TEQUIP:20 || TEQUIP:21
    RETURN 0
;A계 장착도구 사용중의 경우 조교자의 기교 4이상 필수
SIF (TEQUIP:25 || TEQUIP:26) && ABL:PLAYER:기교 < 4
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM661
PRINTL 꼬리 삽입
COM_NAME = 꼬리 삽입
CALL TRAIN_MESSAGE_COM661

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

DOWNBASE:0 += 50
DOWNBASE:기력 += 100

SOURCE:12 = 300
SOURCE:15 = 300

;ABL:V감각을 본다
IF ABL:V감각 == 0
    SOURCE:1 = 1
    SOURCE:20 += 2500
    SOURCE:22 = 175
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 10
    SOURCE:20 += 1000
    SOURCE:22 = 250
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 300
    SOURCE:20 += 600
    SOURCE:22 = 450
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 650
    SOURCE:20 += 300
    SOURCE:22 = 750
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 1500
    SOURCE:22 = 1200
ELSE
    SOURCE:1 = 2000
    SOURCE:22 = 1800
ENDIF

;EXP:V경험을 본다
IF EXP:V경험 < EXPLV:1
    TIMES SOURCE:1 , 0.10
    TIMES SOURCE:20 , 2.00
    TIMES SOURCE:22 , 0.20
    SOURCE:20 = 300
ELSEIF EXP:V경험 < EXPLV:2
    TIMES SOURCE:1 , 0.30
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:22 , 0.50
    SOURCE:20 = 180
ELSEIF EXP:V경험 < EXPLV:3
    TIMES SOURCE:1 , 0.90
    TIMES SOURCE:20 , 1.10
    TIMES SOURCE:22 , 0.80
    SOURCE:20 = 80
ELSEIF EXP:V경험 < EXPLV:4
    TIMES SOURCE:1 , 1.10
    TIMES SOURCE:20 , 1.00
    TIMES SOURCE:22 , 1.00
    SOURCE:20 = 30
ELSEIF EXP:V경험 < EXPLV:5
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:20 , 0.80
    TIMES SOURCE:22 , 1.20
    SOURCE:20 = 0
ELSE
    TIMES SOURCE:1 , 1.90
    TIMES SOURCE:20 , 0.50
    TIMES SOURCE:22 , 1.50
    SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.10
    SOURCE:20 += 2500
    TIMES SOURCE:20 , 5.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.20
    SOURCE:20 += 1000
    TIMES SOURCE:20 , 1.50
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:20 , 0.75
ELSE
    TIMES SOURCE:1 , 2.00
    TIMES SOURCE:20 , 0.20
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.20
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.50
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.70
ELSE
    TIMES SOURCE:1 , 2.00
ENDIF

;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:V둔감
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:22 , 1.50
ELSEIF TALENT:V민감
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:22 , 0.60
ENDIF

;처녀로 정조관념
SIF TALENT:처녀 == 1 && TALENT:정조관념
    TIMES SOURCE:22 , 2.00

TCVAR:0 += 1
RETURN 1
;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM661
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:지문소거판정 == 1 || RESULT != 0
    RETURN 0
PRINTL
PRINTFORM %CALLNAME:TARGET%의 
IF TALENT:처녀 == 1
    PRINT 남자를 모르는 
ELSEIF PALAM:10 >= PALAMLV:4
    PRINT 촉촉히 젖은 
ENDIF
IF TALENT:작은체형
    PRINT 어린 균열에 
ELSE
    PRINT 보지에 
ENDIF
IF TEQUIP:20 == 3
    PRINTFORMW 진동소리를 내는 로터를 단 %CALLNAME:TARGET%의 꼬리를 천천히 삽입했다….
ELSEIF TEQUIP:21
    PRINTFORMW 쿠스코로 활짝 열려있는 보지에 %CALLNAME:TARGET%의 꼬리를 천천히 삽입했다….
ELSE
    PRINTFORMW %CALLNAME:TARGET%의 꼬리를 천천히 삽입했다….
ENDIF
