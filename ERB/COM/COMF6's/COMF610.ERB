    ENDIF
ELSEIF TFLAG:130 == 3
    ;노예의 입⇔주인의 P의 불결이 이동
    IF EXIST_PENIS(MASTER)
        STAIN:입 |= STAIN:MASTER:자지
        STAIN:MASTER:자지 |= STAIN:입
    ENDIF
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 10
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 10
ENDIF
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

;처녀 상실의 후에 페니스를 뽑고 있을까
TFLAG:101 = (TFLAG:101 == 2) ?  1 # 0
RETURN 1

