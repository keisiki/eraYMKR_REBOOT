        STAIN:ASSI:자지 |= STAIN:보지
    ENDIF
ELSEIF TFLAG:131 == 2
    ;노예의 A⇔조수의 P의 불결이 이동
    IF EXIST_PENIS(ASSI)
        STAIN:애널 |= STAIN:ASSI:자지
        STAIN:ASSI:자지 |= STAIN:애널
    ENDIF
ELSEIF TFLAG:131 == 3
    ;노예의 입⇔조수의 P의 불결이 이동
    IF EXIST_PENIS(ASSI)
        STAIN:입 |= STAIN:ASSI:자지
        STAIN:ASSI:자지 |= STAIN:입
    ENDIF
ENDIF

IF TFLAG:130 == 1
    ;노예의 V⇔주인의 P의 불결이 이동
    IF EXIST_PENIS(MASTER)
