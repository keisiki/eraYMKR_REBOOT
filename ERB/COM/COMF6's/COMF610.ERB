            CFLAG:173 += 1
    ENDIF
ENDIF

;-------------------------------------------------
;사정 체크 1(주인)
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(MASTER)

;사정시의 처리
IF TFLAG:91 >= 1
    ;커맨드 마다 고유의 처리는 이 아래에 쓴다

    ;사정처 타입의 설정
    IF TFLAG:130 == 1
        TFLAG:703 = 1
    ELSEIF TFLAG:130 == 2
        TFLAG:703 = 2
    ELSE
        TFLAG:703 = 4
    ENDIF
    ;사정처 선택의 가능·불가
    TFLAG:704 = 0
    ;사정자 판정용의 임시 변수
    TFLAG:93 = MASTER
    ;범용적인 처리는 이 함수로 실시한다
    ;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
    CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;사정 체크 2(조수)
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(ASSI)

;사정시의 처리
IF TFLAG:91 >= 1
    ;커맨드 마다 고유의 처리는 이 아래에 쓴다

    ;사정처 타입의 설정
    TFLAG:703 = 12
    ;사정처 선택의 가능·불가
    TFLAG:704 = 0
    ;사정자 판정용의 임시 변수
    TFLAG:93 = ASSI
    ;범용적인 처리는 이 함수로 실시한다
    ;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
    CALL SAMEN_SHOOT
