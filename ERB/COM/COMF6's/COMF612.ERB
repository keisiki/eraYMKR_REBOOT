            PRINTL [1] - 자궁구
            $INPUT_LOOP
            INPUT
            IF RESULT == 0
                JUMP COM614
            ELSEIF RESULT == 1
                JUMP COM615
            ELSE
                GOTO INPUT_LOOP
            ENDIF
        ELSE
            ;PALAM:욕정을 본다
            IF PALAM:13 < PALAMLV:1
                LOCAL:1 = 1
            ELSEIF PALAM:13 < PALAMLV:2
                LOCAL:1 = 2
            ELSEIF PALAM:13 < PALAMLV:3
                LOCAL:1 = 3
            ELSEIF PALAM:13 < PALAMLV:4
                LOCAL:1 = 4
            ELSE
                LOCAL:1 = 5
            ENDIF
