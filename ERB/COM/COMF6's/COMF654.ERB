;더미 함수(커맨드를 직접 선택할 수는 없음)
@COM_ABLE654
RETURN 0

@EXCOM_ABLE654
;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 16384) == 0
    RETURN 0
;ヤマメのスカートの中は蜘蛛の脚という設定でないとダメ
SIF (FLAG:커맨드확장 & 65536) == 0
    RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 1
    RETURN 0
;V경험レベルが0かつ조교자가 オトコではない場合, 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 4 && EXP:V경험 < EXPLV:1 && TALENT:PLAYER:120 == 0
    RETURN 0
;조교자가 ヤマメでないとダメ
SIF NO:PLAYER != 58
    RETURN 0
;조교 대상이 [소인]일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:127 && TALENT:MASTER:56 == 0
    RETURN 0
;脚の妨げとなる도구の装着はダメ
;Ｖ계 장착구 사용 중에는 불가
SIF TEQUIP:20 || TEQUIP:21
    RETURN 0
;Ａ계 장착구 사용 중에는 불가
SIF TEQUIP:25 || TEQUIP:26
    RETURN 0
;Ｂ계 장착구 사용 중에는 불가
SIF TEQUIP:35 || TEQUIP:36
    RETURN 0
;Ｃ계 장착구 사용 중에는 불가
SIF TEQUIP:30
    RETURN 0
;グルグル巻きにしないとダメ
SIF TEQUIP:40 != 6 && TEQUIP:40 != 7
    RETURN 0
;ボールギャグ装着中はダメ
SIF TEQUIP:42
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;욕실 플레이 중에는 불가
SIF TEQUIP:53
    RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;조수가 코스프레 중에는 불가
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 > 1
    RETURN 0
;알몸 앞치마, 학교수영복 플레이, 코스프레 중에는 불가
SIF TEQUIP:59
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;스커트안에 거미의 다리를 가르치고 있는 ver. 의 야마메궠응
;많이 있는 다리를 풀로 활용시켜, 전신의 구멍에 돌진하게 합니다
;처녀 상실을 수반하므로 주의
@COM654
PRINTL 거미다리6점고문
COM_NAME = 거미다리6점고문
SELECTCOM = 654
CALL TRAIN_MESSAGE_COM654

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

DOWNBASE:0 += 250
DOWNBASE:기력 += 700

SOURCE:11 = 90
SOURCE:12 = 700
SOURCE:17 = 500
SOURCE:18 = 60
SOURCE:20 = 200
SOURCE:30 = 520
SOURCE:31 = 400

;ABL:C감각을 본다
IF ABL:C감각 == 0
    SOURCE:0 = 200
ELSEIF ABL:C감각 == 1
    SOURCE:0 = 400
ELSEIF ABL:C감각 == 2
    SOURCE:0 = 900
ELSEIF ABL:C감각 == 3
    SOURCE:0 = 1600
ELSEIF ABL:C감각 == 4
    SOURCE:0 = 2400
ELSE
    SOURCE:0 = 3000
ENDIF
;ABL:V감각을 본다
IF ABL:V감각 == 0
    SOURCE:1 = 40
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 150
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 400
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 1000
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 1700
ELSE
    SOURCE:1 = 2200
ENDIF
;ABL:A감각을 본다
IF ABL:A감각 == 0
    SOURCE:2 = 10
    SOURCE:22 = 1500
ELSEIF ABL:A감각 == 1
    SOURCE:2 = 30
    SOURCE:22 = 2200
ELSEIF ABL:A감각 == 2
    SOURCE:2 = 500
    SOURCE:22 = 3000
ELSEIF ABL:A감각 == 3
    SOURCE:2 = 1000
    SOURCE:22 = 4500
ELSEIF ABL:A감각 == 4
    SOURCE:2 = 1700
    SOURCE:22 = 6000
ELSE
    SOURCE:2 = 2200
    SOURCE:22 = 9000
ENDIF
;ABL:B감각을 본다
IF ABL:B감각 == 0
    SOURCE:3 = 20
ELSEIF ABL:B감각 == 1
    SOURCE:3 = 100
ELSEIF ABL:B감각 == 2
    SOURCE:3 = 500
ELSEIF ABL:B감각 == 3
    SOURCE:3 = 1200
ELSEIF ABL:B감각 == 4
    SOURCE:3 = 2000
ELSE
    SOURCE:3 = 2800
ENDIF

;EXP:V경험을 본다
IF EXP:V경험 < EXPLV:1
    TIMES SOURCE:1 , 0.20
    SOURCE:20 += 5000
    SIF ASSIPLAY && TALENT:PLAYER:120 == 0
        TCVAR:50 += 1
ELSEIF EXP:V경험 < EXPLV:2
    TIMES SOURCE:1 , 0.60
    SOURCE:20 += 220
ELSEIF EXP:V경험 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 += 30
ELSEIF EXP:V경험 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    SOURCE:20 += 5
ELSEIF EXP:V경험 < EXPLV:5
    TIMES SOURCE:1 , 1.30
    SOURCE:20 += 0
ELSE
    TIMES SOURCE:1 , 1.80
    SOURCE:20 += 0
ENDIF
;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
    TIMES SOURCE:2 , 0.10
    SOURCE:20 += 20000
ELSEIF EXP:A경험 < EXPLV:2
    TIMES SOURCE:2 , 0.30
    SOURCE:20 += 12000
ELSEIF EXP:A경험 < EXPLV:3
    TIMES SOURCE:2 , 0.50
    SOURCE:20 += 5000
ELSEIF EXP:A경험 < EXPLV:4
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 1800
ELSEIF EXP:A경험 < EXPLV:5
    TIMES SOURCE:2 , 1.40
    SOURCE:20 += 1000
ELSE
    TIMES SOURCE:2 , 1.60
    SOURCE:20 += 600
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.10
    TIMES SOURCE:2 , 0.40
    SOURCE:20 += 10000
    TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.40
    TIMES SOURCE:2 , 0.80
    SOURCE:20 += 3500
    TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    SOURCE:20 += 1200
    TIMES SOURCE:20 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:2 , 1.40
    SOURCE:20 += 200
    TIMES SOURCE:20 , 0.20
ELSE
    TIMES SOURCE:1 , 1.80
    TIMES SOURCE:2 , 1.80
    SOURCE:20 += 100
    TIMES SOURCE:20 , 0.10
ENDIF
;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:2 , 0.60
    TIMES SOURCE:11 , 0.30
    TIMES SOURCE:22 , 0.60
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:2 , 0.80
    TIMES SOURCE:11 , 0.60
    TIMES SOURCE:22 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:11 , 1.00
    TIMES SOURCE:22 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:2 , 1.20
    TIMES SOURCE:11 , 1.50
    TIMES SOURCE:22 , 1.20
ELSE
    TIMES SOURCE:1 , 1.50
    TIMES SOURCE:2 , 1.40
    TIMES SOURCE:11 , 1.80
    TIMES SOURCE:22 , 1.40
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:1 , 0.50
    TIMES SOURCE:11 , 0.60
ELSEIF ABL:순종 == 1
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:11 , 0.80
ELSEIF ABL:순종 == 2
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:11 , 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:1 , 1.30
    TIMES SOURCE:11 , 1.20
ELSEIF ABL:순종 == 4
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:11 , 1.40
ELSE
    TIMES SOURCE:1 , 2.00
    TIMES SOURCE:11 , 1.60
ENDIF

;ABL:봉사 정신을 본다
IF ABL:봉사정신 == 0
    SOURCE:15 = 420
    SOURCE:16 = 150
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:봉사정신 == 1
    SOURCE:15 = 500
    SOURCE:16 = 300
    TIMES SOURCE:30 , 2.50
ELSEIF ABL:봉사정신 == 2
    SOURCE:15 = 580
    SOURCE:16 = 600
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:봉사정신 == 3
    SOURCE:15 = 660
    SOURCE:16 = 900
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:봉사정신 == 4
    SOURCE:15 = 740
    SOURCE:16 = 1500
    TIMES SOURCE:30 , 0.50
ELSE
    SOURCE:15 = 820
    SOURCE:16 = 2200
    TIMES SOURCE:30 , 0.10
ENDIF

;정조관념
IF TALENT:30
    IF TALENT:처녀 == 1
        TIMES SOURCE:11 , 0.60
        SOURCE:32 = 10000
        SOURCE:22 /= 3
    ELSE
        TIMES SOURCE:11 , 0.60
        SOURCE:32 = 1000
    ENDIF
;정조무구애
ELSEIF TALENT:31
    IF TALENT:처녀 == 1
        TIMES SOURCE:11 , 0.60
        SOURCE:32 = 300
    ENDIF
ELSE
    SIF TALENT:처녀 == 1
        SOURCE:32 = 3000
ENDIF
;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:103
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:22 , 1.50
ELSEIF TALENT:102
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:22 , 0.60
ENDIF
;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:22 , 0.60
    TIMES SOURCE:31 , 0.60
ENDIF
;[작은체형]
SIF TALENT:128
    TIMES SOURCE:20 , 2.00

;ABL:순종을 본다(반감 추가의 수정)
IF ABL:순종 == 0
    TIMES SOURCE:32 , 2.00
ELSEIF ABL:순종 == 1
    TIMES SOURCE:32 , 1.50
ELSEIF ABL:순종 == 2
    TIMES SOURCE:32 , 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:32 , 0.80
ELSEIF ABL:순종 == 4
    TIMES SOURCE:32 , 0.60
ELSE
    TIMES SOURCE:32 , 0.30
ENDIF

TCVAR:0 += 1
TCVAR:1 += 5

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 2
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 2
ENDIF
;주인 경험 플래그
SIF ASSIPLAY == 0 && ABL:마조끼 >= 2
    TFLAG:50 += 1

LOCAL:1 = 46 + RAND:10
TFLAG:142 -= LOCAL:1
SIF TFLAG:142 <= 0
    CALL SPIDER_SILK_TEAR

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM654
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
PRINTFORM %CALLNAME:PLAYER%의 무수한 다리가 %CALLNAME:TARGET%의 
SIF PALAM:10 >= PALAMLV:5
    PRINT 잔뜩 
SIF PALAM:10 >= PALAMLV:3
    PRINT 점액으로 끈적거리는 
PRINTL 온갖 구멍 안으로 기어들어 비벼댔다….
PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 
IF TALENT:처녀 && TALENT:170 && ABL:레즈끼 > 0 && ASSIPLAY == 0
    PRINTFORMW 자신이 처녀란 것을 잊은 듯이, %CALLNAME:PLAYER%의 다리가 질에 들어가는 감촉에 행복감을 느끼고 있는 것 같다….
ELSEIF TALENT:처녀 && TALENT:170
    PRINTW 자신이 처녀란 것을 잊은 듯이, 앞뒤의 구멍과 입을 동시에 유린당하는 쾌감을 즐기고 있다….
ELSEIF TALENT:처녀
    PRINTFORMW 파과의 아픔에 절규를 지르고 있었지만, 곧바로 입에도 %CALLNAME:MASTER%의 다리가 들어와 무력하게 유린당하고 있다….
ELSEIF (TALENT:150 || TALENT:160) && TALENT:170
    PRINTW 처음엔 약간 곤혹스런 모습이었지만, 양쪽 유두에 쾌감이 주어진 순간부터 모두 잊어버리고 교성을 질러대고 있다….
ELSEIF TALENT:150
    PRINTFORMW 무엇인가 호소하는 것 같이 입을 열었지만 %CALLNAME:PLAYER%의 다리가 가차 없이 그곳을 막아버렸다….
ELSEIF (TALENT:160 || TALENT:170)
    PRINTFORMW 몸 전체로 %CALLNAME:PLAYER%의 다리로부터 주어지는 쾌감을 탐내듯이 몇 번이고 흔들어대고 있다….
ELSE
    PRINTW 더러운 구멍을 서서히 관통하는 다리의 감촉에 비명을 지르려고 했지만, 절묘한 타이밍에 음핵을 집혀 소리를 지르지도 못한 채 입을 헤 벌렸다….
ENDIF
