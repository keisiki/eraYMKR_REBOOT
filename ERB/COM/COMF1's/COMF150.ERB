@COM_ABLE150
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:150 > 0
    RETURN 0
;Ｖ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 2) == 0
    RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:VA이상조교필터 & 1
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;훈도시 착용 중일 경우는 안 됨
SIF TEQUIP:59 == 17
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
;해제는 언제든 가능
SIF TEQUIP:21
    RETURN 1
;아이템을 가지고 있거나, 아이템 없음 설정이 아니면 불가
SIF ITEM:8 == 0 && NOITEM == 0
    RETURN 0
;처녀면 불가
SIF TALENT:처녀 == 1
    RETURN 0
;남자라면 안 됨
SIF TALENT:120
    RETURN 0
;조교 대상이 [소인]일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:127 && TALENT:MASTER:금단의지식 == 0
    RETURN 0
;윤활 부족이고, 순종4미만 · 레즈끼4미만의 ASSI일 경우, 조교 대상이 동성이라면 실행할 수 없다(새드거나 새드끼Lv3이상, 또는 망신이나 복종이 있다면 ＯＫ)
IF PALAM:10 < PALAMLV:2 && ASSIPLAY
    SIF (ABL:ASSI:순종 < 4 || (TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:레즈끼 < 4)) && TALENT:ASSI:81 == 0 && ABL:ASSI:새드끼 < 3 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
        RETURN 0
ENDIF
;그밖의 Ｖ계 장착구 사용 중에는 불가
SIF TEQUIP:20
    RETURN 0
;한증탕과 암반욕 외의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
    RETURN 0
;학교수영복 플레이 중에는 불가
SIF TEQUIP:59 == 2
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM150
COM_TOOL_NAME = 쿠스코
IF FLAG:1100 & 1
    ;技師パッチ有効なら特殊な名前チェック
    FLAG:1110 = 8
    CALL NAMECHECK_CUSTOMTOOL
ELSE
    COM_TOOL_CALLNAME = %COM_TOOL_NAME%
ENDIF
PRINTFORML %COM_TOOL_NAME%
COM_NAME = 쿠스코
CALL TRAIN_MESSAGE_COM150

DOWNBASE:0 += 50
DOWNBASE:기력 += 100

;ABL:V감각을 본다
IF ABL:V감각 == 0
    SOURCE:1 = 40
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 130
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 300
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 500
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 800
ELSE
    SOURCE:1 = 1200
ENDIF

;ABL:노출벽을 본다
IF ABL:노출증 == 0
    SOURCE:12 = 300
    SOURCE:19 = 0
    SOURCE:22 = 600
    SOURCE:24 = 0
    SOURCE:31 = 800
ELSEIF ABL:노출증 == 1
    SOURCE:12 = 500
    SOURCE:19 = 150
    SOURCE:22 = 500
    SOURCE:24 = 150
    SOURCE:31 = 200
ELSEIF ABL:노출증 == 2
    SOURCE:12 = 500
    SOURCE:19 = 800
    SOURCE:22 = 400
    SOURCE:24 = 300
ELSEIF ABL:노출증 == 3
    SOURCE:12 = 500
    SOURCE:19 = 600
    SOURCE:22 = 300
    SOURCE:24 = 600
ELSEIF ABL:노출증 == 4
    SOURCE:12 = 1000
    SOURCE:19 = 1000
    SOURCE:22 = 200
    SOURCE:24 = 1000
ELSE
    SOURCE:12 = 1300
    SOURCE:19 = 1800
    SOURCE:22 = 100
    SOURCE:24 = 1800
ENDIF

;ABL:마조끼를 본다
IF ABL:마조끼 == 0
    TIMES SOURCE:12 , 0.80
ELSEIF ABL:마조끼 == 1
    TIMES SOURCE:12 , 1.10
ELSEIF ABL:마조끼 == 2
    TIMES SOURCE:12 , 1.30
ELSEIF ABL:마조끼 == 3
    TIMES SOURCE:12 , 1.50
ELSEIF ABL:마조끼 == 4
    TIMES SOURCE:12 , 1.70
ELSE
    TIMES SOURCE:12 , 2.10
ENDIF

;EXP:V경험을 본다
IF EXP:V경험 < EXPLV:1
    TIMES SOURCE:1 , 0.20
    SOURCE:20 = 6500
ELSEIF EXP:V경험 < EXPLV:2
    TIMES SOURCE:1 , 0.60
    SOURCE:20 = 2000
ELSEIF EXP:V경험 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 = 500
ELSEIF EXP:V경험 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    SOURCE:20 = 100
ELSEIF EXP:V경험 < EXPLV:5
    TIMES SOURCE:1 , 1.40
    SOURCE:20 = 10
ELSE
    TIMES SOURCE:1 , 1.60
    SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.10
    SOURCE:20 += 1000
    TIMES SOURCE:20 , 4.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.40
    SOURCE:20 += 400
    TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:20 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:20 , 0.20
ELSE
    TIMES SOURCE:1 , 1.50
    TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.10
ELSE
    TIMES SOURCE:1 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:1 , 0.80
ELSEIF ABL:순종 == 1
    TIMES SOURCE:1 , 0.90
ELSEIF ABL:순종 == 2
    TIMES SOURCE:1 , 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:1 , 1.10
ELSEIF ABL:순종 == 4
    TIMES SOURCE:1 , 1.20
ELSE
    TIMES SOURCE:1 , 1.30
ENDIF

;비디오 카메라
SIF TEQUIP:50
    TIMES SOURCE:12 , 1.20
;수치 플레이
SIF TEQUIP:56
    TIMES SOURCE:12 , 1.20

;정조관념
SIF TALENT:30
    TIMES SOURCE:31 , 4.00
;도착적
IF TALENT:80
    TIMES SOURCE:12 , 1.50
    TIMES SOURCE:31 , 1.20
ENDIF
;[작은체형]
SIF TALENT:128
    TIMES SOURCE:20 , 2.50

IF FLAG:1100 & 1
    ;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
    ;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
    FLAG:1110 = 8
    FLAG:1111 = 1
    FLAG:1112 = SOURCE:1
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:1 = FLAG:1120
    FLAG:1112 = SOURCE:12
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:12 = FLAG:1120
    FLAG:1112 = SOURCE:19
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:19 = FLAG:1120
    FLAG:1112 = SOURCE:22
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:22 = FLAG:1120
    FLAG:1112 = SOURCE:24
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:24 = FLAG:1120
    FLAG:1111 = 3
    FLAG:1112 = SOURCE:20
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:20 = FLAG:1120
ENDIF

;레이센이나 에이린에 의한 사용
IF ASSI >= 0 && ASSIPLAY
    SIF NO:ASSI == 28
        TIMES SOURCE:20 , 0.70
    SIF NO:ASSI == 29
        TIMES SOURCE:20 , 0.50
ENDIF

TCVAR:0 += 3

;커맨드 속성：도구(조교자)
TFLAG:98 = 1

;쿠스코의 착탈
IF TEQUIP:21
    TEQUIP:21 = 0
ELSE
    TEQUIP:21 = 1
ENDIF
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;--------------------------------------------------
;쿠스코 삽입중
;--------------------------------------------------
@EQUIP_COM150
COM_TOOL_NAME = 쿠스코
IF FLAG:1100 & 1
    ;技師パッチ有効なら特殊な名前チェック
    FLAG:1110 = 8
    CALL NAMECHECK_CUSTOMTOOL
ENDIF
PRINTFORML <%COM_TOOL_NAME% 삽입 중>

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

DOWNBASE:0 += 10
DOWNBASE:기력 += 50

;ABL:V감각을 본다
IF ABL:V감각 == 0
    LOCAL = 0
ELSEIF ABL:V감각 == 1
    LOCAL = 50
ELSEIF ABL:V감각 == 2
    LOCAL = 100
ELSEIF ABL:V감각 == 3
    LOCAL = 150
ELSEIF ABL:V감각 == 4
    LOCAL = 200
ELSE
    LOCAL = 300
ENDIF

;ABL:노출벽을 본다
IF ABL:노출증 == 0
    LOCAL:1 = 300
    LOCAL:3 = 1000
    SOURCE:22 += 600
ELSEIF ABL:노출증 == 1
    LOCAL:1 = 500
    LOCAL:3 = 500
    SOURCE:22 += 500
ELSEIF ABL:노출증 == 2
    LOCAL:1 = 500
    LOCAL:3 = 150
    SOURCE:22 += 400
    SOURCE:24 += 300
ELSEIF ABL:노출증 == 3
    LOCAL:1 = 500
    LOCAL:3 = 50
    SOURCE:22 += 300
    SOURCE:24 += 600
ELSEIF ABL:노출증 == 4
    LOCAL:1 = 1000
    LOCAL:3 = 0
    SOURCE:22 += 200
    SOURCE:24 += 1000
ELSE
    LOCAL:1 = 1300
    LOCAL:3 = 0
    SOURCE:22 += 100
    SOURCE:24 += 1800
ENDIF

;ABL:마조끼를 본다
IF ABL:마조끼 == 0
    TIMES LOCAL:1 , 0.00
ELSEIF ABL:마조끼 == 1
    TIMES LOCAL:1 , 1.10
ELSEIF ABL:마조끼 == 2
    TIMES LOCAL:1 , 1.30
ELSEIF ABL:마조끼 == 3
    TIMES LOCAL:1 , 1.50
ELSEIF ABL:마조끼 == 4
    TIMES LOCAL:1 , 1.70
ELSE
    TIMES LOCAL:1 , 2.10
ENDIF

;EXP:V경험을 본다
;처녀는 있을 수 없다
IF EXP:V경험 < EXPLV:2
    TIMES LOCAL , 0.60
    LOCAL:2 = 150
ELSEIF EXP:V경험 < EXPLV:3
    TIMES LOCAL , 1.00
    LOCAL:2 = 20
ELSEIF EXP:V경험 < EXPLV:4
    TIMES LOCAL , 1.20
    LOCAL:2 = 0
ELSEIF EXP:V경험 < EXPLV:5
    TIMES LOCAL , 1.40
    LOCAL:2 = 0
ELSE
    TIMES LOCAL , 1.60
    LOCAL:2 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES LOCAL , 0.10
    LOCAL:2 += 400
    TIMES LOCAL:2 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES LOCAL , 0.40
    LOCAL:2 += 150
    TIMES LOCAL:2 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES LOCAL , 1.00
    TIMES LOCAL:2 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
    TIMES LOCAL , 1.40
    TIMES LOCAL:2 , 0.20
ELSE
    TIMES LOCAL , 1.80
    TIMES LOCAL:2 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES LOCAL , 1.10
ELSE
    TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES LOCAL , 0.80
ELSEIF ABL:순종 == 1
    TIMES LOCAL , 0.90
ELSEIF ABL:순종 == 2
    TIMES LOCAL , 1.00
ELSEIF ABL:순종 == 3
    TIMES LOCAL , 1.10
ELSEIF ABL:순종 == 4
    TIMES LOCAL , 1.20
ELSE
    TIMES LOCAL , 1.30
ENDIF

;프라이드 높다
SIF TALENT:16
    TIMES LOCAL:3 , 1.20
;무관심
IF TALENT:21
    TIMES LOCAL:1 , 0.80
    TIMES LOCAL:3 , 0.80
ENDIF
;정조관념
SIF TALENT:30
    TIMES LOCAL:3 , 2.00
;도착적
IF TALENT:80
    TIMES LOCAL:1 , 1.50
    TIMES LOCAL:3 , 1.20
ENDIF
;[작은체형]
SIF TALENT:128
    TIMES LOCAL:2 , 1.50

IF FLAG:1100 & 1
    ;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
    ;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
    FLAG:1110 = 8
    FLAG:1111 = 1
    FLAG:1112 = LOCAL
    CALL SOURCECHECK_CUSTOMTOOL
    LOCAL = FLAG:1120
    FLAG:1112 = LOCAL:1
    CALL SOURCECHECK_CUSTOMTOOL
    LOCAL:1 = FLAG:1120
    FLAG:1111 = 3
    FLAG:1112 = LOCAL:2
    CALL SOURCECHECK_CUSTOMTOOL
    LOCAL:2 = FLAG:1120
ENDIF

;비디오 카메라
SIF TEQUIP:50
    TIMES LOCAL:1 , 1.20
;수치 플레이
SIF TEQUIP:56
    TIMES LOCAL:1 , 1.20

SOURCE:1 += LOCAL
SOURCE:12 += LOCAL:1
SOURCE:20 += LOCAL:2
SOURCE:31 += LOCAL:3

TCVAR:0 += 2

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 1
ENDIF

;노출 쾌락 경험 입수 판정
TFLAG:100 |= 2

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM150
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF TEQUIP:21
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 질로부터 %조사선택(COM_TOOL_NAME, "를")% 뽑아 냈다….
    IF EXP:V경험 >= 200 && TALENT:150
        PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 넓혀진 채로 대량의 점액으로 끈적해진 보지를 황홀한 표정으로 응시하고 
    ELSEIF EXP:V경험 >= 71
        PRINTFORM %CALLNAME:TARGET%의 질구멍은 원래대로 돌아가지 않고, 애액을 흘려 보내고 
    ELSEIF EXP:V경험 >= 21
        PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 얼굴을 붉혀, 아직도 남아있는 이물감에 떨고 
    ELSE
        PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 수치와 압박감에서 해방되어, 안도의 표정을 짓고 
    ENDIF
    PRINTW 있다….
ELSE
    PRINTFORM %조사선택(CALLNAME:PLAYER, "는")% 
    SIF TEQUIP:50
        PRINT 비디오 카메라의 앞에서 
    SIF TEQUIP:56
        PRINT 거울에 비치는 질 내를 자신에게 과시하듯이 
    PRINTFORML %CALLNAME:TARGET%의 질에 %조사선택(COM_TOOL_CALLNAME, "를")% 삽입해 벌렸다.
    PRINTL

    IF PALAM:10 >= PALAMLV:5
        PRINTL 벌려진 질 벽에서 대량의 희고 끈적한 꿀이, 눅진눅진 바닥에 떨어지기 시작했다….
    ELSEIF PALAM:10 >= PALAMLV:3
        PRINTFORML 질 벽에서는 점액이 주륵 주륵 %COM_TOOL_NAME%에 늘어져 떨어지고 있다….
    ELSE
        WAIT
        RETURN 1
    ENDIF

    PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 
    IF EXP:V경험 >= 200 && (TALENT:150 || TALENT:160 || TALENT:169 || TALENT:170)
        PRINTFORM %CALLNAME:PLAYER% 쪽으로 허리를 밀어 올리며, 더욱 더 넓혀달라고 요구하고 
    ELSEIF EXP:V경험 >= 71 && (TALENT:150 || TALENT:160 || TALENT:169 || TALENT:170)
        PRINTFORM 천천히 넓혀 가는 %CALLNAME:PLAYER%의 손을, 군침을 흘리며 바라보고 
    ELSEIF EXP:V경험 >= 71
        PRINTFORM 무엇인가를 포기한 듯한 표정으로, 천천히 넓혀 가는 %CALLNAME:PLAYER%의 손을 바라보고 
    ELSEIF EXP:V경험 >= 21
        PRINT 질 내를 채우는 차가운 이물감에 뭐라고 말하기 힘든 표정을 하고 
    ELSE
        PRINT 하복부가 찢어지는 것 같은 감각에, 소리 높여 신음하고 
    ENDIF
    PRINTW 있다….
ENDIF
