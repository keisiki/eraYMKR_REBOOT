ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = 엉덩이 스마타
CALL TRAIN_MESSAGE_COM183

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
    LOCAL = 400
ELSEIF ABL:12 == 1
    LOCAL = 880
ELSEIF ABL:12 == 2
    LOCAL = 1600
ELSEIF ABL:12 == 3
    LOCAL = 2400
ELSEIF ABL:12 == 4
    LOCAL = 3120
ELSE
    LOCAL = 3680
ENDIF

;윤활을 본다
LOCAL = EJACULATIONCHECK_PALAM_10_3(TARGET, LOCAL)
;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사 정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;조교자의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF EXIST_PENIS(PLAYER)
    BASE:PLAYER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 50
DOWNBASE:1 += 135

SOURCE:12 = 900
SOURCE:14 = 2000
SOURCE:22 = 600
SOURCE:31 = 450

;위 쪽으로 계산한 더러운 데이터
SOURCE:30 = TFLAG:242*10 + 60

;ABL:봉사 정신을 본다
IF ABL:13 == 0
    SOURCE:15 = 200
    SOURCE:16 = 50
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:13 == 1
    SOURCE:15 = 220
    SOURCE:16 = 90
    TIMES SOURCE:30 , 2.50
ELSEIF ABL:13 == 2
    SOURCE:15 = 300
    SOURCE:16 = 125
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:13 == 3
    SOURCE:15 = 425
    SOURCE:16 = 225
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:13 == 4
    SOURCE:15 = 500
    SOURCE:16 = 360
    TIMES SOURCE:30 , 0.50
ELSE
    SOURCE:15 = 650
    SOURCE:16 = 550
    TIMES SOURCE:30 , 0.10
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
    TIMES SOURCE:15 , 0.70
    TIMES SOURCE:16 , 0.70
ELSEIF ABL:12 == 1
    TIMES SOURCE:15 , 0.90
    TIMES SOURCE:16 , 0.90
ELSEIF ABL:12 == 2
    TIMES SOURCE:15 , 1.00
    TIMES SOURCE:16 , 1.00
ELSEIF ABL:12 == 3
    TIMES SOURCE:15 , 1.20
    TIMES SOURCE:16 , 1.20
ELSEIF ABL:12 == 4
    TIMES SOURCE:15 , 1.40
    TIMES SOURCE:16 , 1.40
ELSE
    TIMES SOURCE:15 , 1.60
    TIMES SOURCE:16 , 1.60
ENDIF

;ABL:A감각을 본다
IF ABL:2 == 0
    SOURCE:2 = 10
    SOURCE:22 += 1800
ELSEIF ABL:2 == 1
    SOURCE:2 = 37
    SOURCE:22 += 1850
ELSEIF ABL:2 == 2
    SOURCE:2 = 150
    SOURCE:22 += 1900
ELSEIF ABL:2 == 3
    SOURCE:2 = 350
    SOURCE:22 += 2150
ELSEIF ABL:2 == 4
    SOURCE:2 = 550
    SOURCE:22 += 2500
ELSE
    SOURCE:2 = 750
    SOURCE:22 += 3000
ENDIF

;PALAM:윤활
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:2 , 0.30
    TIMES SOURCE:15 , 0.60
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:2 , 0.60
    TIMES SOURCE:15 , 0.80
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:2 , 1.00
    TIMES SOURCE:15 , 1.00
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:2 , 1.50
    TIMES SOURCE:15 , 1.20
ELSEIF PALAM:10 < PALAMLV:5
    TIMES SOURCE:2 , 2.00
    TIMES SOURCE:15 , 1.40
ELSE
    TIMES SOURCE:2 , 2.50
    TIMES SOURCE:15 , 1.60
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
    ;커맨드 마다 고유의 처리는 이 아래에 쓴다
    TIMES SOURCE:15 , 2.00

    ;ABL:정액 중독을 본다
    IF ABL:31 == 0
        SOURCE:24 = 0
        TIMES SOURCE:16 , 1.50
        TIMES SOURCE:22, 1.40
    ELSEIF ABL:31 == 1
        SOURCE:24 = 200
        TIMES SOURCE:16 , 2.00
        TIMES SOURCE:22, 1.00
    ELSEIF ABL:31 == 2
        SOURCE:24 = 400
        TIMES SOURCE:16 , 2.50
        TIMES SOURCE:22, 0.80
    ELSEIF ABL:31 == 3
        SOURCE:24 = 700
        TIMES SOURCE:16 , 3.00
        TIMES SOURCE:22, 0.50
    ELSEIF ABL:31 == 4
        SOURCE:24 = 1000
        TIMES SOURCE:16 , 4.00
        TIMES SOURCE:22, 0.20
    ELSE
        SOURCE:24 = 1500
        TIMES SOURCE:16 , 5.00
        TIMES SOURCE:22, 0.00
    ENDIF

    ;사정처 타입의 설정
    TFLAG:703 = 13
    ;사정처 선택의 가능·불가
    TFLAG:704 = 0
    ;범용적인 처리는 이 함수로 실시한다
    ;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
    CALL SAMEN_SHOOT
ENDIF

;사정에 관계없이 행해지는 처리는 여기로부터
;노예의 A⇔조교자의 P의 불결이 이동
STAIN:4 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:4

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 7
ELSEIF TALENT:120 && TALENT:PLAYER:120
    TCVAR:13 += 7
ENDIF

SIF PLAYER == MASTER
    TCVAR:41 += 1

;주인 경험 플래그
SIF ASSIPLAY == 0
    TFLAG:50 += 1
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

;조교자가 후타나리
SIF TALENT:PLAYER:121
    SOURCE:22 /= 2

RETURN 1

