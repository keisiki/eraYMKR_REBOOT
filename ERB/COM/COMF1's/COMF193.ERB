;=============================================================================
;유방 전극
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE193
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:193 > 0
    RETURN 0
;Ｂ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 8) == 0
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
;해제는 언제든 가능
SIF TEQUIP:35 == 4
    RETURN 1
;テープ付き전극を持っているか, 아이템なし設定でないとダメ
SIF ITEM:23 == 0 && NOITEM == 0
    RETURN 0
;기계면 불가
SIF TALENT:기계
    RETURN 0
;ASSI일 경우, 기교3이상이 아니면 쓸 수 없다。
IF ASSIPLAY
    SIF ABL:ASSI:기교 < 3
        RETURN 0
ENDIF
;그밖의 Ｂ계 장착구 사용 중에는 불가
SIF (TEQUIP:35 && TEQUIP:35 != 4) || TEQUIP:36
    RETURN 0
;훈도시와 알몸 와이셔츠 외의 코스프레 중에는 안 됨
SIF TEQUIP:59 > 2 && TEQUIP:59 != 11 && TEQUIP:59 != 17
    RETURN 0
;욕실 플레이 중, ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:53 || TEQUIP:55
    RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM193
COM_TOOL_NAME = 전극
IF FLAG:1100 & 1
    ;技師パッチ有効なら特殊な名前チェック
    FLAG:1110 = 23
    CALL NAMECHECK_CUSTOMTOOL
    IF (COM_TOOL_NAME != "전극") && (COM_TOOL_NAME != ITEMNAME:23)
        PRINTFORML 유방 %COM_TOOL_NAME%
    ELSE
        PRINTL 유방 전극
    ENDIF 
ELSE
    COM_TOOL_CALLNAME = %COM_TOOL_NAME%
    PRINTL 유방 전극
ENDIF
COM_NAME = 유방 전극
CALL TRAIN_MESSAGE_COM193

DOWNBASE:0 += 80
DOWNBASE:기력 += 140

SOURCE:31 = 600

;ABL:B감각을 본다
IF ABL:B감각 == 0
    SOURCE:3 = 150
    SOURCE:20 = 600
    SOURCE:22 = 800
ELSEIF ABL:B감각 == 1
    SOURCE:3 = 350
    SOURCE:20 = 600
    SOURCE:22 = 1400
ELSEIF ABL:B감각 == 2
    SOURCE:3 = 600
    SOURCE:20 = 600
    SOURCE:22 = 2300
ELSEIF ABL:B감각 == 3
    SOURCE:3 = 1500
    SOURCE:20 = 800
    SOURCE:22 = 3500
ELSEIF ABL:B감각 == 4
    SOURCE:3 = 2100
    SOURCE:20 = 1000
    SOURCE:22 = 6000
ELSE
    SOURCE:3 = 3000
    SOURCE:20 = 1200
    SOURCE:22 = 9000
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:3 , 0.80
ELSEIF ABL:순종 == 1
    TIMES SOURCE:3 , 0.90
ELSEIF ABL:순종 == 2
    TIMES SOURCE:3 , 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:3 , 1.10
ELSEIF ABL:순종 == 4
    TIMES SOURCE:3 , 1.20
ELSE
    TIMES SOURCE:3 , 1.30
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:3 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:3 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:3 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:3 , 1.10
ELSE
    TIMES SOURCE:3 , 1.20
ENDIF

;B민감, 둔감을 본다
;쾌B자체의 체크는 다음에 정리해 준다
IF TALENT:B둔감
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:B민감
    TIMES SOURCE:20 , 0.60
    TIMES SOURCE:22 , 0.60
    TIMES SOURCE:31 , 0.60
ENDIF

;절벽
IF TALENT:절벽
    TIMES SOURCE:20 , 2.30
;빈유
ELSEIF TALENT:빈유
    TIMES SOURCE:20 , 2.00
ENDIF

IF FLAG:1100 & 1
    ;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
    ;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
    FLAG:1110 = 23
    FLAG:1111 = 1
    FLAG:1112 = SOURCE:3
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:3 = FLAG:1120
    FLAG:1112 = SOURCE:22
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:22 = FLAG:1120
    FLAG:1111 = 2
    FLAG:1112 = SOURCE:20
    CALL SOURCECHECK_CUSTOMTOOL
    SOURCE:20 = FLAG:1120
ENDIF

;커맨드 속성：도구(조교자)
TFLAG:98 = 1

;전극의 착탈
IF TEQUIP:35 == 4
    TEQUIP:35 = 0
ELSE
    TEQUIP:35 = 4
ENDIF
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;--------------------------------------------------
;유방 전극 장착중
;--------------------------------------------------
@EQUIP_COM193
IF FLAG:1100 & 1
    COM_TOOL_NAME = 전극
    ;技師パッチ有効なら特殊な名前チェック
    FLAG:1110 = 23
    CALL NAMECHECK_CUSTOMTOOL
    IF (COM_TOOL_NAME != "전극") && (COM_TOOL_NAME != ITEMNAME:23)
        PRINTFORM <유방 %COM_TOOL_NAME% 
    ELSE
        PRINT <유방 전극 
    ENDIF 
ELSE
    PRINT <유방 전극 
ENDIF
PRINTL 장착 중>

DOWNBASE:0 += 50
DOWNBASE:기력 += 100

LOCAL:2 = 1000

;ABL:B감각을 본다
IF ABL:B감각 == 0
    LOCAL = 250
    LOCAL:1 = 1000
ELSEIF ABL:B감각 == 1
    LOCAL = 600
    LOCAL:1 = 2000
ELSEIF ABL:B감각 == 2
    LOCAL = 1200
    LOCAL:1 = 3000
ELSEIF ABL:B감각 == 3
    LOCAL = 1900
    LOCAL:1 = 5000
ELSEIF ABL:B감각 == 4
    LOCAL = 2500
    LOCAL:1 = 8000
ELSE
    LOCAL = 3900
    LOCAL:1 = 12000
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES LOCAL , 0.80
ELSEIF ABL:순종 == 1
    TIMES LOCAL , 0.90
ELSEIF ABL:순종 == 2
    TIMES LOCAL , 1.00
ELSEIF ABL:순종 == 3
    TIMES LOCAL , 1.10
ELSEIF ABL:순종 == 4
    TIMES LOCAL , 1.20
ELSE
    TIMES LOCAL , 1.30
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES LOCAL , 1.10
ELSE
    TIMES LOCAL , 1.20
ENDIF

;B민감, 둔감을 본다
;쾌B자체의 체크는 다음에 정리해 준다
IF TALENT:B둔감
    TIMES LOCAL , 1.50
    TIMES LOCAL:1 , 1.50
    TIMES LOCAL:2 , 1.50
ELSEIF TALENT:B민감
    TIMES LOCAL , 0.60
    TIMES LOCAL:1 , 0.60
    TIMES LOCAL:2 , 0.60
ENDIF

;절벽
IF TALENT:절벽
    TIMES LOCAL:2 , 2.30
;빈유
ELSEIF TALENT:빈유
    TIMES LOCAL:2 , 2.00
ENDIF

IF FLAG:1100 & 1
    ;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
    ;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
    FLAG:1110 = 23
    FLAG:1111 = 1
    FLAG:1112 = LOCAL
    CALL SOURCECHECK_CUSTOMTOOL
    LOCAL = FLAG:1120
    FLAG:1112 = LOCAL:1
    CALL SOURCECHECK_CUSTOMTOOL
    LOCAL:1 = FLAG:1120
    FLAG:1111 = 3
    FLAG:1112 = LOCAL:2
    CALL SOURCECHECK_CUSTOMTOOL
    LOCAL:2 = FLAG:1120
ENDIF

SOURCE:3 += LOCAL
SOURCE:22 += LOCAL:1
SOURCE:20 += LOCAL:2

IF TALENT:남자 == 0 && TALENT:PLAYER:남자 == 0
    TCVAR:12 += 1
ELSEIF TALENT:남자 && TALENT:PLAYER:남자
    TCVAR:13 += 1
ENDIF
;촉수 경험 플래그
SIF TEQUIP:90
    TFLAG:90 += 1

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM193
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:지문소거판정 == 1 || RESULT != 0
    RETURN 0

PRINTL
IF TEQUIP:35 == 4
    IF MARK:반발각인 > 1
        PRINTFORMW %조사선택(CALLNAME:PLAYER, "가")% 미처 손을 쓰기도 전에, %조사선택(CALLNAME:TARGET, "는")% 유방에서 %조사선택(COM_TOOL_NAME, "를")% 벗어 던져버렸다… 
    ELSE
        PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 유방에서 %조사선택(COM_TOOL_NAME, "를")% 벗겨 주었다….
    ENDIF
ELSE
    IF MARK:반발각인 > 1
        PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% 싫어서 버둥대는 %조사선택(CALLNAME:TARGET, "를")% 억눌렀다.
        PRINTFORMW 그리고, %CALLNAME:TARGET%의 \@(TALENT:B민감) ? 민감한 # \@유방에 %조사선택(COM_TOOL_CALLNAME, "를")% 붙여 스위치를 넣었다….
    ELSE
        PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 \@(TALENT:B민감) ? 민감한 # \@유방에 %조사선택(COM_TOOL_CALLNAME, "를")% 붙여 스위치를 넣었다….
    ENDIF
ENDIF
