;=============================================================================
;자궁탈
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE160
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:160 > 0
    RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
    RETURN 0
;조교 대상のＶ확장경험이 ない場合には, 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && EXP:32 <= 1
    RETURN 0
;Ｖ경험LVが1미만で, 조교자가 オトコではない場合
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && EXP:0 < EXPLV:1 && TALENT:PLAYER:120 == 0
    RETURN 0
;탈자궁이 없으면 안됨
SIF TALENT:탈자궁 == 0
    RETURN 0
;남자라면 안 됨
SIF TALENT:120
    RETURN 0
;[작은체형]なら순종이 4이상かつ마조끼が4이상じゃないと実行できない
IF TALENT:128
    SIF ABL:순종 < 4 || ABL:마조끼 < 4
        RETURN 0
ENDIF
;주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:MASTER:56 == 0
    RETURN 0
;ASSI일 경우, 순종4이상이거나 새드じゃないとできない。
IF ASSIPLAY
    SIF ABL:ASSI:순종 < 4 && TALENT:ASSI:81 == 0
    RETURN 0
ENDIF
;일정의 Ｖ경험이 필요([巨躯]の場合は軽減)
LOCAL = (TALENT:129) ? 75 # 150
SIF EXP:0 < LOCAL
    RETURN 0
;자궁탈 외의 Ｖ계 장착구 사용 중에는 불가
SIF TEQUIP:20 && TEQUIP:20 != 8
    RETURN 0
;쿠스코 계통 사용 중에는 불가
SIF TEQUIP:21
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
    RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
    RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;알몸 앞치마, 학교수영복 플레이, 코스프레 중에는 불가
SIF TEQUIP:59
    RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM160
PRINTL 자궁탈
COM_NAME = 자궁탈
CALL TRAIN_MESSAGE_COM160

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 700
DOWNBASE:1 += 400

SOURCE:20 = 2000
SOURCE:12 = 3000
SOURCE:22 = 4000
SOURCE:31 = 5000
SOURCE:17 = 1000
SOURCE:25 = 4400

;ABL:V감각을 본다
IF ABL:V감각 == 0
    SOURCE:1 = 2500
ELSEIF ABL:V감각 == 1
    SOURCE:1 = 3000
ELSEIF ABL:V감각 == 2
    SOURCE:1 = 3500
ELSEIF ABL:V감각 == 3
    SOURCE:1 = 4000
ELSEIF ABL:V감각 == 4
    SOURCE:1 = 4500
ELSE
    SOURCE:1 = 5000
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
    TIMES SOURCE:1 , 0.20
    SOURCE:20 += 2000
    SIF TALENT:PLAYER:120 == 0
        TCVAR:50 += 1
ELSEIF EXP:0 < EXPLV:2
    TIMES SOURCE:1 , 0.60
    SOURCE:20 += 300
ELSEIF EXP:0 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 += 50
ELSEIF EXP:0 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    SOURCE:20 += 10
ELSEIF EXP:0 < EXPLV:5
    TIMES SOURCE:1 , 1.40
    SOURCE:20 += 0
ELSE
    TIMES SOURCE:1 , 1.60
    SOURCE:20 += 0
ENDIF

;EXP:자궁경험을 본다
IF EXP:47 < EXPLV:1
    TIMES SOURCE:1 , 0.50
    TIMES SOURCE:20 , 1.00
    TIMES SOURCE:25 , 2.00
    TIMES DOWNBASE:0 , 1.50
    TIMES DOWNBASE:1 , 1.50
ELSEIF EXP:47 < EXPLV:2
    TIMES SOURCE:1 , 0.75
    TIMES SOURCE:20 , 1.10
    TIMES SOURCE:25 , 1.50
    TIMES DOWNBASE:0 , 1.30
    TIMES DOWNBASE:1 , 1.30
ELSEIF EXP:47 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:20 , 1.25
    TIMES SOURCE:25 , 1.00
    TIMES DOWNBASE:0 , 1.00
    TIMES DOWNBASE:1 , 1.00
ELSEIF EXP:47 < EXPLV:4
    TIMES SOURCE:1 , 1.25
    TIMES SOURCE:20 , 1.50
    TIMES SOURCE:25 , 0.75
    TIMES DOWNBASE:0 , 0.90
    TIMES DOWNBASE:1 , 0.90
ELSEIF EXP:47 < EXPLV:5
    TIMES SOURCE:1 , 1.50
    TIMES SOURCE:20 , 1.75
    TIMES SOURCE:25 , 0.50
    TIMES DOWNBASE:0 , 0.80
    TIMES DOWNBASE:1 , 0.80
ELSE
    TIMES SOURCE:1 , 2.00
    TIMES SOURCE:20 , 2.00
    TIMES SOURCE:25 , 0.10
    TIMES DOWNBASE:0 , 0.70
    TIMES DOWNBASE:1 , 0.70
ENDIF

;[작은체형]
SIF TALENT:128
    TIMES SOURCE:20 , 2.00

;V민감
IF TALENT:102
    TIMES SOURCE:1, 1.50
    TIMES SOURCE:20, 1.50
;V둔감
ELSEIF TALENT:103
    TIMES SOURCE:1, 0.80
    TIMES SOURCE:20, 0.80
    TIMES DOWNBASE:0 , 0.90
    TIMES DOWNBASE:1 , 0.90
ENDIF

SIF EXP:32 == 0
    TCVAR:50 += 1

;자궁의 탈장과 회귀
IF TEQUIP:20
    TEQUIP:20 = 0
ELSE
    TEQUIP:20 = 8
ENDIF

TCVAR:0 += 30
TCVAR:32 += 1
TCVAR:47 += 2

SIF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 1

RETURN 1

;-------------------------------------------------
;자궁탈 중
;-------------------------------------------------
@EQUIP_COM160
PRINTL <자궁탈 중>

DOWNBASE:0 += 25
DOWNBASE:1 += 100

;ABL:V감각을 본다
IF ABL:V감각 == 0
    LOCAL = 50
ELSEIF ABL:V감각 == 1
    LOCAL = 150
ELSEIF ABL:V감각 == 2
    LOCAL = 400
ELSEIF ABL:V감각 == 3
    LOCAL = 600
ELSEIF ABL:V감각 == 4
    LOCAL = 800
ELSE
    LOCAL = 1000
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
    TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES LOCAL , 1.10
ELSE
    TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:마조끼 == 0
    TIMES LOCAL , 0.60
ELSEIF ABL:마조끼 == 1
    TIMES LOCAL , 0.80
ELSEIF ABL:마조끼 == 2
    TIMES LOCAL , 1.00
ELSEIF ABL:마조끼 == 3
    TIMES LOCAL , 1.20
ELSEIF ABL:마조끼 == 4
    TIMES LOCAL , 1.30
ELSE
    TIMES LOCAL , 1.50
ENDIF

SOURCE:1 += LOCAL
SOURCE:12 += 100
SOURCE:18 += 50
SOURCE:20 += 200
SOURCE:25 += 100

;EXP:자궁경험을 본다
IF EXP:47 < 50
    TIMES SOURCE:1, 0.90
    TIMES SOURCE:25, 0.90
ELSEIF EXP:47 < 100
    TIMES SOURCE:1, 1.00
    TIMES SOURCE:25, 0.80
ELSEIF EXP:47 < 200
    TIMES SOURCE:1, 1.50
    TIMES SOURCE:25, 0.70
ELSE
    TIMES SOURCE:1, 2.00
    TIMES SOURCE:25, 0.60
ENDIF

;V민감
IF TALENT:102
    TIMES SOURCE:1, 1.50
    TIMES SOURCE:20, 1.50
ENDIF
;V둔감
IF TALENT:103
    TIMES SOURCE:1, 0.80
    TIMES SOURCE:20, 0.80
    TIMES DOWNBASE:0 , 0.90
    TIMES DOWNBASE:1 , 0.90
ENDIF

TCVAR:32 += 1
TCVAR:47 += 1

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM160
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

IF TEQUIP:20 == 8
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% 손으로 %CALLNAME:TARGET%의 자궁구를 밀어넣었다.
    IF ABL:V감각 == 5 && EXP:47 >= EXPLV:3
        PRINTL 자궁구에 자꾸 손가락이 들어가 도로 집어넣기가 꽤나 힘들었다.
        PRINTFORMW 마치 %CALLNAME:TARGET%의 자궁이 손가락을 삼키려는 것 같았다.
    ELSEIF ABL:V감각 == 5
        PRINTFORMW %CALLNAME:TARGET%의 자궁은 꽤나 부드럽게 들어갔다.
    ELSEIF ABL:V감각 >= 3
        PRINTFORMW %CALLNAME:TARGET%의 질벽이 조여와서 애 좀 먹었다.
    ELSE
        PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 상당히 지친 듯하다.
    ENDIF
ELSE
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% 손으로 %CALLNAME:TARGET%의 질을 파고들어 자궁구를 끄집어냈다.
    PRINTFORM %CALLNAME:TARGET%의 
    IF ABL:V감각 == 5 && EXP:47 >= EXPLV:3
        PRINTL 자궁은 꽤나 길게 나왔다.
        PRINTFORMW 뒤집힌 질벽으로 이루어진 %CALLNAME:TARGET%의 자궁의 구멍은 크게 벌려진 채 성욕을 갈망하고 있다.
    ELSEIF ABL:V감각 == 5
        PRINTW 자궁구멍은 뻐끔대면서 무언가를 기다리는 것 같다.
    ELSEIF ABL:V감각 >= 3
        PRINTW 보지는 음란하게 자신의 자궁구를 무방비하게 늘어놓고 있다.
    ELSE
        PRINTW 자궁구가 빠진 보지는 움찔움찔 경련하고 있다.
    ENDIF
ENDIF
