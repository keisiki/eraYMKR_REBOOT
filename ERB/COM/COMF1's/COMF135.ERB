ELSE
COM_NAME = 음핵 측정
ENDIF
CALL TRAIN_MESSAGE_COM135

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 50
DOWNBASE:기력 += 90

SOURCE:10 = 200
SOURCE:12 = 250
SOURCE:22 = 250
SOURCE:31 = 500

;ABL:C감각을 본다
IF ABL:C감각 == 0
    SOURCE:0 = 100
    SOURCE:22 += 200
ELSEIF ABL:C감각 == 1
    SOURCE:0 = 200
    SOURCE:22 += 300
ELSEIF ABL:C감각 == 2
    SOURCE:0 = 350
    SOURCE:22 += 600
ELSEIF ABL:C감각 == 3
    SOURCE:0 = 700
    SOURCE:22 += 900
ELSEIF ABL:C감각 == 4
    SOURCE:0 = 1400
    SOURCE:22 += 1300
ELSE
    SOURCE:0 = 1800
    SOURCE:22 += 1700
ENDIF

;ABL:노출벽을 본다
IF ABL:노출증 == 0
    SOURCE:12 += 1500
    SOURCE:30 = 2000
ELSEIF ABL:노출증 == 1
    SOURCE:12 += 1250
    SOURCE:30 = 1000
ELSEIF ABL:노출증 == 2
    SOURCE:12 += 750
    SOURCE:30 = 500
ELSEIF ABL:노출증 == 3
    SOURCE:12 += 500
    SOURCE:30 = 200
ELSEIF ABL:노출증 == 4
    SOURCE:12 += 250
    SOURCE:30 = 100
ELSE
    SOURCE:30 = 50
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:10 , 0.50
    TIMES SOURCE:30 , 1.80
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:10 , 0.80
    TIMES SOURCE:30 , 1.50
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:10 , 1.20
    TIMES SOURCE:30 , 1.20
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:10 , 1.50
    TIMES SOURCE:30 , 0.80
ELSE
    TIMES SOURCE:10 , 1.80
    TIMES SOURCE:30 , 0.50
ENDIF

;부끄럼쟁이·부끄럼없음
IF TALENT:부끄럼쟁이
    TIMES SOURCE:30 , 1.20
ELSEIF TALENT:부끄럼없음
    TIMES SOURCE:30 , 0.50
ENDIF
;C민감, 둔감을 본다
;쾌C자체의 체크는 다음에 정리해 준다
IF TALENT:C민감
    TIMES SOURCE:0 , 1.50
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:C둔감
    TIMES SOURCE:0 , 0.60
    TIMES SOURCE:22 , 0.60
    TIMES SOURCE:31 , 1.50
ENDIF

;처녀, 정조관념
IF TALENT:처녀
    TIMES SOURCE:30 , 1.20
    IF TALENT:처녀 == 1
        SOURCE:12 += 2000
        TIMES SOURCE:22 , 2.00
        TIMES SOURCE:30 , 2.00
        SIF TALENT:정조관념
            TIMES SOURCE:12 , 2.00
    ENDIF
ENDIF

IF ABL:순종 < 3 && MARK:굴복각인 > 0 && MARK:반발각인 < 3
    ;EXP:자위 경험을 본다
    IF EXP:자위경험 < EXPLV:1
        TIMES SOURCE:30 , 1.50
        TIMES SOURCE:22 , 0.20
    ELSEIF EXP:자위경험 < EXPLV:2
        TIMES SOURCE:30 , 1.20
        TIMES SOURCE:22 , 0.50
    ELSEIF EXP:자위경험 < EXPLV:3
        TIMES SOURCE:30 , 1.00
        TIMES SOURCE:22 , 0.80
    ELSEIF EXP:자위경험 < EXPLV:4
        TIMES SOURCE:30 , 0.80
        TIMES SOURCE:22 , 1.00
    ELSEIF EXP:자위경험 < EXPLV:5
        TIMES SOURCE:30 , 0.50
        TIMES SOURCE:22 , 1.20
    ELSE
        TIMES SOURCE:30 , 0.20
        TIMES SOURCE:22 , 1.50
    ENDIF

    SIF ABL:노출증 >= 3
        TCVAR:10 += 1
ENDIF
;노예의 V⇔손가락의 불결이 이동
STAIN:보지 |= STAIN:손
STAIN:손 |= STAIN:보지

IF TALENT:남자 == 0 && TALENT:PLAYER:남자 == 0
    TCVAR:12 += 1
ELSEIF TALENT:남자 && TALENT:PLAYER:남자
    TCVAR:13 += 1
ENDIF

SIF PLAYER == MASTER
    TCVAR:41 += 1

CALL PENIS_SMESSAGE

RETURN 1

