    SIF TFLAG:177 & 1
        PRINTLC [ 1] 미약
    SIF TFLAG:177 & 2
        PRINTLC [ 2] 이뇨제
    SIF TFLAG:177 & 4
        PRINTLC [ 3] 정액
    $INPUT_LOOP
    INPUT
    SIF RESULT < 0 || RESULT > 6
        GOTO INPUT_LOOP
    
    ;이하 아이템 조건 등에 걸렸을 경우는 입력 다시 해
    IF RESULT > 0 && RESULT < 5
        SIF RESULT > 0  && GETBIT(TFLAG:177, RESULT - 1) == 0
            GOTO INPUT_LOOP
    ENDIF
    TFLAG:99 = RESULT + 1
    PRINTFORML %LOCALS:RESULT%관장
    COM_NAME = %LOCALS:RESULT%관장
ENDIF
