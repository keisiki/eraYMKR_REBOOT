SIF FLAG:VA이상조교필터 & 4 && (CFLAG:21 & 1) == 0 && TEQUIP:50
    RETURN 0
;수면 중에는 불가
SIF EQUIP:14
    RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
    RETURN 0
;바이브, 쿠스코, 애널바이브, 요석 외의 Ｖ계 장착구, Ａ계 장착구 사용 중에는 불가
SIF TEQUIP:20 > 2 || TEQUIP:25 > 2 || TEQUIP:26
    RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:40
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
