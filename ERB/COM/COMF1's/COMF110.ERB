@COM110
;노예가 마음대로 했을 경우를 제외해
;전회와 이번 조교자 같고, 전회가 비패개장 과시하고 혹은 상호비패개장의 경우, 상호비패개장에
IF ((PREVCOM == 117 || PREVCOM == 631) && ((ASSIPLAY && TFLAG:133) || (ASSIPLAY == 0 && TFLAG:133 == 0))) && TFLAG:699 == 0
    CALL EXCOM_ABLE631
    SIF RESULT == 1
        JUMP COM631
ENDIF
PRINTL 보지 벌리기 

;-------------------------------------------------
;실행할 수 있을까의 판정
;-------------------------------------------------
IF TFLAG:699 == 0
    CALL COM_ORDER
    ;실행할 수 없다
    SIF RESULT == 0
        RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
COM_NAME = 비패개장
CALL TRAIN_MESSAGE_COM110

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 50
DOWNBASE:기력 += 90

SOURCE:19 = 10
SOURCE:12 = 250
SOURCE:22 = 250
SOURCE:31 = 500

;ABL:C감각을 본다
IF ABL:C감각 == 0
    SOURCE:0 = 100
    SOURCE:22 += 200
ELSEIF ABL:C감각 == 1
    SOURCE:0 = 200
    SOURCE:22 += 300
ELSEIF ABL:C감각 == 2
    SOURCE:0 = 350
    SOURCE:22 += 600
ELSEIF ABL:C감각 == 3
    SOURCE:0 = 700
    SOURCE:22 += 900
ELSEIF ABL:C감각 == 4
    SOURCE:0 = 1400
    SOURCE:22 += 1300
ELSE
    SOURCE:0 = 1800
    SOURCE:22 += 1700
ENDIF

;ABL:노출벽을 본다
IF ABL:노출증 == 0
    SOURCE:30 = 2000
    SOURCE:12 += 1500
ELSEIF ABL:노출증 == 1
    SOURCE:30 = 1000
    SOURCE:12 += 1250
    SOURCE:19 *= 5
ELSEIF ABL:노출증 == 2
    SOURCE:30 = 500
    SOURCE:12 += 750
    SOURCE:19 *= 10
ELSEIF ABL:노출증 == 3
    SOURCE:30 = 200
    SOURCE:12 += 500
    SOURCE:19 *= 50
ELSEIF ABL:노출증 == 4
    SOURCE:30 = 100
    SOURCE:12 += 250
    SOURCE:19 *= 100
ELSE
    SOURCE:30 = 50
    SOURCE:19 *= 200
ENDIF

;EXP:자위 경험을 본다
IF EXP:자위경험 < EXPLV:1
    TIMES SOURCE:30, 1.50
    TIMES SOURCE:22, 0.20
ELSEIF EXP:자위경험 < EXPLV:2
    TIMES SOURCE:30, 1.20
    TIMES SOURCE:22, 0.50
ELSEIF EXP:자위경험 < EXPLV:3
    TIMES SOURCE:30, 1.00
    TIMES SOURCE:22, 0.80
ELSEIF EXP:자위경험 < EXPLV:4
    TIMES SOURCE:30, 0.80
    TIMES SOURCE:22, 1.00
ELSEIF EXP:자위경험 < EXPLV:5
    TIMES SOURCE:30, 0.50
    TIMES SOURCE:22, 1.20
ELSE
    TIMES SOURCE:30, 0.20
    TIMES SOURCE:22, 1.50
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:30 , 1.80
    TIMES SOURCE:19 , 0.50
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:30 , 1.50
    TIMES SOURCE:19 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:30 , 1.20
    TIMES SOURCE:19 , 1.20
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:30 , 0.80
    TIMES SOURCE:19 , 1.50
ELSE
    TIMES SOURCE:30 , 0.50
    TIMES SOURCE:19 , 1.80
ENDIF

;부끄럼쟁이·부끄럼없음
IF TALENT:35
    TIMES SOURCE:30 , 1.20
ELSEIF TALENT:36
    TIMES SOURCE:30 , 0.50
ENDIF

;C민감, 둔감을 본다
;쾌C자체의 체크는 다음에 정리해 준다
IF TALENT:100
    TIMES SOURCE:0 , 1.50
    TIMES SOURCE:22 , 1.50
    TIMES SOURCE:31 , 1.50
ELSEIF TALENT:101
    TIMES SOURCE:0 , 0.60
    TIMES SOURCE:22 , 0.60
    TIMES SOURCE:31 , 1.50
ENDIF

;처녀, 정조관념
IF TALENT:처녀
    TIMES SOURCE:30 , 1.20
    IF TALENT:처녀 == 1
        TIMES SOURCE:30 , 2.00
        TIMES SOURCE:22 , 2.00
        SOURCE:12 += 2000
        SIF TALENT:30
            TIMES SOURCE:12 , 2.00
    ENDIF
ENDIF

SIF ABL:노출증 >= 3
    TCVAR:10 += 1

;노예의 V⇔손가락의 불결이 이동
STAIN:보지 |= STAIN:손
STAIN:손 |= STAIN:보지

IF TALENT:120 == 0 && TALENT:PLAYER:남자 == 0
    TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:남자
    TCVAR:13 += 1
ENDIF

;노출 쾌락 경험 입수 판정
TFLAG:100 |= 2

RETURN 1

