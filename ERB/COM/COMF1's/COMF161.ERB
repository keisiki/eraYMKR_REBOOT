;=============================================================================
;자궁오나홀
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE161
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:161 > 0
    RETURN 0
;V조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:2 & 1
    RETURN 0
;이상계 조교 필터가 켜저있지 않으면 안 됨
SIF FLAG:2 & 4 == 0
    RETURN 0
;조교자에게 페니스가 없는, 한편(아이템 있음 설정이라면) 페니스 밴드도 없으면 안 됨
SIF EXIST_BAR(PLAYER) == 0 && ITEM:2 == 0 && NOITEM == 0
    RETURN 0
;탈자궁이 없으면 안됨
SIF TALENT:3 == 0
    RETURN 0
;주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:MASTER:56 == 0
    RETURN 0
;자궁탈 이외의 Ｖ계 장착구 사용 중에는 불가
SIF TEQUIP:20 && TEQUIP:20 != 8
    RETURN 0
;자궁탈이 아니어도 불가
SIF TEQUIP:20 == 0
    RETURN 0
;쿠스코 계통 사용 중에는 불가
SIF TEQUIP:21
    RETURN 0
;남자라면 안 됨
SIF TALENT:120
    RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
    RETURN 0
;목욕탕 플레이중은 플레이 매트가 없으면 안 됨
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
    RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
    RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
    RETURN 0
;나체정식 중에는 안 됨
SIF TEQUIP:60
    RETURN 0
;촉수 조교중일 시엔 불가
SIF TEQUIP:90
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM161
PRINTL 자궁오나홀
COM_NAME = 자궁오나홀
CALL TRAIN_MESSAGE_COM161

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1
SIF TALENT:0 && TALENT:150 && ASSIPLAY == 0
    TFLAG:2 = TALENT:0

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
    LOCAL = 1500
ELSEIF ABL:12 == 1
    LOCAL = 1600
ELSEIF ABL:12 == 2
    LOCAL = 1800
ELSEIF ABL:12 == 3
    LOCAL = 2000
ELSEIF ABL:12 == 4
    LOCAL = 2400
ELSE
    LOCAL = 3000
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;욕망을 본다
LOCAL = EJACULATIONCHECK_ABL_11_1(TARGET, LOCAL)
;윤활을 본다
LOCAL = EJACULATIONCHECK_PALAM_10_1(TARGET, LOCAL)

;조교자의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF EXIST_PENIS(PLAYER)
    BASE:PLAYER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 500
DOWNBASE:1 += 1000

SOURCE:12 = 2000

;ABL:V감각을 본다
IF ABL:1 == 0
    SOURCE:1 = 400
ELSEIF ABL:1 == 1
    SOURCE:1 = 800
ELSEIF ABL:1 == 2
    SOURCE:1 = 1200
ELSEIF ABL:1 == 3
    SOURCE:1 = 1600
ELSEIF ABL:1 == 4
    SOURCE:1 = 2000
ELSE
    SOURCE:1 = 3000
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
    TIMES SOURCE:1 , 0.10
    SOURCE:20 = 10000
ELSEIF EXP:0 < EXPLV:2
    TIMES SOURCE:1 , 0.50
    SOURCE:20 = 5000
ELSEIF EXP:0 < EXPLV:3
    TIMES SOURCE:1 , 1.00
    SOURCE:20 = 2000
ELSEIF EXP:0 < EXPLV:4
    TIMES SOURCE:1 , 1.20
    SOURCE:20 = 1000
ELSEIF EXP:0 < EXPLV:5
    TIMES SOURCE:1 , 1.30
    SOURCE:20 = 1000
ELSE
    TIMES SOURCE:1 , 1.50
    SOURCE:20 = 1000
ENDIF

;EXP:자궁경험을 본다
IF EXP:0 < EXPLV:1
    TIMES SOURCE:20 , 2.00
    SOURCE:25 = 10000
ELSEIF EXP:0 < EXPLV:2
    TIMES SOURCE:20 , 1.50
    SOURCE:25 = 5000
ELSEIF EXP:0 < EXPLV:3
    TIMES SOURCE:20 , 1.20
    SOURCE:25 = 3000
ELSEIF EXP:0 < EXPLV:4
    TIMES SOURCE:20 , 1.00
    SOURCE:25 = 2000
ELSEIF EXP:0 < EXPLV:5
    TIMES SOURCE:20 , 0.80
    SOURCE:25 = 1000
ELSE
    TIMES SOURCE:20 , 0.60
    SOURCE:25 = 500
    SOURCE:11 = 500
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
    TIMES SOURCE:1 , 0.10
    SOURCE:20 += 1000
    TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
    TIMES SOURCE:1 , 0.40
    SOURCE:20 += 300
    TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:20 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
    TIMES SOURCE:1 , 1.40
    TIMES SOURCE:20 , 0.20
ELSE
    TIMES SOURCE:1 , 1.80
    TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    TIMES SOURCE:1 , 0.60
    TIMES SOURCE:11 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:11 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:11 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
    TIMES SOURCE:1 , 1.20
    TIMES SOURCE:11 , 1.50
ELSE
    TIMES SOURCE:1 , 1.50
    TIMES SOURCE:11 , 1.80
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
    TIMES SOURCE:1 , 0.50
    TIMES SOURCE:32 , 2.00
ELSEIF ABL:10 == 1
    TIMES SOURCE:1 , 0.80
    TIMES SOURCE:32 , 1.50
ELSEIF ABL:10 == 2
    TIMES SOURCE:1 , 1.00
    TIMES SOURCE:32 , 1.00
ELSEIF ABL:10 == 3
    TIMES SOURCE:1 , 1.30
    TIMES SOURCE:32 , 0.80
ELSEIF ABL:10 == 4
    TIMES SOURCE:1 , 1.60
    TIMES SOURCE:32 , 0.60
ELSE
    TIMES SOURCE:1 , 2.00
    TIMES SOURCE:32 , 0.30
ENDIF

;음호
SIF TALENT:172
    TIMES SOURCE:1, 2.00
;V민감
IF TALENT:102
    TIMES SOURCE:1, 1.50
    TIMES SOURCE:20, 1.50
;V둔감
ELSEIF TALENT:103
    TIMES SOURCE:1, 0.80
    TIMES SOURCE:20, 0.80
    TIMES DOWNBASE:0 , 0.90
    TIMES DOWNBASE:1 , 0.90
ENDIF

TCVAR:0 += 1
TCVAR:47 += 3

IF PLAYER == MASTER
    TCVAR:42 += 1
    SIF EXIST_BAR(MASTER)
        CFLAG:173 += 1
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
    ;사정처 타입의 설정
    TFLAG:703 = 1
    ;사정처 선택의 가능·불가
    TFLAG:704 = 1
    ;범용적인 처리는 이 함수로 실시한다
    ;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
    CALL SAMEN_SHOOT
ENDIF

;사정에 관계없이 행해지는 처리는 여기로부터
;노예의 V⇔조교자의 P의 불결이 이동
STAIN:3 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:3

SIF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
    TCVAR:12 += 7
;주인 경험 플래그
SIF ASSIPLAY == 0
    TFLAG:50 += (ABL:1 >= 3) ?  2 # 1

;처녀 상실의 후에 페니스를 뽑고 있을까
TFLAG:101 = (TFLAG:101 == 2) ?  1 # 0
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM161
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 자궁을 움켜쥐고 페니스를 삽입해
PRINTL 여성기를 본딴 자위기구로 자위하듯 쾌락을 탐닉했다.
PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 
IF ABL:1 == 5 && EXP:47 >= EXPLV:3
    PRINTW 아예 자기 손으로도 자궁을 쥐어짜며 스스로 오나홀의 역할을 수행하고 있다.
ELSEIF ABL:1 == 5
    PRINTW 자궁을 범해지는 게 그렇게 나쁘지는 않은 모양이다.
ELSEIF ABL:1 >= 3
    PRINTW 고통스러워 하지만, 묘하게 얼굴에서 흥분이 묻어나오고 있다.
ELSE
    PRINTW 비명을 지르고 있다.
ENDIF
