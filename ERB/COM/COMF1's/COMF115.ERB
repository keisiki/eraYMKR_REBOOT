;=============================================================================
;자위 보여주기
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE115
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:115 > 0
    RETURN 0
;수치 플레이계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:커맨드확장 & 512) == 0
    RETURN 0
;수면 중에는 불가
SIF EQUIP:14
    RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
    RETURN 0
;조수가 훈도시 착용 중일 경우 助手に기교4이상 필수
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17 && ABL:ASSI:기교 < 4
    RETURN 0
;眼隠しと耳栓両方つけていた場合は意味がないので実行できない
SIF TEQUIP:41 && TEQUIP:45
    RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
    RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;대부분 자위에 손 더했을 뿐이야!
@COM115

;전회와 이번 조교자 같고, 전회가 자위 혹은 상호 자위의 경우, 상호 자위에
IF (PREVCOM == 7 || PREVCOM == 628) && ((ASSIPLAY && TFLAG:133) || (ASSIPLAY == 0 && TFLAG:133 == 0))
    CALL EXCOM_ABLE628
    SIF RESULT == 1
        JUMP COM628
ENDIF

PRINTL 자위 보여주기
COM_NAME = 자위보여주기
CALL TRAIN_MESSAGE_COM115

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL =0

;플레이어의 기교를 본다
IF ABL:PLAYER:기교 == 0
    LOCAL = 800
ELSEIF ABL:PLAYER:기교 == 1
    LOCAL = 1600
ELSEIF ABL:PLAYER:기교 == 2
    LOCAL = 2400
ELSEIF ABL:PLAYER:기교 == 3
    LOCAL = 3200
ELSEIF ABL:PLAYER:기교 == 4
    LOCAL = 4000
ELSE
    LOCAL = 4800
ENDIF

;조교자의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_2(PLAYER, LOCAL)

;C감각이 거론되지 않는 주인 또한 남자의 경우 2배(로 해 두는＆그런데도 10000에는 닿지 않는다우후후)
SIF ASSIPLAY == 0 && TALENT:PLAYER:남자
    TIMES LOCAL , 2.00

SIF EXIST_PENIS(PLAYER)
    BASE:PLAYER:사정 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 5
DOWNBASE:기력 += 10

SOURCE:12 = 500
SOURCE:30 = 100
SOURCE:31 = 400

;ABL:자위 중독을 본다
IF ABL:자위중독 == 0
    SOURCE:15 = 25
    SOURCE:22 = 100
    TIMES SOURCE:30 , 4.00
ELSEIF ABL:자위중독 == 1
    SOURCE:15 = 50
    SOURCE:22 = 80
    TIMES SOURCE:30 , 2.50
ELSEIF ABL:자위중독 == 2
    SOURCE:15 = 75
    SOURCE:22 = 70
    TIMES SOURCE:30 , 1.50
ELSEIF ABL:자위중독 == 3
    SOURCE:15 = 100
    SOURCE:22 = 60
    TIMES SOURCE:30 , 1.00
ELSEIF ABL:자위중독 == 4
    SOURCE:15 = 125
    SOURCE:22 = 50
    TIMES SOURCE:30 , 0.50
ELSE
    SOURCE:15 = 150
    SOURCE:22 = 0
    TIMES SOURCE:30 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
    SOURCE:18 = 25
ELSEIF PALAM:13 < PALAMLV:2
    SOURCE:18 = 50
ELSEIF PALAM:13 < PALAMLV:3
    SOURCE:18 = 100
ELSEIF PALAM:13 < PALAMLV:4
    SOURCE:18 = 200
ELSE
    SOURCE:18 = 400
ENDIF

;-------------------------------------------------
;독심의 경우 에로 있고 기분이 옮기는지 과시하고 효과 업(2배)
;-------------------------------------------------
;캐릭터 전용 커맨드가 온의 경우, 대상이[연모]와 마음의 눈을 닫지 않은[깨닫고]소유라면 변화
IF TALENT:사토리 == 1 && TALENT:연모 && (FLAG:커맨드확장 & 16384)
    TIMES SOURCE:15 , 2.00
    TIMES SOURCE:18 , 2.00
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
    ;커맨드 마다 고유의 처리는 이 아래에 쓴다
    TIMES SOURCE:15 , 3.00

    ;ABL:정액 중독을 본다
    IF ABL:정액중독 == 0
        SOURCE:24 = 0
        TIMES SOURCE:22, 2.00
    ELSEIF ABL:정액중독 == 1
        SOURCE:24 = 200
        TIMES SOURCE:22, 1.60
    ELSEIF ABL:정액중독 == 2
        SOURCE:24 = 600
        TIMES SOURCE:22, 1.00
    ELSEIF ABL:정액중독 == 3
        SOURCE:24 = 1500
        TIMES SOURCE:22, 0.70
    ELSEIF ABL:정액중독 == 4
        SOURCE:24 = 3000
        TIMES SOURCE:22, 0.40
    ELSE
        SOURCE:24 = 6000
        TIMES SOURCE:22, 0.10
    ENDIF

    ;사정처 타입의 설정
    TFLAG:703 = 3
    ;사정처 선택의 가능·불가
    TFLAG:704 = 1
    ;범용적인 처리는 이 함수로 실시한다
    ;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
    CALL SAMEN_SHOOT
ENDIF

;사정에 관계없이 행해지는 처리는 여기로부터
;조교자의 손가락⇔조교자의 P의 불결이 이동
IF EXIST_PENIS(PLAYER)
    STAIN:PLAYER:손 |= STAIN:PLAYER:자지
    STAIN:PLAYER:자지 |= STAIN:PLAYER:손
;조교자의 손가락⇔조교자의 V의 불결이 이동
ELSE
    STAIN:PLAYER:보지 |= STAIN:PLAYER:자지
    STAIN:PLAYER:자지 |= STAIN:PLAYER:보지
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:남자 == 0
    TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:남자
    TCVAR:13 += 1
ENDIF

;조교자가 후타나리
SIF TALENT:PLAYER:후타나리
    SOURCE:22 /= 2

;자위 경험
TCVAR:PLAYER:10 += 1

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM115
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:조교시텍스트표시 == 0 || CFLAG:99 == 1 || RESULT != 0
    RETURN 0

PRINTL
;연모 득도함응에 과시하면(자) 득도함응도 대흥분
IF TALENT:사토리 == 1 && TALENT:연모 && (FLAG:커맨드확장 & 16384)
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 과시하듯이 눈앞에서 자위를 시작했다.
    PRINTFORMW 자위 행위로 흥분하고 있는 %조사선택(CALLNAME:PLAYER, "를")% 보며 %CALLNAME:TARGET%도 한층 더 격렬하게 욕정하고 있다….
;반발 Lv2 이상 소유이지만 욕정이 튀고 있는[해방][쾌감에 솔직]를 가지지 않는 비함락 노예 상대에게
ELSEIF MARK:반발각인 > 1 && PALAM:13 >= PALAMLV:3 && TALENT:해방 == 0 && TALENT:70 == 0 && TALENT:연모 == 0 && TALENT:복종 == 0 && TALENT:음란 == 0
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 과시하듯이 눈앞에서 자위를 시작했다.
    PRINTFORML 자위 행위로 흥분하고 있는 %조사선택(CALLNAME:PLAYER, "를")% 불쾌하게 보고 있는 %조사선택(CALLNAME:TARGET, "지만")%, 
    PRINTW 그 신체가 격렬히 욕정하고 있다는 것은 숨길 수 없었다….
;자위 중독 소유로[자위하기 쉬운]이 있어 공포 각인이 없는 경우
ELSEIF ABL:자위중독 > 0 && TALENT:60 && MARK:공포각인 <= 0
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 과시하듯이 눈앞에서 자위를 시작했다.
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 자위 행위로 흥분하고 있는 %조사선택(CALLNAME:PLAYER, "를")% 부러운 듯이 보고 있다….
;[정신 붕괴]하고 있는 경우
ELSEIF TALENT:정신붕괴
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 과시하듯이 눈앞에서 자위를 시작했다.
    PRINTFORMW 그러나, 자위 행위로 흥분하고 있는 %CALLNAME:PLAYER%에게 %조사선택(CALLNAME:TARGET, "는")% 흥미가 없는 것 같다….
;조교자가 대상의 진짜의 부모인 경우(2세대 이상전은 고려하지 않는다)
ELSEIF (NO:TARGET == 148 || NO:TARGET == 149) && (CFLAG:3105 == CFLAG:PLAYER:3109 || CFLAG:3107 == CFLAG:PLAYER:3109)
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 과시하듯이 눈앞에서 자위를 시작했다.
    PRINTFORMW 친부모의 자위행위를 보게 되어 %조사선택(CALLNAME:TARGET, "는")% 깊은 충격을 받으면서도 성적 흥분을 느끼고 있는 것 같다….
;그 외
ELSE
    PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 과시하듯이 눈앞에서 자위를 시작했다….
ENDIF
