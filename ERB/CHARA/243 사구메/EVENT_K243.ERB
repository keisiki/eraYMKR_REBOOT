;==================================================
;@KOJO_MESSAGE_COM 관계
;커맨드 대사 판정 처리
;커맨드 실행시에 출력됩니다
;==================================================
@KOJO_MESSAGE_COM_243
;자고 있을 경우에는 돌아온다
SIF EQUIP:14
    RETURN 0
;재갈이 있고, 재갈을 채우는 커맨드가 아닐 경우에도 돌아온다
SIF (TEQUIP:42 && (SELECTCOM != 226 && SELECTCOM != 229 && SELECTCOM != 506 && SELECTCOM != 509 && SELECTCOM != 583))
    RETURN 0
;기절 중에도 돌아온다
SIF TFLAG:899 >= 1
    RETURN 0

;커맨드 첫회 판정을 갱신
CALL COM_FLAG
;PANCTION 내의 장비품 대사 보완 함수를 사용한다
SIF FLAG:조교시텍스트표시 == 1
    TRYCALL EQUIPTEXT
;커맨드 대사 호출
TRYCALLFORM KOJO_243_TRAIN_MESSAGE_S_COM_{SELECTCOM}

;==================================================
;@SELF_KOJO 관계
;이벤트 대사 판정 처리
;SELF_KOJO 실행시에 출력됩니다
;==================================================
@SELF_KOJO_243
;자고 있을 경우에는 돌아온다
SIF EQUIP:14
    RETURN 0
;조교 중에다 재갈을 물렸을 때에도 돌아온다
SIF TFLAG:999 && TEQUIP:42
    RETURN 0
;조교 중에다 기절 중일 때에도 돌아온다
SIF TFLAG:999 && (TFLAG:899 >= 1 && TFLAG:200 != 12)
    RETURN 0

;이벤트 대사 호출
TRYCALLFORM K243_MESSAGE_EVENT_{(TFLAG:200)}

