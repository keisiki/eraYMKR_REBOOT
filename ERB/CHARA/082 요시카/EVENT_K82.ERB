;パラメータ変動後に口上を発動します
;========================================================
@KOJO_MESSAGE_PALAMCNG_82
;자고 있을 경우에는 돌아온다
SIF EQUIP:14
    RETURN 0
;재갈이 있을 경우에도 돌아온다
SIF TEQUIP:42
    RETURN 0
;기절 중에도 돌아온다
SIF TFLAG:899 >= 1 || TFLAG:898 >= 1
    RETURN 0
;정신붕괴の場合は戻る
SIF TALENT:167
    RETURN 0

;처녀喪失時のメッセージ呼び出し
SIF TFLAG:0
    TRYCALL K82_MESSAGE_LOST_VIRGIN
;初めてPALAMがLV2超えた時の口上呼び出し
FOR LOCAL, 0, 35
    SIF PALAM:LOCAL + UP:LOCAL > PALAMLV:2 && CFLAG:(5320 + LOCAL) == 0
        TRYCALLFORM K82_MESSAGE_OVERLV2_{LOCAL}
NEXT
;絶頂時の口上呼び出し
FOR LOCAL, 0, 50
    SIF NOWEX:LOCAL
        TRYCALLFORM K82_MESSAGE_ORGASM_{LOCAL}
NEXT
;射精させた
FOR LOCAL, 11, 29
    SIF TFLAG:LOCAL
        TRYCALLFORM K82_MESSAGE_EJAC_PLAYER_{LOCAL}
NEXT

;-------------------------------------------------
;처녀喪失
;-------------------------------------------------
@K82_MESSAGE_LOST_VIRGIN
;主人のペニス
IF TFLAG:2
    IF CFLAG:7001
        IF CFLAG:7018 >= 1
            PRINTFORM 「얻어맞는 거랑은 조금 다르게
            SETCOLOR 0xFF0000
            PRINTFORM %UNICODE(0x2665)%
            RESETCOLOR
            PRINTFORM 왠지 머리가 들썩들썩 거린다ー
            SETCOLOR 0xFF0000
            PRINTFORM %UNICODE(0x2665)%
            RESETCOLOR
            PRINTFORMW 」
        ELSEIF CFLAG:7018 == 0
            PRINTFORMW 「오？, 뭘 한거냐ー？」
        ENDIF
    ELSEIF TALENT:150 == 1
        PRINTFORM 「에헤헤ー
        SETCOLOR 0xFF0000
        PRINTFORM %UNICODE(0x2665)%
        RESETCOLOR
        PRINTFORM %CALLNAME:MASTER%에게 줘서 기쁘다ー
        SETCOLOR 0xFF0000
        PRINTFORM %UNICODE(0x2665)%
        RESETCOLOR
        PRINTFORML 」
        PRINTFORMW %CALLNAME:TARGET%는 처녀상실의 아픔에 눈물을 글썽이면서도 쾌활하게 %CALLNAME:MASTER%에게 입맞춤했다
    ELSE
        PRINTFORML 「그만두라고 했단거다ー…」
    ENDIF
;それ以外
ELSE
    IF CFLAG:7001 == 1
        PRINTFORML 「%조사처리(CALLNAME:MASTER,"가")%…좋았을텐데…」
        IF CFLAG:7018 >= 1
            PRINTFORMW %CALLNAME:TARGET%는 처녀상실의 아픔을 느끼지 못하면서도 눈물을 흘리고 있다…
        ELSE
            PRINTFORMW %CALLNAME:TARGET%는 처녀상실의 아픔을 느끼지 못하지만 눈물을 흘리고 있다…
        ENDIF
    ELSEIF TALENT:150 == 1
        PRINTFORMW 「싫어…이런거…」
    ELSE
        PRINTFORMW 「뭔가 했던건가ー？」
    ENDIF
ENDIF

