    PRINTFORMLC [3] \@(TALENT:MASTER:남자) ? 오라버니 # 누님\@
    PRINTFORMLC [4] \@(TALENT:MASTER:남자) ? 파파 # 마마\@
    PRINTLC [5] 서방님
    PRINTFORMLC [6] \@(TALENT:MASTER:남자) ? 아저씨 # 아줌마\@
    $INPUT_LOOP_YOBIKATA_2
    INPUT
    IF RESULT == 0
    CSTR:90 = 주인님
    ELSEIF RESULT == 1
    CSTR:90 = 마스터
    ELSEIF RESULT == 2
    IF TALENT:MASTER:남자
        CSTR:90 = 오빠
    ELSE
        CSTR:90 = 언니
    ENDIF
    ELSEIF RESULT == 3
    IF TALENT:MASTER:남자
        CSTR:90 = 오라버니
    ELSE
        CSTR:90 = 누님
    ENDIF    
    ELSEIF RESULT == 4
    IF TALENT:MASTER:남자
        CSTR:90 = 파파
    ELSE
        CSTR:90 = 마마
    ENDIF
    ELSEIF RESULT == 5
    CSTR:90 = 서방님
    ELSEIF RESULT == 6
    IF TALENT:MASTER:남자
        CSTR:90 = 아저씨
    ELSE
        CSTR:90 = 아줌마
    ENDIF
    ELSE
        GOTO INPUT_LOOP_YOBIKATA_2
    ENDIF
ELSE
    GOTO INPUT_LOOP_YOBIKATA_1
ENDIF

