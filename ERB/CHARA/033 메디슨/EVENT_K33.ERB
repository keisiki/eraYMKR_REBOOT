;-------------------------------------------------
@K33_SYOUKANNO_OTOMODACHI, ARG
;플래그 리셋트
CALLF K33_CFLAG75()
;지금까지 중복 세지 않고 선택한 회수를 리셋트
LOCAL:1 = 1
;ARG로 건네준 캐릭터는 선택이 끝난 상태로 한다
CALLF K33_CFLAG75(2, ARG)
$SET_LOOP
;적당하게 캐릭터 등록 번호를 취득
LOCAL = RAND:CHARANUM
;이미 선택한 캐릭터
IF K33_CFLAG75(1, LOCAL)
    ;돌아옵니다
    GOTO SET_LOOP
ELSE
    ;선택한 플래그를 세운다
    CALLF K33_CFLAG75(2, LOCAL)
    ;선택한 회수를 늘린다
    ++LOCAL:1
ENDIF
;선택한 캐릭터가 창관에 있을까?
IF CFLAG:LOCAL:4 & 1024
    ;플래그 리셋트 해 되돌린다
    CALLF K33_CFLAG75()
    RETURN LOCAL
ENDIF
;선택한 회수가 캐릭터수를 넘고 있다
IF LOCAL:1 >= CHARANUM
    ;플래그 리셋트 해 되돌린다
    CALLF K33_CFLAG75()
    RETURN -1
ELSE
    ;선택하는 여지가 있으므로 좀 더 노력한다
    GOTO SET_LOOP
ENDIF

