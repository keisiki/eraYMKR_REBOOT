    RETURN 0
SIF NO:TARGET != 59
    RETURN 0
CFLAG:5210 = 0
CFLAG:5211 = 0
;調教したフラグ
CFLAG:5207 = 0

;初期面識フラグの代入
;霊夢・魔理沙・キスメ・ヤマメ・勇儀
SIF CFLAG:5801 == 0
    CFLAG:5801 = 1
SIF CFLAG:5802 == 0
    CFLAG:5802 = 1
SIF CFLAG:5857 == 0
    CFLAG:5857 = 1
SIF CFLAG:5858 == 0
    CFLAG:5858 = 1
SIF CFLAG:5860 == 0
    CFLAG:5860 = 1

;--------------------------------------------------
;조수参加後用処理　CFLAG:5217を使用
;保存したキャラ番号をキャラ登録番号にコンバ―トする
;-------------------------------------------------

;前回の調教で口上主が조수をしていないとき, CFLAG:5217 = 0
