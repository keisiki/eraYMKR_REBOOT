        IF RESULT > 20
            PRINTFORML 「으-음……조금 기네요. 좀 더 짧게 할 수 없을까요?」
            GOTO INPUT_LOOP_NAME_EVENT_3
        ELSEIF RESULT == 0
            GOTO INPUT_LOOP_NAME_EVENT_3
        ELSE
            PRINTFORML 「%RESULTS%」……이렇게 부를까요?
            PRINTL [0] 네
            PRINTL [1] 아니오
            $INPUT_LOOP_NAME_EVENT_4
            INPUT
            SIF RESULT >= 2
                GOTO INPUT_LOOP_NAME_EVENT_4

            IF RESULT == 0
                PRINTFORMW 「알겠습니다. 앞으로도 잘 부탁해요, %RESULTS%」
                CSTR:90 = %RESULTS%
            ELSEIF RESULT == 1
                GOTO INPUT_LOOP_NAME_EVENT_2
            ENDIF
        ENDIF
    ENDIF
    RETURN 1

