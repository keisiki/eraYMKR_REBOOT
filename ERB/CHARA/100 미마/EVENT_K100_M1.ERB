;-------------------------------------------------
;※勝手にぱるコン！（仮名）ver. 0.95にて変換したファイルです※
;（RR1317系列用構文をYM用構文に置換したものです）
;-------------------------------------------------
;미마 조교시, 레이무 대사
;
;-------------------------------------------------
;--------------------------------------------------
;@KOJO_MESSAGE_COM 관계
;커맨드 실행시에 출력됩니다
;--------------------------------------------------
@LES_MESSAGE_COM_K100_M1
;-------------------------------------------------
;커맨드 실행시 대사 CFLAG:9000～9999를 사용(커맨드 번호＋9000)
;-------------------------------------------------
;-------------------------------------------------
;애무 CFLAG501
;-------------------------------------------------
IF SELECTCOM == 0
    ;처음
    IF CFLAG:5301 == 0
        ;음란
        IF TALENT:170 == 1
            PRINTFORMW 「레이무……」
            PRINTFORMW 미마는 레이무에게 몸을 맡기며 쾌락에 빠져들었다.
        ;굴복각인Lv2이상
        ELSEIF MARK:2 >= 2
            PRINTFORMW 「앗…… 응……」
            PRINTFORMW 『느끼고 있지 않아?』
        ;그 외
        ELSE
            PRINTFORMW 「레, 레이무!」
            PRINTFORMW 『뭐야, 하나하나 시끄럽게』
        ENDIF
            CFLAG:5301 = 1
            RETURN 0
    ;2회째 이후
    ELSE
        ;음란
        IF TALENT:170 == 1 && (CFLAG:5501 <= 4 || FLAG:11 == 2)
            PRINTFORMW 『어때, 기분 좋지?』
            PRINTFORMW 「아앗, 더 해줘……」
            CFLAG:5501 = 5
        ;굴복각인Lv3
        ELSEIF MARK:2 == 3 && (CFLAG:5501 <= 3 || FLAG:11 == 2)
            PRINTFORMW 「흑…… 앙!」
            PRINTFORMW 『후훗, 더 기분 좋게 해줄게』
            CFLAG:5501 = 4
        ;굴복각인Lv2
        ELSEIF MARK:2 == 2 && (CFLAG:5501 <= 2 || FLAG:11 == 2)
            PRINTFORMW 「흑……」
            PRINTFORMW 『어머, 느끼고 있는 모양이네』
            CFLAG:5501 = 3
        ;그 외
        ELSEIF MARK:2 <= 1 && (CFLAG:5501 <= 1 || FLAG:11 == 2)
            PRINTFORMW 「앗…… 흥……」
            PRINTFORMW 순간 교성이 울렸지만, 미마는 바로 입을 다물었다.
            CFLAG:5501 = 2
        ENDIF
            RETURN 0
    ENDIF
ENDIF

;-------------------------------------------------
;커닐링거스 CFLAG502
;이와시미즈 CFLAG503
;-------------------------------------------------
;커닐링거스
IF SELECTCOM == 1 && TEQUIP:56 == 0
    ;처음
    IF CFLAG:5302 == 0
        ;음란
        IF TALENT:170 == 1
            PRINTFORMW 「핫……으읏!」
            PRINTFORMW 『젖었네……읏、미마의 여기、끈적끈적해』
        ;굴복각인Lv2이상
        ELSEIF MARK:2 >= 2
            PRINTFORMW 「꽤나 능숙해졌네……」
            PRINTFORMW 『미마도、금방 이렇게 될거야』
        ;그 외
        ELSE
            PRINTFORMW 「그、그만……해！」
        ENDIF
            CFLAG:5302 = 1
            RETURN 0
    ;2회째 이후
    ELSE
        ;음란
        IF TALENT:170 == 1 && (CFLAG:5502 <= 3 || FLAG:11 == 2)
            PRINTFORMW 「앗, 아앗！」
            PRINTFORMW 『벌써、혼자서만 기분 좋아지면……』
            CFLAG:5502 = 4
        ;굴복각인Lv3
        ELSEIF MARK:2 == 3 && (CFLAG:5502 <= 2 || FLAG:11 == 2)
            PRINTFORMW 「읏！　레이무、거긴！　크읏！」
            CFLAG:5502 = 3
        ;그 외
        ELSEIF CFLAG:5502 <= 1 || FLAG:11 == 2
            PRINTFORMW 『읏……젖었네』
            PRINTFORMW 「레이무、그곳은……」
            CFLAG:5502 = 2
        ENDIF
            RETURN 0
    ENDIF
ENDIF
;이와시미즈
IF SELECTCOM == 611 || (SELECTCOM == 1 && TEQUIP:56)
    ;처음
    IF CFLAG:5303 == 0
        ;음란
        IF TALENT:170 == 1
            PRINTFORMW 『질척、질척……』
            PRINTFORMW 「하읏、하앗！」
            PRINTFORMW 쾌락의 음색이 울릴 때 마다、미마는 번민에 휩쌓였다.
        ;그 외
        ELSE
            PRINTFORMW 「앗、레이무！ 뭐야！」
            PRINTFORMW 『가만히 있어、엄청 기분 좋으니까……』
        ENDIF
            CFLAG:5303 = 1
            RETURN 0
    ;2회째 이후
    ELSE
        ;음란+노출벽Lv3이상+C감각Lv3이상
        IF TALENT:170 == 1 && ABL:노출증 >= 3 && ABL:C감각 >= 3 && (CFLAG:5503 <= 5 || FLAG:11 == 2)
            PRINTFORMW 『질척해……읏、대홍수네』
            PRINTFORMW 「핫、앗！　레이무、거기、기분 좋아！」
            CFLAG:5503 = 6
        ;음란
        ELSEIF TALENT:170 == 1 && (CFLAG:5503 <= 4 || FLAG:11 == 2)    
            PRINTFORMW 「조、좀더、안되！　너무 좋아、아앗！」
            PRINTFORMW 『이런、칠칠치 못하네……』
            CFLAG:5503 = 5
        ;굴복각인Lv3+노출벽Lv3이상+C감각Lv3이상
        ELSEIF MARK:2 == 3 && ABL:C감각 >= 3 && ABL:노출증 >= 3 && (CFLAG:5503 <= 3 || FLAG:11 == 2)
            PRINTFORMW 「하아、하아……읏！　……앗！」
            PRINTFORMW （이 이상은……견딜 수가 없어！）
            CFLAG:5503 = 4
        ;굴복각인Lv3
        ELSEIF MARK:2 == 3 && (CFLAG:5503 <= 2 || FLAG:11 == 2)
            PRINTFORMW 「하아、하아……읏！　……앗！」
            PRINTFORMW 소리가 울리는 것에 맞추어、미마의 신음 소리가 실내에 울렸다
            CFLAG:5503 = 3
        ;그 외
        ELSEIF CFLAG:5503 <= 1 || FLAG:11 == 2
            PRINTFORMW 『질척、질척……』
            PRINTFORMW 「앗！　응 앗！」
            CFLAG:5503 = 2
        ENDIF
            RETURN 0
    ENDIF
ENDIF

;-------------------------------------------------
;애널 애무 CFLAG504
;-------------------------------------------------
IF SELECTCOM == 2
    ;처음
    IF CFLAG:5304 == 0
        ;미경험
        IF EXP:1 == 1
            ;음란
            IF TALENT:170 == 1
                PRINTFORMW 「하읏……앗……하아……」
                PRINTFORMW 한순간 당혹감을 보였지만、미마는 바로 엉덩이를 흔들기 시작했다
            ;그 외
            ELSE
                PRINTFORMW 「레이무、거기는！」
                PRINTFORMW 『모르는거야？ 정말 기분 좋아』
            ENDIF
        ;경험 있음
        ELSE
            ;음고＋음란
            IF TALENT:173 == 1 && TALENT:170 == 1
                PRINTFORMW 「히얏！　손가락、좋아！」
                PRINTFORMW 『어머, 벌써 개발됬나?』
            ;음란
            ELSEIF TALENT:170 == 1
                PRINTFORMW 「하웃……레이무、거기는……」
                PRINTFORMW 『하？　너무 좋다고 말하고 싶니?』
            ;그 외
            ELSE
                PRINTFORMW 『이쪽의 상태는 어떨까?』
                PRINTFORMW 「큿！」
                PRINTFORMW 애널을 만지작거린 순간, 미마는 몸을 활처럼 뒤로 젖혔다
            ENDIF
        ENDIF
            CFLAG:5304 = 1
            RETURN 0
    ;2회째 이후
    ELSE
        P = PALAM:10 + UP:10
        ;음고＋음란
        IF TALENT:173 == 1 && TALENT:170 == 1 && (CFLAG:5504 <= 6 || FLAG:11 == 2)
            PRINTFORMW 「힛、아앗！ 가앗！」
            PRINTFORMW 미마는 칠칠치 못하게 군침을 늘어뜨리며、쾌락을 갈구했다
            CFLAG:5504 = 7
        ;음란＋A감각Lv3이상
        ELSEIF TALENT:170 == 1 && ABL:A감각 >= 3 && (CFLAG:5504 <= 5 || FLAG:11 == 2)
            PRINTFORMW 『이젠 헐렁헐렁하네』
            PRINTFORMW 「하앗……좀더……해줘……」
            CFLAG:5504 = 6
        ;음란+윤활Lv2이상
        ELSEIF TALENT:170 == 1 && P >= PALAMLV:2 && (CFLAG:5504 <= 4 || FLAG:11 == 2)
            PRINTFORMW 「핫、아앗……기분、좋아……」
            PRINTFORMW 『그렇지？　더 기분 좋게 해줄게』
            CFLAG:5504 = 5
        ;음란+윤활Lv2미만
        ELSEIF TALENT:170 == 1 && P < PALAMLV:2 && (CFLAG:5504 <= 3 || FLAG:11 == 2)
            PRINTFORMW 「핫、크읏……레이무……」
            PRINTFORMW 『좀 서둘렀던 걸까？』
            CFLAG:5504 = 4
        ;윤활Lv2이상＋A감각Lv3이상
        ELSEIF P >= PALAMLV:2 && ABL:A감각 >= 3 && (CFLAG:5504 <= 2 || FLAG:11 == 2)
            PRINTFORMW 『저기저기、기분 좋아졌지？』
            PRINTFORMW 「레이무、그만둬…」
            CFLAG:5504 = 3
        ;그 외
        ELSEIF CFLAG:5504 <= 1 || FLAG:11 == 2
            PRINTFORMW 「레이무、거기는、그만……크읏!」
            CFLAG:5504 = 2
        ENDIF
            RETURN 0
    ENDIF
ENDIF

