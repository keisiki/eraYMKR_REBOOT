;昼・夜が終わった時に出力
;--------------------------------------------------
@EVENTTURNEND
#PRI
;※추가 기능C1『금딸 기능』
;금딸 중に日付が変わった際に判定を行います. 

;하루갈이 금딸 판정
CALL SELFPLAY_3(64)
LOCAL = RESULT:2
SIF RESULT == 2
    RETURN 0
DRAWLINE

;テンプレ分岐
;我慢できなかった
IF RESULT == 1
    DRAWLINE
;    PRINTFORMW 심야, %NAME:(RESULT:2)%의 방…….
    WAIT
;拡張する場合, RAND:4にして↓のコメントアウトを外す
    H = RAND:2
    SIF H == 0
        CALL SELFPLAY_64_A
    SIF H == 1
        CALL SELFPLAY_64_B
;    SIF H == 2
;        CALL SELFPLAY_64_C
;    SIF H == 3
;        CALL SELFPLAY_64_D    
    CALL SELFPLAY_4(64, 1)
;我慢した
ELSE
    ;我慢口上呼び出し判定　補正0上限30
    H = 4
    CALL SELFPLAY(64, 0, 30)
    IF RESULT == 1
;    PRINTFORMW 심야, %NAME:(RESULT:2)%의 방…….
        WAIT
;拡張する場合, RAND:4にして↓のコメントアウトを外す
        H = RAND:2
        SIF H == 0
            CALL SELFPLAY_64_E
        SIF H == 1
            CALL SELFPLAY_64_F
;        SIF H == 2
;            CALL SELFPLAY_64_G
;        SIF H == 3
;            CALL SELFPLAY_64_H
    ENDIF
    A = 0
    SIF H == 4
        A = 3
    CALL SELFPLAY_4(64, A)
ENDIF
DRAWLINE
RETURN 0


;※추가 기능C3『대기 자위 판정』
;助手として見学中, プレイを見せられたときに判定を行います
@SELFPLAY_64_ROOT
;テンプレ分岐
;금딸 중・我慢できなかった
IF RESULT == 1 && CFLAG:ASSI:5208 >= 1
    DRAWLINE
    PRINTFORMW %조사처리(CALLNAME:TARGET,"와")%의 행위를 보고 있던 %CALLNAME:ASSI%의 상태가 이상하다……. 
;拡張する場合A = RAND:3に
    A = RAND:2
    SIF A == 0
        CALL SELFPLAY_64_1
    SIF A == 1
        CALL SELFPLAY_64_2
;拡張する場合↓二行のコメントアウトを外す
;    SIF A == 2
;        CALL SELFPLAY_64_3    
    CALL SELFPLAY_4(64, 1)
    DRAWLINE
;금딸なし・我慢できなかった（질투した）
;冗長なのでコメントアウト. 解放は任意で
ELSEIF RESULT == 1
    DRAWLINE
    PRINTFORMW %조사처리(CALLNAME:TARGET,"와")%의 행위를 보고 있던 %CALLNAME:ASSI%의 상태가 이상하다……. 
    A = RAND:2
    SIF A == 0
        CALL SELFPLAY_64_1
    SIF A == 1
        CALL SELFPLAY_64_2
;    SIF A == 2
;        CALL SELFPLAY_64_3
;    SIF A == 3 && TALENT:ASSI:150
;        CALL SELFPLAY_64_4
;    SIF A == 4 && TALENT:ASSI:150
;        CALL SELFPLAY_64_5
;    SIF A == 5 && TALENT:ASSI:150
;        CALL SELFPLAY_64_6
    DRAWLINE
ENDIF
RETURN 0

