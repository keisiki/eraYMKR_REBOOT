;-------------------------------------------------
;조교자의 호칭
;-------------------------------------------------
@K64_CALLNAME
#FUNCTIONS
;캐릭터 번호 0의 [남자]
IF NO:MASTER == 0  && TALENT:MASTER:120
    ;이름 변경 완료
    IF CFLAG:5154 == 1
        RETURNF @"%CALLNAME:MASTER%"
    ;상애
    ELSEIF TALENT:153
        RETURNF "서방님"
    ELSE
        ;사토리는 언니니까
        RETURNF "오빠"
    ENDIF
;사토리 or 캐릭터 번호 0의 여자
ELSEIF NO:MASTER == 61 || (NO:MASTER == 0 && TALENT:MASTER:120 == 0)
    ;주인의 번호가 0이고 이름 변경 완료
    IF NO:MASTER == 0 && CFLAG:5154 == 1
        RETURNF @"%CALLNAME:MASTER%"
    ELSE
        RETURNF "언니"
    ENDIF
;스와코
ELSEIF NO:MASTER == 44
    RETURNF "신님"
;그 외
ELSE
    RETURNF @"%CALLNAME:MASTER%"
ENDIF


;-------------------------------------------------
;조수의 호칭
;-------------------------------------------------
@K64_CALL_ASSI
#FUNCTIONS
IF ASSI >= 0
    ;캐릭터 번호의 0의 [남자](일단 없지만)
    IF ASSINO() == 0  && TALENT:ASSI:120
        RETURNF "오빠"
    ;사토리 or 캐릭터 번호 0의 여자(사실상 사토리뿐)
    ELSEIF ASSINO() == 61 || (ASSINO() == 0 && TALENT:ASSI:120 == 0)
        RETURNF "언니"
    ;스와코
    ELSEIF ASSINO() == 44
        RETURNF "신님"
    ;그 외
    ELSE
        RETURNF @"%CALLNAME:ASSI%"
    ENDIF
ENDIF

;-------------------------------------------------
;조교 대상의 호칭
;-------------------------------------------------
@K64_CALL_TARGET
#FUNCTIONS
IF TARGET >= 0
    ;캐릭터 번호의 0의 [남자](일단 없지만)
    IF NO:TARGET == 0  && TALENT:120
        RETURNF "오빠"
    ;사토리 or 캐릭터 번호 0의 여자(사실상 사토리뿐)
    ELSEIF NO:TARGET == 61 || (NO:TARGET == 0 && TALENT:120 == 0)
        RETURNF "언니"
    ;스와코
    ELSEIF ASSINO() == 44
        RETURNF "신님"
    ;그 외
    ELSE
        RETURNF @"%CALLNAME:TARGET%"
    ENDIF
ENDIF

