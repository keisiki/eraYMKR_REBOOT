;조교 대상을 바탕으로 되돌린다
TARGET = LOCAL

RETURN 0

;--------------------------------------------------
;@EVENTCOM 관계
;커맨드를 선택했을 때에 출력
;--------------------------------------------------
@EVENTCOM
#PRI
;준정액 경험
IF NO == 72 && CFLAG:5216 & 1p14 && CFLAG:5216 & 1p13
    CFLAG:5216 -= 1p13
;정액＆사정 경험이 0이면 정액 미경험 플래그를 세운다
ELSEIF NO == 72 && EXP:정액경험 == 0 && EXP:사정경험 == 0 && (CFLAG:5216 & 1p13) == 0 && (CFLAG:5216 & 1p14) == 0
    CFLAG:5216 |= 1p13
ENDIF

SIF NO == 72
    TRYCALL PLAYER_72

;※추가 기능 D2 「서포트 플레이 기능」
;조교 대상이 말 주요해, 조수가 있는 경우만 처리
SIF NO == 72 && ASSI >= 0
    TRYCALL COMBOPLAY_3
RETURN 0

@PLAYER_72
;조교자의 통칭 대입
IF ASSIPLAY
    LOCALS = %CALLNAME:ASSI%
ELSE
    LOCALS = %MASTERNAME%
ENDIF

;조르는 말리셋트
@EVENTCOMEND
SIF NO == 72
    TCVAR:300 = 0

;--------------------------------------------------
;@EVENTSHOP 관계
;숍을 열었을 때에 출력
;--------------------------------------------------
;※추가 기능 A5 「지문 플래그 관리 기능」
@EVENTSHOP
#PRI
;조교 대상이 말주의 경우만 처리
SIF TARGET < 0
    RETURN 0
IF NO == 72
    IF CFLAG:5221 & 1p8
        CFLAG:99 = 0
    ELSE
        CFLAG:99 = 1
    ENDIF
ENDIF
RETURN 0

