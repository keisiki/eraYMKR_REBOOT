;ENDIF
;IF ASSI > 0 && ASSIPLAY
;    IF NO:ASSI == Y1
;        CALL LES_MESSAGE_PALAMCNG_K52_MY1
;        RETURN 0
;    ENDIF
;ENDIF

;-------------------------------------------------
;ボ-ルギャグ,強制開口器用口上.
;-------------------------------------------------
IF TEQUIP:42 == 1 || TEQUIP:42 == 3
    ;セックス射精
    IF TFLAG:11 > 0

        ;ボ-ルギャグ用
        IF TEQUIP:42 == 1
            ;TRYCALL K52_BALLGAGED_SHOCK
        ;強制開口器用
        ELSEIF TEQUIP:42 == 3
            ;TRYCALL K52_ANGLEWIDER_SHOCK
        ENDIF

        PRINTFORML 
        
        ;大量射精
        IF TFLAG:11 == 2
            IF RAND:4 == 0
                PRINTFORMW %CALLNAME:TARGET%의 질내에 기세 좋게 대량의 정액을 사정했다.
                PRINTFORMW 질내에 전부 들어가지 못한 정액이 점점 흘러나오기 시작한다…
            ELSEIF RAND:3 == 0
                PRINTFORMW %CALLNAME:TARGET%의 질내에 대량의 정액을 토해냈다.
                PRINTFORMW %CALLNAME:PLAYER%의 육봉에서 정액이 나올 때 %CALLNAME:TARGET%가 전신을 떨었다…
            ELSEIF RAND:2 == 0
                PRINTFORMW 크게 사정함과 동시에 %CALLNAME:TARGET%의 배가 볼록하게 부풀어 올랐다.
                PRINTFORMW 배가 원래대로 돌아감과 동시에 결합부에서 힘차게 정액이 흘러나오기 시작했다…
            ELSE
                PRINTFORMW %조사처리(CALLNAME:PLAYER,"은")% %CALLNAME:TARGET%의 가장 안쪽에 대량의 정액을 토해냈다.
                PRINTFORMW 토해낸 정액이 질내 뿐만이 아니라 자궁까지 채우자 %CALLNAME:TARGET%가 몸을 크게 떨었다.
            ENDIF
        ;通常の射精
        ELSE
            IF RAND:4 == 0
                PRINTFORMW %조사처리(CALLNAME:PLAYER,"은")% %CALLNAME:TARGET%의 허리를 붙잡고 자궁구를 문지르며 사정했다.
            ELSEIF RAND:3 == 0
                PRINTFORMW %CALLNAME:PLAYER%의 육봉에서 정액이 나올 때마다 %CALLNAME:TARGET%는 움찔거리며 몸을 떨었다.
            ELSEIF RAND:2 == 0
                PRINTFORMW %조사처리(CALLNAME:PLAYER,"은")% 작은 신음과 함께 %CALLNAME:TARGET%의 질내에 정액을 토해냈다.
            ELSE
                PRINTFORMW %조사처리(CALLNAME:PLAYER,"은")% %CALLNAME:TARGET%의 허리를 잡아당겨 가장 안쪽에 정액을 사정했다.
            ENDIF
        ENDIF
    ;アナルで射精
    ELSEIF TFLAG:12 > 0
        
        ;ボ-ルギャグ用
        IF TEQUIP:42 == 1
            TRYCALL K52_BALLGAGED_SHOCK
        ;強制開口器用
        ELSEIF TEQUIP:42 == 3
            TRYCALL K52_ANGLEWIDER_SHOCK
        ENDIF
        
        PRINTFORML 
        
        IF ABL:A감각 >= 4
            IF RAND:4 == 0
                PRINTFORMW %조사처리(CALLNAME:PLAYER,"은")% 막대한 양의 정액을 %CALLNAME:TARGET%의 장내에 사정했다.
                PRINTFORMW %CALLNAME:TARGET%는 애널에 정액이 나오자 몸이 크게 뛰며 절정을 맞이했다.
                ;アイマスクされている場合
                IF TEQUIP:41 > 0
                    PRINTFORMW %CALLNAME:TARGET%는 사정이 끝났어도 허리를 흔들며 더욱 큰 쾌락을 탐내고 있다.
                ELSE
                    PRINTFORMW 끝나지 않는 절정에 눈동자가 흐려지면서도 쾌락을 더욱 추구하는 것처럼 허리를 계속 흔들고 있다.
                ENDIF
            ELSEIF RAND:3 == 0
                PRINTFORMW %조사처리(CALLNAME:PLAYER,"은")% 꿀렁거리는 소리와 함께 %CALLNAME:TARGET%의 장내에 대량의 정액을 토해냈다.
                ;アイマスクされている場合
                IF TEQUIP:41 > 0
                    PRINTFORMW 장내에 정액이 꽉 차자 %CALLNAME:TARGET%는 몸을 크게 떨면서 절정을 맞이했다.
                ELSE
                    PRINTFORMW 장내에 정액이 꽉 차자 %CALLNAME:TARGET%는 기쁜 듯한 눈을 하며 쾌락을 탐했다.
                ENDIF
                PRINTFORMW 그리고는 한층 더 큰 쾌락을 탐하는 것처럼 %CALLNAME:TARGET%의 장이 꿈틀거렸다.
            ELSEIF RAND:2 == 0
                PRINTFORMW %조사처리(CALLNAME:PLAYER,"은")% %CALLNAME:TARGET%의 장이 꿈틀대는 것에 맞춰서 사정을 반복했다.
                PRINTFORMW 그 때마다 %CALLNAME:TARGET%의 몸이 크게 떨리면서 쾌락을 탐하고 있다.
                PRINTFORMW 결합부에서 전부 들어가지 못한 정액이 뚝뚝 흘러 떨어지고 있다.
            ELSE
                ;アイマスクされている場合
                IF TEQUIP:41 > 0
                    PRINTFORMW %CALLNAME:TARGET%가 %CALLNAME:PLAYER%의 사정에 맞춰서 능숙하게 애널을 꽉 조였다.
                    PRINTFORMW 그 때마다 %조사처리(CALLNAME:PLAYER,"은")% 텐시의 장내에 대량의 정액을 토해냈다.
                    PRINTFORMW %CALLNAME:TARGET%도 정액이 나올 때마다 몸을 떨며 절정을 반복했다.
                ELSE
                    PRINTFORMW %CALLNAME:PLAYER%의 사정이 가까워지자 %CALLNAME:TARGET%는 미소를 지으며 애널을 단단히 조이기 시작했다.
                    PRINTFORMW %조사처리(CALLNAME:PLAYER,"은")% 그 강렬한 조임을 견디지 못하고 결국 %CALLNAME:TARGET%의 안에 대량의 정액을 토해냈다.
