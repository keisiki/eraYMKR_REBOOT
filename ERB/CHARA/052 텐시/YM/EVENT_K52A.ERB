;    ELSEIF SELECTCOM == 33
;    ELSEIF SELECTCOM == 34
;    ELSEIF SELECTCOM == 37
;    ELSEIF SELECTCOM == 38
;    ENDIF

ENDIF


@K52_ANALSEX_VOICE
;연모か복종か음란か음고の場合
IF TALENT:150 == 1 || TALENT:160 == 1 || TALENT:170 || TALENT:173 == 1

;    IF RAND:5 == 0
