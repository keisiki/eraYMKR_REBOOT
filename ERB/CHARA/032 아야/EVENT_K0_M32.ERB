SOURCE:0 += A
SOURCE:11 += B

EXP:PLAYER:V경험 += 1

IF TALENT:후타나리 || TALENT:후타나리 || TALENT:남자
    ;奴隷のＰ⇔調教者のＶの汚れが移動
    STAIN:자지 |= STAIN:PLAYER:보지
    STAIN:PLAYER:보지 |= STAIN:자지
ENDIF

IF TALENT:남자 == 0 && TALENT:PLAYER:남자 == 0
    PRINTFORML 레즈경험＋4
    EXP:레즈경험 += 4
ENDIF



;-------------------------------------------------
;同フェラ
;Pが立ってるとパイズリ
;-------------------------------------------------
@COMBOPLAY_F
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 0

;パイズリ
IF P
    ;ABL:기교をみる
    IF ABL:기교 == 0
        B = 100
    ELSEIF ABL:기교 == 1
        B = 210
    ELSEIF ABL:기교 == 2
        B = 350
    ELSEIF ABL:기교 == 3
        B = 500
    ELSEIF ABL:기교 == 4
        B = 600
    ELSE
        B = 800
    ENDIF
;フェラ
ELSE
    ;ABL:기교をみる
    IF ABL:기교 == 0
        B = 80
    ELSEIF ABL:기교 == 1
        B = 190
    ELSEIF ABL:기교 == 2
        B = 300
    ELSEIF ABL:기교 == 3
        B = 420
    ELSEIF ABL:기교 == 4
        B = 550
    ELSE
        B = 700
    ENDIF
ENDIF


;ABL:순종をみる
IF ABL:순종 == 0
    TIMES B , 0.80
ELSEIF ABL:순종 == 1
    TIMES B , 0.90
ELSEIF ABL:순종 == 2
    TIMES B , 1.00
ELSEIF ABL:순종 == 3
    TIMES B , 1.10
ELSEIF ABL:순종 == 4
    TIMES B , 1.20
ELSE
    TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:봉사정신 == 0
    TIMES B , 0.50
ELSEIF ABL:봉사정신 == 1
    TIMES B , 0.80
ELSEIF ABL:봉사정신 == 2
    TIMES B , 1.20
ELSEIF ABL:봉사정신 == 3
    TIMES B , 1.50
ELSEIF ABL:봉사정신 == 4
    TIMES B , 1.80
ELSE
    TIMES B , 2.40
ENDIF

;ABL:정액중독をみる
IF ABL:정액중독 == 0
    TIMES B , 1.00
ELSEIF ABL:정액중독 == 1
    TIMES B , 1.20
ELSEIF ABL:정액중독 == 2
    TIMES B , 1.30
ELSEIF ABL:정액중독 == 3
    TIMES B , 1.50
ELSEIF ABL:정액중독 == 4
    TIMES B , 1.70
ELSE
    TIMES B , 2.00
ENDIF

;TALENT:舌使いをみる
SIF TALENT:혀놀림
    TIMES B , 2.00

;TALENT:猫舌をみる
SIF TALENT:고양이혀
    TIMES B , 0.70

;プレイヤーのABL:C감각をみる
IF ABL:PLAYER:C감각 == 0
    TIMES B , 1.00
ELSEIF ABL:PLAYER:C감각 == 1
    TIMES B , 1.50
ELSEIF ABL:PLAYER:C감각 == 2
    TIMES B , 2.00
ELSEIF ABL:PLAYER:C감각 == 3
    TIMES B , 2.50
ELSEIF ABL:PLAYER:C감각 == 4
    TIMES B , 3.50
ELSE
    TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:후타나리 || TALENT:PLAYER:남자
    BASE:PLAYER:사정 += B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:자지 & 1
    Y += 1
;精液の汚れ
SIF STAIN:PLAYER:자지 & 4
    Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:자지 & 8
    Y += 7
SIF STAIN:PLAYER:자지 & 16
    Y += 15

SIF TALENT:냄새둔감
    Y /= 3
SIF TALENT:냄새민감
    Y *= 2

LOSEBASE:0 += 10
LOSEBASE:기력 += 150

SOURCE:12 = 1500
SOURCE:22 = 500

;上のほうで計算した汚れデータ
SOURCE:24 = Y*20 + 50

;ABL:봉사정신をみる
IF ABL:봉사정신 == 0
    A = 200
    B = 80
    TIMES SOURCE:24 , 4.00
ELSEIF ABL:봉사정신 == 1
    A = 240
    B = 150
    TIMES SOURCE:24 , 2.50
ELSEIF ABL:봉사정신 == 2
    A = 300
    B = 300
    TIMES SOURCE:24 , 1.50
ELSEIF ABL:봉사정신 == 3
    A = 330
    B = 450
    TIMES SOURCE:24 , 1.00
ELSEIF ABL:봉사정신 == 4
    A = 370
    B = 750
    TIMES SOURCE:24 , 0.50
ELSE
    A = 400
    B = 1000
    TIMES SOURCE:24 , 0.10
ENDIF


;ABL:기교をみる
IF ABL:기교 == 0
    TIMES A , 0.50
    TIMES B , 0.50
ELSEIF ABL:기교 == 1
    TIMES A , 0.80
    TIMES B , 0.80
ELSEIF ABL:기교 == 2
    TIMES A , 1.00
    TIMES B , 1.00
ELSEIF ABL:기교 == 3
    TIMES A , 1.20
    TIMES B , 1.20
ELSEIF ABL:기교 == 4
    TIMES A , 1.50
    TIMES B , 1.50
ELSE
    TIMES A , 2.00
    TIMES B , 2.00
ENDIF

SOURCE:11 += A
SOURCE:15 += B

IF P
    ;ABL:B감각をみる
    IF ABL:B감각 == 0
        A = 100
    ELSEIF ABL:B감각 == 1
        A = 200
    ELSEIF ABL:노출증== 2
        A = 400
    ELSEIF ABL:B감각 == 3
        A = 800
    ELSEIF ABL:B감각 == 4
        A = 1200
    ELSE
        A = 1500
    ENDIF
    
    IF TALENT:음유
        TIMES A , 1.20
    ENDIF
    IF TALENT:거유
        TIMES A , 1.20
    ENDIF
    IF TALENT:B민감
        TIMES A , 1.20
    ELSEIF TALENT:B둔감
        TIMES A , 0.70
    ENDIF
    
    SOURCE:17 += A
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
S = BASE:PLAYER:사정
EJAC = MAXBASE:PLAYER:사정

IF  S > EJAC * 2
    E = 2
ELSEIF S > EJAC
    E = 1
ELSE
    E = 0
ENDIF

IF E
;射精している
    TIMES SOURCE:11 , 3.00

    ;ABL:정액중독をみる
    IF ABL:정액중독 == 0
        SOURCE:20 = 0
        TIMES SOURCE:15 , 2.00
        TIMES SOURCE:13, 4.00
    ELSEIF ABL:정액중독 == 1
        SOURCE:20 = 500
        TIMES SOURCE:15 , 3.00
        TIMES SOURCE:13, 3.00
    ELSEIF ABL:정액중독 == 2
        SOURCE:20 = 1200
        TIMES SOURCE:15 , 4.00
        TIMES SOURCE:13, 2.50
    ELSEIF ABL:정액중독 == 3
        SOURCE:20 = 3000
        TIMES SOURCE:15 , 6.00
        TIMES SOURCE:13, 2.00
    ELSEIF ABL:정액중독 == 4
        SOURCE:20 = 6000
        TIMES SOURCE:15 , 9.00
        TIMES SOURCE:13, 1.50
    ELSE
        SOURCE:20 = 12000
        TIMES SOURCE:15 , 15.00
        TIMES SOURCE:13, 1.00
    ENDIF
ENDIF

;大量射精
IF E == 2
    TIMES SOURCE:20 , 2.00
    TIMES SOURCE:15 , 1.50

    EXP:PLAYER:사정경험 += 2
    EXP:정액경험 += 9
    PRINTL 대량사정
    PRINTL 정액경험＋９
    ;精液汚れ
    STAIN:PLAYER:자지 |= 4

    BASE:PLAYER:사정 -= EJAC*2
    SIF BASE:PLAYER:사정 >= EJAC
        BASE:PLAYER:사정 = EJAC-1

    ;口で射精させたフラグ
    TFLAG:11 = 2
;通常の射精
ELSEIF E == 1
    EXP:PLAYER:사정경험 += 1
    EXP:정액경험 += 3
    PRINTL 사정
    PRINTL 정액경험＋３
    ;精液汚れ
    STAIN:PLAYER:자지 |= 4

    BASE:PLAYER:사정 -= EJAC
    SIF BASE:PLAYER:사정 >= EJAC
        BASE:PLAYER:사정 = EJAC-1

    ;口で射精させたフラグ
    TFLAG:11 = 1
ENDIF

PRINTFORML 펠라경험＋１
EXP:펠라경험 += 1

;奴隷の口⇔調教者のＰの汚れが移動
STAIN:입 |= STAIN:PLAYER:자지
STAIN:PLAYER:자지 |= STAIN:입

IF P
    ;奴隷のＢ⇔調教者のＰの汚れが移動
    STAIN:가슴 |= STAIN:PLAYER:자지
    STAIN:PLAYER:자지 |= STAIN:가슴
    
    ;奴隷の口⇔調教者のＰの汚れが移動
    STAIN:입 |= STAIN:PLAYER:자지
    STAIN:PLAYER:자지 |= STAIN:입
ENDIF

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF ABL:봉사정신 >= 2 && ABL:기교 >= 2
    STAIN:PLAYER:자지 = 2
    SIF E >= 1
        TFLAG:21 = 1
ENDIF

IF TALENT:남자 == 0 && TALENT:PLAYER:남자 == 0
    PRINT 레즈경험
    PRINTL +7
    EXP:레즈경험 += 7
ELSEIF TALENT:남자 == 1 && TALENT:PLAYER:남자 == 1
    PRINT BL경험
    PRINTL +7
    EXP:BL경험 += 7
ENDIF

SIF ASSIPLAY == 0 && EXP:펠라경험 >= EXPLV:3
    TFLAG:50 += 1

;調教者がふたなり
IF TALENT:PLAYER:후타나리
    SOURCE:12 /= 2
ENDIF



