;コマンド実行時のセリフ CFLAG 8100～を使用
;-------------------------------------------------
;-------------------------------------------------
;愛撫    CFLAG:8200
;-------------------------------------------------
IF SELECTCOM == 0
    ;初めて
    IF CFLAG:8200 == 0
        ;恋慕
        IF TALENT:연모 == 1
            ;엑스트라 모드 선택 커맨드 [0] 선택
            IF CFLAG:8001 == 1
                PRINTFORMW 「후아…주인님의 손…커서, 따뜻해…」
                PRINTFORMW 「식으로써 이런 거, 분에 넘쳐서…기뻐요」
            ;엑스트라 모드 선택 커맨드 [1] 선택
            ELSEIF CFLAG:8001 == 2
                PRINTFORMW 「좋아…거기야…후앙」
                PRINTFORMW 「제법 잘 하게 됐잖아」
            ;その他の場合
            ELSE
            ENDIF
        ;屈服刻印Lv3
        ELSEIF MARK:굴복각인 == 3
            ;엑스트라 모드 선택 커맨드 [0] 선택
            IF CFLAG:8001 == 1
                PRINTFORMW 「읏…뭔가, 주인님의 손…」
                PRINTFORMW 「기분 좋아져버렸어요」
            ;엑스트라 모드 선택 커맨드 [1] 선택
            ELSEIF CFLAG:8001 == 2
                PRINTFORMW 「그렇게 만져지면…」
                PRINTFORMW 「많이, 느껴…버리지 않니…」
            ;その他の場合
            ELSE
            ENDIF
        ;その他の場合
        ELSE
            ;엑스트라 모드 선택 커맨드 [0] 선택
            IF CFLAG:8001 == 1
                PRINTFORMW 「우우우…」
                PRINTFORMW 「그런 곳, 만지며…언…」
            ;엑스트라 모드 선택 커맨드 [1] 선택
            ELSEIF CFLAG:8001 == 2
                PRINTFORMW 「그렇게 끈적끈적하게 들러붙지 맛!」
            ;その他の場合
            ELSE
            ENDIF
        ENDIF
        CFLAG:8200= 1
        RETURN 0
    ;２回目以降
    ELSE
        ;恋慕
        IF TALENT:연모 == 1 && (CFLAG:8200 <= 3|| FLAG:캐릭터대사표시 == 2)
            ;엑스트라 모드 선택 커맨드 [0] 선택
            IF CFLAG:8001 == 1
                ;恋慕
                IF TALENT:연모 == 1
                    ;乱数分岐:2
                    IF RAND:2 == 0
                        PRINTFORMW 「후아…주인님의 손…커서, 따뜻해…」
                        PRINTFORMW 「식으로써 이런 거, 분에 넘쳐서…기뻐요」
                    ;その他の場合
                    ELSE
                        PRINTFORMW 「상냥하게 당하는게, 사랑받고 있는것 같아서」
                        PRINTFORMW 「첸에게도, 유카리 님에게도, 함께 느끼게 해주고 싶을 정도」
                        PRINTFORMW 「하지만, 주인님은, 저만의 주인님이니까요」
                    ENDIF
                ;その他の場合
                ELSE
                ENDIF
            ;엑스트라 모드 선택 커맨드 [1] 선택
            ELSEIF CFLAG:8001 == 2
                ;乱数分岐:2
                IF RAND:2 == 0
                    PRINTFORMW 「좋아…거이야…후앙」
                    PRINTFORMW 「제법 잘 하게 됐잖아」
                ;その他の場合
                ELSE
                    PRINTFORMW 「좀 더, 아래를 괴롭히렴…그래, 응…」
                    PRINTFORMW 「후훗, 편리한 식이 되었구나」
                ENDIF
            ;その他の場合
            ELSE
            ENDIF
            CFLAG:8200 = 4
        ;屈服刻印Lv3
        ELSEIF MARK:굴복각인 == 3 && (CFLAG:8200 <= 2|| FLAG:캐릭터대사표시 == 2)
            ;엑스트라 모드 선택 커맨드 [0] 선택
            IF CFLAG:8001 == 1
                ;乱数分岐:2
                IF RAND:2 == 0
                    PRINTFORMW 「읏…뭔가, 주인님의 손…」
                    PRINTFORMW 「기분 좋아져버렸어요」
                ;その他の場合
                ELSE
                    PRINTFORMW 「좀 더, 만져도 좋다구요?」
                    PRINTFORMW 「전, 주인님의 식이니까」
                ENDIF
            ;엑스트라 모드 선택 커맨드 [1] 선택
            ELSEIF CFLAG:8001 == 2
                ;乱数分岐:2
                IF RAND:2 == 0
                    PRINTFORMW 「그렇게 만져지면…」
                    PRINTFORMW 「많이, 느껴…버리지 않니…」
                ;その他の場合
                ELSE
                    PRINTFORMW 「크으으…」
                    PRINTFORMW 「묘하게 능숙한게 반대로 마음에 안들엇」
                    PRINTFORMW 「내가…괴롭혀주고 싶었는데…」
                ENDIF
            ;その他の場合
            ELSE
            ENDIF
            CFLAG:8200 = 3
        ;その他の場合
        ELSE
            ;엑스트라 모드 선택 커맨드 [0] 선택
            IF CFLAG:8001 == 1
                ;乱数分岐:2
                IF RAND:2 == 0
                    PRINTFORMW 「우우우…」
                    PRINTFORMW 「그런 곳, 만지며…언…」
                ;その他の場合
                ELSE
                    PRINTFORMW 「아, 안돼요오, 주인님…이런 거…」
                ENDIF
            ;엑스트라 모드 선택 커맨드 [1] 선택
            ELSEIF CFLAG:8001 == 2
                ;乱数分岐:2
                IF RAND:2 == 0
                    PRINTFORMW 「그렇게 끈적끈적하게 달라붙지맛!」
                ;その他の場合
                ELSE
                    PRINTFORMW 「너무 우쭐대잖니…」
                ENDIF
            ;その他の場合
            ELSE
            ENDIF
            CFLAG:8200 = 2
        ENDIF
        RETURN 0
    ENDIF
ENDIF
