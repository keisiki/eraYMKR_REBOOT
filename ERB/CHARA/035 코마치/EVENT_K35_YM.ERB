;-------------------------------------------------
;애무    CFLAG:8200
;-------------------------------------------------
IF SELECTCOM == 0 || SELECTCOM == 606
    ;처음
    IF CFLAG:8200 == 0
        ;굴복각인Lv2이상
        IF MARK:2 >= 2
            PRINTFORMW 「……아팟!」
            PRINTFORMW 「가슴은 민감하니까, 부드럽게 만져줘……」
        ;그 외의 경우
        ELSE
            PRINTFORMW 「더러운 손으로 치근덕대지 마」
        ENDIF
        CFLAG:8200= 1
        RETURN 0
    ;2회째 이후
    ELSE
        ;연모
        IF TALENT:150 == 1
            PRINTFORMW 「아아……, 댁의 손, 따뜻해……」
            PRINTFORMW 「나의 몸, 전부……, 댁의 것으로 해줘……. 후아앗!」
            CFLAG:8200 = 4
        ;굴복각인Lv2이상
        ELSEIF MARK:2 >= 2
            PRINTFORMW 「별로……응……, 달라붙지, 말라고……」
            PRINTFORMW 코마치의 목소리에, 가끔 열띤 한숨이 섞인다. 
            CFLAG:8200 = 3
        ;그 외의 경우
        ELSE
            PRINTFORMW 「언제까지 더듬을 생각이야? 기분 나쁘게」
            CFLAG:8200 = 2
        ENDIF
        RETURN 0
    ENDIF
ENDIF
;-------------------------------------------------
;クンニ    CFLAG:8201
;-------------------------------------------------
IF SELECTCOM == 1
    ;처음
    IF CFLAG:8201 == 0
        ;굴복각인Lv2이상
        IF MARK:2 >= 2
            PRINTFORMW 「거기, 결코 깨끗한 곳이 아닌데?」
        ;그 외의 경우
        ELSE
            PRINTFORMW 「비참한 모습이네. 댁의 부모님이 불쌍한걸」
        ENDIF
        CFLAG:8201= 1
        RETURN 0
    ;2회째 이후
    ELSE
        ;연모
        IF TALENT:150 == 1
            PRINTFORMW 「앗, 하아……. 혀로 할짝할짝대는 거, 좋아……」
            PRINTFORMW 코마치는 %CALLNAME:MASTER%의 혀가 가져오는 쾌락에 만취해있다. 
            CFLAG:8201 = 4
        ;굴복각인Lv2이상
        ELSEIF MARK:2 >= 2
            PRINTFORMW 「응읏! 그렇게 핥아대면……, 햐아아!」
            PRINTFORMW 튀어나온 클리토리스를 자극할 때마다, 코마치의 보지가 안타까운 듯 여닫혔다. 
            CFLAG:8201 = 3
        ;그 외의 경우
        ELSE
            PRINTFORMW 「이대로 소변이라도 갈겨주길 바래?」
            PRINTFORMW 「오히려 즐거워하려나, 댁 같은 변태는」
            CFLAG:8201 = 2
        ENDIF
        RETURN 0
    ENDIF
