@CHECK_BOOK, ARG
;ARG:0 = 책의 번호
;체크 성공하면 RETURN 1

SELECTCASE ARG
    ;100 이상은 각인
    CASE 100 TO 102, 104
        LOCAL = ARG - 100
        SIF MARK:LOCAL >= 2 ;다 읽은 책을 던진다
            RETURN 1
    
    ;100 미만은 ABL
    ;순종, 욕망, 기교, CVAB감각 Lv3 이상
    CASE 0 TO 3, 10 TO 12
        SIF ABL:ARG >= 3
            RETURN 1
    ;노출, 봉사, 마조끼 Lv2 이상
    CASE 3, 13, 14, 21
        SIF ABL:ARG >= 2
            RETURN 1
    ;레즈끼, 자위정액레즈BL중독 Lv1 이상
    CASE 22, 30 TO 33
        SIF ABL:ARG >= 1
            RETURN 1
ENDSELECT
