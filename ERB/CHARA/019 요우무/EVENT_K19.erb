        ELSEIF CFLAG:9030 <= 1 || FLAG:11 == 2
            PRINTFORML 「아파아파아파!」
            PRINTFORMW 「빨리 빼내 주세요!」
            CFLAG:9030 = 2
        ENDIF
            RETURN 0
    ENDIF
