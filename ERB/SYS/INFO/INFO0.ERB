;98,主人調教経験　99,助手経験　　　
@PRINT_EXP_COMMON, ARG
CALL STATUS_VIRGIN, ARG
LOCAL:1 = 0
REPEAT 54 ;CSV에 존재하는 경험치 종류 = 56, 그중 이상경험과 레즈(또는 BL)제외
    LOCAL = -1
    SELECTCASE COUNT
        CASE 0 TO 8 ;EXP0~8
            LOCAL = COUNT
        CASE 9, 10 ;EXP10~11
            LOCAL = COUNT + 1
        CASE 11 ;EXP12~13
            LOCAL = 12 + TALENT:ARG:남자 ;(레즈orBL)경험 표시
        CASE 12 ;EXP20~23
            LOCAL = 14
        CASE 13 TO 16 ;EXP20~23
            LOCAL = COUNT +7
        CASE 17 TO 22 ;EXP30~35
            LOCAL = COUNT + 13
        CASE 23 TO 31 ;EXP39~47
            LOCAL = COUNT + 16
        CASE 32 TO 36 ;EXP51~55
            LOCAL = COUNT + 19
        CASE 37 ;EXP:동물성애경험
            SIF FLAG:커맨드확장 & 8192 ;수간 컨피그 켜졌을 때만 표시
                LOCAL = 56
        CASE 38 TO 46  ;EXP70~78
            LOCAL = COUNT + 32
        CASE 47 TO 50 ;EXP80~83
            LOCAL = COUNT + 33
        CASE 51 TO 53 ;EXP97~99
            LOCAL = COUNT + 46
    ENDSELECT
    IF LOCAL < 0
        CONTINUE
    ELSEIF EXP:ARG:LOCAL > 0
        IF FLAG:변동경험강조패치 & 1 && ((LOCAL == 0 && CFLAG:ARG:11 & 1) || (LOCAL == 1 && CFLAG:ARG:11 & 2) || (LOCAL == 50 && CFLAG:ARG:11 & 4)|| (LOCAL == 12 && EXP:ARG:LOCAL > 999 && TALENT:MASTER:남자 && CFLAG:ARG:18 == 0) || (LOCAL == 13 && EXP:ARG:LOCAL > 999 && TALENT:MASTER:남자 == 0 && CFLAG:ARG:18 == 0))
            FONTBOLD
            PRINTFORM %EXPNAME:LOCAL,12%:\@(EXP:ARG:LOCAL > 99999999) ? 99999999 # {EXP:ARG:LOCAL, 8}\@
            FONTREGULAR
        ELSE
            PRINTFORM %EXPNAME:LOCAL,12%:\@(EXP:ARG:LOCAL > 99999999) ? 99999999 # {EXP:ARG:LOCAL, 8}\@
        ENDIF
        LOCAL:1 += 1
        ;改行するのか？ 続けて出力するのか？
        IF LOCAL:1%3 == 0
            PRINTL 
        ELSE
            PRINT 　
        ENDIF
    ENDIF
REND
SIF LOCAL:1%3 != 0
    PRINTL 

