;--------------------------------------------------
;이 파일 내에는 소질에 대한 설명과, 
;소질 서치 시스템이 있습니다.
;--------------------------------------------------
;=============================================================================
;용어사전――소질
;=============================================================================
;--------------------------------------------------
;소질 해설 메인 페이지
;--------------------------------------------------
@DICTIONARY_TALENT_MAIN
VARSET LOCAL, 0
REPEAT 1000
    SIF STRLENS(TALENTNAME:COUNT) <= 0
        CONTINUE

    LOCAL:2 += 1
    ;최대 페이지 수를 가리킨다
    SIF (LOCAL:2 % 60) == 1 && LOCAL:2 > 60
        LOCAL:1 += 1
REND
$PRINT_LIST
DRAWLINE
PRINTFORML 「소질」에 관한 대략적인 설명입니다. ＜page.{LOCAL+1}/{LOCAL:1+1}＞
DRAWLINE
;각 페이지를 표시
LOCAL:3 = 0
REPEAT 1000
    ;존재하지 않는 소질은 제외
    SIF STRLENS(TALENTNAME:COUNT) == 0
        CONTINUE
    ;어디에서 어디까지를 진열하는가
    LOCAL:3 += 1
    SIF LOCAL:3 <= (LOCAL + 1) * 60 && LOCAL:3 > LOCAL * 60
        PRINTFORMLC [{COUNT, 3}] %TALENTNAME:COUNT%
REND
PRINTL 
DRAWLINE
PRINTFORMLC \@(LOCAL <= 0) ? %" " * 16% # [1001] 앞페이지\@
PRINTLC [1000] 돌아가기
PRINTFORMLC \@(LOCAL >= LOCAL:1) ? %" " * 16% # [1009] 다음페이지\@

$INPUT_LOOP
INPUT
IF RESULT == 1000
    RETURN 0
ELSEIF RESULT == 1001 && LOCAL > 0
    LOCAL -= 1
ELSEIF RESULT == 1009 && LOCAL < LOCAL:1
    LOCAL += 1
ELSEIF RESULT < 0 || RESULT > 1000 || STRLENS(TALENTNAME:RESULT) == 0
    GOTO INPUT_LOOP
ELSE
    PRINTFORML 【%TALENTNAME:RESULT%】
    TRYCALLFORM DICTIONARY_TALENT_{RESULT}
ENDIF
GOTO PRINT_LIST

