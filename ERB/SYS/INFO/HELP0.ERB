;용어 사전――캐릭터
;=============================================================================
;--------------------------------------------------
;캐릭터 해설 메인 페이지
;--------------------------------------------------
@DICTIONARY_CHARA_MAIN
;변수의 리셋트
CALLF CLEAR_LIST
VARSET LOCAL, 0

TFLAG:206 = 0

REPEAT 500
    STRLENS ITEMNAME:(COUNT + 100)
    SIF RESULT < 1
        CONTINUE
    ;타이틀에서 진입했을 경우 IRC 캐릭터는 표시하지 않음.
    SIF COUNT > 299 && FLAG:999 == 1
        CONTINUE
    ;디버그 모드나 IRC 캐릭터 해금이 아니면 IRC 캐릭터는 표시되지 않는다
    SIF COUNT > 299 && NO:MASTER != 400 && TALENT:MASTER:지옥의훈장 == 0 && TALENT:MASTER:998 == 0
        CONTINUE
    CALLF SET_LIST, COUNT, 1
    LOCAL:3 += 1
    SIF (LOCAL:3%60) == 1 && LOCAL:3 > 60
        LOCAL:1 += 1
REND
SIF LOCAL:1 < 0
    LOCAL:1 = 0

;페이지를 넘겼을 때 등은 여기로부터
$PRINT_LIST_PAGE
DRAWLINE
PRINTFORML 「캐릭터」에 관한 대략적인 설명입니다.  ＜page. {LOCAL+1}/{LOCAL:1+1}＞
DRAWLINE
LOCAL:2 = 0
REPEAT 500
    ;(표시 가부)(이)가 0의 경우는 제외
    SIF GET_LIST(COUNT) == 0
        CONTINUE
    ;어디에서 어디까지를 진열할까
    LOCAL:2 += 1
    SIF LOCAL:2 > (LOCAL * 60) && LOCAL:2 <= ((LOCAL + 1) * 60)
        PRINTFORMLC [{COUNT, 3}]%ITEMNAME:(COUNT+100)%
REND
PRINTL 
DRAWLINE
PRINTFORMLC \@(LOCAL <= 0) ?  %" " * 16% # [1001] 앞페이지\@
PRINTLC [1000] 돌아가기
PRINTFORMLC \@(LOCAL >= LOCAL:1) ?  %" " * 16% # [1009] 다음페이지\@

$INPUT_LOOP
INPUT
IF RESULT == 1000
    RETURN 0
ELSEIF RESULT == 1001 && LOCAL > 0
    LOCAL -= 1
ELSEIF RESULT == 1009 && LOCAL < LOCAL:1
    LOCAL += 1
ELSEIF RESULT < 0 || RESULT > 500 || GET_LIST(RESULT) == 0
    GOTO INPUT_LOOP
ELSE
    TFLAG:201 = RESULT
    TFLAG:206 = 1
    DRAWLINE
    PRINT 캐릭터 번호 0
    ;캐릭터 해설 본체의 함수에
    CALL CHARA_PROFILE
    WAIT
    TFLAG:206 = 0
ENDIF
GOTO PRINT_LIST_PAGE

