        ENDIF
    ;体力減少
    ELSE
        PRINTFORM %조사처리(CALLNAME:MASTER,"는")% 체력이 
        SIF LOCAL >= 4
            PRINT 크게 
        PRINTW 소모되었다.
        BASE:MASTER:체력 -= 200 * (LOCAL:2/3)
    ENDIF
    IF NO:TARGET == 33
        BASE:MASTER:체력 /= 2
        BASE:MASTER:체력 -= 75 * (RAND:5 + 1)
    ELSE
        BASE:MASTER:체력 -= 75 * (RAND:5 + 1)
    ENDIF
    ;ダメージを与えたフラグON
    CFLAG:150 |= 4
    CFLAG:MASTER:150 |= 16
    IF CFLAG:65
        PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 해냈다는 듯한 눈으로 %조사처리(CALLNAME:MASTER,"를")% 보고 있다.
        CFLAG:65 -= 150
    ENDIF

    ;主人の死亡チェック(EASYでないとき主人が調合知識を持たず、毒を盛った奴隷が[弱味]持ちなおかつ恐怖と反発持ちで一定の確率で主人死亡)
    SIF FLAG:난이도 != 1 && TALENT:MASTER:조합지식 == 0 && TALENT:약점 && MARK:공포각인 && MARK:반발각인 && RAND:20 == 0
