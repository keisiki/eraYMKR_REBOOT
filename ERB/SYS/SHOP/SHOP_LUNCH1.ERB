    ;魅力減少
    ELSEIF LOCAL:1 == 5 && (TALENT:MASTER:90 || TALENT:MASTER:91 || TALENT:MASTER:92 || TALENT:MASTER:94 || TALENT:MASTER:98)
        IF TALENT:MASTER:98
