                    ELSEIF TALENT:(LOCAL:7):92
                        PRINTFORMW %조사처리(CALLNAME:(LOCAL:7),"는")% [수수께끼의 매력]을 잃었다.
                        TALENT:(LOCAL:7):92 = 0
                    ELSE
                        PRINTFORMW %조사처리(CALLNAME:(LOCAL:7),"는")% [매혹]을 잃었다.
                        TALENT:(LOCAL:7):91 = 0
                    ENDIF
                ;技能低下
                ELSEIF LOCAL:1 == 6 && ABL:(LOCAL:7):기교 > 0
