IF TALENT:167 || TALENT:168 || TALENT:169
    LOCAL = 0
ELSEIF TALENT:157 && TALENT:158
    LOCAL = RAND:8
ELSEIF TALENT:157
    LOCAL = RAND:4
ELSEIF CFLAG:30 >= 20
    LOCAL = RAND:3
ELSE
    LOCAL = 2
    LOCAL -= RAND:4
ENDIF

IF LOCAL > 1
    CALL LUNCH_CHOCOLATE
ELSE
    CALL LUNCH_SALE
ENDIF

