                    TALENT:(LOCAL:7):금단의지식 = 0
                ;サド消滅
                ELSEIF LOCAL:1 == 1 && TALENT:(LOCAL:7):81
                    PRINTFORMW %조사처리(CALLNAME:(LOCAL:7),"는")% [새드]를 잃었다.
                    TALENT:(LOCAL:7):81 = 0
                ;汚れ無視消滅
                ELSEIF LOCAL:1 == 3 && TALENT:(LOCAL:7):64
