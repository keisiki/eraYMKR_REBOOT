;工作技能Lv1はここで取得(条件：工作技能Lv0状態で工作経験が上昇している)
IF ABL:(ARG:0):공작기능 == 0 && LOCAL@VALUE_WORKSHOP_EXP_74:(ARG:0) > 0
    IF ARG:2 == 1
        LOCALS = %ABLNAME:73%\@(ARG != TARGET) ? (%CALLNAME:ARG%) # \@＋1
        ABL:(ARG:0):공작기능++
    ELSEIF ARG:2 == 2
        LOCALS = %ABLNAME:73%＋1
        ABL:(ARG:0):공작기능++
    ELSE
        LOCALS = %ABLNAME:73%(%CALLNAME:ARG%)＋1
        ABL:(ARG:0):공작기능++
    ENDIF
    IF ARG:1 == 0
        PRINTFORML %LOCALS%
    ELSEIF ARG:1 == 1
        PRINTFORMW %LOCALS%
    ENDIF
ENDIF
RETURN 0

