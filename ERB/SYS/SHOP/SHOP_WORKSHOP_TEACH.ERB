                    IF LOCAL@VALUE_WORKSHOP_EXP_74:(LOCAL:1) > 0
                        ;技能習得していれば授業終了、待機に戻る
                        CFLAG:(LOCAL:1):690 = 0
                    ELSE
                        ;作業内容を戻しておく
                        CFLAG:(LOCAL:1):690 = 2
                    ENDIF
                CASEELSE
                    CONTINUE
            ENDSELECT
            PRINTW 
