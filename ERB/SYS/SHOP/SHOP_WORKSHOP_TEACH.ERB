            SIF RESULT == -1
                TRYCALLFORM KOJO_WORKSHOP_XX, CFLAG:(LOCAL:0):690, LOCAL:0
            PRINTW 
            ;先生のパラメ表示
            PRINTFORML %CALLNAME:(LOCAL:0)% 수업
            CALL VALUE_WORKSHOP_SUM, LOCAL:0, 0, 2
