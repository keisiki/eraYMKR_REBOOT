    ;전 스킬 5렙으로
    REPEAT 15
        SIF COUNT >= 4 && COUNT < 10
            CONTINUE
        ABL:(LOCAL:1):COUNT = 5
    REND

    ;전 경험 5000회로
    REPEAT 100
        SIF COUNT == 9
            CONTINUE
        SIF COUNT >= 15 && COUNT < 20
            CONTINUE
        SIF COUNT >= 24 && COUNT < 30
            CONTINUE
        SIF COUNT >= 36 && COUNT < 39
            CONTINUE
        SIF COUNT >= 48 && COUNT < 50
            CONTINUE
        SIF COUNT >= 57 && COUNT < 70
            CONTINUE
        SIF COUNT >= 79 && COUNT < 80
            CONTINUE
        SIF COUNT >= 84 && COUNT < 97
            CONTINUE
        EXP:(LOCAL:1):COUNT = 5000
    REND

    ; 봉래부활횟수, 주정 부활횟수
;    CFLAG:(LOCAL:1):163 = 1
;    CFLAG:(LOCAL:1):164 = 1

    ;음란 소질 부여
    TALENT:(LOCAL:1):170 = 1

    ;전 스킬 5렙으로
    REPEAT 99
        SIF COUNT >= 4 && COUNT < 10
            CONTINUE
        SIF COUNT >= 15 && COUNT < 20
            CONTINUE
        SIF COUNT >= 24 && COUNT < 30
            CONTINUE
        SIF COUNT >= 38 && COUNT < 70
            CONTINUE
        SIF COUNT >= 74 && COUNT < 98
            CONTINUE
        ABL:(LOCAL:1):COUNT = 5
    REND
    ;각인 3렙으로
    MARK:(LOCAL:1):고통각인 = 3
    MARK:(LOCAL:1):쾌락각인 = 3
    MARK:(LOCAL:1):굴복각인 = 3
    MARK:(LOCAL:1):약물각인 = 3
    MARK:(LOCAL:1):치욕각인 = 3
    MARK:(LOCAL:1):5 = 3
    MARK:(LOCAL:1):공포각인 = 3
    MARK:(LOCAL:1):불사각인 = 3
ENDIF

;[処女]や[オトコ]の場合, Ｖ感覚は0として扱う
SIF TALENT:(LOCAL:1):처녀 || TALENT:(LOCAL:1):120
    ABL:(LOCAL:1):V감각 = 0
;[抑圧]は消滅して[解放]に変化, [抵抗]は消滅したと仮定
IF TALENT:(LOCAL:1):억압
    TALENT:(LOCAL:1):억압 = 0
    TALENT:(LOCAL:1):해방 = 1
    CFLAG:(LOCAL:1):13 |= 1
    CFLAG:(LOCAL:1):12 |= 65536
ENDIF
IF TALENT:(LOCAL:1):저항
    TALENT:(LOCAL:1):저항 = 0
    CFLAG:(LOCAL:1):13 |= 2
ENDIF


