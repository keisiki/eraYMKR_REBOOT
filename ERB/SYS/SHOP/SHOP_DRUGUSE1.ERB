        ELSE
            PRINTFORMW %조사처리(CALLNAME:ARG,"는")% 시원하다는 표정을 짓고 있다…….
        ENDIF
    ENDIF
    TALENT:ARG:후타나리 = 0
    CALL CHANGE_FUTA_COMMON, ARG
    CALL SELF_KOJO, ARG, 311, 1
