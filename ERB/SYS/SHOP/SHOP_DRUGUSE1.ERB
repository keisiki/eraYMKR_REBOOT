    TIMES LOCAL , 0.20
    LOCAL:1 = 3000
ELSEIF EXP:ARG:V경험 < EXPLV:2
    TIMES LOCAL , 0.60
    LOCAL:1 = 2000
ELSEIF EXP:ARG:V경험 < EXPLV:3
