;증가하는 주라든지 경험의 처리 
;윤간한 옆
;102한사람의 토탈
;103한사람의 C절(삽입 or짝 맞추기 놀이)
;104한사람의 V절
;105한사람의 A절
;106한사람의 사정량
;107C절 시킨 회수
;108V절 시킨 회수
;109A절 시킨 회수
;110B절 시킨 회수
;111정액 경험
;112페라 경험
;113사정시킨 합계 회수
;114V사용 회수
;115A사용 회수
;116입사용 회수

;윤간되는 측
;117합계 C절
;118합계 V절
;119합계 A절
;120합계 B절
;121합계 정액 회수
;123합계 사정
;124합계 V사용
;125합계 A사용
;126합계구 사용


;만드는 것을  잊어 나중에 첨가해
;130참가자 욕정 정도
;131대상 욕정 정도
;132참가자 V사용 회수

;조교 대상이 얼마나 욕정하고 있을까의 초기치를 결정한다(여기서 제로가 되면(자) 이제 어떻게 하지도 없는, 초기치 300)
LOCAL:131 = 300

;대상의 욕망이 낮다
IF ABL:TARGET:욕망 == 0
    LOCAL:131 -= 100
ELSEIF ABL:TARGET:욕망 <= 1
    LOCAL:131 -= 70
ELSEIF ABL:TARGET:욕망 <= 2
    LOCAL:131 -= 50
ELSEIF ABL:TARGET:욕망 <= 3
    LOCAL:131 -= 30
ENDIF
;대상의 순종이 낮다
IF ABL:TARGET:순종 == 0
    LOCAL:131 -= 100
ELSEIF ABL:TARGET:순종 <= 1
    LOCAL:131 -= 70
ELSEIF ABL:TARGET:순종 <= 2
    LOCAL:131 -= 50
ELSEIF ABL:TARGET:순종 <= 3
    LOCAL:131 -= 30
ENDIF

;대상이 반항적
SIF TALENT:TARGET:반항적 == 1
LOCAL:131 -= 20
;대상이 거만함
SIF TALENT:TARGET:건방짐 == 1
LOCAL:131 -= 20
;대상이 무관심
SIF TALENT:TARGET:무관심 == 1
LOCAL:131 -= 20
;대상이 감정 부족하다
SIF TALENT:TARGET:감정결여 == 1
LOCAL:131 -= 20
;대상이 정조 관념 소유
SIF TALENT:TARGET:정조관념 == 1
LOCAL:131 -= 50
;대상이 억압 소유
SIF TALENT:TARGET:억압 == 1
LOCAL:131 -= 30
;대상이 저항
SIF TALENT:TARGET:해방 == 1
LOCAL:131 -= 20
;대상이 여자 증오
SIF TALENT:TARGET:여성혐오 == 1
LOCAL:131 -= 40

LOCAL:131 = LOCAL:131 * LOCAL:501 * LOCAL:500

;윤간측의 판정 개시
REPEAT CHARANUM
;이 IF로 참가하지 않는 캐릭터를 튕기는 즉 여기서 당신으로 여겨지는 측이 튕겨지고 있다
IF LOCAL:COUNT >= 1
    ;윤간 내용으로 뒤죽박죽 분기
    IF LOCAL:COUNT >= 70
        ;우선은 참가자 V경험을＋1
        LOCAL:132 = 1
        ;참가자의 V감각의 10배와 욕망의 5배와 V경험을 더해 200이상이라면 참가자 V절정 4.5회(10배가 되어 내 마지막에 나눈다)
        IF (ABL:COUNT:V감각 * 10) + (ABL:COUNT:욕망 * 5) + ABL:COUNT:C감각 >= 200
            LOCAL:104 = 45
        ;140이상이라면 참가자 V절정 3.6회
        ELSEIF  (ABL:COUNT:V감각 * 10) + (ABL:COUNT:욕망 * 5) + ABL:COUNT:C감각 >= 140
            LOCAL:104 = 36
        ;100이상이라면 참가자 V절정 2.7회
        ELSEIF  (ABL:COUNT:V감각 * 10) + (ABL:COUNT:욕망 * 5) + ABL:COUNT:C감각 >= 100
            LOCAL:104 = 27
        ;60이상이라면 참가자 V절정 1.8회
        ELSEIF  (ABL:COUNT:V감각 * 10) + (ABL:COUNT:욕망 * 5) + ABL:COUNT:C감각 >= 60
            LOCAL:104 = 18
        ;25이상이라면 참가자 V절정 1회
        ELSEIF  (ABL:COUNT:V감각 * 10) + (ABL:COUNT:욕망 * 5) + ABL:COUNT:C감각 >= 25
            LOCAL:104 = 10
        ;그 이외는 제로
        ELSE
            LOCAL:104 = 0
        ENDIF
        ;조교 대상의 C감각과 욕망과 참가자의 기교로 사정의 유무의 결정(전부 4이상으로 대량 사정 판정)
        IF ABL:TARGET:C감각 + ABL:TARGET:욕망 + ABL:COUNT:기교 >= 12
            LOCAL:107 = 10
            LOCAL:111 = 2
            LOCAL:113 = 10
        ELSEIF ABL:TARGET:C감각 + ABL:TARGET:욕망 + ABL:COUNT:기교 >= 6
            LOCAL:107 = 10
            LOCAL:111 = 1
            LOCAL:113 = 10
        ENDIF
    ELSEIF LOCAL:COUNT >= 60
        ;우선은 페라 경험을＋1
        LOCAL:112 = 1        
        ;조교 대상의 C감각과 욕망과 참가자의 기교의 합계로 판정
        IF ABL:TARGET:C감각 + ABL:TARGET:욕망 + ABL:COUNT:기교 >= 12
            LOCAL:107 = 10
            LOCAL:111 = 9
            LOCAL:113 = 10
        ELSEIF ABL:TARGET:C감각 + ABL:TARGET:욕망 + ABL:COUNT:기교 >= 4
            LOCAL:107 = 10
            LOCAL:111 = 3
            LOCAL:113 = 10
        ENDIF
    ELSEIF LOCAL:COUNT >= 50
        ;우선은 A사용 회수를＋1
        LOCAL:115 = 1
        ;참가자의 C감각과 욕망 더해 7이상이라면 참가자 대량 사정
        IF ABL:COUNT:C감각 + ABL:COUNT:욕망 >= 7
            LOCAL:103 = 1
            LOCAL:106 = 2
        ;그 이외는 1회
        ELSE
            LOCAL:103 = 1
            LOCAL:106 = 1
        ENDIF
        ;참가자의 A감각의 10배와 욕망의 5배와 A경험을 더해 225이상이라면 대상 A절정 2.5회(10배가 되어 내 마지막에 나눈다)
        IF (ABL:TARGET:A감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:V감각 >= 225
            LOCAL:109 = 25
        ;160이상이라면 대상 A절정 1.8회(마지막 계산으로 모은다)
        ELSEIF (ABL:TARGET:A감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:V감각 >= 160
            LOCAL:109 = 18
        ;120이상이라면 대상 A절정 1회
        ELSEIF (ABL:TARGET:A감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:V감각 >= 120
            LOCAL:109 = 10
        ;60이상이라면 대상 A절정 0.5회(마지막 계산으로 모은다)
        ELSEIF (ABL:TARGET:A감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:V감각 >= 60
            LOCAL:109 = 5
        ;그 이외는 제로
        ELSE
        ENDIF
        ;응개유로 규정치 가면(자) 사정 1회
        IF (TALENT:TARGET:후타나리 || TALENT:TARGET:현계화) && LOCAL:109 == 25
            LOCAL:107 = 10
            LOCAL:111 = 1
            LOCAL:113 = 1
        ENDIF
    ELSEIF LOCAL:COUNT >= 40
        ;우선은 A사용 회수를＋1
        LOCAL:115 = 1
        ;조교 대상의 A감각의 10배와 욕망의 5배와 A경험을 더해 225이상이라면 대상 A절정 2회(10배가 되어 내 마지막에 나눈다)
        IF (ABL:TARGET:A감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:V감각 >= 225
            LOCAL:109 = 20
        ;160이상이라면 대상 A절정 1.5회(마지막 계산으로 모은다)
        ELSEIF (ABL:TARGET:A감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:V감각 >= 160
            LOCAL:109 = 15
        ;120이상이라면 대상 A절정 0.8회
        ELSEIF (ABL:TARGET:A감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:V감각 >= 120
            LOCAL:109 = 8
        ;60이상이라면 대상 A절정 0.4회(마지막 계산으로 모은다)
        ELSEIF (ABL:TARGET:A감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:V감각 >= 60
            LOCAL:109 = 4
        ;그 이외는 제로
        ELSE
        ENDIF        
        ;응개유로 규정치 가면(자)50%로 사정 1회
        IF (TALENT:TARGET:후타나리 || TALENT:TARGET:현계화) && LOCAL:109 == 25
            IF RAND:2 == 0
                LOCAL:107 = 10
                LOCAL:111 = 1
                LOCAL:113 = 1
            ENDIF
        ENDIF
    ELSEIF LOCAL:COUNT >= 30
        ;우선은 V사용 회수를＋1
        LOCAL:114 = 1
        ;참가자의 C감각과 욕망 더해 7이상이라면 참가자 대량 사정
        IF ABL:COUNT:C감각 + ABL:COUNT:욕망 >= 7
            LOCAL:103 = 1
            LOCAL:106 = 2
        ;그 이외는 1회
        ELSE
            LOCAL:103 = 1
            LOCAL:106 = 1
        ENDIF
        ;조교 대상의 V감각의 10배와 욕망의 5배와 V경험을 더해 200이상이라면 대상 V절정 3회(10배가 되어 내 마지막에 나눈다)
        IF (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 200
            LOCAL:108 = 30
        ;140이상이라면 대상 V절정 2.4회(마지막 계산으로 모은다)
        ELSEIF (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 140
            LOCAL:108 = 24
        ;100이상이라면 대상 V절정 1.8회
        ELSEIF (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 100
            LOCAL:108 = 18
        ;60이상이라면 대상 V절정 1.2회(마지막 계산으로 모은다)
        ELSEIF (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 60
            LOCAL:108 = 12
        ;25이상이라면 대상 V절정 0.6회
        ELSEIF  (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 25
            LOCAL:108 = 6
        ;그 이외는 제로
        ENDIF
    ELSEIF LOCAL:COUNT >= 20
        ;우선은 V사용 회수를＋1
        LOCAL:114 = 1
        ;조교 대상의 V감각의 10배와 욕망의 5배와 V경험을 더해 200이상이라면 대상 V절정 2.5회(10배가 되어 내 마지막에 나눈다)
        IF (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 200
            LOCAL:108 = 27
        ;140이상이라면 대상 V절정 2.4회(마지막 계산으로 모은다)
        ELSEIF (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 140
            LOCAL:108 = 21
        ;100이상이라면 대상 V절정 1.8회
        ELSEIF (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 100
            LOCAL:108 = 15
        ;60이상이라면 대상 V절정 1.2회(마지막 계산으로 모은다)
        ELSEIF (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 60
            LOCAL:108 = 9
        ;25이상이라면 대상 V절정 0.6회
        ELSEIF  (ABL:TARGET:V감각 * 10) + (ABL:TARGET:욕망 * 5) + ABL:TARGET:C감각 >= 25
            LOCAL:108 = 3
        ;그 이외는 제로
        ENDIF
    ELSEIF LOCAL:COUNT >= 10
        ;우선은 입의 사용 회수를＋1
        LOCAL:116 = 1
        ;참가자의 C감각과 욕망에 대상의 기교를 더해 10이상이라면 참가자 대량 사정
        IF ABL:COUNT:C감각 + ABL:COUNT:욕망 + ABL:TARGET:기교 >= 10
            LOCAL:103 = 1
            LOCAL:106 = 2
        ;그 이외는 1회
        ELSE
            LOCAL:103 = 1
            LOCAL:106 = 1
        ENDIF
    ELSE
        ;통상의 애무는 2단계로 나눈다
        ;조교 대상의 C감각과 욕망을 더해 10이상이라면 대상 C절정 1.6회(10배가 되어 내 마지막에 나눈다)
        IF ABL:TARGET:C감각 + ABL:TARGET:욕망 >= 10
            LOCAL:107 = 16
        ;8이상이라면 대상 C절정 1.3회(마지막 계산으로 모은다)
        ELSEIF ABL:TARGET:C감각 + ABL:TARGET:욕망 >= 8
            LOCAL:107 = 13
        ;100이상이라면 대상 C절정 1회
        ELSEIF ABL:TARGET:C감각 + ABL:TARGET:욕망 >= 6
            LOCAL:107 = 10
        ;60이상이라면 대상 C절정 0.7회(마지막 계산으로 모은다)
        ELSEIF ABL:TARGET:C감각 + ABL:TARGET:욕망 >= 4
            LOCAL:107 = 7
        ;25이상이라면 대상 C절정 0.5회
        ELSEIF  ABL:TARGET:C감각 + ABL:TARGET:욕망 >= 2
            LOCAL:107 = 5
        ;그 이외는 0.1회
        ELSE
            LOCAL:108 = 1
        ENDIF
        ;실행 회수 배가 되어 봐 어느 정도 가고 있는＋간유로 대상이 사정
        IF (TALENT:TARGET:후타나리 || TALENT:TARGET:현계화) && (LOCAL:108 * LOCAL:501 / 10 >= 2)
            LOCAL:107 = 10
            LOCAL:111 = 2
            LOCAL:113 = 1            
        ELSEIF (TALENT:TARGET:후타나리 || TALENT:TARGET:현계화) && (LOCAL:108 * LOCAL:501 / 10 >= 1)
            LOCAL:107 = 10
            LOCAL:111 = 1
            LOCAL:113 = 1
        ENDIF
        ;조교 대상의 B감각과 욕망을 더해 10이상이라면 대상 C절정 1.6회(10배가 되어 내 마지막에 나눈다)
        IF ABL:TARGET:B감각 + ABL:TARGET:욕망 >= 10
            LOCAL:110 = 16
        ;8이상이라면 대상 B절정 1.3회(마지막 계산으로 모은다)
        ELSEIF ABL:TARGET:B감각 + ABL:TARGET:욕망 >= 8
            LOCAL:110 = 13
        ;100이상이라면 대상 B절정 1회
        ELSEIF ABL:TARGET:B감각 + ABL:TARGET:욕망 >= 6
            LOCAL:110 = 10
        ;60이상이라면 대상 B절정 0.7회(마지막 계산으로 모은다)
        ELSEIF ABL:TARGET:B감각 + ABL:TARGET:욕망 >= 4
            LOCAL:110 = 7
        ;25이상이라면 참가자 B절정 0.5회
        ELSEIF  ABL:TARGET:B감각 + ABL:TARGET:욕망 >= 2
            LOCAL:110 = 5
        ;그 이외는 0.1회
        ELSE
            LOCAL:110 = 1
        ENDIF
    ENDIF

    ;여기로부터, 대상에 참가자가 얼마나 욕정하고 있을까의 체크(초기치는 500에 해 두는, 실질의 증감치는 실행 회수배)
    LOCAL:130 = 500
    ;대상이 매력 소유
    SIF TALENT:TARGET:매력 == 1
        LOCAL:130 += 30
    ;대상이 매혹 소유
    SIF TALENT:TARGET:매혹 == 1
        LOCAL:130 += 30
    ;대상이 수수께끼의 매력 소유
    SIF TALENT:TARGET:수수께끼의매력 == 1
        LOCAL:130 += 60
    ;대상이 인기 소유
    SIF TALENT:TARGET:인기 == 1
        LOCAL:130 += 15
    ;대상이 고무 소유
    SIF TALENT:TARGET:고무 == 1
        LOCAL:130 += 10
    ;참가자가 호기심 소유
    SIF TALENT:COUNT:호기심 == 1
        LOCAL:130 += 10
    ;참가자가 눈에 띄고 싶어해
    SIF TALENT:COUNT:튀고싶어함 == 1
        LOCAL:130 += 10
    ;참가자가 양도
    SIF TALENT:COUNT:바이 == 1
        LOCAL:130 += 20
    ;참가자가 음란
    SIF TALENT:COUNT:음란 == 1
        LOCAL:130 += 30
    ;참가자의 욕망이 높다
    IF ABL:COUNT:욕망 >= 5
        LOCAL:130 += 20
    ELSEIF ABL:COUNT:욕망 >= 4
        LOCAL:130 += 10
    ENDIF
    ;참가자의 노출버릇이 높다
    IF ABL:COUNT:노출증 >= 5
        LOCAL:130 += 30
    ELSEIF ABL:COUNT:노출증 >= 3
        LOCAL:130 += 20
    ENDIF
    ;참가자 레즈비언기분이 높다
    IF ABL:COUNT:레즈끼 >= 5
        LOCAL:130 += 20
    ELSEIF ABL:COUNT:레즈끼 >= 3
        LOCAL:130 += 10
    ENDIF
    ;참가자 레즈비언 중독이 높다
    IF ABL:COUNT:레즈끼 >= 5
        LOCAL:130 += 30
    ELSEIF ABL:COUNT:레즈끼 >= 2
        LOCAL:130 += 20
    ENDIF
    ;참가자가 여자 증오
    SIF TALENT:COUNT:여성혐오 == 1
        LOCAL:130 -= 40
    ;참가자가 부끄러워해
    SIF TALENT:COUNT:부끄럼쟁이 == 1
        LOCAL:130 -= 20
    ;대상이 광기 소유
    SIF TALENT:TARGET:광기 == 1
        LOCAL:130 -= 20
    ;대상이 붕괴하고 있다
    SIF TALENT:TARGET:정신붕괴 == 1 || TALENT:TARGET:괴뢰 == 1
        LOCAL:130 -= 30
    ;대상이 반항적
    SIF TALENT:TARGET:반항적
        LOCAL:130 -= 20
    ;대상이 감정 부족하다
    SIF TALENT:TARGET:감정결여
        LOCAL:130 -= 20
    ;대상이 저항
    SIF TALENT:TARGET:저항
        LOCAL:130 -= 20
    ;대상이에 똥
    IF ABL:TARGET:기교 <= 2
        LOCAL:130 -= 20
    ELSEIF ABL:TARGET:기교 <= 1
        LOCAL:130 -= 30
    ENDIF    

    ;대상의 욕정도의 변동(좀 어려운에 이기주의자 깎는, 231을 가짜의 용기에)
    ;참가자가에 똥
    SIF ABL:COUNT:기교 <= 2
    LOCAL:231 += 40
    ;232(을)를 용기로 해, 전체의 궁합의 평균을 취할 준비
IF RELATION:TARGET:(NO:COUNT) == 0
    LOCAL:232 += 100
ELSE
    LOCAL:232 += RELATION:TARGET:(NO:COUNT)
ENDIF

