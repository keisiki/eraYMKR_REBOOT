;=============================================================================
;일상 이벤트 분기 처리
;=============================================================================
@DAILY_LIFE
;修正(追加)：夜の場合の集団入浴イベントチェックを先に実行する.
;日常パッチ珠入手発生フラグの仮フラグを本フラグにする
SIF FLAG:997 == 2
    FLAG:997 = 1
IF TIME == 1
    TRYCCALL GROUP_BATH_CHECK
        SIF RESULT == 1
            RETURN 0
    CATCH
    ENDCATCH
ENDIF

;キャラをランダムに選ぶ行程
;借りるフラグをお掃除
CALL RESET_CFLAG75
;今までに重複数えずに選んだ回数をリセット
LOCAL:1 = 0
$SET_LOOP
;適当にキャラ登録番号を取得
LOCAL = RAND:CHARANUM
;既に選んだキャラ
IF CFLAG:LOCAL:75
    ;戻ります
    GOTO SET_LOOP
ELSE
    ;選んだフラグを立てる
    CFLAG:LOCAL:75 = 1
    ;選んだ回数を増やす
    ++LOCAL:1
ENDIF
;日常イベントに登場出来る
IF ABLE_DAILY_LIFE(LOCAL)
    ;フラグのお掃除して次へ進む
    CALL RESET_CFLAG75
ELSE
    ;選んだ回数がキャラ数を越えている
    IF LOCAL:1 >= CHARANUM
        ;フラグのお掃除してｶｴﾚ
        CALL RESET_CFLAG75
        RETURN 0
    ELSE
        ;選ぶ余地があるのでもう少し頑張る
        GOTO SET_LOOP
    ENDIF
ENDIF

;選んだキャラについて口上を呼んでみる
CALL DAILY_LIFE_MESSAGE, LOCAL

;CFLAG:406が立っていなかったら口上があったとしよう
IF !CFLAG:LOCAL:406
    ;汎用イベントを呼ぶ
    CALL DAILY_LIFE_MESSAGE_COMMON, LOCAL
ENDIF



;-------------------------------------------------
;汎用日常イベント
;-------------------------------------------------
@DAILY_LIFE_MESSAGE_COMMON, ARG
;検証(追加)　現在体力と最大体力の表示(デバッグモードのみ)
SIF TALENT:MASTER:Debug
    PRINTFORMW %CALLNAME:ARG%의 현재 체력/최대 체력:({BASE:ARG:체력}/{MAXBASE:ARG:체력})

IF GET_DAILY_LIFE_VIT(ARG)
    ;休むとき
    CALL DAILY_LIFE_MESSAGE_COMMON_REST, ARG
    RETURN 1
ENDIF

VARSET LOCAL, 0
TRYCCALL COUNT_DAILY_LIFE_MESSAGE_COMMON
    LOCAL:998 = LIMIT(RESULT, 100, 900)
CATCH
    LOCAL:998 = 100
ENDCATCH
LOCAL:997 = 0


;調合知識習得イベント、蓬莱の薬生成イベント発生中なら25%の確率で優先的に発生させる
IF (CFLAG:MASTER:1624 > 0 && CFLAG:MASTER:1624 < 20) || (CFLAG:MASTER:1603 > 0 && CFLAG:MASTER:1603 < 50) || (CFLAG:ARG:1624 > 0 && CFLAG:ARG:1624 < 20) && RAND:4 == 0

    ;調合知識習得イベント・蓬莱の薬生成イベント(MASTER・嫁)の3つそれぞれは排他的
    ;蓬莱の薬生成イベント(嫁)が発生中
    IF CFLAG:ARG:1624 > 0 && CFLAG:ARG:1624 < 20
        CALL THE_HOURAINOKUSURI_IS_GENERATED, ARG
        SIF RESULT
            RETURN 1

    ;調合知識習得・蓬莱の薬生成イベント(MASTER)
    ELSE

        ;対象が永琳になっているか
        IF NO:ARG == 29
            CALL DAILY_LIFE_WITH_EIRIN, ARG
            SIF RESULT
                RETURN 1
        ELSE

            ;永琳を探して、いた場合イベントに飛ばす
            CALL DAILY_LIFE_PICKCHARA_NO, ARG, 29
            LOCAL:300 = RESULT
            IF LOCAL:300 > 0
                CALL DAILY_LIFE_WITH_EIRIN, LOCAL:300
                SIF RESULT
                    RETURN 1
            ENDIF
        ENDIF
    ENDIF
ENDIF

$SET_LOOP
LOCAL:999 = RAND:(LOCAL:998)
SIF LOCAL:(LOCAL:999)
    GOTO SET_LOOP
LOCAL:(LOCAL:999) = 1
++LOCAL:997
RESULT = 0
TRYCALLFORM DAILY_LIFE_MESSAGE_COMMON_{TIME != 0}_{LOCAL:999}, ARG
SIF !RESULT && LOCAL:997 < LOCAL:998
    GOTO SET_LOOP



;-------------------------------------------------
;日常宝珠変動汎用ルーチン(難易度により変化をつける)
;-------------------------------------------------
@COMMON_DAILYJUEL, ARG, ARG:1, ARG:2, ARG:3, ARG:4, ARG:5
;主人調教経験により補正を掛けるか
SIF ARG:4
    ARG:2 = LIMIT_DAILY_LIFE_GOT(ARG, ARG:2)
;変動しないなら戻る
SIF ARG:2 == 0
    RETURN 0
;陥落素質無し
IF !TALENT:ARG:연모 && !TALENT:ARG:복종 && !TALENT:ARG:음란
    ;否定的な珠が増加する　もしくは　否定的でない珠が減少する
    IF (ARG:1 == 20 || ARG:1 == 21 || ARG:1 == 22 || ARG:1 == 100) ^^ (ARG:2 < 0)
        ARG:2 = (ARG:1 == 20 || ARG:1 == 21 || ARG:1 == 100) ? CALC_NEGATIVE_DAILYJUEL(ARG, ARG:1, ARG:2) # ARG:2
    ;否定的な珠が減少する　もしくは　否定的でない珠が増加する
    ELSE
        ARG:2 = (ARG:1 != 20 && ARG:1 != 21 && ARG:1 != 22 && ARG:1 != 100) ? CALC_POSITIVE_DAILYJUEL(ARG, ARG:1, ARG:2) # ARG:2
    ENDIF
ENDIF

;補正結果により珠変動が発生したら日常パッチ珠入手発生の仮フラグを立てる
SIF ARG:2 != 0 && !FLAG:997
    FLAG:997 = 2
;CFLAG:45加算処理
SIF ARG:2 > 0 && ARG:1 != 20 && ARG:1 != 21 && ARG:1 != 22 && ARG != 100
    CFLAG:ARG:45 = LIMIT(CFLAG:ARG:45 + ARG:2, 0, 9223372036854775806)

;後は丸投げ.変動0でも補正してくれるはず
CALL COMMON_MOVE_JUEL, ARG, ARG:1, ARG:2 , ARG:3, ARG:5
RETURN RESULT



;-------------------------------------------------
;珠入手の補正
;-------------------------------------------------
@LIMIT_DAILY_LIFE_GOT(ARG, ARG:1)
#FUNCTION
RETURNF MARK:ARG:굴복각인 < 2 && EXP:ARG:주인조교경험 < 100 ? ARG:1 / 2 # ARG:1



;-------------------------------------------------
;日常用CFLAG変動
;-------------------------------------------------
@DAILY_LIFE_MOVE_CFLAG, ARG, ARG:1, ARG:2, ARG:3, ARG:4, ARG:5
;主人調教経験により補正を掛けるか
SIF ARG:4
    ARG:2 = LIMIT_DAILY_LIFE_GOT(ARG, ARG:2)
;変動しないなら戻る
SIF ARG:2 == 0
    RETURN 0
CALL COMMON_MOVE_CFLAG, ARG, ARG:1, ARG:2 , ARG:3, ARG:5
RETURN RESULT



;-------------------------------------------------
;相性上昇汎用処理
;-------------------------------------------------
;ARGのARG:1に対する相性の変更処理をここで行う
;ARG:2の値はイベントの種類(値によって上昇幅が変化)
;なお、相性上昇の上限値はLOCAL:1で変更可能に
@DAILY_RELATION_UP, ARG, ARG:1, ARG:2
;へる子さん専用素質の対策:強制的に相性50%(相愛等は53%)扱いになるので、表示上の相性値との不一致が発生してしまう(バグだと誤解してしまう)のを防止
;スペルカードもｶｴﾚ
SIF TALENT:ARG:거절 || TALENT:ARG:SpellCard
    RETURN 0

;上限値を決定
;日常パッチの強化有無で処理を変える
IF FLAG:SHOP이벤트확장 & 128
    ;対主人
    IF ARG:1 == MASTER
        IF (TALENT:ARG:상애 && ITEM:99) || (TALENT:ARG:예속 && ITEM:98) || TALENT:ARG:괴조인격 || (NO:ARG == 149 && (CFLAG:ARG:3100 == NO:MASTER || CFLAG:ARG:3103 == NO:MASTER) && TALENT:ARG:상애)
            LOCAL:1 = 1000
        ELSEIF (TALENT:ARG:상애 && ITEM:96) || (TALENT:ARG:예속 && ITEM:95)
            LOCAL:1 = 750
        ELSEIF NO:ARG == 149 && (CFLAG:ARG:3100 == NO:MASTER || CFLAG:ARG:3103 == NO:MASTER) && TALENT:ARG:친애
            LOCAL:1 = 500
        ELSEIF NO:ARG == 149 && (CFLAG:ARG:3100 == NO:MASTER || CFLAG:ARG:3103 == NO:MASTER || (CFLAG:ARG:3101 == NO:MASTER && TALENT:ARG:친애))
            LOCAL:1 = 400
        ELSEIF NO:ARG == 149 && CFLAG:ARG:3101 == NO:MASTER
            LOCAL:1 = 200
        ELSEIF TALENT:ARG:친애
            LOCAL:1 = 125
        ELSE
            LOCAL:1 = 110
        ENDIF
    ELSE
        ;調教経験で変動
        SELECTCASE CFLAG:ARG:(1000+NO:(ARG:1))
            CASE IS >= 10000
                LOCAL:1 = 1000
            CASE IS >= 5000
                LOCAL:1 = 750
            CASE IS >= 3000
                LOCAL:1 = 500
            CASE IS >= 2000
                LOCAL:1 = 250
            CASE IS >= 1000
                LOCAL:1 = 125
            CASEELSE
                LOCAL:1 = 110
        ENDSELECT
    ENDIF
ELSE
    LOCAL:1 = 110
ENDIF
;初期値より小さければ補正
IF CFLAG:ARG:0
    CALLFORMF EX_RELATION_{NO:ARG}_{CFLAG:ARG:0}, NO:(ARG:1)
    LOCAL:1 = (LOCAL:1 == 0) ? 100 # RESULT
ELSE
    LOCAL:1 = MAX(LOCAL:1, CSVRELATION(NO:ARG, NO:(ARG:1),0))
ENDIF

;相性値を取得
LOCAL:2 = RELATION:ARG:(NO:(ARG:1)) ? RELATION:ARG:(NO:(ARG:1)) # 100

;既に上限以上
SIF LOCAL:2 >= LOCAL:1
    RETURN 0

;変動値を計算
SELECTCASE ARG:2
    CASE 1
        LOCAL:3 = 1 + RAND:10
    CASE 2
        LOCAL:3 = 1 + RAND:7
    CASE 3
        LOCAL:3 = 1 + RAND:3
    CASE 4
        LOCAL:3 = 1 + RAND:5
    CASEELSE
        LOCAL:3 = 0
ENDSELECT
LOCAL:3 *= 5

;相性変動
RELATION:ARG:(NO:(ARG:1)) = LIMIT(LOCAL:2+LOCAL:3, 0, LOCAL:1)

;相性表示
PRINTFORMW %CALLNAME:ARG%의 %ITEMNAME:(NO:(ARG:1) + 100)%에 대한 궁합이,{RELATION:ARG:(NO:(ARG:1))}퍼센트가 되었습니다.

;日常パッチ珠入手発生の仮フラグを立てる
FLAG:997 = 2


;-------------------------------------------------
;ランダムで選んだかの判定用に借りたフラグのお掃除
;-------------------------------------------------
@RESET_CFLAG75
CVARSET CFLAG, 75, 0



;-------------------------------------------------
;説教イベントなどのBASE値上昇
;-------------------------------------------------
@DAILY_LIFE_BASEUP, ARG, ARG:1, ARG:2
;最大体力/気力上昇処理
FOR LOCAL, 0, 2
    IF CFLAG:ARG:(1600+LOCAL) < MAX_DAILY_LIFE_BASEUP()
        IF ARG:2
            LOCAL:1 = MIN(100 + RAND:51, MAX_DAILY_LIFE_BASEUP()-CFLAG:ARG:(1600+LOCAL))
        ELSE
            LOCAL:1 = MIN(RAND:55 + 1 + (ABL:(ARG:1):순종 * 3) + ((BASE:(ARG:1):체력 + BASE:(ARG:1):기력) / 450) + (CFLAG:(ARG:1):2 / 1024) + (EXP:(ARG:1):애정경험 / 75), 255, MAX_DAILY_LIFE_BASEUP()-CFLAG:ARG:(1600+LOCAL))
        ENDIF
        CFLAG:ARG:(1600+LOCAL) += LOCAL:1
        MAXBASE:ARG:체력 += LOCAL:1
        PRINTFORMW %CALLNAME:ARG%의 최대 %BASENAME:LOCAL%+{LOCAL:1}
    ENDIF
NEXT



;-------------------------------------------------
;説教イベントのBASE値上昇最大値
;-------------------------------------------------
@MAX_DAILY_LIFE_BASEUP
#FUNCTION
RETURNF 3500



;-------------------------------------------------
;否定的珠の日常増加時の補正処理
;-------------------------------------------------
@CALC_NEGATIVE_DAILYJUEL(ARG, ARG:1, ARG:2)
#FUNCTION
;MARK:고통각인をみる(恐怖のみ)
IF ARG:1 == 16
    IF MARK:ARG:쾌락각인 == 0
        TIMES ARG:2 , 1.00
    ELSEIF MARK:ARG:쾌락각인 == 1
        TIMES ARG:2 , 0.50
    ELSEIF MARK:ARG:쾌락각인 == 2
        TIMES ARG:2 , 0.10
    ELSE
        TIMES ARG:2 , 0.05
    ENDIF
ENDIF
;MARK:쾌락각인をみる(快C～快Bのみ)
IF ARG:1 >= 0 && ARG:1 <= 9
    IF MARK:ARG:쾌락각인 == 0
        TIMES ARG:2 , 1.00
    ELSEIF MARK:ARG:쾌락각인 == 1
        TIMES ARG:2 , 0.50
    ELSEIF MARK:ARG:쾌락각인 == 2
        TIMES ARG:2 , 0.10
    ELSE
        TIMES ARG:2 , 0.05
    ENDIF
ENDIF
;MARK:굴복각인をみる(屈服・恥情・苦痛・恐怖のみ)
IF ARG:1 >= 14 && ARG:1 <= 17
    IF MARK:ARG:굴복각인 == 0
        TIMES ARG:2 , 1.00
    ELSEIF MARK:ARG:굴복각인 == 1
        TIMES ARG:2 , 0.50
    ELSEIF MARK:ARG:굴복각인 == 2
        TIMES ARG:2 , 0.10
    ELSE
        TIMES ARG:2 , 0.05
    ENDIF
ENDIF
;MARK:치욕각인をみる(欲情・屈服・恥情のみ)
IF ARG:1 >= 13 && ARG:1 <= 15
    IF MARK:ARG:치욕각인 == 0
        TIMES ARG:2 , 1.00
    ELSEIF MARK:ARG:치욕각인 == 1
        TIMES ARG:2 , 0.50
    ELSEIF MARK:ARG:치욕각인 == 2
        TIMES ARG:2 , 0.10
    ELSE
        TIMES ARG:2 , 0.05
    ENDIF
ENDIF
;MARK:공포각인をみる(恭順・苦痛のみ)
IF ARG:1 == 12 || ARG:1 == 16
    IF MARK:ARG:공포각인 == 0
        TIMES ARG:2 , 1.00
    ELSEIF MARK:ARG:공포각인 == 1
        TIMES ARG:2 , 0.50
    ELSEIF MARK:ARG:공포각인 == 2
        TIMES ARG:2 , 0.10
    ELSE
        TIMES ARG:2 , 0.05
    ENDIF
ENDIF
;MARK:반발각인をみる
IF MARK:ARG:반발각인 == 0
    TIMES ARG:2 , 0.25
ELSEIF MARK:ARG:반발각인 == 1
    TIMES ARG:2 , 0.50
ELSEIF MARK:ARG:반발각인 == 2
    TIMES ARG:2 , 0.75
ELSE
    TIMES ARG:2 , 1.00
ENDIF

;ABL:순종をみる(恭順・欲情・屈服・恐怖のみ)
IF ARG:1 == 12 || ARG:1 == 13 || ARG:1 == 14 || ARG:1 == 17
    IF ABL:ARG:순종 == 0
        TIMES ARG:2 , 1.00
    ELSEIF ABL:ARG:순종 == 1
        TIMES ARG:2 , 0.75
    ELSEIF ABL:ARG:순종 == 2
        TIMES ARG:2 , 0.50
    ELSEIF ABL:ARG:순종 == 3
        TIMES ARG:2 , 0.10
    ELSEIF ABL:ARG:순종 == 4
        TIMES ARG:2 , 0.05
    ELSE
        TIMES ARG:2 , 0.01
    ENDIF
ENDIF
;ABL:욕망をみる(欲情・先導のみ)
IF ARG:1 == 13 || ARG:1 == 32
