    ELSE
        TIMES ARG:2 , 1.00
    ENDIF
ENDIF
;MARK:굴복각인をみる(屈服・恥情・苦痛・恐怖のみ)
IF ARG:1 >= 14 && ARG:1 <= 17
    IF MARK:ARG:굴복각인 == 0
        TIMES ARG:2 , 0.50
    ELSEIF MARK:ARG:굴복각인 == 1
        TIMES ARG:2 , 0.65
    ELSEIF MARK:ARG:굴복각인 == 2
        TIMES ARG:2 , 0.80
    ELSE
        TIMES ARG:2 , 1.00
    ENDIF
ENDIF
;MARK:치욕각인をみる(欲情・屈服・恥情のみ)
IF ARG:1 >= 13 && ARG:1 <= 15
    IF MARK:ARG:치욕각인 == 0
        TIMES ARG:2 , 0.50
    ELSEIF MARK:ARG:치욕각인 == 1
        TIMES ARG:2 , 0.65
    ELSEIF MARK:ARG:치욕각인 == 2
        TIMES ARG:2 , 0.80
    ELSE
        TIMES ARG:2 , 1.00
    ENDIF
ENDIF
