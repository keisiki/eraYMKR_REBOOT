    ENDIF
ENDIF
;MARK:반발각인をみる
IF MARK:ARG:반발각인 == 0
    TIMES ARG:2 , 1.00
ELSEIF MARK:ARG:반발각인 == 1
    TIMES ARG:2 , 0.75
ELSEIF MARK:ARG:반발각인 == 2
    TIMES ARG:2 , 0.50
ELSE
    TIMES ARG:2 , 0.50
ENDIF

;珠の種類により個々の補正を掛ける
SELECTCASE ARG:1
    CASE 0
    CASE 1
    CASE 2
    CASE 3
    CASE 11
        ARG:2 = CALC_POSITIVE_DAILYJUEL_11(ARG, ARG:2)
    CASE 12
        ARG:2 = CALC_POSITIVE_DAILYJUEL_12(ARG, ARG:2)
    CASE 13
        ARG:2 = CALC_POSITIVE_DAILYJUEL_13(ARG, ARG:2)
    CASE 14
