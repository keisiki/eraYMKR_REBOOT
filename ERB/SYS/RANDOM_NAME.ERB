    LOCAL = (LOCAL != -1 && LOCAL != TARGET) ? 1 # 0
    LOCAL:100 += 1
    IF LOCAL && LOCAL:100 < 500
        NICKNAME = 
        GOTO RETRY2
    ELSEIF LOCAL
        NICKNAME = 
