;=============================================================================
;비디오의 타이틀 보너스 부가 처리
;=============================================================================
;'이 몸의 저스티스가 없구나!'라는 사람은 수동으로 추가해주세요
;없으리라곤 생각합니다만 기존 동인지의 타이틀과 중복되었다면 대체안을 마련해 가르쳐주세요
;솔직히 소재가 썰렁하단 느낌이 안 드는 것도 아니군
;--------------------------------------------------
;커맨드 내용에 따른 타이틀 결정(우선도 약간 낮음)
;--------------------------------------------------
@VIDEO_TITLE_BONUS(ARG, ARG:1)
#FUNCTION
LOCAL = 0
LOCAL:1 = JUDGE_VIDEOTITLE()
;보너스는 금후 변화할 가능성 있음(타이틀은 팀 조합 우선)
IF TFLAG:239 == 0
    ;특정 조건에 따른 우선도가 높은 보너스
    IF LOCAL:1 == 98
        COM_NAME = 산결성
        LOCAL += 1500
    ELSEIF LOCAL:1 == 97
        IF TALENT:임신
            COM_NAME = 임양천
            LOCAL += 1100
        ELSE
            COM_NAME = 이혈향
            LOCAL += 900
        ENDIF
        ;조교 대상이 음호나 음고라면 추가 보너스
        SIF TALENT:음호 || TALENT:음고
            LOCAL += 800
    ELSEIF LOCAL:1 == 96
        COM_NAME = 약옥정
        LOCAL += 1150
        ;조교 대상이 초M에다가 약물내성이라면 추가 보너스
        SIF TALENT:초M && TALENT:약물내성
            LOCAL += 650
        ;조교 대상이 초M에다가 미약중독이라면 추가 보너스
        SIF TALENT:초M && TALENT:미약중독
            LOCAL += 650

    ;조교 내용의 경향이 가장 높은 것이 기준이 되는 타이틀 보너스
    ELSEIF LOCAL:1 == 0
        COM_NAME = 두몽상
        LOCAL += 800
        ;조교 대상이 음핵/음경이라면 추가 보너스
        SIF TALENT:음핵／음경
            LOCAL += 700
    ELSEIF LOCAL:1 == 1
        IF TALENT:임신
            COM_NAME = 임월초
            LOCAL += 1000
        ELSE
            COM_NAME = 풍질록
            LOCAL += 800
        ENDIF
        ;조교 대상이 음호라면 추가 보너스
        SIF TALENT:음호
            LOCAL += 800
    ELSEIF LOCAL:1 == 2
        COM_NAME = 항마광
        LOCAL += 700
        ;조교 대상이 애널광이라면 추가 보너스
        SIF TALENT:음고
            LOCAL += 800
    ELSEIF LOCAL:1 == 3
        COM_NAME = 유유몽
        LOCAL += 800
        ;조교 대상이 음유라면 추가 보너스
        SIF TALENT:음유
            LOCAL += 700
    ELSEIF LOCAL:1 == 4
        COM_NAME = 우식사기
        LOCAL += 800
        ;조교 대상이 헌신적이면 추가 보너스
        SIF TALENT:헌신적
            LOCAL += 800
    ELSEIF LOCAL:1 == 5
        COM_NAME = 성련천
        LOCAL += 500
        ;조교 대상이 오줌싸개 버릇에다 유아 or 유치 or 유아퇴행이라면 추가 보너스
        IF TALENT:오줌싸개버릇 && (TALENT:유아 || TALENT:유치 || TALENT:유아퇴행)
            LOCAL += 900
        ;조교 대상이 오줌싸개 버릇이라면 추가 보너스
        ELSEIF TALENT:오줌싸개버릇
            LOCAL += 500
        ENDIF
        ;조교 대상이 요도광이라면 추가 보너스
        SIF TALENT:요도광
            LOCAL += 500
    ELSEIF LOCAL:1 == 10
        COM_NAME = 치령전
        LOCAL += 800
        ;조교 대상이 고무라면 추가 보너스
        SIF TALENT:고무
            LOCAL += 1000
    ELSEIF LOCAL:1 == 11
        COM_NAME = 순정천
        LOCAL += 700
        ;조교 대상이 키스마라면 추가 보너스
        SIF TALENT:키스마
            LOCAL += 900
    ELSEIF LOCAL:1 == 12
        COM_NAME = 촉수담
        LOCAL += 600
        ;조교 대상이 기생이라면 추가 보너스
        SIF TALENT:기생
            LOCAL += 600
        ;조교 대상이 회란이라면 추가 보너스
        SIF TALENT:회란
            LOCAL += 600
    ELSEIF LOCAL:1 == 13
        COM_NAME = 수륜당
        LOCAL += 1000
    ELSEIF LOCAL:1 == 14
        COM_NAME = 가영속
        LOCAL += 750
        ;조교 대상이 초M이라면 추가 보너스
        SIF TALENT:초M
            LOCAL += 750
    ELSEIF LOCAL:1 == 15
        COM_NAME = 약이공
        LOCAL += 1000
        ;조교 대상이 약물내성이라면 추가 보너스
        SIF TALENT:약물내성
            LOCAL += 600
        ;조교 대상이 미약중독이라면 추가 보너스
        SIF TALENT:미약중독
            LOCAL += 600
    ELSE
        COM_NAME = 영야창
    ENDIF
    SIF ASSI >= 0 && (TALENT:ASSI:연모 || TALENT:ASSI:복종 || TALENT:ASSI:괴조인격 || TALENT:ASSI:음란 || TALENT:ASSI:음마) && (TALENT:연모 || TALENT:복종 || TALENT:괴조인격 || TALENT:음란 || TALENT:음마)
        LOCAL += VIDEO_EXTRA_TITLE(ARG, ARG:1)
    TFLAG:239 = 1
ENDIF
RETURNF LOCAL

;--------------------------------------------------
;타이틀 결정 판별용
;--------------------------------------------------
@JUDGE_VIDEOTITLE
#FUNCTION
LOCAL:1 = 99
IF TFLAG:120
    LOCAL:1 = 98
ELSEIF GET_GENRE(1) >= 8 && GET_GENRE(2) >= 8
    LOCAL:1 = 97
ELSEIF GET_GENRE(14) >= 8 && GET_GENRE(15) >= 8
    LOCAL:1 = 96
ELSE
    FOR LOCAL, 0, 16
        LOCAL:2 = GET_GENRE(LOCAL)
        SIF LOCAL:2 >= 8 && LOCAL:2 >= GET_GENRE(LOCAL:1)
                LOCAL:1 = LOCAL
    NEXT
ENDIF
RETURNF LOCAL:1

