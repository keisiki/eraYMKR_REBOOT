;-----------------------------------------------
;남자가 됐을 경우
;-----------------------------------------------
@RANDOM_CHARA_MALE_SET, ARG
TALENT:ARG:0 = 0
TALENT:ARG:110 = 0
TALENT:ARG:111 = 0
TALENT:ARG:112 = 0
TALENT:ARG:113 = 0
TALENT:ARG:120 = 1
TALENT:ARG:121 = 0
ABL:ARG:22 = 0
ABL:ARG:32 = 0
EXP:ARG:0 = 0
EXP:ARG:12 = 0
EXP:ARG:32 = 0
EXP:ARG:42 = 0
EXP:ARG:45 = 0
EXP:ARG:51 = 0
EXP:ARG:52 = 0
MAXBASE:ARG:2 = CALC_GAUGE2_COMMON(ARG)

;-----------------------------------------------
;여자가 됐을 경우
;-----------------------------------------------
@RANDOM_CHARA_FEMALE_SET, ARG
TALENT:ARG:120 = 0
TALENT:ARG:121 = 0
ABL:ARG:23 = 0
ABL:ARG:33 = 0
EXP:ARG:13 = 0
MAXBASE:ARG:2 = 0

;-----------------------------------------------
;체력 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_0, ARG
MAXBASE:ARG:0 += (TALENT:ARG:120) ? 100 * RAND:11 + 100 # 100 * RAND:11

;-----------------------------------------------
;기력 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_1, ARG
MAXBASE:ARG:1 += (TALENT:ARG:120) ? 100 * RAND:10 # 100 * RAND:11

;-----------------------------------------------
;처녀인지 아닌지 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_2, ARG
SIF TALENT:ARG:120
    RETURN 0
;人形は処女膜が作られないことが多い
LOCAL = (TALENT:ARG:208) ? RAND:100 # RAND:4
SIF LOCAL < 3
    TALENT:처녀 = 1

;-----------------------------------------------
;臆病および気丈의 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_3, ARG
IF NO:ARG == 180
    ;캇파의 경우
    LOCAL = RAND:8
    IF LOCAL <= 1 && TALENT:ARG:12 == 0
        TALENT:ARG:10 = 1
    ELSEIF LOCAL == 3 && TALENT:ARG:10 == 0
        TALENT:ARG:12 = 1
    ENDIF
ELSE
    ;그 외의 경우
    LOCAL = RAND:8
    IF LOCAL == 0 && TALENT:ARG:12 == 0
        TALENT:ARG:10 = 1
    ELSEIF LOCAL == 1 && TALENT:ARG:10 == 0
        TALENT:ARG:12 = 1
    ENDIF
ENDIF

;-----------------------------------------------
;好奇心・楽観的・悲観的의 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_4, ARG
IF NO:ARG == 185 || NO:ARG == 189
    ;天人・仙人의 경우
    SIF RAND:16 == 0
        TALENT:ARG:23 = 1
    LOCAL = RAND:16
    IF LOCAL < 5 && TALENT:ARG:26 == 0
        TALENT:ARG:25 = 1
    ELSEIF LOCAL == 7 && TALENT:ARG:25 == 0
        TALENT:ARG:26 = 1
    ENDIF
ELSE
    ;그 외의 경우
    SIF RAND:4 == 0
        TALENT:ARG:23 = 1
    LOCAL = RAND:12
    SIF CFLAG:ARG:0 == 1 && LOCAL < 1
        LOCAL = 5
    IF LOCAL < 2 && TALENT:ARG:26 == 0
        TALENT:ARG:25 = 1
    ELSEIF LOCAL == 5 && TALENT:ARG:25 == 0
        TALENT:ARG:26 = 1
    ENDIF
ENDIF

;-----------------------------------------------
;恥じらい・恥薄い의 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_5, ARG
IF NO:ARG == 176 && ARG != MASTER
    ;코코아의 경우
    LOCAL = RAND:16
    IF (LOCAL == 0 || LOCAL == 1) && TALENT:ARG:36 == 0
        TALENT:ARG:35 = 1
    ELSEIF LOCAL <= 4 && TALENT:ARG:35 == 0
        TALENT:ARG:36 = 1
    ENDIF
ELSE
    ;그 외의 경우
    LOCAL = RAND:8
    IF LOCAL == 0 && TALENT:ARG:36 == 0
        TALENT:ARG:35 = 1
    ELSEIF LOCAL == 1 && TALENT:ARG:35 == 0
        TALENT:ARG:36 = 1
    ENDIF
ENDIF

;-----------------------------------------------
;痛みに弱い/強い・サド/マゾ의 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_6, ARG
;痛みに弱い/強い
IF NO:ARG == 181 || NO:ARG == 182
    ;오니들
    LOCAL = RAND:12
    IF LOCAL == 0 && TALENT:ARG:41 == 0
        TALENT:ARG:40 = 1
    ELSEIF LOCAL >= 3 && TALENT:ARG:40 == 0
        TALENT:ARG:41 = 1
    ENDIF
ELSE
    ;그 외의 경우
    LOCAL = RAND:8
    IF LOCAL == 0 && TALENT:ARG:41 == 0
        TALENT:ARG:40 = 1
    ELSEIF LOCAL == 1 && TALENT:ARG:40 == 0
        TALENT:ARG:41 = 1
    ENDIF
ENDIF
;サド/マゾ
LOCAL = RAND:10
IF LOCAL <= 2 && (NO:ARG == 174 || NO:ARG == 175)
    ;텐구의 경우
