;=============================================================================
;무명 캐릭터 스테이터스 생성 처리
;=============================================================================
;여기서는 무명 캐릭터의 체력 · 기력 · 소질 · 경험 · 능력 등을 랜덤으로 결정.
;-----------------------------------------------
;노예용 랜덤 스테이터스 생성 처리
;-----------------------------------------------
;그 이름대로 노예용의 처리. SHOP2 또는 SYSTEM에서 호출된다.
@CHARA_RANDOM_STATE
;성별
;무명 캐릭터 성별 랜덤이 유효할 때, 앨리스의 인형이나 마계인 외의 경우, 랜덤으로 [남자]가 된다
IF FLAG:SHOP이벤트확장 & 2048 && RAND:4 == 0 && (FLAG:570 & 8) == 0
    SIF NO:TARGET != 172 && NO:TARGET != 173
        CALL RANDOM_CHARA_MALE_SET, TARGET
ENDIF

;디폴트 이름 관리
CALL CHARA_RANDOM_NAME_DEFAULT

;소질
;흑요정 판정
SIF NO:TARGET >= 160 && NO:TARGET < 170
    CFLAG:0 = 1
FOR LOCAL, 0, 28
    CALLFORM RANDOM_CHARA_TALENT_SET_{LOCAL}, TARGET
NEXT
;음란계 소질(악마 전용)
SIF NO:TARGET == 176 && RAND:32 == 0
    CALL STATE_LITTLE_DEAMON_RANDOM
;폭발물(앨리스의 인형 전용)
SIF NO:TARGET == 172 && RAND:32 == 0
    TALENT:220 = 14
;능력
FOR LOCAL, 0, 9
    CALLFORM RANDOM_CHARA_ABL_SET_{LOCAL}, TARGET
NEXT
;경험
FOR LOCAL, 0, 9
    CALLFORM RANDOM_CHARA_EXP_SET_{LOCAL}, TARGET
NEXT
;반발각인(흑요정만)
IF CFLAG:0 == 1 && RAND:4 == 0
    MARK:반발각인 += RAND:2
    MARK:97 = MARK:반발각인
ENDIF

;V경험が有り、異常경험が有るなら低確率で被強姦경험や被輪姦경험が入る.
;攫われる前からあったのか、攫われてくる最中に襲われたのかはご想像に.
IF RAND:20 == 0 && EXP:V경험 > 0 && EXP:정액경험 > 0 && EXP:이상경험 > 0
    IF RAND:2 == 0 && EXP:V경험 > 1
        EXP:피윤간경험 += 1
    ELSE
        EXP:피강간경험 += 1 + RAND:(EXP:V경험)
        EXP:피강간경험 = MIN(EXP:피강간경험, EXP:정액경험)
    ENDIF
;むしろ強姦していた場合.
;犯罪率からいえば、本来は遥かに低いわけだが、そういう奴が[周囲からはぶられて奴隷に身を落とした]的な解釈で.
ELSEIF RAND:50 == 0 && (TALENT:120 || TALENT:121) && EXP:사정경험 > 0 && EXP:이상경험 > 0
    EXP:강간경험 += 1 + RAND:(EXP:사정경험)
    EXP:강간경험 = MIN(EXP:강간경험, EXP:사정경험)
ENDIF

;랜덤 첫경험 판정
CALL RANDOM_VIRGIN, TARGET

;마지막으로 체력과 기력을 전쾌시키고, 흑요정 판정을 해제하는 처리를 깔작입니다
CALL RANDOM_CHARA_FINISH, TARGET

;이름 랜덤 생성
IF FLAG:게임모드 >= 50
    CALL CHARA_RANDOM_NAME_C
ELSE
    CALL CHARA_RANDOM_NAME
ENDIF
RETURN 1

;-----------------------------------------------
;음란계 소질 초기 추가 처리
;-----------------------------------------------
;현재 코코아 전용란
;ごく稀に生まれながらにしてえっちな素質持ちの코코아が出ますって解釈の元に
;淫乱系の素質が始めから付いている(引き継ぎも有効)
@STATE_LITTLE_DEAMON_RANDOM
TALENT:170 = 1
CFLAG:16 |= 1
LOCAL = RAND:100
IF LOCAL < 10
    ;淫核／淫茎
    TALENT:171 = 1
    CFLAG:16 |= 2
ELSEIF LOCAL < 20
    ;[남자]持ちは弾く
    SIF TALENT:120
        RESTART
    ;淫壷
    TALENT:172 = 1
    CFLAG:16 |= 4
ELSEIF LOCAL < 30
    ;尻穴狂い
    TALENT:173 = 1
    CFLAG:16 |= 8
ELSEIF LOCAL < 40
    ;淫乳
    TALENT:174 = 1
    CFLAG:16 |= 16
ELSEIF LOCAL < 45
    ;尿道狂
    TALENT:175 = 1
    CFLAG:16 |= 32
ELSEIF LOCAL < 50
    ;キス魔
    TALENT:176 = 1
    CFLAG:16 |= 64
ELSEIF LOCAL < 60
    ;ドＳ
    TALENT:177 = 1
    CFLAG:16 |= 128
ELSEIF LOCAL < 70
    ;ドＭ
    TALENT:178 = 1
    CFLAG:16 |= 256
ELSEIF LOCAL == 99
    ;즉각함락
    TALENT:179 = 1
    CFLAG:16 |= 512
ENDIF

;-----------------------------------------------
;주인용 랜덤 스테이터스 생성 처리
;-----------------------------------------------
;SYSTEM.ERB에서 호출된다
;EXモードで主人を名無しキャラに設定した場合、ここでその処理を行う
;大まかな処理は奴隷用と変わりはないが、[快感の否定]など一部の素質を取得しないようになっている
@CHARA_RANDOM_STATE_M
;サイズ縮小のため、主人＝調教対象にする
;능력生成中は対象と主人の登録番号は一致しているため、NO:MASTERの処理は変更しないことに(だって、めんどくさいかｒ(ry

LOCAL:1 = TARGET
TARGET = MASTER

;성별
;앨리스의 인형, 마계인 말고는 성별 선택이 가능
IF NO:MASTER != 172 && NO:MASTER != 173
    PRINTFORML %CALLNAME:MASTER%의 성별을 선택해 주세요.
    PRINTL [0] ♂
    PRINTL [1] ♀
    PRINTL 
    $INPUT_LOOP
    INPUT
    IF RESULT == 0
        CALL RANDOM_CHARA_MALE_SET, TARGET
        PRINTW 성별:♂으로 시작합니다.
    ELSEIF RESULT == 1
        CALL RANDOM_CHARA_FEMALE_SET, TARGET
        PRINTW 성별:♀로 시작합니다.
    ELSE
        GOTO INPUT_LOOP
    ENDIF
ENDIF

;디폴트 이름 관리
CALL CHARA_RANDOM_NAME_DEFAULT

;소질
;흑요정 판정
SIF NO:MASTER >= 160 && NO:MASTER < 170
    CFLAG:0 = 1
FOR LOCAL, 0, 23
    SELECTCASE LOCAL
        CASE 0 TO 20
            CALLFORM RANDOM_CHARA_TALENT_SET_{LOCAL}, TARGET
        CASE 21
            CALLFORM RANDOM_MASTER_TALENT_SET_{LOCAL}, TARGET
    ENDSELECT
NEXT
;능력
FOR LOCAL, 1, 9
    SIF LOCAL == 3 || LOCAL == 4
        CONTINUE
    CALLFORM RANDOM_CHARA_ABL_SET_{LOCAL}, TARGET
NEXT
;경험
FOR LOCAL, 0, 5
    CALLFORM RANDOM_CHARA_EXP_SET_{LOCAL}, TARGET
NEXT

;最後に体力と気力を全快させ、黒요정判定を解除する処理を噛ます
CALL RANDOM_CHARA_FINISH, TARGET

;이름 랜덤 생성
CALL CHARA_RANDOM_NAME_M

;조교 대상 해제 처리
TARGET = LOCAL:1 
RETURN 1

