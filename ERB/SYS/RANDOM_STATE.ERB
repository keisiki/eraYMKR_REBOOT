REPEAT 4
    LOCAL = 1
    POWER LOCAL, 2, COUNT
    SIF TALENT:ARG:(150+COUNT)
        CFLAG:ARG:14 |= LOCAL
REND
;服従系素質
REPEAT 3
    LOCAL = 1
