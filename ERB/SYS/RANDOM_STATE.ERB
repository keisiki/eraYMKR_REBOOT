;-----------------------------------------------
;남자가 됐을 경우
;-----------------------------------------------
@RANDOM_CHARA_MALE_SET, ARG
TALENT:ARG:처녀 = 0
TALENT:ARG:절벽 = 0
TALENT:ARG:빈유 = 0
TALENT:ARG:거유 = 0
TALENT:ARG:폭유 = 0
TALENT:ARG:남자 = 1
TALENT:ARG:후타나리 = 0
ABL:ARG:레즈끼 = 0
ABL:ARG:레즈중독 = 0
EXP:ARG:V경험 = 0
EXP:ARG:레즈경험 = 0
EXP:ARG:V확장경험 = 0
EXP:ARG:V조율경험 = 0
EXP:ARG:유선개발경험 = 0
EXP:ARG:산란경험 = 0
EXP:ARG:출산경험 = 0
MAXBASE:ARG:사정 = CALC_GAUGE2_COMMON(ARG)

;-----------------------------------------------
;여자가 됐을 경우
;-----------------------------------------------
@RANDOM_CHARA_FEMALE_SET, ARG
TALENT:ARG:남자 = 0
TALENT:ARG:후타나리 = 0
ABL:ARG:BL끼 = 0
ABL:ARG:BL중독 = 0
EXP:ARG:BL경험 = 0
MAXBASE:ARG:사정 = 0

;-----------------------------------------------
;체력 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_0, ARG
MAXBASE:ARG:체력 += (TALENT:ARG:남자) ? 100 * RAND:11 + 100 # 100 * RAND:11

;-----------------------------------------------
;기력 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_1, ARG
MAXBASE:ARG:기력 += (TALENT:ARG:남자) ? 100 * RAND:10 # 100 * RAND:11

;-----------------------------------------------
;처녀인지 아닌지 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_2, ARG
SIF TALENT:ARG:남자
    RETURN 0
;人形は処女膜が作られないことが多い
LOCAL = (TALENT:ARG:208) ? RAND:100 # RAND:4
SIF LOCAL < 3
    TALENT:처녀 = 1

;-----------------------------------------------
;臆病および気丈의 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_3, ARG
IF NO:ARG == 180
    ;캇파의 경우
    LOCAL = RAND:8
    IF LOCAL <= 1 && TALENT:ARG:꿋꿋함 == 0
        TALENT:ARG:겁쟁이 = 1
    ELSEIF LOCAL == 3 && TALENT:ARG:겁쟁이 == 0
        TALENT:ARG:꿋꿋함 = 1
    ENDIF
ELSE
    ;그 외의 경우
    LOCAL = RAND:8
    IF LOCAL == 0 && TALENT:ARG:꿋꿋함 == 0
        TALENT:ARG:겁쟁이 = 1
    ELSEIF LOCAL == 1 && TALENT:ARG:겁쟁이 == 0
        TALENT:ARG:꿋꿋함 = 1
    ENDIF
ENDIF

;-----------------------------------------------
;好奇心・楽観的・悲観的의 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_4, ARG
IF NO:ARG == 185 || NO:ARG == 189
    ;天人・仙人의 경우
    SIF RAND:16 == 0
        TALENT:ARG:호기심 = 1
    LOCAL = RAND:16
    IF LOCAL < 5 && TALENT:ARG:비관적 == 0
        TALENT:ARG:낙관적 = 1
    ELSEIF LOCAL == 7 && TALENT:ARG:낙관적 == 0
        TALENT:ARG:비관적 = 1
    ENDIF
ELSE
    ;그 외의 경우
    SIF RAND:4 == 0
        TALENT:ARG:호기심 = 1
    LOCAL = RAND:12
    SIF CFLAG:ARG:0 == 1 && LOCAL < 1
        LOCAL = 5
    IF LOCAL < 2 && TALENT:ARG:비관적 == 0
        TALENT:ARG:낙관적 = 1
    ELSEIF LOCAL == 5 && TALENT:ARG:낙관적 == 0
        TALENT:ARG:비관적 = 1
    ENDIF
ENDIF

;-----------------------------------------------
;恥じらい・恥薄い의 설정
;-----------------------------------------------
@RANDOM_CHARA_TALENT_SET_5, ARG
IF NO:ARG == 176 && ARG != MASTER
    ;코코아의 경우
    LOCAL = RAND:16
    IF (LOCAL == 0 || LOCAL == 1) && TALENT:ARG:부끄럼없음 == 0
