    LOCAL += 1
;Ｂ鈍感(相手が棒なし)
ELSEIF TALENT:107 && TALENT:MASTER:120 == 0 && TALENT:MASTER:121 == 0
    LOCAL -= 1
ENDIF
;연모
SIF TALENT:150
    LOCAL += 1
;조건을 만족시키지 않았다
SIF LOCAL < 1
    RETURN 0

CFLAG:154 |= 4
CFLAG:MASTER:154 |= 32

DRAWLINE
;지문 컷
IF CFLAG:99 == 0
