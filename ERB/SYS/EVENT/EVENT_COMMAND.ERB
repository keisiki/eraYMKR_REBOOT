        PRINTL 이상경험＋1
        RESETCOLOR
    ELSEIF (CFLAG:20 & 8) == 0
        CFLAG:11 |= 4
        CFLAG:20 |= 8
        EXP:이상경험 += 1
        CALL SETCOLOR_TORIKOMODE, "MAGENTA", 3        
        PRINTL 이상경험＋1
        RESETCOLOR
    ENDIF
ELSE
    LOCAL = TEQUIP:93
    LOCAL:1 = TEQUIP:94
    TCVAR:51 += LOCAL
    SIF LOCAL > 1
        LOCAL /= 2
    SIF LOCAL:1 > 1
        LOCAL:1 /= 2
    TCVAR:32 += LOCAL
    TCVAR:33 += LOCAL:1

    IF (CFLAG:20 & 4) == 0 && (CFLAG:20 & 8) == 0
        TCVAR:50 += 2
        CFLAG:20 |= 4
        CFLAG:20 |= 8
    ELSEIF (CFLAG:20 & 4) == 0
        TCVAR:50 += 1
        CFLAG:20 |= 4
    ELSEIF (CFLAG:20 & 8) == 0
        TCVAR:50 += 1
        CFLAG:20 |= 8
    ENDIF
ENDIF
WAIT

