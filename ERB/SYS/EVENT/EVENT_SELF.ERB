;=============================================================================
;조교 후의 자위 · 성교섭 처리
;=============================================================================
;夜, 厳しい条件をクリアすると…？
;調教後性交渉追加版(2008/04/17 byα)
;調教後の自慰に排他性追加(2009/02/09 by芋)
@SELF_CHECK_MAIN
LOCAL = 0
;強精神薬使用と産卵イベントと調教後押し倒しイベントは互いに排他的
;アイアンメイデンによる終了の場合は基本的に以下のイベントは起こらない（触手すらも）
IF SELECTCOM == 252
    TFLAG:128 = 0
;眠ったり失神したりしていても…触手は暴れまわる！
ELSEIF (EQUIP:14 || TFLAG:899 > 0 || TFLAG:198 & 16) && TEQUIP:90
    CALL SELF_CHECK11
    TFLAG:128 = 0
;調教終了時に眠っているまたは失神している場合, 処理を飛ばす
ELSEIF EQUIP:14 || TFLAG:899 > 0 || TFLAG:198 & 16
    TFLAG:128 = 0
;触手を召喚したまま調教を終了した場合, 触手に襲われる
ELSEIF TEQUIP:90
    CALL SELF_CHECK10
    TFLAG:128 = 0
;[卵生]か[産卵体質]で[妊娠]及び[懐卵]ではない, かつ産卵判定もしくは中田氏があるときは卵を産む
ELSEIF (TALENT:135 || TALENT:136) && TALENT:140 == 0 && TALENT:145 == 0 && (CFLAG:71 > 0 || TFLAG:128)
    CALL EVENT_LAY_EGG
;それ以外のときはCONFIGで調教後行為を抑制するがＯＮでなければ押し倒しイベントを見る
ELSEIF (FLAG:12 & 4096) == 0
    CALL SELF_CHECK2
    TFLAG:128 = 0
    LOCAL = 1
ENDIF
;調教終了時に眠っているまたは失神している場合, アイアンメイデン幽閉終了もしくは調教後行為を抑制している場合ここで処理を終わらせる
SIF EQUIP:14 || TFLAG:899 > 0 || TFLAG:198 & 16 || FLAG:12 & 4096 || SELECTCOM == 252
    RETURN 0

;調教後自慰
CALL SELF_CHECK3, LOCAL

;調教後搾乳は他の自慰イベントとの競合性はない
CALL SELF_CHECK4

;調教後レズプレイと一晩６９は共存可能.しかし夜這いとは互いに排他的
TFLAG:220 = 0
;助手がいて時間に余裕がある場合
IF ASSI >= 0 && TFLAG:107 <= 100
    ;調教対象も助手もオトコでなくてレズ中毒が共にある場合
    IF TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:레즈중독 && ABL:ASSI:레즈중독
        CALL SELF_CHECK5
        ;調教対象も助手もフタナリの場合
        SIF TALENT:121 && TALENT:ASSI:121
            CALL SELF_CHECK6
    ;調教対象も助手もオトコでありＢＬ中毒が共にある場合
    ELSEIF TALENT:120 && TALENT:ASSI:120 && ABL:BL중독 && ABL:ASSI:BL중독
        CALL SELF_CHECK7
    ENDIF
ENDIF
;夜イベント(時間に余裕がある場合)
;TFLAG:220 == 0 とすることでレズプレイとの排他
IF TFLAG:220 == 0 && TFLAG:107 <= 100
    ;CONFIGで夜イベント拡張がＯＮでないなら調教後夜這い処理へ
    SIF (FLAG:12 & 256) == 0
        CALL SELF_CHECK8
ENDIF

;-----------------------------------------------
;調教後の押し倒し処理
;-----------------------------------------------
;強精神薬使用時は押し倒しのイベントは発生しない
;タイプ1 主人棒あり＞相手Ｖあり
;タイプ2 主人棒あり＞相手Ｖなし
;タイプ3 主人棒なしＶあり＞相手棒あり
;タイプ4 主人棒なしＶあり＞相手棒なしＶあり
@SELF_CHECK2
VARSET LOCAL, 0
;前提条件
;恋慕がないとダメ
SIF TALENT:150 == 0
    RETURN 0
;体力必要
SIF BASE:0 < 500
    RETURN 0
;時間も必要
SIF TFLAG:107 > 100
    RETURN 0
;ここでタイプ分け
IF EXIST_PENIS(MASTER)
    ;タイプ1
    IF TALENT:120 == 0
        ;非処女じゃないとダメ
        SIF TALENT:처녀 || EXP:V경험 <= 0
            RETURN 0

        ;Ｖ感覚
        IF ABL:V감각 == 0
            LOCAL = 0
        ELSEIF ABL:V감각 == 1
            LOCAL = 1
        ELSEIF ABL:V감각 <= 3
            LOCAL = 2
        ELSE
            LOCAL = 3
        ENDIF

        ;欲望と奉仕精神によるボーナス
        IF ABL:봉사정신 >= 4 && PALAM:13 >= PALAMLV:4
            IF ABL:욕망 >= 5
                LOCAL += 2
            ELSEIF ABL:욕망 == 4
                LOCAL += 1
            ELSEIF ABL:욕망 == 3 && ABL:봉사정신 >= 5
                LOCAL += 1
            ENDIF
        ENDIF
        ;快感に素直, 快感の否定
        IF TALENT:70 && LOCAL
            LOCAL += 1
        ELSEIF TALENT:71 && LOCAL
            LOCAL -= 2
        ENDIF
        ;淫乱
        SIF TALENT:170 && LOCAL
            LOCAL += 1
        ;ある程度時間が経過している
        IF TFLAG:107 > 50 && LOCAL
            LOCAL -= 1
        ;殆ど時間が経過していない
        ELSEIF TFLAG:107 < 10 && LOCAL
            LOCAL += 1
        ENDIF

        ;条件を満たさない場合は処理をここで終了させる
        SIF LOCAL < 1
            RETURN 0

        ;表現のみ制限(処理は変わらず)
        TFLAG:226 = LOCAL
        TFLAG:226 = TABLE_CALC(TFLAG:226, 5, 2)
        TFLAG:226 = MIN(TFLAG:226, 10)

        DRAWLINE
        ;地の文章カット
        IF CFLAG:99 == 0
            PRINTFORM %조사처리(CALLNAME:MASTER,"과")% %조사처리(CALLNAME:TARGET,"는")% 아직 식지 않은 흥분을 억누르지 못하고, 
            PRINTFORML 다시 {TFLAG:226}번 침대 위에서 뒹굴었다….
        ENDIF

        ;調教後セックス口上(TFLAG:99によるタイプわけをしてください)
        CALL SELF_KOJO, TARGET, 150, 1

        ;Ｖ経験
        CALL COMMON_UP_EXP,TARGET, 0, TFLAG:226, 0
        CFLAG:173 += TFLAG:226
        ;Ｖ調律経験
        CALL COMMON_UP_EXP,TARGET, 42, TFLAG:226, 0
        ;快Ｖ의 구슬
        CALL COMMON_UP_JUEL, TARGET, 1, (LOCAL*200), 0
        ;恭順의 구슬
        CALL COMMON_UP_JUEL, TARGET, 12, (LOCAL*100), 0
        ;欲情의 구슬
        CALL COMMON_UP_JUEL, TARGET, 13, (LOCAL*250), 1
        ;Ｖ感覚＋従順＋奉仕精神が大きい奴隷は名残を惜しむ描写を地の文章表示のときにはおこなう
        IF (ABL:V감각 + ABL:순종 + ABL:봉사정신) >= 13 && CFLAG:99 == 0
            PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 아직 미련이 남은 듯 옷자락을 잡아끌었다.
            PRINTFORMW 하지만 %조사처리(CALLNAME:MASTER,"는")% 그 손을 뿌리치고, 방을 뒤로 했다….
        ENDIF
    ;タイプ2
    ELSE
        ;Ａ経験ないとダメ
        SIF EXP:A경험 <= 0
            RETURN 0

        ;Ａ感覚
        IF ABL:A감각 == 0
            LOCAL = 0
        ELSEIF ABL:A감각 == 1
            LOCAL = 1
        ELSEIF ABL:A감각 <= 3
            LOCAL = 2
        ELSE
            LOCAL = 3
        ENDIF

        ;欲望と奉仕精神によるボーナス
        IF ABL:봉사정신 >= 4 && PALAM:13 >= PALAMLV:4
            IF ABL:욕망 >= 5
                LOCAL += 2
            ELSEIF ABL:욕망 == 4
                LOCAL += 1
            ELSEIF ABL:욕망 == 3 && ABL:봉사정신 >= 5
                LOCAL += 1
            ENDIF
        ENDIF
        ;快感に素直, 快感の否定
        IF TALENT:70 && LOCAL
            LOCAL += 1
        ELSEIF TALENT:71 && LOCAL
            LOCAL -= 2
        ENDIF
        ;淫乱
        SIF TALENT:170 && LOCAL
            LOCAL += 1
        ;ある程度時間が経過している
        IF TFLAG:107 > 50 && LOCAL
            LOCAL -= 1
        ;殆ど時間が経過していない
        ELSEIF TFLAG:107 < 10 && LOCAL
            LOCAL += 1
        ENDIF

        ;条件を満たさない場合は処理をここで終了させる
        SIF LOCAL < 1
            RETURN 0

        ;表現のみ制限(処理は変わらず)
        TFLAG:226 = LOCAL
        TFLAG:226 = TABLE_CALC(TFLAG:226, 5, 2)
        TFLAG:226 = MIN(TFLAG:226, 10)

        DRAWLINE
        ;地の文章カット
        IF CFLAG:99 == 0
            PRINTFORM %조사처리(CALLNAME:MASTER,"과")% %조사처리(CALLNAME:TARGET,"는")% 아직 식지 않은 흥분을 억누르지 못하고, 
            PRINTFORML 다시 {TFLAG:226}번 침대 위에서 뒹굴었다….
        ENDIF

        ;調教後セックス口上(TFLAG:99によるタイプわけをしてください)
        CALL SELF_KOJO, TARGET, 150, 2

        ;Ａ経験
        CALL COMMON_UP_EXP,TARGET, 1, TFLAG:226, 0
        CFLAG:174 += TFLAG:226
        ;快Ａ의 구슬
        CALL COMMON_UP_JUEL, TARGET, 2, (LOCAL*200), 0
        ;恭順의 구슬
        CALL COMMON_UP_JUEL, TARGET, 12, (LOCAL*100), 0
        ;欲情의 구슬
        CALL COMMON_UP_JUEL, TARGET, 13, (LOCAL*250), 1
        ;Ａ感覚＋従順＋奉仕精神が大きい奴隷は名残を惜しむ描写を地の文章表示のときにはおこなう
        IF (ABL:A감각 + ABL:순종 + ABL:봉사정신) >= 13 && CFLAG:99 == 0
            PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 아직 미련이 남은 듯 옷자락을 잡아끌었다.
            PRINTFORMW 하지만 %조사처리(CALLNAME:MASTER,"는")% 그 손을 뿌리치고, 방을 뒤로 했다….
        ENDIF
    ENDIF
;タイプ3
ELSEIF TALENT:120 || TALENT:121
    ;主人が非処女じゃないとダメ
    SIF TALENT:MASTER:0 || EXP:MASTER:V경험 <= 0
        RETURN 0

    ;Ｃ感覚
    IF ABL:C감각 == 0
        LOCAL = 0
    ELSEIF ABL:C감각 == 1
        LOCAL = 1
    ELSEIF ABL:C감각 <= 3
        LOCAL = 2
    ELSE
        LOCAL = 3
    ENDIF

    ;欲望と奉仕精神によるボーナス
    IF ABL:봉사정신 >= 4 && PALAM:13 >= PALAMLV:4
        IF ABL:욕망 >= 5
            LOCAL += 2
        ELSEIF ABL:욕망 == 4
            LOCAL += 1
        ELSEIF ABL:욕망 == 3 && ABL:봉사정신 >= 5
            LOCAL += 1
        ENDIF
    ENDIF
    ;快感に素直, 快感の否定
    IF TALENT:70 && LOCAL
        LOCAL += 1
    ELSEIF TALENT:71 && LOCAL
        LOCAL -= 2
    ENDIF
    ;淫乱
    SIF TALENT:170 && LOCAL
        LOCAL += 1
    ;ある程度時間が経過している
    IF TFLAG:107 > 50 && LOCAL
        LOCAL -= 1
    ;殆ど時間が経過していない
    ELSEIF TFLAG:107 < 10 && LOCAL
        LOCAL += 1
    ENDIF

    ;条件を満たさない場合は処理をここで終了させる
    SIF LOCAL < 1
        RETURN 0

    ;表現のみ制限(処理は変わらず)
    TFLAG:226 = LOCAL
    TFLAG:226 = TABLE_CALC(TFLAG:226, 5, 2)
    TFLAG:226 = MIN(TFLAG:226, 10)

    DRAWLINE
    ;地の文章カット
    IF CFLAG:99 == 0
        PRINTFORM %조사처리(CALLNAME:MASTER,"과")% %조사처리(CALLNAME:TARGET,"는")% 아직 식지 않은 흥분을 억누르지 못하고, 
        PRINTFORML 다시 {TFLAG:226}번 침대 위에서 뒹굴었다….
    ENDIF

    ;調教後セックス口上(TFLAG:99によるタイプわけをしてください)
    CALL SELF_KOJO, TARGET, 150, 3

    ;主人のＶ経験
    CALL COMMON_UP_EXP,MASTER, 0, TFLAG:226, 0
    SIF TALENT:121 || TALENT:1
        CFLAG:175 += TFLAG:226
    ;レズ経験
    SIF TALENT:121 || TALENT:1
        CALL COMMON_UP_EXP,TARGET, 12, TFLAG:226, 0
    ;快Ｃ의 구슬
    CALL COMMON_UP_JUEL, TARGET, 0, (LOCAL*150), 0
    ;恭順의 구슬
    CALL COMMON_UP_JUEL, TARGET, 12, (LOCAL*100), 0
    ;欲情의 구슬
    CALL COMMON_UP_JUEL, TARGET, 13, (LOCAL*250), 1
    ;Ｃ感覚＋従順＋奉仕精神が大きい奴隷は名残を惜しむ描写を地の文章表示のときにはおこなう
    IF (ABL:C감각 + ABL:순종 + ABL:봉사정신) >= 13 && CFLAG:99 == 0
        PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 아직 미련이 남은 듯 옷자락을 잡아끌었다.
        PRINTFORMW 하지만 %조사처리(CALLNAME:MASTER,"는")% 그 손을 뿌리치고, 방을 뒤로 했다….
    ENDIF
;タイプ4
ELSE
    ;レズっ気ないとダメ
    SIF ABL:레즈끼 < 1
        RETURN 0

    ;レズっ気
    LOCAL = MIN(ABL:레즈끼, 5)

    ;レズ中毒によるボーナス
    IF ABL:레즈중독 >= 5
        LOCAL += 3
    ELSEIF ABL:레즈중독 == 4
        LOCAL += 2
    ELSEIF ABL:레즈중독 == 3
        LOCAL += 1
    ENDIF

    ;快感に素直, 快感の否定
    IF TALENT:70 && LOCAL
        LOCAL += 1
    ELSEIF TALENT:71 && LOCAL
        LOCAL -= 2
    ENDIF
    ;淫乱
    SIF TALENT:170 && LOCAL
        LOCAL += 1
    ;ある程度時間が経過している
    IF TFLAG:107 > 50 && LOCAL
        LOCAL -= 1
    ;殆ど時間が経過していない
    ELSEIF TFLAG:107 < 10 && LOCAL
        LOCAL += 1
    ENDIF

    ;条件を満たさない場合は処理をここで終了させる
    SIF LOCAL < 1
        RETURN 0

    ;表現のみ制限(処理は変わらず)
    TFLAG:226 = LOCAL
    TFLAG:226 = TABLE_CALC(TFLAG:226, 5, 2)
    TFLAG:226 = MIN(TFLAG:226, 10)

    DRAWLINE
    ;地の文章カット
    IF CFLAG:99 == 0
        PRINTFORM %조사처리(CALLNAME:MASTER,"과")% %조사처리(CALLNAME:TARGET,"는")% 아직 식지 않은 흥분을 억누르지 못하고, 
        PRINTFORML 다시 {TFLAG:226}번 침대 위에서 뒹굴었다….
    ENDIF
    ;調教後セックス口上(TFLAG:99によるタイプわけをしてください)
    CALL SELF_KOJO, TARGET, 150, 4
    ;レズ経験
    CALL COMMON_UP_EXP,TARGET, 12, TFLAG:226, 0
    ;快Ｃ의 구슬
    CALL COMMON_UP_JUEL, TARGET, 0, (LOCAL*150), 0
    ;習得의 구슬
    CALL COMMON_UP_JUEL, TARGET, 11, (LOCAL*100), 0
    ;欲情의 구슬
    CALL COMMON_UP_JUEL, TARGET, 13, (LOCAL*150), 0
    ;屈服의 구슬
    CALL COMMON_UP_JUEL, TARGET, 14, (LOCAL*150), 1
    ;Ｃ感覚＋従順＋奉仕精神が大きい奴隷は名残を惜しむ描写を地の文章表示のときにはおこなう
    IF (ABL:C감각 + ABL:순종 + ABL:봉사정신) >= 13 && CFLAG:99 == 0
        PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 아직 미련이 남은 듯 옷자락을 잡아끌었다.
        PRINTFORMW 하지만 %조사처리(CALLNAME:MASTER,"는")% 그 손을 뿌리치고, 방을 뒤로 했다….
    ENDIF
ENDIF

;-----------------------------------------------
;調教後自慰・アナル自慰処理
;-----------------------------------------------
@SELF_CHECK3, ARG
;自慰中毒による自慰・アナル自慰チェック
VARSET LOCAL, 0

;珠変化量を記録
TFLAG:670 = JUEL:0
TFLAG:671 = JUEL:1
TFLAG:672 = JUEL:2
TFLAG:673 = JUEL:3
TFLAG:674 = JUEL:12
TFLAG:675 = JUEL:13
TFLAG:676 = JUEL:14
TFLAG:677 = JUEL:15

;経験変化量も記録
TFLAG:680 = EXP:V경험
TFLAG:681 = EXP:A경험
TFLAG:682 = EXP:사정경험
TFLAG:683 = EXP:분유경험
TFLAG:684 = EXP:자위경험
TFLAG:685 = EXP:조교자위경험
TFLAG:686 = EXP:봉사쾌락경험

;自慰中毒を見る
IF ABL:자위중독 == 0
    LOCAL:1 = 0
ELSEIF ABL:자위중독 == 1
    LOCAL:1 = 1
ELSEIF ABL:자위중독 == 2
    LOCAL:1 = 2
ELSEIF ABL:자위중독 == 3
    LOCAL:1 = 4
ELSEIF ABL:자위중독 == 4
    LOCAL:1 = 6
ELSE
    LOCAL:1 = 9
ENDIF

;Ｃ感覚が4以上かつ欲望のパラメーターが10000を超えていた場合, 欲望が4以上なら追加
IF ABL:C감각 >= 4 && PALAM:13 >= PALAMLV:4
    IF ABL:욕망 == 4
        LOCAL:1 += 1
    ELSEIF ABL:욕망 == 5
        LOCAL:1 += 2
    ENDIF
ENDIF

;自慰しやすい
IF TALENT:60
    LOCAL:1 *= 2
    ;欲望ＬＶ３でもAに+1する
    SIF ABL:C감각 >= 3 && ABL:욕망 == 3 && PALAM:13 >= PALAMLV:3
        LOCAL:1 += 1
ENDIF
;快感に素直, 快感の否定
IF TALENT:70 && LOCAL:1
    LOCAL:1 += 1
ELSEIF TALENT:71 && LOCAL:1
    LOCAL:1 -= 2
ENDIF
;淫乱
SIF TALENT:170 && LOCAL:1
    LOCAL:1 += 1

;助手が鼓舞
SIF ASSI >= 0 && TALENT:ASSI:95 && LOCAL:1
    LOCAL:1 += 1

;残り体力が100未満
IF BASE:0 < 100
    LOCAL:1 = 0
;残り体力が250未満
ELSEIF BASE:0 < 250
    LOCAL:1 -= 2
;残り体力が500未満
ELSEIF BASE:0 < 500
    LOCAL:1 -= 1
ENDIF
;ある程度時間が経過している
IF TFLAG:107 > 50
    TIMES LOCAL:1 , 0.50
;殆ど時間が経過していない
ELSEIF TFLAG:107 < 10
    TIMES LOCAL:1 , 1.50
ENDIF

;条件を満たさない場合は処理をここで終了させる
SIF LOCAL:1 < 1
    RETURN 0

;自慰回数制限(自慰経験以外の処理はそのまま)
LOCAL = LOCAL:1
LOCAL = TABLE_CALC(LOCAL, 5, 2)
LOCAL = TABLE_CALC(LOCAL, 10, 5)
LOCAL = MIN(LOCAL, 20)
;時間切れで終了した場合
SIF TFLAG:107 > 100
    LOCAL /= 2
;条件を満たさない場合は処理をここで終了させる
SIF LOCAL < 1
    RETURN 0

;Ａ系拡張コマンドＯＮのときにはアナニーをする可能性がある
IF FLAG:15 & 4
    ;自慰, アナニー判定.ポイントが同じ場合は自慰優先(Ｃ感覚が０の場合はアナニーのみ)
    ;Ａ感覚見る
    LOCAL:2 = ABL:A감각
    ;Ｃ感覚見る
    LOCAL:3 = ABL:C감각

    ;調教時にＡ快楽, Ｃ快楽をより稼いだほうに+1
    IF PALAM:2 > PALAM:0
        LOCAL:2 += 1
    ELSEIF PALAM:0 > PALAM:2
        LOCAL:3 += 1
    ENDIF

    ;尻穴狂いだとアナニーしやすい+
    ;淫核／淫茎だと通常の自慰しやすい+
    IF TALENT:173
        LOCAL:2 += 1
    ELSEIF TALENT:171
        LOCAL:3 += 1
    ENDIF
ELSE
    ;Ａ系拡張コマンド（アナニーパッチ）が有効でないなら両方の判定を0に(強制的に通常自慰になる)
    LOCAL:2 = 0
    LOCAL:3 = 0
ENDIF

;貞操帯を装着している場合, 自慰できなくなって悶々とすることに
IF CFLAG:42 & 2
    CALL SELF_CHECK9, LOCAL:1, ARG
    RETURN 0
ENDIF

;自慰隠蔽発覚パッチＯＮのときの処理
IF FLAG:1700 & 1
    LOCAL:4 = CHECK_BARE(LOCAL:1)

    IF LOCAL:4 > RAND:100
        ;ばれた場合
        CALL SELF_BARE, LOCAL
    ELSE
        ;ばれなかった場合
        ;自慰の報告　従順+露出+マゾ-反発刻印*2
        LOCAL:4 = ABL:순종 + ABL:노출증 + ABL:마조끼 - ( MARK:반발각인 * 2 )
        ;確定値になってる場合は確率次第ということで1/2で
        SIF LOCAL:4 >= 10
            LOCAL:4 = RAND:11 + 5
