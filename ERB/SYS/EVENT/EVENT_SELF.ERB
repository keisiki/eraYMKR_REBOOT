    SIF TALENT:104 && TALENT:173
        LOCAL += 5

    ;両方ささってて, Ｖ敏感もしくは淫壺だとさらに気付けません
    SIF TEQUIP:20 == 1 && (TALENT:102 || TALENT:172)
        LOCAL += 5
