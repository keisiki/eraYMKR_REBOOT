IF TEQUIP:20 == 1 && (TALENT:102 || TALENT:172)
    LOCAL += 5
    
    ;両方だとさらに声が大きくなり, 周囲にも気付けなくなる
    SIF TALENT:102 && TALENT:172
        LOCAL += 5

    ;両方ささってて, Ａ敏感もしくは尻穴狂いだとさらに気付けません
    SIF TEQUIP:25 == 1 && (TALENT:104 || TALENT:173)
        LOCAL += 5
