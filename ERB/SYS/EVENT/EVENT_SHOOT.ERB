        ENDIF
    ENDIF
;일반모드
ELSE
    IF (FLAG:18 & 1) == 0 || FLAG:19 == 8 || TFLAG:704 == 0
        CFLAG:(TFLAG:94):74 = (TEQUIP:(TFLAG:92):31) ? 9 # 0
    ELSEIF FLAG:19 < 8
