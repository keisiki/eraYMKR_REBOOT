@TROPHY_COUNT
LOADGLOBAL

VARSET B, 0
VARSET S, 0
VARSET G, 0
VARSET P, 0
VARSET LOCAL, 0

FOR LOCAL:1, 200, 203
    FOR LOCAL, 0, 50
        TRYCALLFORM TROPHY_{LOCAL:1}_{LOCAL}
        SIF RESULTS:0 == ""
            CONTINUE
        GETBIT GLOBAL:(LOCAL:1), LOCAL
        IF RESULTS:0 == "브론즈"
            B:0 += 1
            B:1 += RESULT
        ELSEIF RESULTS:0 == "실버"
            S:0 += 1
            S:1 += RESULT
        ELSEIF RESULTS:0 == "골드"
            G:0 += 1
            G:1 += RESULT
        ELSEIF RESULTS:0 == "플래티넘"
            P:0 += 1
            P:1 += RESULT
        ENDIF
        VARSET RESULTS, ""
    NEXT
NEXT

