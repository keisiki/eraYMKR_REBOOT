    MONEY:1 = LOCAL
    CALL SHOW_MONEY_INCREACE
    ;--------------------------------------------------
    ;모유で稼いだ最高金額判定처리
    SIF MONEY:1 > MONEY:980
        CALL NEW_HIGH_INCOME_ALL, 980
    SIF MONEY:1 > CFLAG:87
        CALL NEW_HIGH_INCOME_SLAVE, 87
    CALL TOTAL_INCOME_SLAVE, MONEY:1
    ;--------------------------------------------------
    WAIT
ENDIF

