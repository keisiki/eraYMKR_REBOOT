;-------------------------------------------------
;通常の絶頂処理
;-------------------------------------------------
;絶頂カウント用
TFLAG:180 = 0
TFLAG:181 = 0
TFLAG:182 = 0
TFLAG:183 = 0
;表示用
LOCALS:0 = 절정Ｃ
LOCALS:1 = 절정Ｖ
LOCALS:2 = 절정Ａ
LOCALS:3 = 절정Ｂ
LOCALS:9 = 

;絶頂
REPEAT 4
    LOCAL:COUNT = 0
    LOCAL:99 = UP:COUNT + PALAM:COUNT
    IF LOCAL:99 >= PALAMLV:4
        LOCAL:98 = PALAMLV:4 * 2
        IF LOCAL:99 >= 1000000
            LOCAL:COUNT = 9
            LOCALS:8 = 최강
        ELSEIF LOCAL:99 >= 200000
            LOCAL:COUNT = 4
            LOCALS:8 = 극강
        ELSEIF LOCAL:99 >= PALAMLV:4 * 2
            LOCAL:COUNT = 2
            LOCALS:8 = 강
        ELSE
            LOCAL:COUNT = 1
            LOCALS:8 = 
            LOCAL:98 = PALAMLV:4
        ENDIF
        LOCALS:9 = %LOCALS:9 + LOCALS:8 + LOCALS:COUNT%　
        DOWN:COUNT = (LOCAL:98 - 1000)
        ;DOWNで下げても絶頂以上なら
        ;その値-1になるように調整(10000で絶頂なら9999)
        SIF LOCAL:99 - DOWN:COUNT >= PALAMLV:4
            DOWN:COUNT = LOCAL:99 - PALAMLV:4 + 1
    ENDIF
REND
SIF STRLENS(LOCALS:9) > 0
    PRINTFORML %CALLNAME:TARGET%：%LOCALS:9%

TFLAG:180 = LOCAL:0
TFLAG:181 = LOCAL:1
TFLAG:182 = LOCAL:2
TFLAG:183 = LOCAL:3

;[卵生]か[産卵体質]で[妊娠]及び[懐卵]ではない、かつ産卵促進剤使用中で強絶頂Ｖ以上のとき産卵判定
SIF (TALENT:135 || TALENT:136) && TALENT:140 == 0 && TALENT:145 == 0 && TEQUIP:9 && TFLAG:181 >= 2
    CALL EQUIP_COM68

;絶頂すると好感度にボーナスが入る処理
TFLAG:51 = TFLAG:180 + TFLAG:181 + TFLAG:182 + TFLAG:183

;焦らし失敗時は焦らしプレイが終了する
SIF TFLAG:51 && TEQUIP:68
    TEQUIP:68 = 0

;절정에 따른 욕망 LV UP 처리
CALL YOKUBO_UP_EX

;絶頂補助処理
SIF (TFLAG:180 +TFLAG:181 + TFLAG:182 + TFLAG:183) > 0
    CALL DISPLAY_SLAVE_ORGASM

;-------------------------------------------------
;絶頂することで変動するソースや体力気力の消耗
;-------------------------------------------------
;絶頂時の消耗を感覚Lvが高い場合は体力気力の消費が軽減される
;Ｃ絶頂
IF TFLAG:180
    SOURCE:12 += 500 * TFLAG:180
    SOURCE:22 += 200 * TFLAG:180

    DOWNBASE:0 += (ABL:0 < 5) ? 20 # 15
    DOWNBASE:1 += (ABL:0 < 5) ? 10 # 10
ENDIF

;Ｖ絶頂
IF TFLAG:181
    SOURCE:12 += 700 * TFLAG:181
    SOURCE:16 += 500 * TFLAG:181
    SOURCE:18 += 800 * TFLAG:181
    SOURCE:22 += 400 * TFLAG:181

    LOCAL:9 = (ABL:1 < 5) ? 40 # 20
    
    ;PALAM:潤滑をみる(Ｖ快感は潤滑で消耗を軽減できる)
    IF PALAM:10 < PALAMLV:3
        TIMES LOCAL:9 , 1.00
    ELSEIF PALAM:10 < PALAMLV:4
        TIMES LOCAL:9 , 0.90
    ELSEIF PALAM:10 < PALAMLV:5
        TIMES LOCAL:9 , 0.80
    ELSEIF PALAM:10 < PALAMLV:6
        TIMES LOCAL:9 , 0.75
    ELSE
        TIMES LOCAL:9 , 0.70
    ENDIF

    DOWNBASE:0 += LOCAL:9
    DOWNBASE:1 += LOCAL:9 / 2
ENDIF

;Ａ絶頂
IF TFLAG:182
    SOURCE:12 += 600 * TFLAG:182
    SOURCE:17 += 700 * TFLAG:182
    SOURCE:18 += 900 * TFLAG:182
    SOURCE:22 += 1200 * TFLAG:182

    LOCAL:9 = (ABL:2 < 5) ? 60 # 30

    ;PALAM:潤滑をみる(Ａ快感は潤滑で消耗を軽減できる)
    IF PALAM:10 < PALAMLV:3
        TIMES LOCAL:9 , 1.00
    ELSEIF PALAM:10 < PALAMLV:4
        TIMES LOCAL:9 , 0.90
    ELSEIF PALAM:10 < PALAMLV:5
        TIMES LOCAL:9 , 0.80
    ELSEIF PALAM:10 < PALAMLV:6
        TIMES LOCAL:9 , 0.75
    ELSE
        TIMES LOCAL:9 , 0.70
    ENDIF

    DOWNBASE:0 += LOCAL:9
    DOWNBASE:1 += LOCAL:9 / 2
ENDIF

;Ｂ絶頂
IF TFLAG:183
    SOURCE:12 += 500 * TFLAG:183
    SOURCE:22 += 200 * TFLAG:183
    
    DOWNBASE:0 += (ABL:3 < 5) ? 20 # 10
    DOWNBASE:1 += (ABL:3 < 5) ? 10 # 5
ENDIF

;-------------------------------------------------
;絶頂経験追加処理
;-------------------------------------------------
;NOWEXにデータを入れる（絶頂時口上に使う）
NOWEX:0 = TFLAG:180
NOWEX:1 = TFLAG:181
NOWEX:2 = TFLAG:182
NOWEX:3 = TFLAG:183

;絶頂回数を増やす
EX:0 += TFLAG:180
EX:1 += TFLAG:181
EX:2 += TFLAG:182
EX:3 += TFLAG:183

;絶頂経験を増やす
TCVAR:2 += TFLAG:180 + TFLAG:181 + TFLAG:182 + TFLAG:183

SIF TFLAG:450 & 1
    LOCAL:30 = 4
SIF TFLAG:450 & 2
    LOCAL:31 = 4

CALL TROPHY_CHECK_201, LOCAL:30, LOCAL:31

;=============================================================================
;焦らしプレイ関連の処理
;=============================================================================
;-------------------------------------------------
;焦らしプレイの処理
;-------------------------------------------------
@IRRITATING_PLAY
;表示用
LOCALS:0 = 절정직전 멈추기Ｃ
LOCALS:1 = 절정직전 멈추기Ｖ
LOCALS:2 = 절정직전 멈추기Ａ
LOCALS:3 = 절정직전 멈추기Ｂ
LOCALS:9 = 

;絶頂寸止め
REPEAT 4
    LOCAL:99 = UP:COUNT + PALAM:COUNT
    IF LOCAL:99 >= 1000000
        NOWEX:(20 + COUNT) = 9
    ELSEIF LOCAL:99 >= 200000
        NOWEX:(20 + COUNT) = 4
    ELSEIF LOCAL:99 >= PALAMLV:4 * 2
        NOWEX:(20 + COUNT) = 2
    ELSEIF LOCAL:99 >= PALAMLV:4
        NOWEX:(20 + COUNT) = 1
    ENDIF
REND

IF (NOWEX:20 || NOWEX:21 || NOWEX:22 || NOWEX:23) && NOWEX:41 == 0
    ;焦らし中自慰の判定を各部位ごとに行う
    TFLAG:180 = 0
    TFLAG:181 = 0
    TFLAG:182 = 0
    TFLAG:183 = 0
    LOCAL = -20
    ;自慰が我慢できない素質
    ;プライド低い
    SIF TALENT:17
        LOCAL += 5
    ;自慰しやすい
    SIF TALENT:60
        LOCAL += 10
    ;快感に素直
    SIF TALENT:70
        LOCAL += 2
    ;淫核／淫茎
    SIF TALENT:171
        TFLAG:180 += 5
    ;淫壷
    SIF TALENT:172
        TFLAG:181 += 5
    ;淫尻
    SIF TALENT:173
        TFLAG:182 += 5
    ;淫乳
    SIF TALENT:174
        TFLAG:183 += 5
    ;Ｃ敏感
    SIF TALENT:100
        TFLAG:180 += 3
    ;Ｖ敏感
    SIF TALENT:102
        TFLAG:181 += 3
    ;Ａ敏感
    SIF TALENT:104
        TFLAG:182 += 3
    ;Ｂ敏感
    SIF TALENT:106
        TFLAG:183 += 3
    ;幼稚
    SIF TALENT:131
        LOCAL += 3

    ;サドっ気
    LOCAL -= ABL:20

    ;むしろ我慢するのが好きだったりする素質
    ;気丈
    SIF TALENT:12
        LOCAL -= 3
    ;プライド高い
    SIF TALENT:16
        LOCAL -= 5
    ;好奇心
    SIF TALENT:23
        LOCAL -= 5
    ;快感の否定
    SIF TALENT:71
        LOCAL += 5
    ;倒錯的
    SIF TALENT:80
        LOCAL -= 5
    ;中毒しやすい
    SIF TALENT:84
        LOCAL += 5

    ;マゾっ気
    LOCAL -= ABL:21 * 2
    ;従順
    LOCAL -= ABL:10 * 2
    ;自慰中毒
    LOCAL += ABL:30 * 10

    ;欲情
    IF PALAM:13 >= PALAMLV:9
        LOCAL += 20
    ELSEIF PALAM:13 >= PALAMLV:8
        LOCAL += 16
    ELSEIF PALAM:13 >= PALAMLV:7
        LOCAL += 14
    ELSEIF PALAM:13 >= PALAMLV:6
        LOCAL += 12
    ELSEIF PALAM:13 >= PALAMLV:5
        LOCAL += 10
    ELSEIF PALAM:13 >= PALAMLV:4
        LOCAL += 8
    ELSEIF PALAM:13 >= PALAMLV:3
        LOCAL += 5
    ELSEIF PALAM:13 >= PALAMLV:2
        LOCAL += 2
    ENDIF

    ;寸止め継続回数
    TFLAG:180 += EX:20 * 5 + LOCAL
    TFLAG:181 += EX:21 * 5 + LOCAL
    TFLAG:182 += EX:22 * 5 + LOCAL
    TFLAG:183 += EX:23 * 5 + LOCAL

    ;拘束中は無条件にクリア
    IF TEQUIP:40 || TEQUIP:71
        TFLAG:180 = 0
        TFLAG:181 = 0
        TFLAG:182 = 0
        TFLAG:183 = 0
    ENDIF

    LOCAL:0 = TFLAG:180
    LOCAL:1 = TFLAG:181
    LOCAL:2 = TFLAG:182
    LOCAL:3 = TFLAG:183

    ;絶頂寸止め判定
    REPEAT 4
        SIF NOWEX:(20+COUNT) < 1
            CONTINUE
        IF NOWEX:(20+COUNT) == 9
            TIMES TFLAG:180 , 3.00
            LOCALS:8 = 최강
        ELSEIF NOWEX:(20+COUNT) == 4
            TIMES TFLAG:180 , 2.00
            LOCALS:8 = 초강
        ELSEIF NOWEX:(20+COUNT) == 2
            TIMES TFLAG:180 , 1.50
            LOCALS:8 = 강
        ELSE
            NOWEX:(20+COUNT) = 1
            LOCALS:8 = 
        ENDIF
        LOCAL:90 = RAND:100
        IF LOCAL:COUNT > LOCAL:90
            NOWEX:42 += NOWEX:(20+COUNT)
        ELSE
            EX:(20+COUNT) += NOWEX:(20+COUNT)
            UP:COUNT = 0
            PALAM:COUNT = PALAMLV:4 - 1
            LOCALS:9 = %LOCALS:9 + LOCALS:8 + LOCALS:COUNT%　
        ENDIF
    REND
ENDIF
SIF STRLENS(LOCALS:9) > 0
