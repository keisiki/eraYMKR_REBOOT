@COLLECTION_LIST, ARG
;변수의 리셋
VARSET LOCAL, 0

;ページをめくったときなどはここから
$PRINT_LIST_PAGE
LOCAL:1 = 0
CALL PAGE_COLLECTION_LIST, ARG
LOCAL:5 = RESULT

DRAWLINE
PRINTFORML %COM_NAME% ＜page.{LOCAL:4 + 1}＞
CALL COLLECTION_ROOM, ARG, LOCAL:4
DRAWLINE
PRINTFORMLC \@(LOCAL:4 <= 0) ? %" " * 16% # [1001] 앞페이지\@
PRINTLC [1000] 뒤로
PRINTFORMLC \@(LOCAL:4 >= LOCAL:5) ? %" " * 16% # [1009] 다음페이지\@

$INPUT_LOOP_1
INPUT
IF RESULT == 1000
    RETURN 0
ELSEIF RESULT == 1001 && LOCAL:4 > 0
    LOCAL:4 -= 1
    GOTO PRINT_LIST_PAGE
ELSEIF RESULT == 1009 && LOCAL:4 < LOCAL:5
    LOCAL:4 += 1
    GOTO PRINT_LIST_PAGE
ELSE
    GOTO INPUT_LOOP_1
ENDIF

;--------------------------------------------------
;수집품 일람의 페이지 수 결정용
;--------------------------------------------------
@PAGE_COLLECTION_LIST, ARG
LOCAL = 0
LOCAL:1 = -1
CALLF CLEAR_LIST

;蒐集品が存在していたら表示可能フラグを立てる
;(ここでは条件を満たしていないと弾く処理を行っている)
REPEAT 500
    ;各蒐集品存在フラグ
    SIF DITEMTYPE:(100+COUNT):(100+ARG) == 0
        CONTINUE
    CALLF SET_LIST, COUNT, 1
    LOCAL += 1
    SIF (LOCAL%20) == 1
        LOCAL:1 += 1
REND
LOCAL:1 = MAX(LOCAL:1 , 0)

RETURN LOCAL:1

