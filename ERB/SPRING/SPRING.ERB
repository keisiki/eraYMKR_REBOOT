    SIF LOCAL@SPRING_DUMMY:0 > MONEY
        LOCAL@SPRING_DUMMY:0 = MONEY
    PRINTFORMW 치료비로 {LOCAL@SPRING_DUMMY:0}원을 잃었습니다.
    MONEY -= LOCAL@SPRING_DUMMY:0
    BASE:COUNT:0 = 1
    MARK:COUNT:9 = 3
    SIF ABL:COUNT:10
        ABL:COUNT:10 -= 1
    IF CFLAG:COUNT:1501
