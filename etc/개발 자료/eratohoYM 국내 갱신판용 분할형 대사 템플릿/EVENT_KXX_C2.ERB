@KX1_MESSAGE_COM_290
SELECTCASE TFLAG:99
    ;두 사람 다 장착 가능하고 조교자가 착탈
    CASE 0
        $COM290_KX1_0
        IF TEQUIP:PLAYER:31 == 0
            ;PRINTFORMW 「조교자 콘돔 해제시의 대사입니다」
        ELSE
            ;PRINTFORMW 「조교자 콘돔 장착시의 대사입니다」
        ENDIF
    ;두 사람 다 장착 가능하고 노예가 착탈
    CASE 1
        $COM290_KX1_1
        IF TEQUIP:31 == 0
            ;PRINTFORMW 「노예 콘돔 해제시의 대사입니다」
        ELSE
            ;PRINTFORMW 「노예 콘돔 장착시의 대사입니다」
        ENDIF
    ;조교자만 착탈 가능 또는 노예만 착탈 가능할 경우
    CASE 2
        ;조교자가 착탈
        IF EXIST_PENIS(PLAYER)
            GOTO COM290_KX1_0
        ;노예가 착탈
        ELSE
            GOTO COM290_KX1_1
        ENDIF
ENDSELECT
